<HEAD><TITLE>Japanese   Growth Fund</TITLE>
<SCRIPT LANGUAGE="Javascript">
<!-- Beginning
     
function getCookie(name) {
     var dcookie=document.cookie;
     var cname = name + "=";
     var clen = dcookie.length;
     var cbegin = 0;

       while (cbegin < clen) {
            var vbegin = cbegin + cname.length;
                if(dcookie.substring(cbegin,vbegin) == cname) {
                var vend = dcookie.indexOf (";", vbegin);
                    if(vend == -1) vend = clen;
                    return unescape(dcookie.substring(vbegin,vend));
                   }
                cbegin = dcookie.indexOf(" ",cbegin) + 1;
                if(cbegin == 0) break;
            }
      return null;
} 

var cookieName = "Greenline";

// -->
</SCRIPT></HEAD>
<BODY BGCOLOR="#ffffff">
<TABLE WIDTH="500" BORDER="0" CELLSPACING="0" CELLPADDING="0">

<TR>
<SCRIPT LANGUAGE = "Javascript">
<!-- Beginning
	if(getCookie(cookieName) == "true") {
            document.write('<TD ROWSPAN="3" VALIGN="top"><A HREF="http://www.tdbank.ca/tdbank/Greenline/new/mfns.html"><IMG ALIGN=TOP SRC="http://www.tdbank.ca/tdbank/Greenline/english/gifs/glislogo.gif"  WIDTH="140" HEIGHT="40" BORDER="0"></A></TD>');
           }
// -->
