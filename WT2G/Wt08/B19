       function jumpPage(form,value)
       {
             
               i = form.SelectMenu.selectedIndex;
              
               if (i == 0) return;
               if(value == 1) {
                        window.location.href = url1[i+1];
               }
                if(value == 2) {
                        window.location.href = url2[i+1];
               }
                if(value == 3) {
                        window.location.href = url3[i+1];
               }
                if(value == 4) {
                        window.location.href = url4[i+1];
               }
                if(value == 5) {
                        window.location.href = url5[i+1];
               }
                if(value == 6) {
                        window.location.href = url6[i+1];
               }
                if(value == 7) {
                        window.location.href = url7[i+1];
               }
               if(value == 8) {
                        window.location.href = url8[i+1];
               }
               if(value == 9) {
                        window.location.href = url9[i+1];
               }
               if(value == 10) {
                        window.location.href = url10[i+1];
               }
       }

// end hiding -->
      
