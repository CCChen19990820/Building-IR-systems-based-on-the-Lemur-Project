E-mail:      <INPUT NAME="email" TYPE="text" SIZE="30">
Address1:    <INPUT NAME="address" TYPE="text" SIZE="35">
Address2:    <INPUT NAME="address" TYPE="text" SIZE="35">
City:        <INPUT NAME="phone" TYPE="text" SIZE="35">
