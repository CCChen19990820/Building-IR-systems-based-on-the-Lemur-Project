   <title>LinkFinder Site Submission Form</title>

<SCRIPT LANGUAGE="JavaScript">
<!--

/*  Author's Name:        Andy Augustine
    JavaScript Snippet:	  'mailIt'
    E-mail address:       jspro@inquiry.com
    Original Location:    http://www.inquiry.com/techtips/js_pro/

	Permission granted to freely distribute and use this
	code as	long as this header remains in tact.  
	         (c)1996 Andy Augustine                            

Modifications copyright 1996, 1997 David Crowder                              */

function mailIt(form) {
  var data = document.dataForm
  var userInfo = ""

  userInfo += "Page Title:  " + document.title + "\n"
  userInfo += "Mailed From: " + document.location + "\n\n"

  userInfo += "Revised:  " + data.NewOrOld.checked + "\n"
  userInfo += "Website URL:  " + data.URL.value + "\n"
  userInfo += "Website Name:  " + data.SiteName.value + "\n"
  userInfo += "Subject:  " + data.Subject[data.Subject.selectedIndex].text + "\n"
  userInfo += "New Category:  " + data.NewCategory.value + "\n\nDescription: "

  form.mailBody.value = userInfo + data.body.value + "\n\n" + '<p><a href="' + data.URL.value + '">' + data.SiteName.value + "</a> - " + data.body.value + "\n"
  return true
}
// -->
</SCRIPT>
</HEAD>

