var price=10

function preview(form) {

//create document

str='<HTML><BODY BGCOLOR="FFFFFF"><P>';



str+='<A HREF="';
str+=form.c7.value;
str+='">';
str+=form.c8.value;
str+='</a><BR>';
str+=form.c10.value;
str+='<p><p><center>';
str+='<a href="javascript:self.window.close()"><font size=-1><b>[CLOSE WINDOW]</b></font></a>';
str+='</center></body>';
previewWin=window.open('', 'preview');
previewWin.document.open();
previewWin.document.write(str);
previewWin.document.close();
}

function validateForm(form) {

if (form.elements[9].value=='') {
alert ('Please enter password before submitting this form')
return false
  }

if (form.elements[8].value.length>250) {
alert ('Please limit your description to 25 words')
return false
 }
return true
} 



function checkPrice(form) {

if (validateForm(form)) {


for (var i=3; i<6; i++) {

if (form.elements[i].selectedIndex>0) {
 price+=3;
 }
}



if (price==0) {

form.submit();
}

else
  {
OK=confirm('The charge for your listing will be $'+price+'. Do you want to proceed?')
if (OK) {
form.elements[1].value=price;
form.submit();
    }
 else {
  price=10
     }
  }

 }
} 

