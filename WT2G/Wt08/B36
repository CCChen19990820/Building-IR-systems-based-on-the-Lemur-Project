<!--
var price=20

function validateForm(form) {

if (form.elements[34].value=='') {
alert ('Please enter password before submitting this form')
return false
  }

if (form.elements[33].value.length>250) {
alert ('This listing is limited to 25 words of describtion')
return false
 }
return true
} 


function checkPrice(form) {
if (validateForm(form)) {

for (var i=3; i<6; i++) {
if (form.elements[i].selectedIndex>0) {
  price+=3;
  }
}

for (var i=14; i<27; i+=6) {
if (form.elements[i].value != '') {
  price+=3;
  }
}

for (var i=19; i<32; i+=6) {
if (form.elements[i].value != '') {
  price+=3;
  }
}


OK=confirm('The charge for your listing will be $'+price+'. Proceed?')
if (OK) {
form.elements[1].value=price;
form.submit();
    }
else {
price=20
}
  

 }
} 

