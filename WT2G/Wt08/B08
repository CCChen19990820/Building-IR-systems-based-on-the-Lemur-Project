<TR><TD WIDTH=25%><a href="/cgi-bin/linker3?/pto7/NEW/INDEX+4790782+F">4,790,782</a></TD><TD WIDTH=25%>Dec., 1988</TD><TD WIDTH=25% ALIGN=LEFT>McCormick</TD><TD WIDTH=25% ALIGN=RIGHT> <a href="/CLASSES/440/440.html">440</a>/ 75</TD></TR>
<TR><TD WIDTH=25%><a href="/cgi-bin/linker3?/pto7/NEW/INDEX+4792314+F">4,792,314</a></TD><TD WIDTH=25%>Dec., 1988</TD><TD WIDTH=25% ALIGN=LEFT>McCormick</TD><TD WIDTH=25% ALIGN=RIGHT> <a href="/CLASSES/440/440.html">440</a>/ 81</TD></TR>
<TR><TD WIDTH=25%><a href="/cgi-bin/linker3?/pto7/NEW/INDEX+4793773+F">4,793,773</a></TD><TD WIDTH=25%>Dec., 1988</TD><TD WIDTH=25% ALIGN=LEFT>Kinouchi et al.</TD><TD WIDTH=25% ALIGN=RIGHT> <a href="/CLASSES/416/416.html">416</a>/129A</TD></TR>
<TR><TD WIDTH=25%><a href="/cgi-bin/linker3?/pto7/NEW/INDEX+4795382+F">4,795,382</a></TD><TD WIDTH=25%>Jan., 1989</TD><TD WIDTH=25% ALIGN=LEFT>McCormick</TD><TD WIDTH=25% ALIGN=RIGHT> <a href="/CLASSES/440/440.html">440</a>/ 81</TD></TR>
<TR><TD WIDTH=25%><a href="/cgi-bin/linker3?/pto7/NEW/INDEX+4828518+F">4,828,518</a></TD><TD WIDTH=25%>May, 1989</TD><TD WIDTH=25% ALIGN=LEFT>Kouda et al.</TD><TD WIDTH=25% ALIGN=RIGHT> <a href="/CLASSES/440/440.html">440</a>/ 75</TD></TR>
<TR><TD WIDTH=25%><a href="/cgi-bin/linker3?/pto7/NEW/INDEX+4832570+F">4,832,570</a></TD><TD WIDTH=25%>May, 1989</TD><TD WIDTH=25% ALIGN=LEFT>Solia</TD><TD WIDTH=25% ALIGN=RIGHT> <a href="/CLASSES/440/440.html">440</a>/ 67</TD></TR>
