<HEAD>
<TITLE>Learn Chinese: Introduction</TITLE>
<SCRIPT LANGUAGE="JavaScript">
<!-- Hide JavaScript from Java-Impaired Browsers


function getNavigator(form) {
        if (form.choices.selectedIndex == 0) 
                self.location="basic1pronoun.htm"
        if (form.choices.selectedIndex == 1) 
                self.location="basic2adjective.htm"
        if (form.choices.selectedIndex == 2) 
                self.location="basic3verbs.htm"
        if (form.choices.selectedIndex == 3) 
