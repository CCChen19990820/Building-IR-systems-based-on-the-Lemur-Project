	<TD COLSPAN=2 ROWSPAN=1 WIDTH=335 ALIGN="center" VALIGN="center"><IMG HEIGHT=223 WIDTH=335 SRC="../assets/images/elsinore.jpg"  BORDER=0  ALT="Lake Elsinore Diamond photo" ></TD>
	<TD COLSPAN=2 HEIGHT=223></TD>
   </TR>
   <TR VALIGN="top" ALIGN="left">
	<TD COLSPAN=6 HEIGHT=15></TD>
