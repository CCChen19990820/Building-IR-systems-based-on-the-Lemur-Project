	<TD COLSPAN=3 ROWSPAN=1 WIDTH=320 ALIGN="center" VALIGN="center"><IMG HEIGHT=233 WIDTH=320 SRC="../assets/auto_generated_images/img_5d1195e5.jpg"  BORDER=0  ALT="Richmond Expressway" ></TD>
	<TD COLSPAN=2 HEIGHT=233></TD>
