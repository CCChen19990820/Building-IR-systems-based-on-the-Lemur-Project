   <TR VALIGN="top" ALIGN="left">
	<TD COLSPAN=2 ROWSPAN=1 WIDTH=503 ALIGN="center" VALIGN="center"><IMG HEIGHT=36 WIDTH=503 SRC="../assets/images/hntbtype.gif"  BORDER=0 ></TD>
