	<TD COLSPAN=1 WIDTH=1><IMG SRC="../assets/images/dot_clear.gif" WIDTH=1 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=314><IMG SRC="../assets/images/dot_clear.gif" WIDTH=314 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=1><IMG SRC="../assets/images/dot_clear.gif" WIDTH=1 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=19><IMG SRC="../assets/images/dot_clear.gif" WIDTH=19 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=0><IMG SRC="../assets/images/dot_clear.gif" WIDTH=0 HEIGHT=1 BORDER=0></TD>
