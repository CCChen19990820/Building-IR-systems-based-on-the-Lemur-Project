/* Copyright (C)1996 Q&D Software Development
All Rights Reserved.
*/
function scroll_status (seed)
{
    var msg = "Let us know how you feel. If you choose to send a public message and we feel it is of interest to Stormfront readers, it will be published within a day or two.";
    var out = " ";
    var c = 1;
    if (150 < seed) {
            seed--;
            var cmd="scroll_status(" + seed + ")";
            timerTwo = window.setTimeout(cmd, 25);
    }
    else if (seed <= 150 && 0 < seed) {
            for (c=0 ; c < seed ; c++) {
                    out+=" ";
            }
            out+=msg;
            seed--;
            var cmd="scroll_status(" + seed + ")";
                window.status=out;
            timerTwo=window.setTimeout(cmd,25);
    }
    else if (seed <= 0) {
            if (-seed < msg.length) {
                    out+=msg.substring(-seed,msg.length);
                    seed--;
            var cmd="scroll_status(" + seed + ")";
                    window.status=out;
                    timerTwo=window.setTimeout(cmd,25);
            }
            else {
                    window.status=" ";
                    timerTwo=window.setTimeout("scroll_status(150)",25);
            }
    }
}
// -- End of JavaScript code -------------- -->

