<TR>
<TH ><FONT SIZE=2>Source/<BR> Receptor<BR> Area<BR> No.</FONT> </TH>
<TH ><FONT SIZE=2>Location<BR> of<BR> Air Monitoring<BR> Station</FONT> </TH>
<TH ><FONT SIZE=2>No.<BR> Days<BR> of<BR> Data</FONT> </TH>
<TH ><FONT SIZE=2>Max.<BR> Conc.<BR> in &#181;g/m3<BR> 24-hour</FONT> </TH>
<TH WIDTH=82><U><FONT SIZE=2>Federal<BR> </FONT></U><FONT SIZE=2>&gt;150 &#181;g/m3<BR> 24-hour</FONT> </TH>
<TH WIDTH=78><U><FONT SIZE=2>State<BR> </FONT></U><FONT SIZE=2>&gt;50 &#181;g/m3<BR> 24-hour</FONT> </TH>
<TH WIDTH=97><FONT SIZE=2>AAM<BR> Conc.<BR> &#181;g/m3</FONT> </TH>
<TH WIDTH=55><FONT SIZE=2>AGM<BR> Conc.<BR> &#181;g/m3</FONT> </TH>
</TR>

<TR><TD ></TD><TD ></TD><TD ></TD><TD >
</TD><TD WIDTH=82></TD><TD WIDTH=78></TD><TD WIDTH=97></TD><TD WIDTH=55>
</TD></TR>

