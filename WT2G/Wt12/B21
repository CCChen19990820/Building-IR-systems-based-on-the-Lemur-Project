     function MakeArray(size) {
         this.length = size;
         for(var i = 1; i <= size; i++) {
             this[i] = 0;
         }
         return this;
     }

     function insertRandomImage() {
          d = new Date();s = d.getSeconds();
          idx = s % rndImages;idx++;
          document.write(rndImageArray[idx]);
