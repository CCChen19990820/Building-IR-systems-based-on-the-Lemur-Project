&nbsp;&nbsp;rule&nbsp;char_seq_append_full_char&nbsp;(s,b,chsz)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;s&nbsp;*?&nbsp;-{0,&nbsp;1}-<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(len&nbsp;cseq!current_char(s,chsz))&nbsp;&gt;=&nbsp;chsz+1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;cseq!char_seq(s&nbsp;&nbsp;.&lt;b&gt;.,chsz)&nbsp;<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;if&nbsp;b&nbsp;=&nbsp;stopbit&nbsp;then&nbsp;<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;cseq!char_seq(s,chsz)&nbsp;&nbsp;.&lt;tail&nbsp;cseq!current_char(s,chsz)&gt;.<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;else&nbsp;cseq!char_seq(s,chsz)&nbsp;end&nbsp;if<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;char_seq_append_full_char;<BR>
&nbsp;&nbsp;INDUCT&nbsp;cseq!CHAR_SEQ&nbsp;(S,&nbsp;CHSZ);<BR>
