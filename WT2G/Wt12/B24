<B>&lt;</B><I>getm</I><B>&gt;</B><B>[</B>209<B>]</B><A NAME=14590>&#160;</A><B>M</B><B>=</B> <BR><TT>&nbsp;</TT> <TT><BR> begin{verbatim}<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;reset;<BR>
&nbsp;&nbsp;set&nbsp;library&nbsp;&quot;/vereng/app/seves.lib/&quot;;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;load&nbsp;setrules;<BR>
&nbsp;&nbsp;load&nbsp;nat;<BR>
&nbsp;&nbsp;load&nbsp;rel;<BR>
&nbsp;&nbsp;load&nbsp;tr;<BR>
&nbsp;&nbsp;load&nbsp;pr;<BR>
&nbsp;&nbsp;load&nbsp;fpt;<BR>
&nbsp;&nbsp;load&nbsp;nset;<BR>
&nbsp;&nbsp;load&nbsp;cseq;<BR>
&nbsp;&nbsp;load&nbsp;fn;<BR>
&nbsp;&nbsp;load&nbsp;reqs;<BR>
&nbsp;&nbsp;load&nbsp;defs;<BR>
&nbsp;&nbsp;load&nbsp;bfr;<BR>
&nbsp;&nbsp;load&nbsp;mach;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;function&nbsp;Get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;reqs!seq_buff_alpha&nbsp;({inbit},&nbsp;-{0,&nbsp;1}-,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;++&nbsp;reqs!seq_buff_alpha&nbsp;({mid},&nbsp;cseq!char_set(chsz),&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;Get_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;restrict_Get_alpha&nbsp;(tr1,&nbsp;chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;null&nbsp;tr1&nbsp;|&nbsp;get_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;&nbsp;null&nbsp;tr1&nbsp;|&nbsp;reqs!buffer_alpha&nbsp;({inbit},&nbsp;-{0,&nbsp;1}-)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;null&nbsp;tr1&nbsp;|&nbsp;reqs!buffer_alpha&nbsp;({mid},&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;null&nbsp;tr1&nbsp;|&nbsp;{tick})<BR>
&nbsp;&nbsp;end&nbsp;restrict_Get_alpha;<BR>
&nbsp;&nbsp;induct&nbsp;tr1&nbsp;|&nbsp;get_alpha&nbsp;(chsz,tick);<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;tick_in_get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;tick&nbsp;in&nbsp;get_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;tick_in_Get_alpha;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;Get_alpha_sub_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;get_alpha&nbsp;(chsz,tick)&nbsp;&lt;&lt;&nbsp;defs!rep_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;Get_alpha_sub_rep_alpha;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(defs!rep_alpha,&nbsp;Get_alpha,&nbsp;defs!rep_alpha_ext,&nbsp;nset!in_two_set)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_in_bit_get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(inbit,&nbsp;get_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buff_map&nbsp;(inbit,&nbsp;nset!two_set&nbsp;(0,&nbsp;1))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_in_bit_Get_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_mid_get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(mid,&nbsp;get_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buff_map&nbsp;(mid,&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_mid_Get_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_msgs_in_bit_get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs&nbsp;(inbit,&nbsp;get_alpha&nbsp;(chsz,tick))&nbsp;=&nbsp;nset!two_set&nbsp;(0,&nbsp;1)<BR>
&nbsp;&nbsp;end&nbsp;select_msgs_in_bit_Get_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_msgs_mid_get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs&nbsp;(mid,&nbsp;get_alpha&nbsp;(chsz,tick))&nbsp;=&nbsp;cseq!char_set(chsz)<BR>
&nbsp;&nbsp;end&nbsp;select_msgs_mid_Get_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;make_comm_mid_Get_alpha&nbsp;(x,&nbsp;chsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chan&nbsp;x&nbsp;=&nbsp;mid<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;x&nbsp;in&nbsp;get_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;x&nbsp;=&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;iscomm&nbsp;x<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;msg&nbsp;x&nbsp;in&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;end&nbsp;make_comm_mid_Get_alpha;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;make_comm_in_bit_Get_alpha&nbsp;(x,&nbsp;chsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chan&nbsp;x&nbsp;=&nbsp;inbit<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;x&nbsp;in&nbsp;get_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;x&nbsp;=&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;iscomm&nbsp;x<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;msg&nbsp;x&nbsp;in&nbsp;nset!two_set&nbsp;(0,&nbsp;1))<BR>
&nbsp;&nbsp;end&nbsp;make_comm_in_bit_Get_alpha;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;tail_append_not_null&nbsp;(s1,&nbsp;s2)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;null&nbsp;s1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;istrace&nbsp;s2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;(tail&nbsp;s1)&nbsp;&nbsp;s2&nbsp;=&nbsp;tail&nbsp;(s1&nbsp;&nbsp;s2)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;tail_append_not_null;<BR>
&nbsp;&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;</TT>
