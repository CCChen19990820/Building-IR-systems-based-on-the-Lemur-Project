&nbsp;&nbsp;INVOKE&nbsp;cseq!CURRENT_CHAR&nbsp;(S,&nbsp;CHSZ);<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;Get_not_over_capacity_InOutchar_lemma1(b,cchr_0,cchr,tr1_0,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tr1,chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;b&nbsp;in&nbsp;-{0,&nbsp;1}-<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;InOutchar_pre(cchr_0,tr1_0,chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;valid_Inchar(cchr_0,cchr,tr1_0,tr1,chsz,tick)<BR>
