&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs(outbit,&nbsp;Put_alpha(chsz,tick))&nbsp;=&nbsp;-{0,&nbsp;1}-<BR>
