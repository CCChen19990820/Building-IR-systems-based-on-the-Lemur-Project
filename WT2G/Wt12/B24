<B>&lt;</B><I>Get Concurrent Restriction Condition</I><B>&gt;</B><B>[</B><A HREF="node77.html#maclab52">52</A><B>]</B><A NAME=15519>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;invoke&nbsp;reqs!concurrent_restriction_condition_conjunct;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;with&nbsp;disabled&nbsp;(tr!subseq)&nbsp;reduce;<BR>
&nbsp;&nbsp;split<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;null&nbsp;tr1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;tick&nbsp;=&nbsp;last&nbsp;tr1;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;invoke&nbsp;get!Get_not_over_capacity;<BR>
&nbsp;&nbsp;invoke&nbsp;get!valid_char_get;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$0&nbsp;=&nbsp;tr2&nbsp;|&nbsp;get!Get_alpha&nbsp;(chsz,tick),&nbsp;tr3&nbsp;=&nbsp;tr2;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;invoke&nbsp;get!Get_not_over_capacity;<BR>
&nbsp;&nbsp;invoke&nbsp;get!valid_char_get;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;tr1&nbsp;|=&nbsp;mid;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$0&nbsp;=&nbsp;tr2&nbsp;|&nbsp;get!Get_alpha&nbsp;(chsz,tick),&nbsp;tr3$0&nbsp;=&nbsp;tr2;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr3$1&nbsp;=&nbsp;cseq!char_seq&nbsp;(tr1&nbsp;|=&nbsp;inbit,&nbsp;chsz);<BR>
&nbsp;&nbsp;instantiate&nbsp;tr3$2&nbsp;=&nbsp;cseq!char_seq&nbsp;(tr1&nbsp;|=&nbsp;inbit,&nbsp;chsz);<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr3$0=CSEQ!CHAR_SEQ&nbsp;(TR1&nbsp;|=&nbsp;INBIT,&nbsp;CHSZ);<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Put Concurrent Restriction Condition</I><B>&gt;</B><B>[</B><A HREF="node77.html#maclab53">53</A><B>]</B><A NAME=15528>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;invoke&nbsp;reqs!concurrent_restriction_condition_conjunct;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;with&nbsp;disabled&nbsp;(tr!subseq)&nbsp;reduce;<BR>
&nbsp;&nbsp;split<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;null&nbsp;tr1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;tick&nbsp;=&nbsp;last&nbsp;tr1;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;invoke&nbsp;put!valid_char_put;<BR>
&nbsp;&nbsp;invoke&nbsp;put!put_not_over_capacity;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$0&nbsp;=&nbsp;tr2&nbsp;|&nbsp;put!put_alpha&nbsp;(chsz,tick),&nbsp;tr3&nbsp;=&nbsp;tr2;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;invoke&nbsp;put!valid_char_put;<BR>
