&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_Put_step(cbuff&nbsp;&nbsp;.&lt;startbit,stopbit&gt;.,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;.&lt;mid&nbsp;.&nbsp;.&lt;&gt;.&gt;.,chsz,&nbsp;buffsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Put_step_tack_mid_trace_char_empty;<BR>
&nbsp;&nbsp;use&nbsp;valid_Put_step_tack_mid&nbsp;cbuff=cbuff,tr1=.&lt;&gt;.,chsz=chsz,buffsz=buffsz,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tick=tick,chr=.&lt;&gt;.;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;valid_Put_step_tack_mid_cbuff_trace_empty(chsz,&nbsp;buffsz,tick,chr)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;buffsz*(chsz+2)&nbsp;&gt;=&nbsp;chsz&nbsp;+&nbsp;2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char(chr,chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;valid_Put_step(.&lt;&gt;.,.&lt;&gt;.,chsz,&nbsp;buffsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_Put_step(startbit&nbsp;]-&nbsp;(chr&nbsp;&nbsp;.&lt;stopbit&gt;.),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;.&lt;mid.chr&gt;.,chsz,&nbsp;buffsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Put_step_tack_mid_cbuff_trace_empty;<BR>
&nbsp;&nbsp;use&nbsp;valid_Put_step_tack_mid_cbuff_empty&nbsp;tr1=.&lt;&gt;.,chsz=chsz,buffsz=buffsz,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tick=tick,chr=chr;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;valid_Put_step_tack_mid_cbuff_trace_char_empty(chsz,&nbsp;buffsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;buffsz*(chsz+2)&nbsp;&gt;=&nbsp;chsz&nbsp;+&nbsp;2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char(.&lt;&gt;.,chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;valid_Put_step(.&lt;&gt;.,.&lt;&gt;.,chsz,&nbsp;buffsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_Put_step(.&lt;startbit,stopbit&gt;.,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;.&lt;mid&nbsp;.&nbsp;.&lt;&gt;.&gt;.,chsz,&nbsp;buffsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Put_step_tack_mid_cbuff_trace_char_empty;<BR>
&nbsp;&nbsp;use&nbsp;valid_Put_step_tack_mid_cbuff_trace_empty&nbsp;chsz=chsz,buffsz=buffsz,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tick=tick,chr=.&lt;&gt;.;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>valid_Put_step lemma <BR> htmlref98maclab98</I><B>&gt;</B><B>[</B><A HREF="node106.html#maclab98">98</A><B>]</B><A NAME=15033>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;b;<BR>
&nbsp;&nbsp;invoke&nbsp;valid_Put_step;<BR>
&nbsp;&nbsp;invoke&nbsp;invariant_over_buffer;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;apply&nbsp;tr!inc_mod_def;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$0&nbsp;=&nbsp;tr2;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;instantiate&nbsp;cbuff2$1&nbsp;=&nbsp;cbuff2;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;tr2;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;split&nbsp;tr!remove_first_n&nbsp;(tr2,&nbsp;len&nbsp;tr1)&nbsp;=&nbsp;.&lt;outbit&nbsp;.&nbsp;head&nbsp;cbuff&gt;.;<BR>
&nbsp;&nbsp;equality&nbsp;substitute;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;instantiate&nbsp;cbuff2$0&nbsp;=&nbsp;tail&nbsp;cbuff;<BR>
