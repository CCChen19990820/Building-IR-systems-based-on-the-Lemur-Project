<SCRIPT LANGUAGE = "JavaScript">
<!-- Begin to hide script content from old browsers.


version = 0;
browserName = navigator.appName;
browserVer = parseInt(navigator.appVersion);


if (browserName == "Netscape" && browserVer >= 3) {
	version = 3;
}
else if (browserName == "Netscape" && browserVer == 2) {
	version = 2;
}
else if (browserName == "Microsoft Internet Explorer" && browserVer >= 3) {
	version = 1;
}

if (version == 3) {
  info0 = new Image(13, 13);
  info0.src = "/newpage/tops/infobuttb.gif"; // onMouseOff image

  info1 = new Image(13, 13);
  info1.src = "/newpage/tops/infobuttw.gif";// onMouseOver image
//
  nefsc0 = new Image(13, 13);
  nefsc0.src = "/newpage/tops/nefscbuttb.gif"; // onMouseOff image

  nefsc1 = new Image(13, 13);
  nefsc1.src = "/newpage/tops/nefscbuttw.gif";// onMouseOver image
//
  faq0 = new Image(13, 13);
  faq0.src = "/newpage/tops/faqbuttb.gif"; // onMouseOff image

  faq1 = new Image(13, 13);
  faq1.src = "/newpage/tops/faqbuttw.gif";// onMouseOver image
//
  history0 = new Image(13, 13);
  history0.src = "/newpage/tops/histbuttw.gif"; // onMouseOff image

  history1 = new Image(13, 13);
  history1.src = "/newpage/tops/histbuttw.gif";//  onMouseOver image
//
  www0 = new Image(13, 13);
  www0.src = "/newpage/tops/wwwbuttb.gif"; // onMouseOff image

  www1 = new Image(13, 13);
  www1.src = "/newpage/tops/wwwbuttw.gif";// onMouseOver image
//
  woodshole0 = new Image(13, 13);
  woodshole0.src = "/newpage/tops/whbuttb.gif"; // onMouseOff image

  woodshole1 = new Image(13, 13);
  woodshole1.src = "/newpage/tops/whbuttw.gif";//  onMouseOver image
}

function change_info_0() {
  if (version == 3){
    document.images[2].src = info0.src; // [0] changes first image in document
  }
}

function change_info_1() {
  if (version == 3){
    document.images[2].src = info1.src; // [0] changes first image in document
  }
}

function change_nefsc_0() {
  if (version == 3){
    document.images[3].src = nefsc0.src;  // [1] changes second image in document
  }
}

function change_nefsc_1() {
  if (version == 3){
    document.images[3].src = nefsc1.src; // [1] changes second image in document
  }
}

function change_faq_0() {
  if (version == 3){
    document.images[4].src = faq0.src; // [2] changes third image in document
  }
}

function change_faq_1() {
  if (version == 3){
    document.images[4].src = faq1.src; // [2] changes third image in document
  }
}

function change_history_0() {
  if (version == 3){
    document.images[5].src = history0.src;  // [3] changes fourth image in document
  }
}

function change_history_1() {
  if (version == 3){
    document.images[5].src = history1.src;  // [3] changes fourth image in document
  }
}

function change_www_0() {
  if (version == 3){
    document.images[6].src = www0.src; // [2] changes third image in document
  }
}

function change_www_1() {
  if (version == 3){
    document.images[6].src = www1.src; // [2] changes third image in document
  }
}

function change_woodshole_0() {
  if (version == 3){
    document.images[7].src = woodshole0.src;  // [3] changes fourth image in document
  }
}

function change_woodshole_1() {
  if (version == 3){
    document.images[7].src = woodshole1.src;  // [3] changes fourth image in document
  }
}


// End the hiding here . -->
</SCRIPT>

