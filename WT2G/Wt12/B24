<H1><A NAME="SECTION064110000000000000000">Model Unit for put</A></H1>
<P>
<P>
<A NAME="maclab212">&#160;</A>
<B>putm.sv</B><B>[</B>212<B>]</B><B>=</B> <BR><TT>&nbsp;</TT><B>&lt;</B><I>putm</I><B>&gt;</B><B>[</B>213<B>]</B><A NAME=14974>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT>
<P>
This macro is attached to an output file.<P>
<P>
<P>
<P>
<P>
<A NAME="maclab213">&#160;</A>
<B>&lt;</B><I>putm</I><B>&gt;</B><B>[</B>213<B>]</B><A NAME=14982>&#160;</A><B>M</B><B>=</B> <BR><TT>&nbsp;</TT> <TT><BR> begin{verbatim}<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;reset;<BR>
&nbsp;&nbsp;set&nbsp;library&nbsp;&quot;/vereng/app/seves.lib/&quot;;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;load&nbsp;setrules;<BR>
&nbsp;&nbsp;load&nbsp;tr;<BR>
&nbsp;&nbsp;load&nbsp;pr;<BR>
&nbsp;&nbsp;load&nbsp;cseq;<BR>
&nbsp;&nbsp;load&nbsp;fn;<BR>
&nbsp;&nbsp;load&nbsp;nset;<BR>
&nbsp;&nbsp;load&nbsp;reqs;<BR>
&nbsp;&nbsp;load&nbsp;defs;<BR>
&nbsp;&nbsp;load&nbsp;mach;<BR>
&nbsp;&nbsp;load&nbsp;bfr;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;function&nbsp;Put_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;reqs!seq_buff_alpha({outbit},&nbsp;-{0,&nbsp;1}-,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;++&nbsp;reqs!seq_buff_alpha({mid},&nbsp;cseq!char_set(chsz),&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;Put_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;restrict_Put_alpha(tr1,&nbsp;chsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;null&nbsp;tr1&nbsp;|&nbsp;Put_alpha(chsz,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;&nbsp;null&nbsp;tr1&nbsp;|&nbsp;reqs!buffer_alpha({outbit},&nbsp;-{0,&nbsp;1}-)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;null&nbsp;tr1&nbsp;|&nbsp;reqs!buffer_alpha({mid},&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;null&nbsp;tr1&nbsp;|&nbsp;{tick})<BR>
&nbsp;&nbsp;end&nbsp;restrict_Put_alpha;<BR>
&nbsp;&nbsp;induct;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Put_alpha)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;tick_in_Put_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;tick&nbsp;in&nbsp;Put_alpha(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;tick_in_Put_alpha;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Put_alpha)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;Put_alpha_sub_rep_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;Put_alpha(chsz,tick)&nbsp;&lt;&lt;&nbsp;defs!rep_alpha(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;Put_alpha_sub_rep_alpha;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Put_alpha,&nbsp;defs!rep_alpha,&nbsp;defs!rep_alpha_ext,&nbsp;nset!in_two_set)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_out_bit_Put_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;pr!is_comm(tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm(outbit,&nbsp;Put_alpha(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buff_map(outbit,&nbsp;-{0,&nbsp;1}-)<BR>
&nbsp;&nbsp;end&nbsp;select_comm_out_bit_Put_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Put_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_mid_Put_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm(mid,&nbsp;Put_alpha(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buff_map(mid,&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_mid_Put_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Put_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
