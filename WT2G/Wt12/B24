<P>
<A NAME="maclab219">&#160;</A>
<B>&lt;</B><I>reps</I><B>&gt;</B><B>[</B>219<B>]</B><A NAME=15338>&#160;</A><B>M</B><B>=</B> <BR><TT>&nbsp;</TT> <TT></TT><TT><BR>
&nbsp;&nbsp;reset;<BR>
&nbsp;&nbsp;set&nbsp;library&nbsp;&quot;/vereng/app/seves.lib/&quot;;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;load&nbsp;setrules;<BR>
&nbsp;&nbsp;load&nbsp;tr;<BR>
&nbsp;&nbsp;load&nbsp;pr;<BR>
&nbsp;&nbsp;load&nbsp;cseq;<BR>
&nbsp;&nbsp;load&nbsp;fn;<BR>
&nbsp;&nbsp;load&nbsp;nset;<BR>
&nbsp;&nbsp;load&nbsp;reqs;<BR>
&nbsp;&nbsp;load&nbsp;defs;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Definition of no_over_flow</I><B>&gt;</B><B>[</B><A HREF="node41.html#maclab18">18</A><B>]</B><A NAME=15349>&#160;</A> <TT>)<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Definition of no_error_condition</I><B>&gt;</B><B>[</B><A HREF="node41.html#maclab17">17</A><B>]</B><A NAME=15358>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;in_no_error_condition&nbsp;(x,&nbsp;a,&nbsp;chsz,&nbsp;buffsz)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;x&nbsp;in&nbsp;no_error_condition&nbsp;(a,&nbsp;chsz,&nbsp;buffsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;&nbsp;pair!is_pair&nbsp;(x)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;pair!fst&nbsp;(x)&nbsp;in&nbsp;a&nbsp;*<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;pair!snd&nbsp;(x)&nbsp;=&nbsp;no_over_flow&nbsp;(pair!fst&nbsp;(x),&nbsp;chsz,&nbsp;buffsz))<BR>
&nbsp;&nbsp;end&nbsp;in_no_error_condition;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;dom_no_error_condition&nbsp;(a,&nbsp;chsz,&nbsp;buffsz)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;rel!dom&nbsp;(no_error_condition&nbsp;(a,&nbsp;chsz,&nbsp;buffsz))&nbsp;=&nbsp;a&nbsp;*<BR>
&nbsp;&nbsp;end&nbsp;dom_no_error_condition;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;no_error_condition_is_function&nbsp;(a,&nbsp;chsz,&nbsp;buffsz)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;fn!is_function&nbsp;(no_error_condition&nbsp;(a,&nbsp;chsz,&nbsp;buffsz))&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;end&nbsp;no_error_conditiois_function;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;apply_no_error_condition&nbsp;(x,&nbsp;a,&nbsp;chsz,&nbsp;buffsz)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;x&nbsp;in&nbsp;a&nbsp;*<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;fn!apply&nbsp;(no_error_condition&nbsp;(a,&nbsp;chsz,&nbsp;buffsz),&nbsp;x)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;no_over_flow&nbsp;(x,&nbsp;chsz,&nbsp;buffsz)<BR>
&nbsp;&nbsp;end&nbsp;apply_no_error_condition;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Definition of valid_input_chars</I><B>&gt;</B><B>[</B><A HREF="node41.html#maclab14">14</A><B>]</B><A NAME=15367>&#160;</A> <TT>)<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Constraints on Rptr's trace</I><B>&gt;</B><B>[</B><A HREF="node38.html#maclab9">9</A><B>]</B><A NAME=15376>&#160;</A> <TT>)<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>valid_relay</I><B>&gt;</B><B>[</B><A HREF="node38.html#maclab8">8</A><B>]</B><A NAME=15385>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Definition Stub of Rptr</I><B>&gt;</B><B>[</B><A HREF="node37.html#maclab1">maclab1</A><B>]</B><A NAME=15394>&#160;</A> <TT></TT><TT><BR>
