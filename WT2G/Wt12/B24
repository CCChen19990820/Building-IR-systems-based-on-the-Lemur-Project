<B>&lt;</B><I>Definition of Rptr</I><B>&gt;</B><B>[</B><A HREF="node65.html#maclab50">50</A><B>]</B><A NAME=15501>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_inbit_get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(inbit,&nbsp;get!Get_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buffer_alpha&nbsp;({inbit},&nbsp;-{0,&nbsp;1}-)<BR>
&nbsp;&nbsp;end&nbsp;select_comm_inbit_get_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(get!Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_mid_get_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(mid,&nbsp;get!Get_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buffer_alpha&nbsp;({mid},&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_mid_get_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(get!Get_alpha,&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;rep_alpha_ext_eq_alphabet_set_div&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;defs!rep_alpha_ext&nbsp;(tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;tick&nbsp;adj&nbsp;get!Get_alpha&nbsp;(chsz,tick)&nbsp;//&nbsp;put!put_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;rep_alpha_ext_eq_alphabet_set_div;<BR>
&nbsp;&nbsp;invoke&nbsp;defs!rep_alpha_ext;<BR>
&nbsp;&nbsp;invoke&nbsp;get!Get_alpha;<BR>
&nbsp;&nbsp;invoke&nbsp;put!put_alpha;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(nset!in_two_set)&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(nset!in_two_set)&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;rep_alpha_ext_eq_alphabet_set_div_rule&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;tick&nbsp;adj&nbsp;get!Get_alpha&nbsp;(chsz,tick)&nbsp;//&nbsp;put!put_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;defs!rep_alpha_ext&nbsp;(tick)<BR>
&nbsp;&nbsp;end&nbsp;rep_alpha_ext_eq_alphabet_set_div_rule;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(defs!rep_alpha_ext,&nbsp;get!Get_alpha,&nbsp;put!put_alpha,&nbsp;nset!in_two_set)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;rep_alpha_eq_alphabet_union&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;defs!rep_alpha(chsz,tick)&nbsp;=&nbsp;get!Get_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;++&nbsp;put!put_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;rep_alpha_eq_alphabet_union;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(defs!rep_alpha,&nbsp;defs!rep_alpha_ext,&nbsp;get!Get_alpha,&nbsp;put!put_alpha,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;rep_alpha_eq_alphabet_union_rule&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;get!Get_alpha&nbsp;(chsz,tick)&nbsp;++&nbsp;put!put_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;defs!rep_alpha(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;rep_alpha_eq_alphabet_union_rule;<BR>
&nbsp;&nbsp;apply&nbsp;setrules!extensionality;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(defs!rep_alpha,&nbsp;defs!rep_alpha_ext,&nbsp;get!Get_alpha,&nbsp;put!put_alpha,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;nset!in_two_set)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;filter_req_cond_no_error_condition&nbsp;(chsz,&nbsp;buffsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;buffsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;tr!filter_req_cond&nbsp;(no_error_condition&nbsp;(defs!rep_alpha(chsz,tick),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz,&nbsp;buffsz),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;defs!rep_alpha(chsz,tick),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;defs!rep_alpha_ext&nbsp;(tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;end&nbsp;filter_req_cond_no_error_condition;<BR>
&nbsp;&nbsp;invoke&nbsp;tr!filter_req_cond;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;split&nbsp;no_over_flow&nbsp;(tr2,&nbsp;chsz,&nbsp;buffsz);<BR>
&nbsp;&nbsp;simplify;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(no_over_flow)&nbsp;reduce;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$1&nbsp;=&nbsp;tr3;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;tr2$0;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(no_over_flow)&nbsp;reduce;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$1&nbsp;=&nbsp;tr2$0&nbsp;|&nbsp;defs!rep_alpha_ext&nbsp;(tick),&nbsp;tr3&nbsp;=&nbsp;tr2$0;<BR>
&nbsp;&nbsp;split&nbsp;null&nbsp;tr2;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;filter_cond_no_error_condition&nbsp;(tr1,&nbsp;chsz,&nbsp;buffsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;buffsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;tr1&nbsp;*?&nbsp;defs!rep_alpha(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;get!Get_not_over_capacity&nbsp;(tr1,&nbsp;chsz,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;put!Put_not_over_capacity&nbsp;(tr1,&nbsp;chsz,&nbsp;buffsz,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!filter_cond&nbsp;(tr1,&nbsp;no_error_condition&nbsp;(defs!rep_alpha(chsz,tick),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz,&nbsp;buffsz))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;end&nbsp;filter_cond_no_error_condition;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(get!Get_not_over_capacity,&nbsp;put!put_not_over_capacity,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;pr!filter_cond,&nbsp;no_over_flow)&nbsp;reduce;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;use&nbsp;tr!subseq_transitivity&nbsp;s&nbsp;=&nbsp;tr2,&nbsp;t&nbsp;=&nbsp;seq1,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;u&nbsp;=&nbsp;tr1,&nbsp;a&nbsp;=&nbsp;defs!rep_alpha(chsz,tick);<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$0&nbsp;=&nbsp;tr2;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$1&nbsp;=&nbsp;tr2;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Rptr Compose Restriction Condition</I><B>&gt;</B><B>[</B><A HREF="node77.html#maclab51">51</A><B>]</B><A NAME=15510>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;invoke&nbsp;reqs!compose_restriction_condition;<BR>
&nbsp;&nbsp;with&nbsp;disabled&nbsp;(pr!set_div,&nbsp;tr!subseq,&nbsp;union'definition)&nbsp;reduce;<BR>
&nbsp;&nbsp;invoke&nbsp;valid_relay_spec;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;invoke&nbsp;valid_input_chars;<BR>
&nbsp;&nbsp;rewrite;<BR>
