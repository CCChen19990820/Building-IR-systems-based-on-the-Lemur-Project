<B>&lt;</B><I>Definition of valid_char_Put</I><B>&gt;</B><B>[</B><A HREF="node57.html#maclab46">46</A><B>]</B><A NAME=15078>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;function&nbsp;char_seq_tail_help(s1,&nbsp;s2,&nbsp;chsz)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;measure&nbsp;len&nbsp;s1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;if&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;null&nbsp;s1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;head&nbsp;s1&nbsp;=&nbsp;startbit<BR>
&nbsp;&nbsp;&nbsp;&nbsp;then&nbsp;char_seq_tail_help(tail&nbsp;s1,&nbsp;s2,&nbsp;chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;elseif&nbsp;cseq!has_char(s1,&nbsp;chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;then&nbsp;char_seq_tail_help(cseq!char_tail(s1,&nbsp;chsz),<BR>
&nbsp;&nbsp;v&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tail&nbsp;s2,&nbsp;chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;elseif&nbsp;not&nbsp;null&nbsp;s1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;then&nbsp;char_seq_tail_help(cseq!char_tail(s1,&nbsp;chsz),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tail&nbsp;s2,&nbsp;chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;else&nbsp;true&nbsp;end&nbsp;if<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;char_seq_tail_help;<BR>
&nbsp;&nbsp;reduce;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;len_char_seq_bit_seq_relation_lemma(bs,cs,chsz)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;bs&nbsp;*?&nbsp;-{0,1}-<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char_seq(cs,&nbsp;chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;bs&nbsp;.&lt;=.&nbsp;cseq!flatten(cs,chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;((len&nbsp;cseq!char_seq(bs,chsz))*(chsz+2))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;((len&nbsp;bs)&nbsp;-&nbsp;((len&nbsp;bs)&nbsp;mod&nbsp;(chsz&nbsp;+&nbsp;2)))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;len_char_seq_bit_seq_relation_lemma;<BR>
&nbsp;&nbsp;induct&nbsp;char_seq_tail_help(bs,&nbsp;cs,&nbsp;chsz);<BR>
&nbsp;&nbsp;invoke&nbsp;(len&nbsp;bs)&nbsp;mod&nbsp;(chsz&nbsp;+&nbsp;2);<BR>
&nbsp;&nbsp;invoke&nbsp;cseq!char_seq(bs,&nbsp;chsz);<BR>
&nbsp;&nbsp;split&nbsp;null&nbsp;cs;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;split&nbsp;len&nbsp;bs&nbsp;&gt;=&nbsp;chsz&nbsp;+&nbsp;2;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;len_char_seq_bit_seq_relation&nbsp;(tr1,chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;tr1&nbsp;*?&nbsp;put_alpha(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char_seq(tr1&nbsp;|=&nbsp;mid,&nbsp;chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(tr1&nbsp;|=&nbsp;outbit)&nbsp;.&lt;=.&nbsp;cseq!flatten(tr1&nbsp;|=&nbsp;mid,chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;((len&nbsp;cseq!char_seq(tr1&nbsp;|=&nbsp;outbit,chsz))*(chsz+2))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;((len&nbsp;tr1&nbsp;|=&nbsp;outbit)&nbsp;-&nbsp;((len&nbsp;tr1&nbsp;|=&nbsp;outbit)&nbsp;mod&nbsp;(chsz+2)))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;len_char_seq_bit_seq_relation;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Put lemma <BR> htmlref94maclab94</I><B>&gt;</B><B>[</B><A HREF="node105.html#maclab94">94</A><B>]</B><A NAME=15091>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;invoke&nbsp;valid_Put_step;<BR>
&nbsp;&nbsp;invoke&nbsp;invariant_over_buffer;<BR>
&nbsp;&nbsp;invoke&nbsp;Put_not_over_capacity;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;split&nbsp;null&nbsp;tr1;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$2&nbsp;=&nbsp;tr2$0;<BR>
&nbsp;&nbsp;use&nbsp;tr!subseq_transitivity&nbsp;s&nbsp;=&nbsp;tr2$0,&nbsp;t&nbsp;=&nbsp;tr2,&nbsp;u&nbsp;=&nbsp;tr1,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;a&nbsp;=&nbsp;Put_alpha(chsz,tick);<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;split<BR>
&nbsp;&nbsp;&nbsp;&nbsp;(buffsz&nbsp;+&nbsp;(len&nbsp;cseq!char_seq(tr2$0&nbsp;|=&nbsp;outbit,&nbsp;chsz)))&nbsp;*&nbsp;(chsz&nbsp;+&nbsp;2)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&gt;=&nbsp;(len&nbsp;tr2$0&nbsp;|=&nbsp;mid)&nbsp;*&nbsp;(chsz&nbsp;+&nbsp;2);<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;apply&nbsp;tr!ge_monotonic;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;use&nbsp;cseq!is_char_seq_monotonic&nbsp;s1&nbsp;=&nbsp;tr2$0&nbsp;|=&nbsp;mid,&nbsp;s2&nbsp;=&nbsp;tr1&nbsp;|=&nbsp;mid,&nbsp;chsz&nbsp;=&nbsp;chsz;<BR>
&nbsp;&nbsp;apply&nbsp;tr!times_distributes_plus;<BR>
&nbsp;&nbsp;use&nbsp;len_char_seq_bit_seq_relation&nbsp;tr1&nbsp;=&nbsp;tr2$0,&nbsp;chsz&nbsp;=&nbsp;chsz,&nbsp;tick&nbsp;=&nbsp;tick;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;(len&nbsp;cseq!char_seq&nbsp;(tr2$0&nbsp;|=&nbsp;outbit,&nbsp;chsz))&nbsp;*&nbsp;(chsz&nbsp;+&nbsp;2);<BR>
&nbsp;&nbsp;use&nbsp;pr!subseq_vals&nbsp;s1&nbsp;=&nbsp;tr2$0,&nbsp;s2&nbsp;=&nbsp;tr1,&nbsp;c&nbsp;=&nbsp;mid;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;cseq!flatten(tr2$0&nbsp;|=&nbsp;mid,&nbsp;chsz);<BR>
&nbsp;&nbsp;use&nbsp;tr!trace_append_length&nbsp;s1&nbsp;=&nbsp;cseq!flatten(tr2$0&nbsp;|=&nbsp;mid,&nbsp;chsz),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;s2&nbsp;=&nbsp;tr2$0&nbsp;|=&nbsp;outbit,&nbsp;s3&nbsp;=&nbsp;cbuff2,a=-{0,1}-;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;len&nbsp;tr2$0&nbsp;|=&nbsp;outbit;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;use&nbsp;pr!is_trace_of_vals_select_msgs<BR>
&nbsp;&nbsp;&nbsp;&nbsp;tr1&nbsp;=&nbsp;tr2$0,&nbsp;a1&nbsp;=&nbsp;Put_alpha(chsz,tick),&nbsp;a2&nbsp;=&nbsp;-{0,&nbsp;1}-,&nbsp;c&nbsp;=&nbsp;outbit;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;flatten_to_char_seq_prefix&nbsp;(cbuff,tr1,chsz)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char_seq(tr1&nbsp;|=&nbsp;mid,&nbsp;chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;istrace&nbsp;cbuff<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!flatten(tr1&nbsp;|=&nbsp;mid,chsz)&nbsp;=&nbsp;(tr1&nbsp;|=&nbsp;outbit)&nbsp;&nbsp;cbuff<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;(cseq!char_seq(tr1&nbsp;|=&nbsp;outbit,chsz)&nbsp;.&lt;=.&nbsp;(tr1&nbsp;|=&nbsp;mid))&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;flatten_to_char_seq_prefix;<BR>
&nbsp;&nbsp;use&nbsp;cseq!char_seq_monotonic<BR>
&nbsp;&nbsp;&nbsp;&nbsp;s1&nbsp;=&nbsp;tr1&nbsp;|=&nbsp;outbit,&nbsp;s2&nbsp;=&nbsp;cseq!flatten(tr1&nbsp;|=&nbsp;mid,&nbsp;chsz),&nbsp;chsz&nbsp;=&nbsp;chsz;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;cseq!flatten(tr1&nbsp;|=&nbsp;mid,&nbsp;chsz);<BR>
&nbsp;&nbsp;rewrite;<BR>
