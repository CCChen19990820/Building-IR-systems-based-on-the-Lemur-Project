&nbsp;&nbsp;rule&nbsp;restrict_Put_alpha&nbsp;(tr1,&nbsp;chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;null&nbsp;tr1&nbsp;|&nbsp;Put_alpha(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;&nbsp;null&nbsp;tr1&nbsp;|&nbsp;reqs!buffer_alpha&nbsp;({outbit},&nbsp;nset!two_set&nbsp;(0,&nbsp;1))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;null&nbsp;tr1&nbsp;|&nbsp;reqs!buffer_alpha&nbsp;({mid},&nbsp;cseq!char_set(chsz)))<BR>
&nbsp;&nbsp;end&nbsp;restrict_Put_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;tick_in_Put_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;tick&nbsp;in&nbsp;Put_alpha(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;tick_in_Put_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;Put_alpha_sub_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;Put_alpha(chsz,tick)&nbsp;&lt;&lt;&nbsp;defs!rep_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;Put_alpha_sub_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_out_bit_Put_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(outbit,&nbsp;Put_alpha(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buffer_alpha&nbsp;({outbit},&nbsp;nset!two_set&nbsp;(0,&nbsp;1))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_out_bit_Put_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_mid_Put_alpha(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(mid,&nbsp;Put_alpha(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buffer_alpha&nbsp;({mid},&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_mid_Put_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
