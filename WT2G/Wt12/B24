<B>&lt;</B><I>Definition of valid_Inchar</I><B>&gt;</B><B>[</B><A HREF="node92.html#maclab73">73</A><B>]</B><A NAME=14655>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;frule&nbsp;valid_Inchar_constants&nbsp;(cchr_0,cchr,tr_0,tr1,chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;valid_Inchar&nbsp;(cchr_0,cchr,tr_0,tr1,chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;(&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;chsz&nbsp;+&nbsp;2&nbsp;&lt;=&nbsp;int'last()<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;null&nbsp;tr1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;null&nbsp;cchr_0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(len&nbsp;cchr_0)&nbsp;&lt;=&nbsp;chsz+1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;null&nbsp;cchr<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(len&nbsp;cchr)&nbsp;&lt;=&nbsp;chsz+1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;tr1&nbsp;*?&nbsp;Get_alpha(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Inchar_constants;<BR>
&nbsp;&nbsp;invoke&nbsp;valid_Inchar;<BR>
&nbsp;&nbsp;invoke&nbsp;invariant_over_char;<BR>
&nbsp;&nbsp;split&nbsp;null&nbsp;tr1;<BR>
&nbsp;&nbsp;EQUALITY&nbsp;SUBSTITUTE&nbsp;CCHR;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Inchar lemma #1</I><B>&gt;</B><B>[</B><A HREF="node94.html#maclab83">83</A><B>]</B><A NAME=14664>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;INVOKE&nbsp;valid_Inchar;<BR>
&nbsp;&nbsp;INVOKE&nbsp;INVARIANT_OVER_CHAR;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;PRENEX;<BR>
&nbsp;&nbsp;CASES;<BR>
&nbsp;&nbsp;INSTANTIATE&nbsp;PTR1$0&nbsp;=&nbsp;ptr1;<BR>
&nbsp;&nbsp;REARRANGE;<BR>
&nbsp;&nbsp;PRENEX;<BR>
&nbsp;&nbsp;INSTANTIATE&nbsp;PCCHR$1&nbsp;=&nbsp;CCHR;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;NEXT;<BR>
&nbsp;&nbsp;CASES;<BR>
&nbsp;&nbsp;EQUALITY&nbsp;SUBSTITUTE&nbsp;ptr1;<BR>
&nbsp;&nbsp;INSTANTIATE&nbsp;PCCHR$0&nbsp;=&nbsp;CCHR;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;NEXT;<BR>
&nbsp;&nbsp;INSTANTIATE&nbsp;PCCHR$2&nbsp;=&nbsp;CCHR&nbsp;&nbsp;.&lt;B&gt;.;<BR>
&nbsp;&nbsp;SPLIT&nbsp;TR!REMOVE_FIRST_N&nbsp;(ptr1,&nbsp;LEN&nbsp;TR1)&nbsp;=&nbsp;.&lt;INBIT&nbsp;.&nbsp;B&gt;.;<BR>
&nbsp;&nbsp;equality&nbsp;substitute;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;split&nbsp;NOT&nbsp;NULL&nbsp;TR!REMOVE_FIRST_N&nbsp;(ptr1,&nbsp;LEN&nbsp;TR1);<BR>
&nbsp;&nbsp;rewrite;<BR>
