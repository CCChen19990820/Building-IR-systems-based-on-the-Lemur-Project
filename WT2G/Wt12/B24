&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;apply&nbsp;tr!inc_mod_def;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;split&nbsp;null&nbsp;tr!remove_first_n&nbsp;(tr2,&nbsp;len&nbsp;tr1);<BR>
&nbsp;&nbsp;simplify;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;valid_Put_step_tack_outbit_trace_empty(b,cbuff,chsz,&nbsp;buffsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;null&nbsp;cbuff<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(head&nbsp;cbuff)&nbsp;=&nbsp;b<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;valid_Put_step(cbuff,.&lt;&gt;.,chsz,&nbsp;buffsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_Put_step(tail&nbsp;cbuff,.&lt;outbit.b&gt;.,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz,&nbsp;buffsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Put_step_tack_outbit_trace_empty;<BR>
&nbsp;&nbsp;use&nbsp;valid_Put_step_tack_outbit&nbsp;cbuff=cbuff,tr1=.&lt;&gt;.,chsz=chsz,buffsz=buffsz,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tick=tick;<BR>
&nbsp;&nbsp;rewrite;<BR>
