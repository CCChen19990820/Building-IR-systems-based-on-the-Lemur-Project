&nbsp;&nbsp;rule&nbsp;valid_Put_step_tack_mid_trace_char_empty(cbuff,chsz,&nbsp;buffsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;buffsz*(chsz+2)&nbsp;&gt;=&nbsp;(len&nbsp;cbuff)&nbsp;+&nbsp;chsz&nbsp;+&nbsp;2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char(.&lt;&gt;.,chsz)<BR>
