&nbsp;&nbsp;use&nbsp;pr!vals_restrict_select_comm<BR>
&nbsp;&nbsp;&nbsp;&nbsp;tr1&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tr!filter&nbsp;(tr1,&nbsp;defs!rep_alpha(chsz,tick),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;no_error_condition&nbsp;(defs!Rep_alpha(chsz,tick),&nbsp;chsz,&nbsp;buffsz)),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;c&nbsp;=&nbsp;inbit,&nbsp;a&nbsp;=&nbsp;defs!Rep_alpha(chsz,tick),&nbsp;b&nbsp;=&nbsp;defs!Rep_alpha_ext&nbsp;(tick);<BR>
