&nbsp;&nbsp;INVOKE&nbsp;INVARIANT_OVER_CHAR&nbsp;(CCHR_0,&nbsp;PCCHR,&nbsp;TR1_0,&nbsp;TR1,&nbsp;TR1,&nbsp;CHSZ,&nbsp;TICK);<BR>
&nbsp;&nbsp;REARRANGE;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;SPLIT&nbsp;LEN&nbsp;CSEQ!CURRENT_CHAR&nbsp;(TR1&nbsp;|=&nbsp;INBIT,&nbsp;CHSZ)&nbsp;&lt;=&nbsp;CHSZ;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;axiom&nbsp;valid_char_Get_sandwich_lemma&nbsp;(a,b,c)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;b&nbsp;*?&nbsp;c<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(len&nbsp;a)&nbsp;&gt;=&nbsp;(len&nbsp;b)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;a&nbsp;.&lt;=.&nbsp;b<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;b&nbsp;=&nbsp;a<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_char_Get_sandwich_lemma;<BR>
&nbsp;&nbsp;INDUCT&nbsp;A&nbsp;.&lt;=.&nbsp;B;<BR>
&nbsp;&nbsp;REDUCE;<BR>
&nbsp;&nbsp;INVOKE&nbsp;LEN&nbsp;B;<BR>
&nbsp;&nbsp;SIMPLIFY;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;valid_char_Get_sandwich&nbsp;(tr1,chsz,c)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;istrace&nbsp;tr1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(len&nbsp;tr1&nbsp;|=&nbsp;mid)&nbsp;<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&gt;=&nbsp;(len&nbsp;cseq!char_seq(tr1&nbsp;|=&nbsp;inbit,chsz)&nbsp;|&nbsp;c)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;(tr1&nbsp;|=&nbsp;mid)&nbsp;<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;.&lt;=.&nbsp;(cseq!char_seq(tr1&nbsp;|=&nbsp;inbit,chsz)&nbsp;|&nbsp;c)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;(cseq!char_seq(tr1&nbsp;|=&nbsp;inbit,&nbsp;chsz)&nbsp;|&nbsp;c)&nbsp;=&nbsp;(tr1&nbsp;|=&nbsp;mid)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_char_Get_sandwich;<BR>
&nbsp;&nbsp;USE&nbsp;VALID_CHAR_GET_SANDWICH_LEMMA<BR>
&nbsp;&nbsp;&nbsp;&nbsp;A&nbsp;=&nbsp;TR1&nbsp;|=&nbsp;MID,&nbsp;B&nbsp;=&nbsp;CSEQ!CHAR_SEQ&nbsp;(TR1&nbsp;|=&nbsp;INBIT,&nbsp;CHSZ)&nbsp;|&nbsp;C,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;C&nbsp;=&nbsp;CSEQ!CHAR_SET&nbsp;(CHSZ);<BR>
&nbsp;&nbsp;REARRANGE;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;rule&nbsp;valid_char_Get_InOutchar_lemma3&nbsp;(cchr_0,cchr,tr1_0,<BR>
