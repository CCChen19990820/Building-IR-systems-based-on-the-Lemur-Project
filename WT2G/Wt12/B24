&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;InOutchar_pre(cchr_0,tr1_0,chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;valid_Inchar(cchr_0,cchr,tr1_0,tr1,chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;odd_parity(tail&nbsp;cchr)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;Outchar_pre(cchr,tr1,chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_char_Get(tr1&nbsp;&nbsp;.&lt;inbit&nbsp;.&nbsp;stopbit,&nbsp;mid&nbsp;.&nbsp;tail&nbsp;cchr&gt;.,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_char_Get_InOutchar_lemma3;<BR>
