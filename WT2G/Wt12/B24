&nbsp;&nbsp;rule&nbsp;valid_Put_step_tack_mid_trace_empty(cbuff,chsz,buffsz,tick,chr)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;buffsz*(chsz+2)&nbsp;&gt;=&nbsp;(len&nbsp;cbuff)&nbsp;+&nbsp;chsz&nbsp;+&nbsp;2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char(chr,chsz)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;valid_Put_step(cbuff,.&lt;&gt;.,chsz,buffsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_Put_step(cbuff&nbsp;(startbit&nbsp;]-&nbsp;(chr&nbsp;&nbsp;.&lt;stopbit&gt;.)),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;.&lt;mid.chr&gt;.,chsz,buffsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Put_step_tack_mid_trace_empty;<BR>
&nbsp;&nbsp;use&nbsp;valid_Put_step_tack_mid&nbsp;cbuff=cbuff,tr1=.&lt;&gt;.,chsz=chsz,buffsz=buffsz,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tick=tick,chr=chr;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;valid_Put_step_tack_mid_char_empty(cbuff,tr1,chsz,buffsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;buffsz*(chsz+2)&nbsp;&gt;=&nbsp;(len&nbsp;cbuff)&nbsp;+&nbsp;chsz&nbsp;+&nbsp;2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cseq!is_char(.&lt;&gt;.,chsz)<BR>
