&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_Put_step(startbit&nbsp;]-&nbsp;(chr&nbsp;&nbsp;.&lt;stopbit&gt;.),<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tr1&nbsp;&nbsp;.&lt;mid.chr&gt;.,chsz,&nbsp;buffsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Put_step_tack_mid_cbuff_empty;<BR>
