<P>
<P>
<P>
<A NAME="maclab203">&#160;</A>
<B>&lt;</B><I>defss</I><B>&gt;</B><B>[</B>203<B>]</B><A NAME=14350>&#160;</A><B>M</B><B>=</B> <BR><TT>&nbsp;</TT> <TT>reset;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;set&nbsp;library&nbsp;&quot;/vereng/app/seves.lib/&quot;;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;load&nbsp;nset;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;load&nbsp;pr;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;load&nbsp;reqs;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;load&nbsp;cseq;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Channels</I><B>&gt;</B><B>[</B><A HREF="node37.html#maclab3">3</A><B>]</B><A NAME=14361>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>mid channel</I><B>&gt;</B><B>[</B><A HREF="node52.html#maclab25">25</A><B>]</B><A NAME=14370>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Unique channels</I><B>&gt;</B><B>[</B><A HREF="node37.html#maclab4">4</A><B>]</B><A NAME=14379>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Unique mid channel</I><B>&gt;</B><B>[</B><A HREF="node52.html#maclab26">26</A><B>]</B><A NAME=14388>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Alphabet</I><B>&gt;</B><B>[</B><A HREF="node37.html#maclab5">5</A><B>]</B><A NAME=14397>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;tick_in_rep_alpha_ext&nbsp;(tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;tick&nbsp;in&nbsp;rep_alpha_ext&nbsp;(tick)<BR>
&nbsp;&nbsp;end&nbsp;tick_in_rep_alpha_ext;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;disabled<BR>
&nbsp;&nbsp;&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Definition of Rptr universe of events</I><B>&gt;</B><B>[</B><A HREF="node52.html#maclab28">28</A><B>]</B><A NAME=14406>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;tick_in_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;tick&nbsp;in&nbsp;rep_alpha&nbsp;(chsz,tick)<BR>
&nbsp;&nbsp;end&nbsp;tick_in_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_comm_in_bit_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(inbit,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;pr!select_comm&nbsp;(inbit,&nbsp;rep_alpha_ext&nbsp;(tick))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_in_bit_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_comm_out_bit_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(outbit,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;pr!select_comm&nbsp;(outbit,&nbsp;rep_alpha_ext&nbsp;(tick))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_out_bit_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_in_bit_rep_alpha_ext&nbsp;(tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(inbit,&nbsp;rep_alpha_ext&nbsp;(tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buffer_alpha&nbsp;({inbit},&nbsp;-{0,1}-)<BR>
&nbsp;&nbsp;end&nbsp;select_comm_in_bit_rep_alpha_ext;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_out_bit_rep_alpha_ext&nbsp;(tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(outbit,&nbsp;rep_alpha_ext&nbsp;(tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buffer_alpha&nbsp;({outbit},&nbsp;-{0,1}-)<BR>
&nbsp;&nbsp;end&nbsp;select_comm_out_bit_rep_alpha_ext;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_msgs_in_bit_rep_alpha_ext&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs&nbsp;(inbit,&nbsp;rep_alpha_ext&nbsp;(tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;pr!select_msgs&nbsp;(inbit,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;end&nbsp;select_msgs_in_bit_rep_alpha_ext;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_msgs_out_bit_rep_alpha_ext&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs&nbsp;(outbit,&nbsp;rep_alpha_ext&nbsp;(tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;pr!select_msgs&nbsp;(outbit,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;end&nbsp;select_msgs_out_bit_rep_alpha_ext;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_msgs_in_bit_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs&nbsp;(inbit,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;-{0,1}-<BR>
&nbsp;&nbsp;end&nbsp;select_msgs_in_bit_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_msgs_mid_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs&nbsp;(mid,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;cseq!char_set(chsz)<BR>
&nbsp;&nbsp;end&nbsp;select_msgs_mid_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;select_msgs_out_bit_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_msgs&nbsp;(outbit,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;-{0,1}-<BR>
&nbsp;&nbsp;end&nbsp;select_msgs_out_bit_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;grule&nbsp;select_comm_mid_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;pr!select_comm&nbsp;(mid,&nbsp;rep_alpha&nbsp;(chsz,tick))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;reqs!buffer_alpha&nbsp;({mid},&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;end&nbsp;select_comm_mid_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;rep_alpha_ext_subset_rep_alpha&nbsp;(chsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;(rep_alpha_ext&nbsp;(tick)&nbsp;&lt;&lt;&nbsp;rep_alpha&nbsp;(chsz,tick))&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;end&nbsp;rep_alpha_ext_subset_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;make_comm_in_bit_rep_alpha&nbsp;(x,&nbsp;chsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chan&nbsp;x&nbsp;=&nbsp;inbit<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;x&nbsp;in&nbsp;rep_alpha&nbsp;(chsz,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;x&nbsp;=&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;iscomm&nbsp;x<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;msg&nbsp;x&nbsp;in&nbsp;-{0,1}-)<BR>
&nbsp;&nbsp;end&nbsp;make_comm_in_bit_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;make_comm_mid_rep_alpha&nbsp;(x,&nbsp;chsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chan&nbsp;x&nbsp;=&nbsp;mid<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;x&nbsp;in&nbsp;rep_alpha&nbsp;(chsz,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;x&nbsp;=&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;iscomm&nbsp;x<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;msg&nbsp;x&nbsp;in&nbsp;cseq!char_set(chsz))<BR>
&nbsp;&nbsp;end&nbsp;make_comm_mid_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;make_comm_out_bit_rep_alpha&nbsp;(x,&nbsp;chsz,&nbsp;tick)&nbsp;=<BR>
&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;chan&nbsp;x&nbsp;=&nbsp;outbit<BR>
&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;x&nbsp;in&nbsp;rep_alpha&nbsp;(chsz,&nbsp;tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;=&nbsp;(&nbsp;&nbsp;&nbsp;x&nbsp;=&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;iscomm&nbsp;x<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;msg&nbsp;x&nbsp;in&nbsp;-{0,1}-)<BR>
&nbsp;&nbsp;end&nbsp;make_comm_out_bit_rep_alpha;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;make&nbsp;defs&nbsp;spec;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<P>
This macro is invoked in definitions <A HREF="node154.html#maclab186">186</A> and <A HREF="node173.html#maclab202">202</A>.<P>
