&nbsp;&nbsp;frule&nbsp;valid_Put_step_constants(cbuff,tr1,chsz,buffsz,tick)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;valid_Put_step(cbuff,tr1,chsz,buffsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;(&nbsp;&nbsp;&nbsp;&nbsp;chsz&nbsp;&gt;=&nbsp;0<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;buffsz&nbsp;&gt;=&nbsp;1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;not&nbsp;iscomm&nbsp;tick<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;buffsz*(chsz+2)&nbsp;&gt;=&nbsp;chsz&nbsp;+&nbsp;2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;tr1&nbsp;*?&nbsp;Put_alpha(chsz,tick)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;cbuff&nbsp;*?&nbsp;-{0,1}-<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;buffsz*(chsz+2)&nbsp;&gt;=&nbsp;(len&nbsp;cbuff))<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_Put_step_constants;<BR>
&nbsp;&nbsp;invoke&nbsp;valid_Put_step;<BR>
&nbsp;&nbsp;invoke&nbsp;invariant_over_buffer;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;use&nbsp;tr!mod_ge_0&nbsp;a&nbsp;=&nbsp;len&nbsp;tr1&nbsp;|=&nbsp;outbit,&nbsp;b&nbsp;=&nbsp;chsz&nbsp;+&nbsp;2;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;</TT>
<B>&lt;</B><I>Put lemma #1</I><B>&gt;</B><B>[</B><A HREF="node105.html#maclab93">93</A><B>]</B><A NAME=15011>&#160;</A> <TT></TT><TT><BR>
