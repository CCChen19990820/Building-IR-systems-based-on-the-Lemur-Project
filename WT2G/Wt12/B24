&nbsp;&nbsp;with&nbsp;enabled&nbsp;(Put_alpha)&nbsp;reduce;<BR>
&nbsp;&nbsp;</TT><TT><BR>
&nbsp;&nbsp;rule&nbsp;tail_append_not_null(s1,&nbsp;s2)&nbsp;=<BR>
&nbsp;&nbsp;&nbsp;&nbsp;begin<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;not&nbsp;null&nbsp;s1<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and&nbsp;istrace&nbsp;s2<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;(tail&nbsp;s1)&nbsp;&nbsp;s2&nbsp;=&nbsp;tail(s1&nbsp;&nbsp;s2)<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;tail_append_not_null;<BR>
&nbsp;&nbsp;reduce;<BR>
