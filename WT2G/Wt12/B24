<B>&lt;</B><I>valid_Put_step lemma #1</I><B>&gt;</B><B>[</B><A HREF="node106.html#maclab97">97</A><B>]</B><A NAME=15020>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;invoke&nbsp;valid_put_step;<BR>
&nbsp;&nbsp;invoke&nbsp;invariant_over_buffer;<BR>
&nbsp;&nbsp;label&nbsp;chsz&nbsp;+&nbsp;2;<BR>
&nbsp;&nbsp;prenex;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;cseq!flatten&nbsp;(tr1&nbsp;|=&nbsp;mid,&nbsp;chsz);<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;split&nbsp;len&nbsp;tr1&nbsp;|=&nbsp;outbit&nbsp;=&nbsp;x&nbsp;*&nbsp;(len&nbsp;tr1&nbsp;|=&nbsp;mid)&nbsp;-&nbsp;(len&nbsp;cbuff);<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;len&nbsp;tr1&nbsp;|=&nbsp;outbit;<BR>
&nbsp;&nbsp;use&nbsp;tr!mod_remainder_added&nbsp;a&nbsp;=&nbsp;x&nbsp;+&nbsp;(len&nbsp;cbuff),&nbsp;b&nbsp;=&nbsp;x,&nbsp;c&nbsp;=&nbsp;buffsz;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;use&nbsp;cseq!len_flatten&nbsp;cs&nbsp;=&nbsp;tr1&nbsp;|=&nbsp;mid,&nbsp;chsz&nbsp;=&nbsp;chsz;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;cseq!flatten&nbsp;(tr1&nbsp;|=&nbsp;mid,&nbsp;chsz);<BR>
&nbsp;&nbsp;equality&nbsp;substitute;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;instantiate&nbsp;tr2$0&nbsp;=&nbsp;tr2;<BR>
&nbsp;&nbsp;rearrange;<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;next;<BR>
&nbsp;&nbsp;cases;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;tr2;<BR>
&nbsp;&nbsp;instantiate&nbsp;cbuff2$0&nbsp;=&nbsp;cbuff&nbsp;&nbsp;startbit&nbsp;]-&nbsp;(chr&nbsp;&nbsp;.&lt;stopbit&gt;.);<BR>
&nbsp;&nbsp;rewrite;<BR>
&nbsp;&nbsp;equality&nbsp;substitute&nbsp;cseq!flatten&nbsp;(tr1&nbsp;|=&nbsp;mid,&nbsp;chsz);<BR>
