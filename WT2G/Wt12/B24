<B>&lt;</B><I>Inchar lemma #3</I><B>&gt;</B><B>[</B><A HREF="node94.html#maclab85">85</A><B>]</B><A NAME=14686>&#160;</A> <TT></TT><TT><BR>
&nbsp;&nbsp;INVOKE&nbsp;VALID_INCHAR;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;INVOKE<BR>
&nbsp;&nbsp;&nbsp;&nbsp;INVARIANT_OVER_CHAR&nbsp;(CCHR&nbsp;&nbsp;.&lt;B&gt;.,&nbsp;PCCHR$1,&nbsp;TR1&nbsp;&nbsp;.&lt;INBIT&nbsp;.&nbsp;B&gt;.,&nbsp;PTR1$0,&nbsp;TR2,<BR>
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;CHSZ,&nbsp;TICK);<BR>
&nbsp;&nbsp;INVOKE&nbsp;INVARIANT_OVER_CHAR&nbsp;(CCHR,&nbsp;PCCHR$2,&nbsp;TR1,&nbsp;PTR1$1,&nbsp;TR2,&nbsp;CHSZ,&nbsp;TICK);<BR>
&nbsp;&nbsp;PRENEX;<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;SPLIT&nbsp;LEN&nbsp;PTR1&nbsp;=&nbsp;(LEN&nbsp;TR1);<BR>
&nbsp;&nbsp;CASES;<BR>
&nbsp;&nbsp;USE&nbsp;TR!SUBSEQ_EQ&nbsp;S1&nbsp;=&nbsp;TR1,&nbsp;S2&nbsp;=&nbsp;PTR1,&nbsp;A&nbsp;=&nbsp;defs!Rep_ALPHA&nbsp;(CHSZ,&nbsp;TICK);<BR>
&nbsp;&nbsp;REWRITE;<BR>
&nbsp;&nbsp;NEXT;<BR>
