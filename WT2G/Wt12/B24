&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-&gt;&nbsp;valid_char_Get(.&lt;&gt;.,chsz,tick)&nbsp;=&nbsp;true<BR>
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;valid_char_Get_empty;<BR>
&nbsp;&nbsp;with&nbsp;enabled&nbsp;(valid_char_Get)&nbsp;reduce;<BR>
