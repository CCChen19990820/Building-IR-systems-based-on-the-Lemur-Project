	<TD COLSPAN=1 WIDTH=2><IMG SRC="../assets/images/dot_clear.gif" WIDTH=2 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=320><IMG SRC="../assets/images/dot_clear.gif" WIDTH=320 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=23><IMG SRC="../assets/images/dot_clear.gif" WIDTH=23 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=2><IMG SRC="../assets/images/dot_clear.gif" WIDTH=2 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=0><IMG SRC="../assets/images/dot_clear.gif" WIDTH=0 HEIGHT=1 BORDER=0></TD>
