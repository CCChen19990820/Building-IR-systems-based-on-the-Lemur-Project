<DOCNO>WT12-B31-362</DOCNO>
<DOCOLDNO>IA050-000864-B030-135</DOCOLDNO>
<DOCHDR>
http://www.gac.com.au:80/cgi-bin/vsc/cgi-bin/search.pl?L+gac+ahtf6092 203.59.127.74 19970210121433 text/html 3612
HTTP/1.0 200 OK
Date: Mon, 10 Feb 1997 12:14:44 GMT
Server: Apache/1.1.1
Content-type: text/html
Set-Cookie: Apache=widener3678855576884626; path=/
</DOCHDR>
#! /usr/bin/perl

# Read in standard templates and defines
require "config";

open D , ">/tmp/debug";

$remote = $ENV{'REMOTE_HOST'};
$sessionid = $ENV{'QUERY_STRING'};

print "Content-type: text/html\n\n";

if ($ENV{'REQUEST_METHOD'} eq "POST") {
	$len = $ENV{'CONTENT_LENGTH'};
	read(STDIN, $result, $len);
	print D "$result<P>\n" if $debug;
	&explode($result);
} else {
#	html_page("Invalid Request\n");
}

if ($Data{'search_type'} eq "web") {
	# Link into glimpse and search fixed web pages
} else {
	# Search through the database
	# First read in the database template and construct an
	# index of the fields from it.
	open F1 , $db_template;
	chop($headers = <F1>);
	chop($headers);
	@headers = split(/,/, $headers);
	$count = 0;
	foreach $i (@headers) {
		$i =~ tr/A-Z/a-z/;	# make lowercase
		$index{$i} = $count;	
		print D "index $i is $count<p>\n" if $debug;
		++$count;
	}
	close (F1);

	if ($Data{'query'} eq "") {
		# No search keywords were entered.  Output the help
		# web page.
		open T, $helptemplate1;
		while (<T>) {
			print;
		}
		close (T);
		close (D);
		exit(0);
	}

	print D "category = $Data{'engine'}<p>\n" if $debug;

	open DB, $database;

	# Split the search query keywords
	@words = split(/\s+/, $Data{'query'});
	$search = join('|', @words);

	# Run a grep over the database
	@start = grep(/$search/, <DB>);

	# Go through all the matched results
	foreach $prod (@start) {
		# Search for each of the keywords
		foreach $i (@words) {
			($prod =~ /$i/) && $result{$prod}++;
			($prod =~ /\b$i/) && $result{$prod}++;
			($prod =~ /\b$i\b/) && $result{$prod}++;
		}
	}

	# Sort the product by most frequent keyword match
	@res = sort($result{$a} <=> $result{$b},  keys(%result));

	$count = 0;
	foreach $line (@res) {
		@fields = split(/#/, $line);

		# Does this product match the search category?
                if (($Data{'engine'} eq "All Categories") ||
                    ($Data{'engine'} eq $fields[$index{'category'}])) {

			$results .= '<TR><TD VALIGN=TOP><FONT SIZE="3">'
			.'<a href="/cgi-bin/vsc/cgi-bin/show.pl?'
			.$fields[$index{'record'}].'?L+gac+ahtf6092">'
			.$fields[$index{'name'}].'</a></FONT></td>'
			.'<TD VALIGN=BOTTOM>'
			.$fields[$index{'description'}]
			.'<FONT SIZE="-1"><B> Uses: '
			.$fields[$index{'uses'}].'</FONT></B></TD>'
			.'<TD ALIGN=RIGHT VALIGN=TOP NOWRAP><B>$'
			.$fields[$index{'price1'}].'</B></TD></TR>'
			.'<TR><TD COLSPAN="3"><HR></TD></TR>'."\n";

			++$count;
		}
	}
	close (DB);
	# For some reason, the count variable is 1 higher than it should be
	--$count;

	# The results variable now holds all the matched products.
	# Open the keyword search results template and output the results.
	if ($count eq 0) {
		open T, $template11;
	} else {
		open T, $template1;
	}
	while (<T>) {
		# Output the template with the appropriate values
		# substituted in.

		s/%RESULTS%/$results/g;
		s/%COUNT%/$count/g;

		print;
	}
	close (T);
}

if ($debug) {
	print D "<PRE>\n";
	foreach $i (keys %ENV) { print D "$i: $ENV{$i}\n";  }
	print D "</PRE>\n";
}

# Finished
close (D);
exit(0);


sub explode {
	local($str) = @_;
	local($item, @list, $index);

	@list = split(/\&/, $str);
	foreach $item (@list) {
		if ($item =~ /^(.*)=(.*)$/) {
			$index = $1;
			$Data{$index} = $2;
			while ($Data{$index} =~ /%([0-9a-fA-F][0-9a-fA-F])/) {
				local($c); $c = sprintf("%c", hex($1));
				$Data{$index} =~ s/%([0-9a-fA-F][0-9a-fA-F])/$c/;
			}
			$Data{$index} =~ s/\+/ /g;
		}
	}
}


