<TR VALIGN=top><TD ALIGN=left>Cattail 3</TD><TD ALIGN=left>21 a</TD><TD ALIGN=left>6a</TD><TD ALIGN=left>1.40a</TD><TD ALIGN=left>34.8ab</TD><TD ALIGN=left>-266cd</TD><TD ALIGN=left>-63bcd</TD></TR>
