<TR ALIGN="CENTER"><TD></TD> <TD></TD> <TD ALIGN="LEFT">SWE</TD> <TD></TD>
<TD ALIGN="LEFT">Retail sales yoy</TD> <TD></TD> <TD></TD> <TD>April</TD>
<TD>-1.6%</TD> <TD>-0.5%</TD> </TR>
<TR ALIGN="CENTER"><TD></TD> <TD></TD> <TD ALIGN="LEFT">SWE</TD> <TD></TD>
<TD ALIGN="LEFT">Industrial production yoy</TD> <TD></TD> <TD></TD> <TD>April</TD>
<TD>+0.6%</TD> <TD>+0.2%</TD> </TR>
