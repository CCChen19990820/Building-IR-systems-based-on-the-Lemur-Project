<OPTION SELECTED>100 <OPTION>250 <OPTION>500 <OPTION>1000 </SELECT></FORM><FORM NAME="buttonbar"><INPUT TYPE="button" VALUE="Back"  onClick="history.back()"><INPUT TYPE="button" VALUE="Next" onClick="history.forward()"></FORM>
