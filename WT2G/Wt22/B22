<PRE>      SUBROUTINE Graph (x, u, time, k)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
      REAL*8 x(jmax), u(jmax,3), time
      INTEGER k,j,i
      i=jmax+1
C
      IF (k .EQ. 0) THEN
         open(1,file='aaa',status='UNKNOWN')
      ELSE IF (k .EQ. 1) THEN
         write(1,100)(time*TRef,x(j)*XRef,j=1,jmax)
 0100    format(2e13.5)
      ELSE IF (k.eq.3) THEN
         close(1)
      ELSE
        PRINT*,'Invalid k: use 0,1,2,3'
        STOP
      ENDIF
      RETURN
      END</PRE>
