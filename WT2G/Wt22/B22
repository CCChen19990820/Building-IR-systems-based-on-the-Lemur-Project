<H2><A NAME="SECTION00811000000000000000">copy.f</A></H2>
<P>
<PRE>      SUBROUTINE Copy (x, y)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
      INTEGER j
      REAL*8 x(jmax), y(jmax)
C
C***** Copy X into Y
C
      DO j = 1, jmax
        y(j) = x(j)
      END DO
C
C***** Return to Whence You Came!!
C
      RETURN
      END</PRE>
