<BR> <P>
<H2><A NAME="SECTION00816000000000000000">decode.f</A></H2>
<P>
<PRE>      SUBROUTINE Decode (u, r, v, e, p, c)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
      INTEGER j
      REAL*8 u(jmax,3), r(jmax), v(jmax), e(jmax), p(jmax), c(jmax)
C
C***** Decode U
C
      DO j = 1, jmax
        r(j) = u(j,1)
        v(j) = u(j,2) / r(j)
        e(j) = u(j,3) / r(j)
        p(j) = (gamma - 1.d0) * r(j) * (e(j) - 0.5d0 * v(j)**2)
        c(j) = SQRT (gamma * p(j) / r(j))
      END DO
C
C***** Return to Whence You Came!
C
      RETURN
      END</PRE>
<BR> <HR>
