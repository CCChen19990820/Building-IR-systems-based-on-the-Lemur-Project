<HEAD>
<TITLE>corrector.f</TITLE>
<META NAME="description" CONTENT="corrector.f">
<META NAME="keywords" CONTENT="thesis">
<META NAME="resource-type" CONTENT="document">
<META NAME="distribution" CONTENT="global">
<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html497" HREF="node35.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html495" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html489" HREF="node33.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html498" HREF="node35.html">dag.f</A>
<B>Up:</B> <A NAME="tex2html496" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html490" HREF="node33.html">copy.f</A>
<BR> <P>
<H2><A NAME="SECTION00812000000000000000">corrector.f</A></H2>
<P>
<PRE>      SUBROUTINE Corrector
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER i,j
      REAL*8 ut
C
C***** Apply B.C.'s
C
      DO i=1,3
         up(1,i)    = ub(1,i)
         up(jmax,i) = ub(jmax,i)
      END DO
      xp(  1 ) = xb(  1 )
      xp(jmax) = xb(jmax)
      Vp(  1 ) = Vb(  1 )
      Vp(jmax) = Vb(jmax)
C
C***** Corrector
C
      DO j = 2, jmax-1
         xp(j) = 0.5d0 * (x(j) + xb(j) + DeltaT * zb(j))
         Vp(j) = 0.5d0 * (V(j) + Vb(j) + DeltaT * (zb(j+1) - zb(j)))
         DO i = 1, 3
            ut = (fb( j ,i) - zb( j ) * ub( j ,i))
     &gt;         - (fb(j+1,i) - zb(j+1) * ub(j+1,i))
     &gt;         + 2.d0 * diss(j,i) / DeltaT
            up(j,i) = 0.5d0 * (u (j,i) * V (j)
     &gt;                       + ub(j,i) * Vb(j) + DeltaT * ut) / Vp(j)
         END DO
      END DO
C
C***** Return to Whence You Came!
C
      RETURN
      END</PRE>
<BR> <HR>
