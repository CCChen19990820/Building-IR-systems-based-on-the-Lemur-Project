<H2><A NAME="SECTION008120000000000000000">output.f</A></H2>
<P>
<PRE>      SUBROUTINE Output
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      CHARACTER rfname*9, vfname*9, pfname*9, efname*9, gfname*9
     &gt;         ,dfname*7, ident*3
      INTEGER j
      REAL*8 Xphys
C
C***** output file for plotting
C
      WRITE (*, *) 'Enter 3 Character Case Identifier:'
      READ (*, '(a3)') ident
      rfname(1:3) = ident
      rfname(4:9) = '_r.plt'
      vfname(1:3) = ident
      vfname(4:9) = '_v.plt'
      pfname(1:3) = ident
      pfname(4:9) = '_p.plt'
      efname(1:3) = ident
      efname(4:9) = '_e.plt'
      gfname(1:3) = ident
      gfname(4:9) = '_g.grd'
      dfname(1:3) = ident
      dfname(4:7) = '.dat'
      OPEN (1, file = rfname, status='UNKNOWN')
      OPEN (2, file = vfname, status='UNKNOWN')
      OPEN (3, file = pfname, status='UNKNOWN')
      OPEN (4, file = efname, status='UNKNOWN')
      OPEN (10, file = gfname, status='UNKNOWN')
      OPEN (11, file = dfname, status='UNKNOWN')
      WRITE (11, *) 'No. of Points = ', jmax
      WRITE (11, *) 'Time = ', tmax * TRef
      WRITE (11, *) 'CFL = ', CFL
      WRITE (11, *) 'A = ', A
      WRITE (11, *) 'Kappa = ', Kappa
      WRITE (11, *) 'No. Steps = ', NSteps
      WRITE (11, *) 'CPU Time = ', DeltaCPU
      WRITE (11, *) 'Rho Error = ', RhoError
      WRITE (11, *) 'U   Error = ', UError
      WRITE (11, *) 'E   Error = ', EError
      CALL Decode(u, rho,vel, energy, press, c)
      DO j = 1, jmax
         Xphys = x(j) * XRef
         WRITE (1, *) Xphys, rho(j) * RhoRef
         WRITE (2, *) Xphys, vel(j) * URef
         WRITE (3, *) Xphys, press(j) * PRef
         WRITE (4, *) Xphys, energy(j) * EtRef
         WRITE (10, *) j, Xphys
      END DO
      CLOSE(1)
      CLOSE(2)
      CLOSE(3)
      CLOSE(4)
      CLOSE(10)
      CLOSE(11)
C
C***** Return to Whence You Came!!
C
      RETURN
      END</PRE>
