<HEAD>
<TITLE>dag.f</TITLE>
<META NAME="description" CONTENT="dag.f">
<META NAME="keywords" CONTENT="thesis">
<META NAME="resource-type" CONTENT="document">
<META NAME="distribution" CONTENT="global">
<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html507" HREF="node36.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html505" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html499" HREF="node34.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html508" HREF="node36.html">dag.input.f</A>
<B>Up:</B> <A NAME="tex2html506" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html500" HREF="node34.html">corrector.f</A>
<BR> <P>
<H2><A NAME="SECTION00813000000000000000">dag.f</A></H2>
<P>
<PRE>      PROGRAM DAG
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      REAL cputime, Time1, Time2
C
C***** Initialize the Problem
C
      CALL Initialize
C
C***** It Is the Beginning of Time!!!
C
      Time = 0.d0
      NSteps = 0
      IF (PlotSolution .EQ. 1) CALL Graph (x, u, Time, 0)
C
C***** Initialize Timing
C
      Time1 = cputime()
C
C***** While Not at the End of Time
C
      DO WHILE (Time .LT. Tmax)
        IF (PlotSolution .EQ. 1) CALL Graph (x, u, Time, 1)
        NSteps = NSteps + 1
C
C***** Take a Step in Time
C
        CALL SolutionStep
      END DO
C
C***** Find Elapsed Time From 1st CPU Call
C
      Time2 = cputime()
      DeltaCPU = Time2
C
C***** Graphics
C
      IF (PlotSolution .EQ. 1) THEN
        CALL Graph (x, u, Time, 1)
        CALL Graph (x, u, Time, 3)
      END IF
C
C***** Error Calculations
C
      CALL Error
C
C***** Generate Output
C
      CALL Output
      STOP
      END</PRE>
<BR> <HR>
