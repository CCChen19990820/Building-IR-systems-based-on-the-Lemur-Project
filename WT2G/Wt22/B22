<H2><A NAME="SECTION008112000000000000000">flux.f</A></H2>
<P>
<PRE>      SUBROUTINE Flux (u, f)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
      INTEGER j
      real*8 u(jmax,3), f(jmax,3),
     &gt;       rho(jmax), vel(jmax), energy(jmax), press(jmax), c(jmax)
c
c----&gt; Generate the Flux Diadic
c
      CALL Decode (u, rho, vel, energy, press, c)
      DO j = 1, jmax
        f(j,1) = rho(j) * vel(j)
        f(j,2) = press(j) + rho(j) * vel(j)**2
        f(j,3) = (press(j) + rho(j) * energy(j)) * vel(j)
      END DO
c
c----&gt; Return to Whence You Came!
c
      RETURN
      END</PRE>
