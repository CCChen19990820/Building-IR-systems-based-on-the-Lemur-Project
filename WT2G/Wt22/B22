<BR> <P>
<H2><A NAME="SECTION008123000000000000000">smooth.f</A></H2>
<P>
<PRE>      SUBROUTINE Smooth (u, us)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
      INTEGER i, j
      REAL*8 u(jmax), us(jmax), utemp(jmax)
C
C***** Make SmoothCount Smoothing Passes
C
      CALL Copy (u, us)
      DO i = 1, SmoothCount
        DO j = 2, jmax-1
          utemp(j) = us(j) + Mu * (us(j+1) - 2.d0 * us(j) + us(j-1))
        END DO
        utemp(1) = us(1)
        utemp(jmax) = us(jmax)
        CALL Copy (utemp, us)
      END DO
C
C***** Return to Whence you Came!!!
C
      RETURN
      END</PRE>
<BR> <HR>
