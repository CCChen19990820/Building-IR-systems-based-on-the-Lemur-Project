<BR> <P>
<H2><A NAME="SECTION008121000000000000000">predictor.f</A></H2>
<P>
<PRE>      SUBROUTINE Predictor
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER i,j
      REAL*8 ut
c
c----&gt; MacCormick Predictor Step
c
      DO i = 1, 3
        ub(  1 ,i) = u(  1 ,i)
        ub(jmax,i) = u(jmax,i)
      END DO
      xb(  1 ) = x(  1 )
      xb(jmax) = x(jmax)
      Vb(  1 ) = V(  1 )
      Vb(jmax) = V(jmax)
      DO j = 2, jmax-1
         xb(j) = x(j) + DeltaT * z(j)
         Vb(j) = V(j) + DeltaT * (z(j)-z(j-1))
         DO i = 1, 3
            ut=(f(j-1,i) - z(j-1) * u(j-1,i))
     &gt;        -(f( j ,i) - z( j ) * u( j ,i))
            ub(j,i) = (u(j,i) * V(j) + DeltaT * ut) / Vb(j)
         END DO
      END DO
C
C***** Return to Whence You Came
C
      RETURN
      END</PRE>
<BR> <HR>
