<BR> <P>
<H2><A NAME="SECTION008122000000000000000">pvector.f</A></H2>
<P>
<PRE>      SUBROUTINE PVector(u, p)
      IMPLICIT NONE             
#include &quot;dag.input.f&quot;
      INTEGER j
      REAL*8 u(jmax), p(jmax),
     &gt;       re, ree, w, we
C
C***** Calculate the Adaption Function P(xi)
C
      DO j = 2, jmax-1
         re = 0.5d0 * (u(j+1) - u(j-1))
         ree = u(j+1) - 2.d0 * u(j) + u(j-1)
         w = 1.d0 + a * re**2
         we = 2.d0 * a * re * ree
         p(j) = we / w
      END DO
C
C***** B.C.'s
C
      p(1)    = 0.d0
      p(jmax) = 0.d0
C
C***** Return to Whence You Came!
C
      RETURN
      END</PRE>
<BR> <HR>
