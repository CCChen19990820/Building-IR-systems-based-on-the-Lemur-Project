<H2><A NAME="SECTION008116000000000000000">initialcondition.f</A></H2>
<P>
<PRE>      SUBROUTINE InitialCondition
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER j
c
c----&gt; Set Up Ubar Vector (U)
c
      DO j = 1, jmax
c
c----&gt; Driver Section of Shock Tube
c
           IF (x(j) .lt. 0.5d0) THEN
             u(j,1) = 0.445d0 / RhoRef
             u(j,2) = 0.311d0 / (RhoRef * URef)
             u(j,3) = 8.928d0 / (RhoRef * EtRef)
c
c----&gt; Driven Section of Shock Tube
c
           ELSE
             u(j,1) = 0.5d0 / RhoRef
             u(j,2) = 0.0d0 / (RhoRef * URef)
             u(j,3) = 1.4275d0 / (RhoRef * EtRef)
           ENDIF
      END DO
c
c----&gt; Return to Whence You Came!
c
      RETURN
      END</PRE>
