<BR> <P>
<H2><A NAME="SECTION008125000000000000000">timestep.f</A></H2>
<P>
<PRE>      SUBROUTINE TimeStep
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER j
      REAL*8 check, lambda
c
c----&gt; Set Checker for find a Max
c
      check = 0.d0
c
c----&gt; Find the Max Lambda
c
      DO j = 2, jmax-1
        lambda = (ABS (vel(j) - z(j)) + c(j)) / V(j)
        check = MAX (check, lambda)
      END DO
c
c----&gt; Maximum Time Step Size
c
      DeltaT = CFL / check
c
c----&gt; Return to Whence You Came!
c
      RETURN
      END</PRE>
<BR> <HR>
<P><ADDRESS>
<I>Bradley Carl Beck <BR>
Mon Oct 21 15:09:26 PDT 1996</I>
