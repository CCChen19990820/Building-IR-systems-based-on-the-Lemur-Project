<HTML>
<HEAD>
<TITLE>exacttube.f</TITLE>
<META NAME="description" CONTENT="exacttube.f">
<META NAME="keywords" CONTENT="thesis">
<META NAME="resource-type" CONTENT="document">
<META NAME="distribution" CONTENT="global">
<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html587" HREF="node44.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html585" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html579" HREF="node42.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html588" HREF="node44.html">flux.f</A>
<B>Up:</B> <A NAME="tex2html586" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html580" HREF="node42.html">error.f</A>
<BR> <P>
<H2><A NAME="SECTION008111000000000000000">exacttube.f</A></H2>
<P>
<PRE>      subroutine InitializeExact
      implicit double precision (a-h,o-z)
      implicit integer (i-n)
      common/ realcn / eps0,p1p5,ws
      common/ gascon / gamma,gm1,gp1,gm12g,gp12g
      common /properties/u5,p5,r5,a5,e5
     &gt;                  ,u1,p1,r1,a1,e1
     &gt;                  ,u2,p2,r2,a2,e2
     &gt;                  ,u3,p3,r3,a3,e3
     &gt;                  ,u4,p4,r4,a4,e4
c
c---&gt; open the input data file
c
      open(10,file='exacttube.dat',status='old')
c
c---&gt; get file input from input_shock.dat
c
      read(10,*) gamma
      read(10,*) eps0
c
c---&gt; define the specific heat ratio paramters
c
      gm1   = gamma - 1.d0
      gp1   = gamma + 1.d0
      gm12g = gm1 /(2.d0 * gamma)
      gp12g = gp1 /(2.d0 * gamma)
c
c----&gt; determine region 5 properties
c
c      write(*,*)'r5 : u5 : e5'
      read(10,*)r5,u5,e5
      p5=gm1*r5*(e5-u5**2/2.d0)
      a5 = sqrt(gamma*p5/r5)
c
c----&gt; determine region 1 properties
c            
c      write(*,*)'r1 : u1 : e1'
      read(10,*)r1,u1,e1
      p1=gm1*r1*(e1-u1**2/2.d0)
      a1=sqrt(gamma*p1/r1)
      p1p5=p1/p5
C
C----&gt; Close Input File
C
      close(10)
c
c----&gt; determine region 4 properties
c
      call preratio(p4p5,u1,a1,a5)
      p4 = p5 * p4p5
      r4 = r5*(1.d0+(gp1/gm1)*(p4p5))/((gp1/gm1)+p4p5)
      a4 = sqrt(gamma*p4/r4)
      u4 = 2.d0/gm1*a1*(1.d0-(p4p5/p1p5)**gm12g)+u1
      e4 = p4/(r4*gm1)+u4**2/2.d0
      ws = a5*sqrt((gp12g*(p4p5-1.d0)+1.d0))
c
c----&gt; determine region 3 properties
c
      u3 = u4
      p3 = p4
      r3 = r1 * (p3/p1)**(1.d0/gamma)
      a3 = sqrt(gamma*p3/r3)
      e3 = p3/(r3*gm1)+u3**2/2.d0
c
c----&gt; Return to Whence You Came
c
      return
      end
c----------------------------------------------------------------------
c---------------------- subroutine exact ------------------------------
c----------------------------------------------------------------------
      subroutine exact(xe,xstart,t,ue,ae,pe,re,ee)
      implicit real*8 (a-h, o-z)
      implicit integer (i-n)
      common/ realcn / eps0,p1p5,ws
      common/ gascon / gamma,gm1,gp1,gm12g,gp12g
      common /properties/u5,p5,r5,a5,e5
     &gt;                  ,u1,p1,r1,a1,e1
     &gt;                  ,u2,p2,r2,a2,e2
     &gt;                  ,u3,p3,r3,a3,e3
     &gt;                  ,u4,p4,r4,a4,e4
c
c----&gt; determine x location of various waves
c
      xh = xstart +(u1- a1)*t
      xt = xstart + (u3 - a3)*t
      xcs = xstart + u3*t
      xs = xstart + ws*t 
c
c ---&gt; determine conditions in each region
c
      if (xe .le. xh) then
        pe = p1
        re = r1
        ue = u1
        ae = a1
        ee = e1
      else if (xe .le. xt)  then
c          ratio = (xt - xe)/(xt - xh)
c          ue = u1 + ratio * (u3 - u1)
          ue = 2.0d0 / gp1 * (a1 + 0.5d0 * gm1 * u1 
     &gt;                        + (xe - xstart) / t  )
          ratio = 1.d0 + 0.5d0 * (gamma - 1.d0)  * (u1 - ue) / a1
          ae = ratio * a1
          pe = p1 * ratio**(1.d0 / gm12g)
          re = r1 * ratio**(2.d0 / gm1)
          ee = pe / (re * gm1) + 0.5d0 * ue**2
        else if (xe .le. xcs) then
            ue = u3
            ae = a3
            pe = p3
            re = r3
            ee = e3
          else if (xe .le. xs) then
              ue = u4
              ae = a4
              pe = p4
              re = r4
              ee = e4
            else
              ue = u5
              ae = a5
              pe = p5
              re = r5
              ee = e5
      end if
      return
      end
c----------------------------------------------------------------------
c---------------------- subroutine preratio ---------------------------
c----------------------------------------------------------------------
      subroutine preratio(rnew,u1,a1,a5)
      implicit double precision (a-h,o-z)
      implicit integer (i-n)
      common/ realcn / eps0,p1p5,ws
      common/ gascon / gamma,gm1,gp1,gm12g,gp12g
      rr = 1.d0
      do 10 iter = 1,1001
      deltar = ef(rr,u1,a1,a5)/efp(rr,u1,a1,a5)
      rnew = rr - deltar
      if (dabs(deltar).lt. eps0) go to 11
      rr = rnew
   10 continue
      write(*,110) iter
      go to 20
   11 continue
      write(*,100) iter,rnew
  100 format(///,t5,'after ',i5,' iterations, the value of p4/p5 = ',
     &gt;f12.8)
  110 format(///,t5,'did not converge after ',i5,' iterations')
   20 continue
      return
      end
c----------------------------------------------------------------------
c---------------------- function f for exact --------------------------
c----------------------------------------------------------------------
      function ef(rr,u1,a1,a5)
      implicit double precision (a-h,o-z)
      implicit integer (i-n)
      common/ realcn / eps0,p1p5,ws
      common/ gascon / gamma,gm1,gp1,gm12g,gp12g
      p5p1 = 1.d0 / p1p5
      pp1=rr-1.d0
      pp2=rr+(gamma-1.d0)/(gamma+1.d0)
      pp3=2.d0*gamma/gp1/pp2
      pp4=a5/gamma*pp1*sqrt(pp3)-u1
      ef = 1.d0/rr*(1.d0-gm1/(2.d0*a1)*pp4)**(2.d0*gamma/gm1)-p5p1
      return
      end
c----------------------------------------------------------------------
c---------------------- function fp for exact -------------------------
c----------------------------------------------------------------------
      function efp(rr,u1,a1,a5)
      implicit double precision (a-h,o-z)
      implicit integer (i-n)
      common/ realcn / eps0,p1p5,ws
      common/ gascon / gamma,gm1,gp1,gm12g,gp12g
      pp1=rr-1.d0
      pp2=rr+gm1/gp1
      pp3=2.d0*gamma/gp1/pp2
      pp4=a5/gamma*pp1*sqrt(pp3)-u1
      pp5=1.d0-gm1/(2.d0*a1)*pp4
      efp = -1.d0/rr**2*pp5**(2.d0*gamma/gm1)
     1   -a5/a1/rr*pp5**(gp1/gm1)
     2   *(sqrt(pp3)-pp1*gamma/gp1/sqrt(pp3)/pp2**2)
      return
      end</PRE>
