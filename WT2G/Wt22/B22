<META NAME="resource-type" CONTENT="document">
<META NAME="distribution" CONTENT="global">
<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html577" HREF="node43.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html575" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html569" HREF="node41.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html578" HREF="node43.html">exacttube.f</A>
<B>Up:</B> <A NAME="tex2html576" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html570" HREF="node41.html">equalgrid.f</A>
<BR> <P>
<H2><A NAME="SECTION008110000000000000000">error.f</A></H2>
<P>
<PRE>      SUBROUTINE Error
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER j
      REAL*8 xe, xeo, xse, te, ue, ueo, ae, aeo, pe, peo
     &gt;      ,re, reo, ee, eeo, dX
      te = Tmax * TRef
      xse = 0.5d0 * XRef
      CALL InitializeExact
      xeo = x(1) * XRef
      RhoError = 0.0d0
      UError = 0.0d0
      EError = 0.0d0
      OPEN (1, FILE='exactr.plt', status='UNKNOWN')
      OPEN (2, FILE='exactu.plt', status='UNKNOWN')
      OPEN (3, FILE='exacte.plt', status='UNKNOWN')
      CALL Exact (xeo, xse, te, ueo, aeo, peo, reo, eeo)
      WRITE (1,*) xeo, reo
      WRITE (2,*) xeo, ueo
      WRITE (3,*) xeo, eeo
      DO j = 2, jmax
        xe = x(j) * XRef
        dX = 0.5d0 * (xe - xeo)
        CALL Exact (xe, xse, te, ue, ae, pe, re, ee)
        WRITE (1,*) xe, re
        WRITE (2,*) xe, ue
        WRITE (3,*) xe, ee
        RhoError = RhoError
     &gt;           + ABS (dX
     &gt;             * (re + reo - (u(j,1) + u(j-1,1)) * RhoRef))
        UError = UError
     &gt;         + ABS (dX
     &gt;           * (ue + ueo - (u( j ,2) / u( j ,1)
     &gt;                        + u(j-1,2) / u(j-1,1)) * URef))
        EError = EError
     &gt;         + ABS (dX
     &gt;           * (ee + eeo - (u( j ,3) / u( j ,1)
     &gt;                        + u(j-1,3) / u(j-1,1)) * URef))
        xeo = xe
        ueo = ue
        aeo = ae
        peo = pe
        reo = re
        eeo = ee
      END DO
      CLOSE (1)
      CLOSE (2)
      CLOSE (3)
      RETURN
      END</PRE>
<BR> <HR>
<P><ADDRESS>
<I>Bradley Carl Beck <BR>
Mon Oct 21 15:09:26 PDT 1996</I>
