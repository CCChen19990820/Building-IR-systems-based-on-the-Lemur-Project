<HEAD>
<TITLE>dissipation.f</TITLE>
<META NAME="description" CONTENT="dissipation.f">
<META NAME="keywords" CONTENT="thesis">
<META NAME="resource-type" CONTENT="document">
<META NAME="distribution" CONTENT="global">
<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html557" HREF="node41.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html555" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html549" HREF="node39.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html558" HREF="node41.html">equalgrid.f</A>
<B>Up:</B> <A NAME="tex2html556" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html550" HREF="node39.html">diag.f</A>
<BR> <P>
<H2><A NAME="SECTION00818000000000000000">dissipation.f</A></H2>
<P>
<PRE>      SUBROUTINE Dissipation
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER i,j
      REAL*8 dup, dum, gp(jmax), gm(jmax)
C
C***** Calculate the TVD terms
C                                       
      CALL CalculateGs (gp, gm)
      DO i = 1, 3
        diss(1,i)    = 0.0d0
        diss(jmax,i) = 0.0d0
      END DO
      DO j=2,jmax-1
        DO i=1,3
          dup = (u(j+1,i) - u(j,i)) * (x(j+1) - x(j))
          dum = (u(j,i) - u(j-1,i)) * (x(j) - x(j-1))
          diss(j,i) = (gp(j) + gm(j+1)) * dup
     &gt;              - (gp(j-1) + gm(j)) * dum
        END DO
      END DO
      RETURN
      END

      REAL*8 FUNCTION Phi (r)
      IMPLICIT NONE
      REAL*8 r
      Phi = MAX ( 0.d0, MIN ( 2.0d0 * r, 1.0d0))
      RETURN
      END

      REAL*8 FUNCTION CofNu (nu)
      IMPLICIT NONE
      REAL*8 nu
      IF (nu .GT. 0.5d0) THEN
        CofNu = 0.25d0
      ELSE
        CofNu = nu * (1.0d0 - nu)
      END IF
      RETURN
      END

      SUBROUTINE CalculateGs (gp, gm)
      IMPLICIT NONE
      REAL*8 Small
      PARAMETER (Small = 1.0d-10)
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER j, i
      REAL*8 rplus, rminus, gp(jmax), gm(jmax),
     &gt;       duplus, duminus, rpd, rmd, rn,
     &gt;       Phi, CofNu, nu
C
C***** Boundary Conditions
C
      gp(1)    = 0.0d0
      gm(1)    = 0.0d0
      gp(jmax) = 0.0d0
      gm(jmax) = 0.0d0
C
C***** Interior
C
      DO j = 2, jmax-1
        rpd = 0.d0
        rmd = 0.d0
        rn  = 0.d0
        DO i = 1, 3
          duplus = u(j+1,i) - u(j,i)
          duminus = u(j,i) - u(j-1,i)
          rpd = rpd + duplus**2
          rmd = rmd + duminus**2
          rn  = rn  + duplus*duminus
        END DO
        IF (ABS (rpd) .LT. Small) THEN
          rplus = SIGN (1.0d0, rn)
        ELSE
          rplus = rn / rpd
        END IF
        IF (ABS (rmd) .LT. Small) THEN
          rminus = SIGN (1.0d0, rn)
        ELSE
          rminus = rn / rmd
        END IF
        nu = (ABS (vel(j) - z(j)) + c(j)) * DeltaT / v(j)
        gp(j) = 0.5d0 * CofNu (nu) * (1.0d0 - Phi (rplus))
        gm(j) = 0.5d0 * CofNu (nu) * (1.0d0 - Phi (rminus))
      END DO
C
C***** Return to Whence You Came
C
      RETURN
      END</PRE>
<BR> <HR>
