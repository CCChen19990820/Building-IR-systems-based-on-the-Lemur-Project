<BODY LANG="EN">
 <A NAME="tex2html657" HREF="node51.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html655" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html649" HREF="node49.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html658" HREF="node51.html">jacobian.f</A>
<B>Up:</B> <A NAME="tex2html656" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html650" HREF="node49.html">initialize.f</A>
<BR> <P>
<H2><A NAME="SECTION008118000000000000000">input.f</A></H2>
<P>
<PRE>      SUBROUTINE Input
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
C
C***** Read Input Data File
C
      OPEN (1, file = 'input.dat', status='UNKNOWN')
C
C***** X Reference
C
      READ (1, *) XRef
C
C***** Rho Reference
C
      READ (1, *) RhoRef
C
C***** U Reference
C
      READ (1,*) URef
      URef = URef / RhoRef
C
C***** E Total Reference
C
      EtRef = URef**2
C
C***** P Reference
C
      PRef = RhoRef * URef**2
C
C***** T Reference
C
      TRef = XRef / URef
c
c----&gt; Get Maximum Time Limit
c
      READ (1, *) Tmax
      Tmax = Tmax / TRef
c
c----&gt; Get Courant #
c
      READ (1, *) CFL
c
c----&gt; How Much Adaption?
c
      READ (1, *) A
C
C***** Feedback Control Constant
C
      READ (1, *) Kappa
C
C***** Smoothing Coefficient
C
      READ (1, *) Mu
C
C***** Number of Smoothing Passes
C
      READ (1, *) SmoothCount
C
C***** Do I Plot the Solution?
C***** 1 = Yes, 0 = No
C
      READ (1, *) PlotSolution
C
C***** Maximum Values For Plotting
C
      READ (1, *) RhoMax
      RhoMax = RhoMax / RhoRef
      READ (1, *) UMax
      UMax = UMax / URef
      READ (1, *) EMax
      EMax = EMax / EtRef
      CLOSE (1)
c
c----&gt; Return to Whence You Came!
c
      RETURN
      END</PRE>
