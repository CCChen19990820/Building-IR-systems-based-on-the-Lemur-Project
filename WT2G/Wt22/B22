<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html717" HREF="node57.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html715" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html709" HREF="node55.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html718" HREF="node57.html">timestep.f</A>
<B>Up:</B> <A NAME="tex2html716" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html710" HREF="node55.html">smooth.f</A>
<BR> <P>
<H2><A NAME="SECTION008124000000000000000">solutionstep.f</A></H2>
<P>
<PRE>      SUBROUTINE SolutionStep
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER i, j 
C
C***** Update the Jacobian
C
      CALL Jacobian (x, V)
C
C***** Flux @ n
C
      CALL Flux (u, f)
C
C     *...Calculate P(xi) Distribution
C
      CALL Smooth (u, us)
      CALL PVector(us, p)
C
C***** Grid Speed @ n
C
      CALL GridSpeed (0)
C
C***** Maximum Time Step
C
      CALL Decode (u, rho, vel, energy, press, c)
      CALL TimeStep
      IF ((Time + DeltaT) .GT. Tmax) DeltaT = Tmax - Time
      Time = Time + DeltaT
C
C***** TVD Dissipation Terms
C
      CALL Dissipation
C
C***** Predictor
C
      CALL Predictor

C
C***** Flux @ *
C
      CALL Flux (ub, fb)
C
C     *...Calculate P(xi) Distribution
C
      CALL Smooth (ub, us)
      CALL PVector(us, pb)
C
C***** Grid Speed @ *
C
      CALL GridSpeed (1)
C
C***** Corrector
C
      CALL Corrector
C
C***** Transfer n+1 -&gt; n
C
      DO j = 1, jmax
        x(j) = xp(j)
        DO i = 1, 3
          u(j,i) = up(j,i)
        END DO
      END DO
C
C***** How Far Am I???
C
C      WRITE (*,*) 'Step: ', NStepS, ' Time: ', Time * TRef
C
C***** Return to Whence You Came
C
      RETURN
      END</PRE>
