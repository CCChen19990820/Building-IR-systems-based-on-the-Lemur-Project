<META NAME="keywords" CONTENT="thesis">
<META NAME="resource-type" CONTENT="document">
<META NAME="distribution" CONTENT="global">
<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html617" HREF="node47.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html615" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html609" HREF="node45.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html618" HREF="node47.html">gridspeed.f</A>
<B>Up:</B> <A NAME="tex2html616" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html610" HREF="node45.html">graphics.f</A>
<BR> <P>
<H2><A NAME="SECTION008114000000000000000">gridlaw.f</A></H2>
<P>
<PRE>      SUBROUTINE Gridlaw
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      LOGICAL Unconverged
      INTEGER j
      REAL*8 d(jmax,3), rhs(jmax), xnew(jmax), rmsold, rms, Small
      PARAMETER (Small = 1.0d-6)
C
C     *...This program generates a 1-D grid using the Brackbill-Saltzman
C     *...approach with the adaption function W coming from a monitor
C     *...surface (Eiseman).
C
C     *...In this case:
C
C     *.......W = 1 + a (d(u)/d(xi))^2
C
C     *.......where,
C     *.........a = Adaption Constant (user input)
C
C***** Set Loop Monitor
C
      Unconverged = .TRUE.
      rmsold = 0.0d0
C
C***** Loop Until Grid is Converged
C
      DO WHILE (Unconverged)
C
C***** Apply Initial Conditions
C
        CALL InitialCondition
C
C     *...Calculate P(xi) Distribution
C
        CALL Smooth (u, us)
        CALL PVector(us, p)
C
C***** Build D (Trid Matrix)
C
        DO j = 2, jmax-1
          d(j,1) = 1.d0 - 0.5d0 * p(j)
          d(j,2) = -2.d0
          d(j,3) = 1.d0 + 0.5d0 * p(j)
        END DO
        d(1,1) = 0.d0
        d(1,2) = 1.d0
        d(1,3) = 0.d0
        d(jmax,1) = 0.d0
        d(jmax,2) = 1.d0
        d(jmax,3) = 0.d0
C
C***** Build Right-Hand-Side
C
        DO j = 2, jmax-1
           rhs(j) = 0.d0
        END DO
C
C***** Apply Boundary Conditions
C
        rhs(1)    = 0.d0
        rhs(jmax) = 1.d0
C
C***** Solve the TRID System D.x=rhs
C
        CALL Diag(d, rhs, xnew, jmax, 1, jmax, 3)
C
C***** How much did the grid change?
C
        rms = 0.0d0
        DO j = 1, jmax
          rms = rms + (xnew(j) - x(j))**2
          x(j) = xnew(j)
        END DO
        rms = SQRT (rms / DBLE (jmax))
        WRITE (*,*) 'Grid RMS: ', rms
        IF ((rms .LT. Small) .OR. (rms .EQ. rmsold))
     &gt;     Unconverged = .FALSE.
        rmsold = rms
      END DO
        CALL InitialCondition
C
C***** Return to whence you came!
C
      RETURN
      END</PRE>
