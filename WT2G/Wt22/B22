<HTML>
<HEAD>
<TITLE>gridspeed.f</TITLE>
<META NAME="description" CONTENT="gridspeed.f">
<META NAME="keywords" CONTENT="thesis">
<META NAME="resource-type" CONTENT="document">
<META NAME="distribution" CONTENT="global">
<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html627" HREF="node48.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html625" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html619" HREF="node46.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html628" HREF="node48.html">initialcondition.f</A>
<B>Up:</B> <A NAME="tex2html626" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html620" HREF="node46.html">gridlaw.f</A>
<BR> <P>
<H2><A NAME="SECTION008115000000000000000">gridspeed.f</A></H2>
<P>
<PRE>      SUBROUTINE GridSpeed (IStep)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER IStep,
     &gt;        j
      REAL*8 d(jmax,9), rhs(jmax)
C
C***** Setup 9-D system for X_tau
C
      CALL BuildSystem (d, rhs, IStep)
C
C***** Solve the 9-D system D.z = rhs
C
      IF (IStep .EQ. 0) THEN
        CALL Diag (d, rhs, z, jmax, 1, jmax, 9)
      ELSE
        CALL Diag (d, rhs, zb, jmax, 1, jmax, 9)
      END IF
C
C***** Return to Whence You Came
C
      RETURN
      END

      SUBROUTINE BuildSystem (d, rhs, IStep)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER IStep, i, j
      REAL*8 d(jmax,9), rhs(jmax), Q(jmax,7)
C
C***** Calculate the Q's that are used in:
C***** P_tau = Q * u_tau
C
      CALL GetQs (Q)
C
C****** Boundary Conditions
C
      DO i = 1, 9
        d(1,i) = 0.0d0
        d(2,i) = 0.0d0
        d(3,i) = 0.0d0
        d(4,i) = 0.0d0
        d(jmax-3,i) = 0.0d0
        d(jmax-2,i) = 0.0d0
        d(jmax-1,i) = 0.0d0
        d(jmax  ,i) = 0.0d0
      END DO
C
C***** Z1 = 0
C
      d(1,5) = 1.0d0
      rhs(1) = 0.0d0
C
C***** Zjmax = 0
C
      d(jmax,5) = 1.0d0
      rhs(jmax) = 0.0d0
C
C***** Average to get X_tau's near the boundary
C
      DO j = 2, 4
        d(j,4) = 0.5d0
        d(j,5) = - 1.0d0
        d(j,6) = 0.5d0
        rhs(j) = 0.d0
      END DO
      DO j = jmax-3, jmax-1
        d(j,4) = 0.5d0
        d(j,5) = - 1.0d0
        d(j,6) = 0.5d0
        rhs(j) = 0.d0
      END DO
C
C***** Setup the Interior
C
      IF (IStep .EQ. 0) THEN
        DO j = 5, jmax-4
          d(j,1) = V(j) / V(j-3) * Q(j,1) * (u(j-3,1) - u(j-4,1))
          d(j,2) = V(j) / V(j-2) * Q(j,2) * (u(j-2,1) - u(j-3,1))
          d(j,3) = V(j) / V(j-1) * Q(j,3) * (u(j-1,1) - u(j-2,1))
          d(j,4) =                 Q(j,4) * (u(j,1)   - u(j-1,1))
     &gt;           + (1.0d0 - 0.5d0 * P(j))
          d(j,5) = V(j) / V(j+1) * Q(j,5) * (u(j+1,1) - u(j,1)  )
     &gt;           - 2.0d0
          d(j,6) = V(j) / V(j+2) * Q(j,6) * (u(j+2,1) - u(j+1,1))
     &gt;           + (1.0d0 + 0.5d0 * P(j))
          d(j,7) = V(j) / V(j+3) * Q(j,7) * (u(j+3,1) - u(j+2,1))
          d(j,8) = 0.0d0
          d(j,9) = 0.0d0
          rhs(j) = V(j) / V(j-3) * Q(j,1) * (f(j-3,1) - f(j-4,1))
     &gt;           + V(j) / V(j-2) * Q(j,2) * (f(j-2,1) - f(j-3,1))
     &gt;           + V(j) / V(j-1) * Q(j,3) * (f(j-1,1) - f(j-2,1))
     &gt;           +                 Q(j,4) * (f(j,1)   - f(j-1,1))
     &gt;           + V(j) / V(j+1) * Q(j,5) * (f(j+1,1) - f(j,1)  )
     &gt;           + V(j) / V(j+2) * Q(j,6) * (f(j+2,1) - f(j+1,1))
     &gt;           + V(j) / V(j+3) * Q(j,7) * (f(j+3,1) - f(j+2,1))
     &gt;   - Kappa * (x(j-1) - 2.0d0 * x(j) + x(j+1)
     &gt;           +  0.5d0 * P(j) * (x(j+1) - x(j-1)))
        END DO
      ELSE
        DO j = 5, jmax-4
          d(j,1) = 0.0d0
          d(j,2) = 0.0d0
          d(j,3) = Vb(j) / Vb(j-3) * Q(j,1) * (ub(j-2,1) - ub(j-3,1))
          d(j,4) = Vb(j) / Vb(j-2) * Q(j,2) * (ub(j-1,1) - ub(j-2,1))
     &gt;           + (1.0d0 - 0.5d0 * Pb(j))
          d(j,5) = Vb(j) / Vb(j-1) * Q(j,3) * (ub(j,1)   - ub(j-1,1))
     &gt;           - 2.0d0
          d(j,6) =                   Q(j,4) * (ub(j+1,1) - ub(j,1)  )
     &gt;           + (1.0d0 + 0.5d0 * Pb(j))
          d(j,7) = Vb(j) / Vb(j+1) * Q(j,5) * (ub(j+2,1) - ub(j+1,1))
          d(j,8) = Vb(j) / Vb(j+2) * Q(j,6) * (ub(j+3,1) - ub(j+2,1))
          d(j,9) = Vb(j) / Vb(j+3) * Q(j,7) * (ub(j+4,1) - ub(j+3,1))
          rhs(j) = Vb(j) / Vb(j-3) * Q(j,1) * (fb(j-2,1) - fb(j-3,1)
     &gt;           - 2.0d0 * diss(j-3,1) / DeltaT)
     &gt;           + Vb(j) / Vb(j-2) * Q(j,2) * (fb(j-1,1) - fb(j-2,1)
     &gt;           - 2.0d0 * diss(j-2,1) / DeltaT)
     &gt;           + Vb(j) / Vb(j-1) * Q(j,3) * (fb(j,1)   - fb(j-1,1)
     &gt;           - 2.0d0 * diss(j-1,1) / DeltaT)
     &gt;           +                   Q(j,4) * (fb(j+1,1) - fb(j,1)  
     &gt;           - 2.0d0 * diss(j  ,1) / DeltaT)
     &gt;           + Vb(j) / Vb(j+1) * Q(j,5) * (fb(j+2,1) - fb(j+1,1)
     &gt;           - 2.0d0 * diss(j+1,1) / DeltaT)
     &gt;           + Vb(j) / Vb(j+2) * Q(j,6) * (fb(j+3,1) - fb(j+2,1)
     &gt;           - 2.0d0 * diss(j+2,1) / DeltaT)
     &gt;           + Vb(j) / Vb(j+3) * Q(j,7) * (fb(j+4,1) - fb(j+3,1)
     &gt;           - 2.0d0 * diss(j+3,1) / DeltaT)
     &gt;  - Kappa * (xb(j-1) - 2.0d0 * xb(j) + xb(j+1)
     &gt;          +  0.5d0 * Pb(j) * (xb(j+1) - xb(j-1)))
        END DO
      END IF
C
C***** Return to Whence You Came!
C
      RETURN
      END

      SUBROUTINE GetQs (Q)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
#include &quot;dag.solution.f&quot;
      INTEGER IStep, j
      REAL*8 Q(jmax,7), dPdu(jmax,3), S1, S2, S3, S1p, S2p
C
C***** (dUs/dU)^2 coefficients
C
      S1  = 1.0d0 - 4.0d0 * Mu + 6.0d0 * Mu**2
      S1p = 1.0d0 - 4.0d0 * Mu + 5.0d0 * Mu**2
      S2  = 2.0d0 * Mu - 4.0d0 * Mu**2
      S2p = 2.0d0 * (Mu - Mu**2)
      S3  = Mu**2
C
C***** Calculate dP/dUs
C
      CALL Pu (us, dPdu)
C
C***** Build the Q's
C
C
C***** J = 1
C
      j = 1
      Q(j,1) = 0.0d0
      Q(j,2) = 0.0d0
      Q(j,3) = 0.0d0
      Q(j,4) = dPdu(j,2) + dPdu(j,3) * S2p
      Q(j,5) =             dPdu(j,3) * S1p
      Q(j,6) =             dPdu(j,3) * S2
      Q(j,7) =             dPdu(j,3) * S3
C
C***** J = 2
C
      j = 2
      Q(j,1) = 0.0d0
      Q(j,2) = 0.0d0
      Q(j,3) = dPdu(j,1) + dPdu(j,2) * S2p + dPdu(j,3) * S3
      Q(j,4) =             dPdu(j,2) * S1p + dPdu(j,3) * S2
      Q(j,5) =             dPdu(j,2) * S2  + dPdu(j,3) * S1
      Q(j,6) =             dPdu(j,2) * S3  + dPdu(j,3) * S2
      Q(j,7) =                               dPdu(j,3) * S3
C
C***** J = 3
C
      j = 3
      Q(j,1) = 0.0d0
      Q(j,2) = dPdu(j,1) * S2p + dPdu(j,2) * S3
      Q(j,3) = dPdu(j,1) * S1p + dPdu(j,2) * S2 + dPdu(j,3) * S3
      Q(j,4) = dPdu(j,1) * S2  + dPdu(j,2) * S1 + dPdu(j,3) * S2
      Q(j,5) = dPdu(j,1) * S3  + dPdu(j,2) * S2 + dPdu(j,3) * S1
      Q(j,6) =                   dPdu(j,2) * S3 + dPdu(j,3) * S2
      Q(j,7) =                                    dPdu(j,3) * S3
C
C***** J = 4 to jmax-3
C
      DO j = 4, jmax-3
        Q(j,1) = dPdu(j,1) * S3
        Q(j,2) = dPdu(j,1) * S2 + dPdu(j,2) * S3
        Q(j,3) = dPdu(j,1) * S1 + dPdu(j,2) * S2 + dPdu(j,3) * S3
        Q(j,4) = dPdu(j,1) * S2 + dPdu(j,2) * S1 + dPdu(j,3) * S2
        Q(j,5) = dPdu(j,1) * S3 + dPdu(j,2) * S2 + dPdu(j,3) * S1
        Q(j,6) =                  dPdu(j,2) * S3 + dPdu(j,3) * S2
        Q(j,7) =                                   dPdu(j,3) * S3
      END DO
C
C***** J = jmax-2
C
      j = jmax-2
      Q(j,1) = dPdu(j,1) * S3
      Q(j,2) = dPdu(j,1) * S2 + dPdu(j,2) * S3
      Q(j,3) = dPdu(j,1) * S1 + dPdu(j,2) * S2 + dPdu(j,3) * S3
      Q(j,4) = dPdu(j,1) * S2 + dPdu(j,2) * S1 + dPdu(j,3) * S2
      Q(j,5) = dPdu(j,1) * S3 + dPdu(j,2) * S2 + dPdu(j,3) * S1p
      Q(j,6) =                  dPdu(j,2) * S3 + dPdu(j,3) * S2p
      Q(j,7) = 0.0d0
C
C***** J = jmax-1
C
      j = jmax-1
      Q(j,1) = dPdu(j,1) * S3
      Q(j,2) = dPdu(j,1) * S2 + dPdu(j,2) * S3
      Q(j,3) = dPdu(j,1) * S1 + dPdu(j,2) * S2
      Q(j,4) = dPdu(j,1) * S2 + dPdu(j,2) * S1p
      Q(j,5) = dPdu(j,1) * S3 + dPdu(j,2) * S2p + dPdu(j,3)
      Q(j,6) = 0.0d0
      Q(j,7) = 0.0d0
C
C***** J = jmax
C
      j = jmax
      Q(j,1) = dPdu(j,1) * S3
      Q(j,2) = dPdu(j,1) * S2
      Q(j,3) = dPdu(j,1) * S1p
      Q(j,4) = dPdu(j,1) * S2p + dPdu(j,2)
      Q(j,5) = 0.0d0
      Q(j,6) = 0.0d0
      Q(j,7) = 0.0d0
C
C***** Return to Whence You Came!!!
C
      RETURN
      END

      SUBROUTINE Pu (u, dPdu)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
      INTEGER i, j
      REAL*8 u(jmax), dPdu(jmax,3), ue, uee, w
C
C***** Boundary Conditons
C
      DO i = 1, 3
        dPdu(1,i)    = 0.0d0
        dPdu(jmax,i) = 0.0d0
      END DO
C
C***** Interior Values
C
      DO j = 2, jmax-1
        ue = u(j+1) - u(j-1)
        uee = u(j+1) - 2.0d0 * u(j) + u(j-1)
        w = 1.0d0 + 0.25d0 * A * ue**2
        dPdu(j,1) = 2.0d0 * A * (u(j) - u(j-1)) / w
     &gt;            + 0.5d0 * A**2 * ue**2 * uee / w**2
        dPdu(j,2) = - 2.0d0 * A * ue / w
        dPdu(j,3) = 2.0d0 * A * (u(j+1) - u(j)) / w
     &gt;            - 0.5d0 * A**2 * ue**2 * uee / w**2
      END DO
C
C***** Return to Whence You Came
C
      RETURN
      END</PRE>
<BR> <HR>
