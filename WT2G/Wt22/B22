<LINK REL=STYLESHEET HREF="thesis.css">
</HEAD>
<BODY LANG="EN">
 <A NAME="tex2html547" HREF="node40.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html545" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html539" HREF="node38.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html548" HREF="node40.html">dissipation.f</A>
<B>Up:</B> <A NAME="tex2html546" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html540" HREF="node38.html">decode.f</A>
<BR> <P>
<H2><A NAME="SECTION00817000000000000000">diag.f</A></H2>
<P>
<PRE>      SUBROUTINE Diag (d, r, x, idim, imin, imax, ndiag)
      IMPLICIT NONE
      INTEGER idim, imin, imax, ndiag, i, id, j, k
      REAL*8 d(idim,ndiag), r(idim), x(idim), z
C
C***** Lower Diagonal Elimination
C
      id = ndiag / 2 + 1
      DO i = imin, imax-1
        DO j = 1, id-1
          IF (i+j .LE. imax) THEN
            z = d(i+j,id-j) / d(i,id)
            DO k = id, ndiag
              d(i+j,k-j) = d(i+j,k-j) - z * d(i,k)
            END DO
            r(i+j) = r(i+j) - z * r(i)
          END IF
        END DO
      END DO
C
C***** Back Substitution
C
      DO i = imax, imin, -1
        DO j = 1, min((imax-i),(id-1))
          r(i) = r(i) - d(i,id+j) * x(i+j)
        END DO
        x(i) = r(i) / d(i,id)
      END DO
C
C***** Return to Whence You Came!
C
      RETURN
      END</PRE>
<BR> <HR>
