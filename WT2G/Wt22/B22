 <A NAME="tex2html567" HREF="node42.html"><IMG WIDTH=37 HEIGHT=24 ALIGN=BOTTOM ALT="next" SRC="http://www.ccalmr.ogi.edu/Img/next_motif.gif"></A> <A NAME="tex2html565" HREF="node32.html"><IMG WIDTH=26 HEIGHT=24 ALIGN=BOTTOM ALT="up" SRC="http://www.ccalmr.ogi.edu/Img/up_motif.gif"></A> <A NAME="tex2html559" HREF="node40.html"><IMG WIDTH=63 HEIGHT=24 ALIGN=BOTTOM ALT="previous" SRC="http://www.ccalmr.ogi.edu/Img/previous_motif.gif"></A>   <BR>
<B> Next:</B> <A NAME="tex2html568" HREF="node42.html">error.f</A>
<B>Up:</B> <A NAME="tex2html566" HREF="node32.html">Strongly Coupled Scheme</A>
<B> Previous:</B> <A NAME="tex2html560" HREF="node40.html">dissipation.f</A>
<BR> <P>
<H2><A NAME="SECTION00819000000000000000">equalgrid.f</A></H2>
<P>
<PRE>      SUBROUTINE EqualGrid (x)
      IMPLICIT NONE
#include &quot;dag.input.f&quot;
      INTEGER j
      REAL*8 x(jmax),
     1       d(jmax,3), rhs(jmax)
C
C     *...This program generates an equally spaced 1-D grid
C
C
C***** Build D (Trid Matrix)
C
      DO j = 2, jmax-1
        d(j,1) =  1.d0
        d(j,2) = -2.d0
        d(j,3) =  1.d0
      END DO
      d(1,1) = 0.d0
      d(1,2) = 1.d0
      d(1,3) = 0.d0
      d(jmax,1) = 0.d0
      d(jmax,2) = 1.d0
      d(jmax,3) = 0.d0
C
C***** Build Right-Hand-Side
C
      DO j = 2, jmax-1
         rhs(j) = 0.d0
      END DO
C
C***** Apply Boundary Conditions
C
      rhs(1)    = 0.d0
      rhs(jmax) = 1.d0
C
C***** Solve the TRID System D.x=rhs
C
      CALL Diag (d, rhs, x, jmax, 1, jmax, 3)
C
C***** Return to whence you came!
C
      RETURN
      END</PRE>
<BR> <HR>
