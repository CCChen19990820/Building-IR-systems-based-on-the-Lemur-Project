<HEAD>
<TITLE>
Euromoney World Link
</TITLE>

<SCRIPT LANGUAGE="JavaScript">

<!-- Beginning of JavaScript Applet -------------------

function scrollit_r2l(seed)
{
        var m1  = "Happy New Year from all at EMWL...";
        var m2  = "...New weekly commentary on the FX-market, German bond market and equities via DG Bank (see Sponsors)....";
        var m3  = "...New external link to Belgium and Petercam via Sponsors...";
        var m4  = "...Read the December 1996 issue of Euromoney...";
        var m5  = "...Up now,the latest Euroweek (January 10) ....";
        var m6  = "...Read about the 1997 International Bond Congress - click on Events Diary ....";
        var m7  = "...Up soon, 1996 year end league tables...";
        
        
        var msg=m1+m2+m3+m4+m5+m6+m7;
        var out = " ";
        var c   = 1;

        if (seed > 100) {
                seed--;
                var cmd="scrollit_r2l(" + seed + ")";
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 100 && seed > 0) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="scrollit_r2l(" + seed + ")";
                    window.status=out;
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="scrollit_r2l(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,100);
                }
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("scrollit_r2l(100)",75);
                }
        }
}
// -- End of JavaScript code -------------- -->

</SCRIPT>

</HEAD>

