<HTML>

<HEAD>

<TITLE>ZIA Education Schools on the Net Page</title><META NAME="DESCRIPTION" CONTENT="Schools on the Net, k-12,  safe links, lesson plans, great new sites">
<META NAME="KEYWORDS" CONTENT="Schools on the Net, k-12, education, kids, safe links">

<SCRIPT LANGUAGE="LiveScript">

<!-- Hide JavaScript code from older browsers
function update(form){
if(form.select1[0].selected){
                        top.location.assign('http://www.zia.com/')}
if(form.select1[1].selected){
                        top.location.assign('http://www.zia.com/educate/art/default.htm')}
if(form.select1[2].selected){
                        top.location.assign('http://www.zia.com/educate/books/default.htm')}
if(form.select1[3].selected){
                        top.location.assign('http://www.zia.com/educate/art/eclip.htm')}
if(form.select1[4].selected){
                        top.location.assign('http://www.zia.com/educate/college/default.htm')}
if(form.select1[5].selected){
                        top.location.assign('http://www.zia.com/educate/computer/default.htm')}
if(form.select1[6].selected){
                        top.location.assign('http://www.zia.com/educate/curricul/default.htm')}

if(form.select1[7].selected){
                        top.location.assign('http://www.zia.com/educate/english/default.htm')}

if(form.select1[8].selected){
                        top.location.assign('http://www.zia.com/educate/edist/default.htm')}

