                        top.location.assign('http://www.zia.com/kids/museum/')}
if(form.select1[16].selected){
                        top.location.assign('http://www.zia.com/kids/music/')}
if(form.select1[17].selected){
                        top.location.assign('http://www.zia.com/kids/music/kchrist.htm')}
if(form.select1[18].selected){
                        top.location.assign('http://www.zia.com/kids/music/kclassic.htm')}
if(form.select1[19].selected){
                        top.location.assign('http://www.zia.com/kids/music/kcountry.htm')}
if(form.select1[20].selected){
                        top.location.assign('http://www.zia.com/kids/music/keducat.htm')}
