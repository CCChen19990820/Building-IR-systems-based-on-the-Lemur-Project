<!-- Hide JavaScript code from older browsers
function update(form){
if(form.select1[0].selected){
                        top.location.assign('http://www.zia.com/fresh/default.htm')}
if(form.select1[1].selected){
                        top.location.assign('http://www.zia.com/educate/default.htm')}
if(form.select1[2].selected){
                        top.location.assign('http://www.zia.com/kids/default.htm')}
if(form.select1[3].selected){
                        top.location.assign('http://www.zia.com/')}
                     
}<!-- Done hiding JavaScript -->
