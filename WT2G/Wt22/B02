<SCRIPT LANGUAGE="JavaScript">



<!------------------- Beginning of JavaScript Applet -------------------

/* Copyrighted By East Side Union High School District

		Created By Melvin Trinidad

*/



function scrollit_r2l(seed)

{

        var m1  = "   Welcome To The Independence High School Networking Page. . . . .";

        var m2  = "   This Page Contains Information About The Class And Course Curriculum Outline. . . ";

        var m3  = "   Please,  Take Your Time. . . .";

        var m4  = "   Look Around. . . .";

        var m5  = "   We Hope You Enjoy Your Stay!!";
			
				var msg=m1+m2+m3+m4+m5;

        var out = " ";

        var c   = 1;



        if (seed > 100) {

                seed--;

                var cmd="scrollit_r2l(" + seed + ")";

                timerTwo=window.setTimeout(cmd,100);

        }

        else if (seed <= 100 && seed > 0) {

                for (c=0 ; c < seed ; c++) {

                        out+=" ";

                }

                out+=msg;

                seed--;

                var cmd="scrollit_r2l(" + seed + ")";

                    window.status=out;

                timerTwo=window.setTimeout(cmd,100);

        }

        else if (seed <= 0) {

                if (-seed < msg.length) {

                        out+=msg.substring(-seed,msg.length);

                        seed--;

                        var cmd="scrollit_r2l(" + seed + ")";

                        window.status=out;

                        timerTwo=window.setTimeout(cmd,100);

                }

                else {

                        window.status=" ";

                        timerTwo=window.setTimeout("scrollit_r2l(100)",75);

                }

        }

}

// ----------------- End of JavaScript code ---------------->



</SCRIPT>

<BODY ONLOAD="timerONE=window.setTimeout('scrollit_r2l(100)',500);"> 

