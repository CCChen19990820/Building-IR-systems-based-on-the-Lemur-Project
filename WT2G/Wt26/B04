<AREA SHAPE=RECT COORDS="2,77,107,97" HREF="/Categories/Index/0,3,3,00.html?ntb.intrnt">

<AREA SHAPE=RECT COORDS="0,97,107,115" HREF="/Categories/Index/0,3,4,00.html?ntb.bsns">

<AREA SHAPE=RECT COORDS="0,115,107,136" HREF="/Radio/index.html?ntb.radio">

<AREA SHAPE=RECT COORDS="0,136,107,157" HREF="/Perspectives/perspectives.html?ntb.pers">

<AREA SHAPE=RECT COORDS="0,157,107,176" HREF="/Newsmakers/index.html?ntb.newsmkr">

<AREA SHAPE=RECT COORDS="0,176,107,197" HREF="/Rumors/Entry/0,26,,00.html?ntb.rumor">



