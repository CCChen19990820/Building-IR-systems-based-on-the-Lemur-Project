        function OpenWindow()

      {

//      window.open('http://www.news.com/Pages/top20.html', 'newWin', 'resizable=yes,scrollbars=yes,status=0,width=145,height=450');
      var myWindow = window.open('', 'newWin', 'resizable=yes,scrollbars=yes,status=0,width=145,height=450');
//      myWindow.location = "http://www.news.com/Pages/top20.html";
      myWindow.location = "http://www.news.com/Pages/top20.html";
      }

      // -->


