<ahref="http://registration.cnet.com/Registration/1,2,,0800.html?webAbbrev=ne&from=http://www.news.com/index.html"><img width=116 height=22border=0 src="/Graphics/join.gif"></a><br><p><img width=116 height=564 border=0 src="/Graphics/menubar2.gif"alt="Menu bar" usemap="#menubar" border=0><br><p><!-- here are the map coordinates --><MAP NAME="menubar"><!-- Departments --><AREA SHAPE=RECT COORDS="0,17,107,37" HREF="/?ntb.news"><AREA SHAPE=RECT COORDS="0,37,107,57" HREF="/Categories/Index/0,3,1,00.html?ntb.net"><AREA SHAPE=RECT COORDS="0,57,107,77" HREF="/Categories/Index/0,3,2,00.html?ntb.cmptng"><AREA SHAPE=RECT COORDS="2,77,107,97" HREF="/Categories/Index/0,3,3,00.html?ntb.intrnt"><AREA SHAPE=RECT COORDS="0,97,107,115" HREF="/Categories/Index/0,3,4,00.html?ntb.bsns"><AREA SHAPE=RECT COORDS="0,115,107,136" HREF="/Radio/index.html?ntb.radio"><AREA SHAPE=RECT COORDS="0,136,107,157" HREF="/Perspectives/perspectives.html?ntb.pers"><AREA SHAPE=RECT COORDS="0,157,107,176" HREF="/Newsmakers/index.html?ntb.newsmkr"><AREA SHAPE=RECT COORDS="0,176,107,197" HREF="/Rumors/Entry/0,26,,00.html?ntb.rumor"><!-- Resources --><AREA SHAPE=RECT COORDS="0,233,107,253" HREF="/Personalized/FrontDoor/1,2,,00.html?nocache=1"><AREA SHAPE=RECT COORDS="0,253,107,273" HREF="/Dispatch/Entry/0,65,,00.html"><AREA SHAPE=RECT COORDS="0,273,107,293" HREF="http://registration.cnet.com/MemberServices/1,9,,00.html?webAbbrev=ne&from=http://www.news.com"><AREA SHAPE=RECT COORDS="0,293,107,313" HREF="/Staff/index.html?ntb.staff"><AREA SHAPE=RECT COORDS="0,313,107,333" HREF="/Help/Item/0,24,1,00.html?ntb.help"><AREA SHAPE=RECT COORDS="0,333,107,353" HREF="/Searching/Entry/0,17,0,00.html?ntb.srchg"><!-- Services --><AREA SHAPE=RECT COORDS="0,389,107,409" HREF="http://www.cnet.com/?ntb.cnet"><AREA SHAPE=RECT COORDS="0,409,107,429" HREF="http://www.search.com/?ntb.search"><AREA SHAPE=RECT COORDS="0,429,107,449" 