<AREA SHAPE=RECT COORDS="0,233,107,253" HREF="/Personalized/FrontDoor/1,2,,00.html?nocache=1">

<AREA SHAPE=RECT COORDS="0,253,107,273" HREF="http://registration.cnet.com/MemberServices/1,9,,00.html?webAbbrev=ne&from=http://www.news.com">

<AREA SHAPE=RECT COORDS="0,273,107,293" HREF="/Staff/index.html?ntb.staff">

<AREA SHAPE=RECT COORDS="0,293,107,313" HREF="/Help/Item/0,24,2,00.html?ntb.help">

