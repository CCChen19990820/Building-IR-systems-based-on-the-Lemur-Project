<AREA SHAPE=RECT COORDS="0,369,107,389" HREF="http://www.cnet.com/?ntb.cnet">

<AREA SHAPE=RECT COORDS="0,389,107,409" HREF="http://www.search.com/?ntb.search">

<AREA SHAPE=RECT COORDS="0,409,107,429" HREF="http://www.download.com/?ntb.dl">

<AREA SHAPE=RECT COORDS="0,429,107,449" HREF="http://www.shareware.com/?ntb.sw">

<AREA SHAPE=RECT COORDS="0,449,107,469" HREF="http://www.gamecenter.com/?ntb.gc">

<AREA SHAPE=RECT COORDS="0,469,107,489" HREF="http://www.mediadome.com/index.html?ntb.md">




