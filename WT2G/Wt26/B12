<AREA SHAPE=RECT COORDS="0,17,107,37" HREF="/?ntb.news">

<AREA SHAPE=RECT COORDS="0,37,107,57" HREF="/Categories/Index/0,3,1,00.html?ntb.net">

<AREA SHAPE=RECT COORDS="0,57,107,77" HREF="/Categories/Index/0,3,2,00.html?ntb.cmptng">

<AREA SHAPE=RECT COORDS="2,77,107,97" HREF="/Categories/Index/0,3,3,00.html?ntb.intrnt">

<AREA SHAPE=RECT COORDS="0,97,107,115" HREF="/Categories/Index/0,3,4,00.html?ntb.bsns">

<AREA SHAPE=RECT COORDS="0,115,107,136" HREF="/Radio/index.html?ntb.radio">

