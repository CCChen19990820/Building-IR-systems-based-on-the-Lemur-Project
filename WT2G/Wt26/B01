<H2><A NAME="point_element_representation">point_element_representation</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY point_element_representation
  SUBTYPE OF (<A HREF="#element_representation">element_representation</A>);
  model_ref           : <A HREF="#fea_model_3d">fea_model_3d</A>;
  description         : <A HREF="#text">text</A>;
UNIQUE
  UR1:                model_ref, element_id;
WHERE
  WR1:                SIZEOF (QUERY(item <* SELF\definitional_representation.items |
              'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
              'FEA_AXIS2_PLACEMENT_3D'
              IN TYPEOF (item))) = 1;
  WR2:                SIZEOF (QUERY(item <* SELF\definitional_representation.items |
              'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
              'POINT_ELEMENT_MATRIX'
              IN TYPEOF (item))) >= 1;
  FU1:                required_0d_nodes (
        SELF\element_representation.required_node_list);
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>model_ref<DD>
<DT>description<DD>
</DL>
<H3>Formal propositions:</H3>
<DL>
<DT>UR1<DD>
<DT>WR1<DD>
<DT>WR2<DD>
<DT>FU1<DD>
</DL>
