<B><I>SCHEMA</I></B><B> Diana_Linsta_Material_Specs</B>;

<HR>
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S1S1DIANA_DATA_TYPES">Diana_Data_Types</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S2S2DIANA_COMMON">Diana_Common</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S4S4DIANA_LINSTA_MATERIAL_PROPERTIES">Diana_Linsta_Material_Properties</A></B>;

<HR>
<A NAME=S8STANDARD_LINSTA_MATERIAL_ISO_SPEC></A>
<B><I>ENTITY</I></B><B> Standard_Linsta_Material_Iso_Spec</B>;
    youngs_modulus_iso                      : <A HREF="#S4YOUNGS_MODULUS_ISO"><B>Youngs_Modulus_Iso</B></A>;
    poissons_ratio_iso                      : <A HREF="#S4POISSONS_RATIO_ISO"><B>Poissons_Ratio_Iso</B></A>;
    thermal_expansion_coefficient_iso       :
                        <B><I>OPTIONAL</I></B> <A HREF="#S4THERMAL_EXPANSION_COEFFICIENT_ISO"><B>Thermal_Expansion_Coefficient_Iso</B></A>;
    concentration_expansion_coefficient_iso :
                        <B><I>OPTIONAL</I></B> <A HREF="#S4CONCENTRATION_EXPANSION_COEFFICIENT_ISO"><B>Concentration_Expansion_Coefficient_Iso</B></A>;

    -- NEC: thermal_expansion_coefficient_iso obligatory in case of
    --      temperature load
    -- NEC: concentration_expansion_coefficient_iso obligatory in case of
    --      concentration load
<B><I>END_ENTITY</I></B>;

<HR>
