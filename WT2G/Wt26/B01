<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S4POINT_MASS_ISO"><B>Point_Mass_Iso</B></A>,
				                <A HREF="#S4POINT_MASS_ORTHO"><B>Point_Mass_Ortho</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S4POINT_MASS_ISO></A>
<B><I>ENTITY</I></B><B> Point_Mass_Iso</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S4POINT_MASS"><B>Point_Mass</B></A>);
	m : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
	default_m : <B><I>REAL</I></B> := 0;
    translation_specification_point_mass_iso :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'm', '/materi(*)/mass' ) ];
<B><I>WHERE</I></B>
	WR1 : (M &gt;= 0);
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S4POINT_MASS_ORTHO></A>
<B><I>ENTITY</I></B><B> Point_Mass_Ortho</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S4POINT_MASS"><B>Point_Mass</B></A>);
	m1 : <B><I>REAL</I></B>;
	m2 : <B><I>REAL</I></B>;
	m3 : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
	default_m1 : <B><I>REAL</I></B> := 0;
	default_m2 : <B><I>REAL</I></B> := 0;
	default_m3 : <B><I>REAL</I></B> := 0;
    translation_specification_point_mass_ortho :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'm1', '/materi(*)/mass(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'm2', '/materi(*)/mass(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'm3', '/materi(*)/mass(3)' ) ];
<B><I>WHERE</I></B>
	WR1 : (M1 &gt;= 0);
	WR2 : (M2 &gt;= 0);
	WR3 : (M3 &gt;= 0);
<B><I>END_ENTITY</I></B>;

