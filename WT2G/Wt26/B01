-- DIANA Vectors
 
<A NAME=S2LOCAL_DIRECTION_OR_GLOBAL_DIRECTION></A>
<B><I>TYPE</I></B><B> Local_Direction_Or_Global_Direction</B> = <B><I>SELECT</I></B> ( <A HREF="#S2LOCAL_DIRECTION"><B>Local_Direction</B></A>,
				                                    <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2LOCAL_DIRECTION></A>
<B><I>TYPE</I></B><B> Local_Direction</B> = <B><I>SELECT</I></B> (	<A HREF="#S2ISO_PARAMETRIC_LOCAL_DIRECTION"><B>Iso_Parametric_Local_Direction</B></A>,
					            <A HREF="#S2NON_ISO_PARAMETRIC_LOCAL_DIRECTION"><B>Non_Iso_Parametric_Local_Direction</B></A>);
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2ISO_PARAMETRIC_LOCAL_DIRECTION></A>
<B><I>TYPE</I></B><B> Iso_Parametric_Local_Direction</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Ksi</B>,
                                                       <B>Eta</B>,
                                                       <B>Zeta</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2NON_ISO_PARAMETRIC_LOCAL_DIRECTION></A>
<B><I>TYPE</I></B><B> Non_Iso_Parametric_Local_Direction</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Normal</B>,
                                                           <B>Shear</B>,
                                                           <B>X</B>,
                                                           <B>Y</B>,
                                                           <B>Z</B>,
                                                           <B>Shrksi</B>,
                                                           <B>Shreta</B>,
                                                           <B>Edge1</B>,
                                                           <B>Edge2</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2GLOBAL_DIRECTION_TYPE></A>
<B><I>TYPE</I></B><B> Global_Direction_Type</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Global_Direction</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2DIANA_VECTOR></A>
<B><I>ENTITY</I></B><B> Diana_Vector</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B>;
	x_component : <B><I>REAL</I></B>;
	y_component : <B><I>REAL</I></B>;
	z_component : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
	default_z_component : <B><I>REAL</I></B> := 0.;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2GLOBAL_DIRECTION></A>
<B><I>ENTITY</I></B><B> Global_Direction</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S2DIANA_VECTOR"><B>Diana_Vector</B></A>);
	global_direction_id : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_global_direction :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'global_direction_id', '/direct(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'x_component',         '/direct(*)/xyz(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y_component',         '/direct(*)/xyz(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z_component',         '/direct(*)/xyz(3)' ) ];
<B><I>UNIQUE</I></B>
	UR1 : GLOBAL_DIRECTION_ID;
<B><I>END_ENTITY</I></B>;

<HR>
 
