<H2><A NAME="cylindrical_harmonic_number">cylindrical_harmonic_number</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
TYPE cylindrical_harmonic_number = INTEGER;
WHERE
  WR1:                cylindrical_harmonic_number >= 0;
END_TYPE;
(*</PRE>

<H3>Formal propositions:</H3>
<DL>
<DT>WR1<DD>
</DL>
<H2><A NAME="volume_3d_face">volume_3d_face</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
TYPE volume_3d_face = INTEGER;
WHERE
  WR1:                (volume_3d_face >= 1) AND (volume_3d_face <= 6);
END_TYPE;
(*</PRE>

