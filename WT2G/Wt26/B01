<HR>
(*
*)
<A NAME=S0BAG_TO_SET></A>
<B><I>FUNCTION</I></B><B> Bag_To_Set</B> (THE_BAG : BAG <B><I>OF</I></B> <B><I>GENERIC</I></B> : INTYPE) : <B><I>SET</I></B> <B><I>OF</I></B> <B><I>GENERIC</I></B> : INTYPE;

  <B><I>LOCAL</I></B>
    THE_SET: <B><I>SET</I></B> <B><I>OF</I></B> <B><I>GENERIC</I></B> : INTYPE := [];
    I      : <B><I>INTEGER</I></B>;
  <B><I>END_LOCAL</I></B>;

  <B><I>IF</I></B> <B><I>SIZEOF</I></B> (THE_BAG) &gt; 0 <B><I>THEN</I></B>
    <B><I>REPEAT</I></B> I := 1 <B><I>TO</I></B> <B><I>HIINDEX</I></B> (THE_BAG);
      THE_SET := THE_SET + THE_BAG [I];
    <B><I>END_REPEAT</I></B>;
  <B><I>END_IF</I></B>;

  <B><I>RETURN</I></B> (THE_SET);

<B><I>END_FUNCTION</I></B>;

<HR>
(*
*)
-- END_SCHEMA; -- support_resource_schema
