        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'top/y_coordinate', '/geomet(*)/cone(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'top/z_coordinate', '/geomet(*)/cone(3)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'axis_direction',   '/geomet(*)/cone(2,2)' ) ];
<B><I>END_ENTITY</I></B>;

