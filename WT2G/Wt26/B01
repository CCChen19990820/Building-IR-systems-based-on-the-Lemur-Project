<H2><A NAME="explicit_element_matrix">explicit_element_matrix</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY explicit_element_matrix
  SUBTYPE OF (<A HREF="#fea_representation_item">fea_representation_item</A>);
  property_type       : <A HREF="#matrix_property_type">matrix_property_type</A>;
  symmetry            : <A HREF="#matrix_symmetry">matrix_symmetry</A>;
  node_dof_list       : LIST [1:?] OF LIST [1:?] OF <A HREF="#degree_of_freedom">degree_of_freedom</A>;
  matrix_values       : LIST [1:?] OF <A HREF="#measure_value">measure_value</A>;
WHERE
  WR1:                SIZEOF (node_dof_list) = SIZEOF (
        SELF\element_representation.required_node_list);
  WR2:                SIZEOF (matrix_values) = number_of_terms (node_dof_list, symmetry);
END_ENTITY;
(*</PRE>

