<H2><A NAME="direction_node">direction_node</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY direction_node
  SUBTYPE OF (<A HREF="#direction">direction</A>);
  node_1              : <A HREF="#node_representation">node_representation</A>;
  node_2              : <A HREF="#node_representation">node_representation</A>;
DERIVE
  direction_ratios    : LIST [2:3] OF REAL := build_direction
                                     (node_1, node_2);
WHERE
  WR1:                NOT ((direction_ratios[1] = 0.0) AND
            (direction_ratios[2] = 0.0) AND
            (direction_ratios[3] = 0.0));
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>node_1<DD>
<DT>node_2<DD>
<DT>direction_ratios<DD>
</DL>
<H3>Formal propositions:</H3>
<DL>
<DT>WR1<DD>
</DL>
