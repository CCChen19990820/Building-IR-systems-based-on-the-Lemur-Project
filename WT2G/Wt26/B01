<H2><A NAME="fea_model">fea_model</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY fea_model
  SUPERTYPE OF (ONEOF(
    <A HREF="#fea_model_2d">fea_model_2d</A>,
    <A HREF="#fea_model_3d">fea_model_3d</A>))
  SUBTYPE OF (<A HREF="#representation">representation</A>);
  id                  : <A HREF="#identifier">identifier</A>;
  creating_software   : <A HREF="#text">text</A>;
  intended_analysis_code: <A HREF="#text">text</A>;
  description         : <A HREF="#text">text</A>;
  analysis_type       : <A HREF="#text">text</A>;
  response_property   : <A HREF="#structural_response_property">structural_response_property</A>;
UNIQUE
  UR1:                id;
WHERE
  WR1:                TYPEOF (response_property.product_definition) = 
      'STRUCTURAL_RESPONSE_DEFINITION_SCHEMA.FEA_MODEL_DEFINITION';
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>id<DD>
<DT>creating_software<DD>
<DT>intended_analysis_code<DD>
<DT>description<DD>
<DT>analysis_type<DD>
<DT>response_property<DD>
</DL>
<H3>Formal propositions:</H3>
<DL>
<DT>UR1<DD>
<DT>WR1<DD>
</DL>
