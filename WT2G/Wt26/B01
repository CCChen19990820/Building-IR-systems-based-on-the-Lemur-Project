  axis_line : <A HREF="#S0LINE"><B>Line</B></A> :=
        <A HREF="#S0LINE"><B>Line</B></A>(AXIS_POSITION.LOCATION, <A HREF="#S0VECTOR"><B>Vector</B></A>(AXIS_POSITION.Z, 1.0)||
        <A HREF="#S0GEOMETRIC_REPRESENTATION_ITEM"><B>Geometric_Representation_Item</B></A> ()||<A HREF="#S0REPRESENTATION_ITEM"><B>Representation_Item</B></A> (''))||
        <A HREF="#S0CURVE"><B>Curve</B></A>()||
        <A HREF="#S0GEOMETRIC_REPRESENTATION_ITEM"><B>Geometric_Representation_Item</B></A>()||
        <A HREF="#S0REPRESENTATION_ITEM"><B>Representation_Item</B></A> ('');
