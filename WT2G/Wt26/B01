  load_type            : <A HREF="#S6LOAD_TYPE"><B>Load_Type</B></A>;
  load_location_types  : <B><I>SET</I></B> [0:?] <B><I>OF</I></B> <A HREF="#S6LOAD_LOCATION_TYPE"><B>Load_Location_Type</B></A>;
  load_direction_types : <B><I>SET</I></B> [0:?] <B><I>OF</I></B> <A HREF="#S6LOAD_DIRECTION_TYPE"><B>Load_Direction_Type</B></A>;
