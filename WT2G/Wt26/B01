    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'eys', '/geomet(*)/points' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3LOCAL_STRESS_POINTS_LOCATION_3D></A>
<B><I>ENTITY</I></B><B> Local_Stress_Points_Location_3d</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S3LOCAL_STRESS_POINTS_LOCATION"><B>Local_Stress_Points_Location</B></A>);
	ezs : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_local_stress_points_location_3d :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'eys', '/geomet(*)/points((P)1,2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'ezs', '/geomet(*)/points((P)2,2)' ) ];
<B><I>END_ENTITY</I></B>;

