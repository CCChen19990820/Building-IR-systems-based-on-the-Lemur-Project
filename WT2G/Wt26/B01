-- Beam profiles

<A NAME=S3PIPE_DIAMETER></A>
<B><I>TYPE</I></B><B> Pipe_Diameter</B> = <B><I>REAL</I></B>;
<B><I>WHERE</I></B>
    WR1 : <B><I>SELF</I></B> &gt; 0.0;
<B><I>END_TYPE</I></B>;

<HR>
<A NAME=S3BEAM_PROFILE_PREDEFINED></A>
<B><I>ENTITY</I></B><B> Beam_Profile_Predefined</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S3BEAM_PROFILE_PREDEFINED_RECTANGLE"><B>Beam_Profile_Predefined_Rectangle</B></A>,
                                <A HREF="#S3BEAM_PROFILE_PREDEFINED_BOX"><B>Beam_Profile_Predefined_Box</B></A>,
                                <A HREF="#S3BEAM_PROFILE_PREDEFINED_I_SHAPE"><B>Beam_Profile_Predefined_I_Shape</B></A>,
                                <A HREF="#S3BEAM_PROFILE_PREDEFINED_T_SHAPE"><B>Beam_Profile_Predefined_T_Shape</B></A>,
                                <A HREF="#S3BEAM_PROFILE_PREDEFINED_L_SHAPE"><B>Beam_Profile_Predefined_L_Shape</B></A>,
                                <A HREF="#S3BEAM_PROFILE_PREDEFINED_U_SHAPE"><B>Beam_Profile_Predefined_U_Shape</B></A>,
                                <A HREF="#S3BEAM_PROFILE_PREDEFINED_PIPE"><B>Beam_Profile_Predefined_Pipe</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3BEAM_PROFILE_CLASS1_2D"><B>Beam_Profile_Class1_2d</B></A>,
             <A HREF="#S3BEAM_PROFILE_CLASS1_3D"><B>Beam_Profile_Class1_3d</B></A>,
             <A HREF="#S3BEAM_PROFILE_CLASS2_OR_CLASS3_2D"><B>Beam_Profile_Class2_Or_Class3_2d</B></A>,
             <A HREF="#S3BEAM_PROFILE_CLASS2_OR_CLASS3_3D"><B>Beam_Profile_Class2_Or_Class3_3d</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S3BEAM_PROFILE_PREDEFINED_RECTANGLE></A>
<B><I>ENTITY</I></B><B> Beam_Profile_Predefined_Rectangle</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3BEAM_PROFILE_PREDEFINED"><B>Beam_Profile_Predefined</B></A> );
    heights  : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    breadths : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_beam_profile_predefined_rectangle :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'heights', '/geomet(*)/rectan(1,2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'breadths', '/geomet(*)/rectan(2,2)' ) ];
<B><I>WHERE</I></B>
    WR1 : <B><I>SIZEOF</I></B> ( <B><I>QUERY</I></B> (TEMP &lt;* HEIGHTS| <B><I>NOT</I></B>({0 &lt;= TEMP &lt;= 1} ) ) ) = 0;
    WR2 : <B><I>SIZEOF</I></B> ( <B><I>QUERY</I></B> (TEMP &lt;* BREADTHS| <B><I>NOT</I></B>({0 &lt;= TEMP &lt;= 1} ) ) ) = 0;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S3BEAM_PROFILE_PREDEFINED_BOX></A>
<B><I>ENTITY</I></B><B> Beam_Profile_Predefined_Box</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3BEAM_PROFILE_PREDEFINED"><B>Beam_Profile_Predefined</B></A> );
    heights       : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    breadths      : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    thicknesses_1 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    thicknesses_2 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    thicknesses_3 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    thicknesses_4 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_beam_profile_predefined_box :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'heights',       '/geomet(*)/box(1,6)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'breadths',      '/geomet(*)/box(2,6)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'thicknesses_1', '/geomet(*)/box(3,6)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'thicknesses_2', '/geomet(*)/box(4,6)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'thicknesses_3', '/geomet(*)/box(5,6)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'thicknesses_4', '/geomet(*)/box(6,6)' ) ];
<B><I>WHERE</I></B>
    WR1 : <B><I>SIZEOF</I></B> ( <B><I>QUERY</I></B> (TEMP &lt;* HEIGHTS| <B><I>NOT</I></B>({0 &lt;= TEMP &lt;= 1} ) ) ) = 0;
    WR2 : <B><I>SIZEOF</I></B> ( <B><I>QUERY</I></B> (TEMP &lt;* BREADTHS| <B><I>NOT</I></B>({0 &lt;= TEMP &lt;= 1} ) ) ) = 0;
<B><I>END_ENTITY</I></B>;

