<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3BEAM_PROFILE_CLASS1_3D"><B>Beam_Profile_Class1_3d</B></A> );
    crosse : <A HREF="#S3CROSSE"><B>Crosse</B></A>;
    moment_of_inertia_3d : <A HREF="#S3MOMENT_OF_INERTIA_3D"><B>Moment_Of_Inertia_3d</B></A>;
	torsional_rigidity : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_beam_profile_class1_3d_arbitrary :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'torsional_rigidity', '/geomet(*)/torsio' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
 
<A NAME=S3BEAM_PROFILE_CLASS2_OR_CLASS3_2D_ARBITRARY></A>
<B><I>ENTITY</I></B><B> Beam_Profile_Class2_Or_Class3_2d_Arbitrary</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3BEAM_PROFILE_CLASS2_OR_CLASS3_2D"><B>Beam_Profile_Class2_Or_Class3_2d</B></A> );
	number_of_zones : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    y1 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    z1 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    y2 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    z2 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_beam_profile_class2_or_class3_2d_arbitrary :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'number_of_zones', '/geomet(*)/nzones' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y1',              '/geomet(*)/zone2d(1,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z1',              '/geomet(*)/zone2d(2,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y2',              '/geomet(*)/zone2d(3,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z2',              '/geomet(*)/zone2d(4,4)' ) ];
<B><I>WHERE</I></B>
	WR1 : NUMBER_OF_ZONES &lt;= 20;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3BEAM_PROFILE_CLASS2_OR_CLASS3_3D_ARBITRARY></A>
<B><I>ENTITY</I></B><B> Beam_Profile_Class2_Or_Class3_3d_Arbitrary</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3BEAM_PROFILE_CLASS2_OR_CLASS3_3D"><B>Beam_Profile_Class2_Or_Class3_3d</B></A> );
	number_of_zones : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    y1 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    z1 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    y2 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    z2 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    y3 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    z3 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    y4 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
    z4 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
	torsional_rigidity : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_beam_profile_class2_or_class3_3d_arbitrary :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'number_of_zones',    '/geomet(*)/nzones' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y1',                 '/geomet(*)/zones(1,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z1',                 '/geomet(*)/zones(2,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y2',                 '/geomet(*)/zones(3,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z2',                 '/geomet(*)/zones(4,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y3',                 '/geomet(*)/zones(5,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z3',                 '/geomet(*)/zones(6,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y4',                 '/geomet(*)/zones(7,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z4',                 '/geomet(*)/zones(8,8)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'torsional_rigidity', '/geomet(*)/torsio' ) ];
<B><I>WHERE</I></B>
	WR1 : NUMBER_OF_ZONES &lt;= 20;
<B><I>END_ENTITY</I></B>;

