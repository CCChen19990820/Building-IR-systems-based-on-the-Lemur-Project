  SUBTYPE OF (<A HREF="#fea_representation_item">fea_representation_item</A>);
  coordinates         : <A HREF="#fea_parametric_point">fea_parametric_point</A>;
WHERE
  WR1:                SIZEOF (QUERY (point <* fea_parametric_point.coordinates |
             SIZEOF (QUERY( representation_item <*
                    using_representations(point) |
                         HIINDEX(point.coordinates) <> 
               representation_item.context_of_items\
               geometric_representation_context.coordinate_space_dimension-1
               ))>0
          ))=0;
END_ENTITY;
(*</PRE>

