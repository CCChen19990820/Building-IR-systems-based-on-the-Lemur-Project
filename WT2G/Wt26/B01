-- Shapes

<A NAME=S3BOOLEAN_SHAPE></A>
<B><I>TYPE</I></B><B> Boolean_Shape</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Flat</B>,
                                      <B>Hyperbolic_Parabolic</B>,
                                      <B>Parabolic</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S3CURVED_SHELL_SHAPE></A>
<B><I>ENTITY</I></B><B> Curved_Shell_Shape</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S3FLAT"><B>Flat</B></A>,
						        <A HREF="#S3CYLINDRICAL"><B>Cylindrical</B></A>,
        						<A HREF="#S3SPHERICAL"><B>Spherical</B></A>,
	        					<A HREF="#S3CONICAL"><B>Conical</B></A>,
		        				<A HREF="#S3HYPERBOLIC_PARABOLIC"><B>Hyperbolic_Parabolic</B></A>,
			        			<A HREF="#S3PARABOLIC"><B>Parabolic</B></A>,
				        		<A HREF="#S3ARBITRARY_CURVED_SHELL_SHAPE"><B>Arbitrary_Curved_Shell_Shape</B></A> ) );
<B><I>DERIVE</I></B>
	default_subtype : <B><I>STRING</I></B> := 'hyperbolic_parabolic';
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3SHELL_OF_REVOLUTION_SHAPE></A>
<B><I>ENTITY</I></B><B> Shell_Of_Revolution_Shape</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S3FLAT"><B>Flat</B></A>,
						        <A HREF="#S3SPHERICAL"><B>Spherical</B></A>,
						        <A HREF="#S3PARABOLIC"><B>Parabolic</B></A> ) );
<B><I>DERIVE</I></B>
	default_subtype : <B><I>STRING</I></B> := 'parabolic';
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3FLAT></A>
<B><I>ENTITY</I></B><B> Flat</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3CURVED_SHELL_SHAPE"><B>Curved_Shell_Shape</B></A>, <A HREF="#S3SHELL_OF_REVOLUTION_SHAPE"><B>Shell_Of_Revolution_Shape</B></A> );
    flat : <A HREF="#S3BOOLEAN_SHAPE"><B>Boolean_Shape</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_flat :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'flat', '/geomet(*)/flat' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3CYLINDRICAL></A>
