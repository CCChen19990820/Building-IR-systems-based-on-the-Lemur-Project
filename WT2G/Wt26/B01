  control_values : <A HREF="#S0NUMERIC_ARRAY"><B>Numeric_Array</B></A>;
  basis           : <B><I>LIST</I></B> [1: ?] <B><I>OF</I></B> <A HREF="#S0BSPLINE_BASIS"><B>Bspline_Basis</B></A>;
-- WHERE
--  consistent_size(basis, SIZEOF(control_values));
<B><I>END_ENTITY</I></B>;

<HR>
(*
*)
<A NAME=S0BSPLINE_BASIS></A>
<B><I>ENTITY</I></B><B> Bspline_Basis</B>;
  degree : <A HREF="#S0POSITIVE_INTEGER"><B>Positive_Integer</B></A>;
  num_basis : <A HREF="#S0POSITIVE_INTEGER"><B>Positive_Integer</B></A>;
  breakpoints : <B><I>LIST</I></B> [2:?] <B><I>OF</I></B> <A HREF="#S0PARAMETER_VALUE"><B>Parameter_Value</B></A>;
  multiplicities : <B><I>LIST</I></B> [2:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
  knot_number : <B><I>INTEGER</I></B> := <B><I>SIZEOF</I></B>(BREAKPOINTS);
<B><I>WHERE</I></B>
  WR1 : <B><I>SIZEOF</I></B>(BREAKPOINTS) = <B><I>SIZEOF</I></B>(MULTIPLICITIES);
  WR2 : <A HREF="#S0CONSTRAINTS_PARAM_B_SPLINE"><B>Constraints_Param_B_Spline</B></A>(DEGREE, KNOT_NUMBER, NUM_BASIS - 1,
                     MULTIPLICITIES, BREAKPOINTS);
<B><I>END_ENTITY</I></B>;

<HR>
(*
*)
<A NAME=S0CONSISTENT_SIZE></A>
<B><I>FUNCTION</I></B><B> Consistent_Size</B>(B_LIST : <B><I>LIST</I></B>[1:?] <B><I>OF</I></B> <A HREF="#S0BSPLINE_BASIS"><B>Bspline_Basis</B></A>;
                         N : <B><I>INTEGER</I></B>) : <B><I>BOOLEAN</I></B>;
  <B><I>LOCAL</I></B>
    M, P, I : <B><I>INTEGER</I></B>;
    TEST    : <B><I>BOOLEAN</I></B> := <B><I>FALSE</I></B>;
  <B><I>END_LOCAL</I></B>;
    M := <B><I>SIZEOF</I></B>(B_LIST);
    P := B_LIST[1].NUM_BASIS;
    <B><I>IF</I></B> (M &gt; 1) <B><I>THEN</I></B>
      <B><I>REPEAT</I></B> I := 2 <B><I>TO</I></B> M;
        P := P*(B_LIST[I].NUM_BASIS);
      <B><I>END_REPEAT</I></B>;
    <B><I>END_IF</I></B>;
    <B><I>IF</I></B> (P = N) <B><I>THEN</I></B>
      TEST:= <B><I>TRUE</I></B>;
    <B><I>END_IF</I></B>;
    <B><I>RETURN</I></B>(TEST);
 <B><I>END_FUNCTION</I></B>;

