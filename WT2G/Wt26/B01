<A NAME=S5DIANA_SPECIAL_PROPERTIES></A>
<B><I>SCHEMA</I></B><B> Diana_Special_Properties</B>;

<HR>
 
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S1S1DIANA_DATA_TYPES">Diana_Data_Types</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S2S2DIANA_COMMON">Diana_Common</A></B>;

<HR>
 
-- Integration schemes
 
-- element:
-- structure:
-- truss, beam 							                        &gt; line
-- plane_stress, plane_strain, axial_symmetric, plate, shell 	&gt; plane
-- solid 								                        &gt; volume
-- interface							                        &gt; point, line, plane
-- spring							                            &gt; point, line
-- mass								                            &gt; point
 
-- flow:		
-- flow_beam_cross_section					                    &gt; plane
-- flow_regular						                            &gt; point, line, plane
-- flow_ground_water						                    &gt; line, plane, volume
-- structure_flow_interaction					                &gt; line, plane
 
-- reinforcement:
-- bar								                            &gt; point, line
-- grid								                            &gt; line, plane
 
<A NAME=S5NLC_GENERAL></A>
<B><I>ENTITY</I></B><B> Nlc_General</B>;
    nlc : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_nlc_general :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nlc', '/data(*)/ngaus' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5NXI_GENERAL></A>
<B><I>ENTITY</I></B><B> Nxi_General</B>;
    nxi : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_nxi_general :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nxi',  '/data(*)/ngaus' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
