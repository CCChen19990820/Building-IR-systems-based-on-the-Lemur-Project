<A NAME=S3DIANA_GEOMETRY_PROPERTIES></A>
<B><I>SCHEMA</I></B><B> Diana_Geometry_Properties</B>;

<HR>
 
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S1S1DIANA_DATA_TYPES">Diana_Data_Types</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S2S2DIANA_COMMON">Diana_Common</A></B>;

<HR>
 
-- Cross sections

<A NAME=S3CROSS_SECTIONS_LOCATION></A>
<B><I>ENTITY</I></B><B> Cross_Sections_Location</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S3CROSS_SECTIONS_LOCATION_BY_NUMBER"><B>Cross_Sections_Location_By_Number</B></A>,
                                <A HREF="#S3CROSS_SECTIONS_LOCATION_ABSOLUTE"><B>Cross_Sections_Location_Absolute</B></A>,
                                <A HREF="#S3CROSS_SECTIONS_LOCATION_RELATIVE"><B>Cross_Sections_Location_Relative</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3CROSS_SECTIONS_LOCATION_BY_NUMBER></A>
<B><I>ENTITY</I></B><B> Cross_Sections_Location_By_Number</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3CROSS_SECTIONS_LOCATION"><B>Cross_Sections_Location</B></A> );
	number_of_sections : <B><I>INTEGER</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_cross_sections_location_by_number :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'number_of_sections', '/geomet(*)/numsec' ) ];
<B><I>WHERE</I></B>
	WR1 : NUMBER_OF_SECTIONS &lt;= 21;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S3CROSS_SECTIONS_LOCATION_ABSOLUTE></A>
<B><I>ENTITY</I></B><B> Cross_Sections_Location_Absolute</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3CROSS_SECTIONS_LOCATION"><B>Cross_Sections_Location</B></A>  );
    values : <B><I>SET</I></B> [1:21] <B><I>OF</I></B> <B><I>REAL</I></B>;
    -- NEC: 0 &lt;= values[i] &lt;= length of beam
<B><I>DERIVE</I></B>
    translation_specification_cross_sections_location_absolute :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'values', '/geomet(*)/abssec' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S3CROSS_SECTIONS_LOCATION_RELATIVE></A>
<B><I>ENTITY</I></B><B> Cross_Sections_Location_Relative</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3CROSS_SECTIONS_LOCATION"><B>Cross_Sections_Location</B></A>  );
    values : <B><I>SET</I></B> [1:21] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_cross_sections_location_relative :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'values', '/geomet(*)/relsec' ) ];
<B><I>WHERE</I></B>
   WR1 : <B><I>SIZEOF</I></B> ( <B><I>QUERY</I></B> (TEMP &lt;* VALUES| <B><I>NOT</I></B>({0 &lt;= TEMP &lt;= 1} ) ) ) = 0;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S3CROSSE></A>
<B><I>ENTITY</I></B><B> Crosse</B>;
	areas : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_crosse :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'areas', '/geomet(*)/crosse' ) ];
<B><I>WHERE</I></B>
    WR1 : <B><I>SIZEOF</I></B> ( <B><I>QUERY</I></B> (TEMP &lt;* AREAS| <B><I>NOT</I></B>({0 &lt;= TEMP &lt;= 1} ) ) ) = 0;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3CROSS_SECTIONAL_AREA_SUBJECTED_TO_WIND_OR_WATER></A>
<B><I>ENTITY</I></B><B> Cross_Sectional_Area_Subjected_To_Wind_Or_Water</B>;
	area : <B><I>REAL</I></B>;
	-- NEC: if cross-section is pipe: default = pipe-diameter
<B><I>DERIVE</I></B>
    translation_specification_cross_sectional_area_subjected_to_wind_or_water :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'area', '/geomet(*)/aflow' ) ];
<B><I>WHERE</I></B>
    WR1 : AREA &gt; 0.0;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3DRAG_COEFFICIENT></A>
<B><I>ENTITY</I></B><B> Drag_Coefficient</B>;
	cd : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
	default_cd : <B><I>REAL</I></B> := 1.3;
    translation_specification_drag_coefficient :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'cd', '/geomet(*)/cdrag' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
 
-- Moments of inertia
 
<A NAME=S3MOMENT_OF_INERTIA></A>
<B><I>ENTITY</I></B><B> Moment_Of_Inertia</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S3MOMENT_OF_INERTIA_2D"><B>Moment_Of_Inertia_2d</B></A>,
         						<A HREF="#S3MOMENT_OF_INERTIA_3D"><B>Moment_Of_Inertia_3d</B></A>,
								<A HREF="#S3MOMENT_OF_INERTIA_FOR_BENDING_FORCES"><B>Moment_Of_Inertia_For_Bending_Forces</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3MOMENT_OF_INERTIA_2D></A>
<B><I>ENTITY</I></B><B> Moment_Of_Inertia_2d</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3MOMENT_OF_INERTIA"><B>Moment_Of_Inertia</B></A> );
	izs : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_moment_of_inertia_2d :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'izs', '/geomet(*)/inerti' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3MOMENT_OF_INERTIA_3D></A>
<B><I>ENTITY</I></B><B> Moment_Of_Inertia_3d</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S3MOMENT_OF_INERTIA"><B>Moment_Of_Inertia</B></A> );
	iys  : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
	izs  : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
	iyzs : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
	its  : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_moment_of_inertia_3d :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'iys',  '/geomet(*)/inerti(1,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'izs',  '/geomet(*)/inerti(2,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'iyzs', '/geomet(*)/inerti(3,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'its', '/geomet(*)/inerti(4,4)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S3MOMENT_OF_INERTIA_FOR_BENDING_FORCES></A>
<B><I>ENTITY</I></B><B> Moment_Of_Inertia_For_Bending_Forces</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S3MOMENT_OF_INERTIA"><B>Moment_Of_Inertia</B></A>);
    ixx : <B><I>REAL</I></B>;
    iyy : <B><I>REAL</I></B>;
    inu : <B><I>REAL</I></B>;
    ixy : <B><I>REAL</I></B>;
    iyx : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_moment_of_inertia_for_bending_forces :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'ixx', '/geomet(*)/benp(1,5)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'iyy', '/geomet(*)/benp(2,5)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'inu', '/geomet(*)/benp(3,5)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'ixy', '/geomet(*)/benp(4,5)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'iyx', '/geomet(*)/benp(5,5)' ) ];
<B><I>WHERE</I></B>
    WR1 : IXX &gt; 0.0;
    WR2 : IYY &gt; 0.0;
    WR3 : INU &gt; 0.0;
    WR4 : IXY &gt; 0.0;
    WR5 : IYX &gt; 0.0;
<B><I>END_ENTITY</I></B>;

<HR>
 
 
-- Local stress points location
 
<A NAME=S3LOCAL_STRESS_POINTS_LOCATION></A>
<B><I>ENTITY</I></B><B> Local_Stress_Points_Location</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S3LOCAL_STRESS_POINTS_LOCATION_2D"><B>Local_Stress_Points_Location_2d</B></A>,
						        <A HREF="#S3LOCAL_STRESS_POINTS_LOCATION_3D"><B>Local_Stress_Points_Location_3d</B></A> ) );
	eys : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>WHERE</I></B>
	WR1 : <B><I>SIZEOF</I></B> (EYS) &lt;= 50;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3LOCAL_STRESS_POINTS_LOCATION_2D></A>
<B><I>ENTITY</I></B><B> Local_Stress_Points_Location_2d</B>
