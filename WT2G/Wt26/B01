-- Nodes
-- used for location definition of elements, compositions and reinforcements

<A NAME=S11NODE></A>
<B><I>ENTITY</I></B><B> Node</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S2DIANA_POINT"><B>Diana_Point</B></A>);
	node_id                   : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    node_axes                 : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11NODE_AXIS"><B>Node_Axis</B></A>;
	external_analysis_results : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S12EXTERNAL_ANALYSIS_RESULT"><B>External_Analysis_Result</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_node : <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'node_id',      '/nodes(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'x_coordinate', '/nodes(*)/coord(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'y_coordinate', '/nodes(*)/coord(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'z_coordinate', '/nodes(*)/coord(3)' ) ];
<B><I>UNIQUE</I></B>
	UR1 : NODE_ID;
<B><I>END_ENTITY</I></B>;

