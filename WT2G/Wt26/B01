<H2><A NAME="definitional_representation">definitional_representation</A></H2>
 context_of_items : parametric_representation_context;
<P>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY definitional_representation
  SUPERTYPE OF (ONEOF(
    <A HREF="#element_representation">element_representation</A>))
  SUBTYPE OF (<A HREF="#representation">representation</A>);
WHERE
  WR1:                SIZEOF(USEDIN(SELF,'')) > 0;
END_ENTITY;
(*</PRE>

<H3>Formal propositions:</H3>
<DL>
<DT>WR1<DD>
</DL>
<H2><A NAME="representation">representation</A></H2>
 shape_representation,
             kinematic_link_representation,
             fea_representation,
         
<P>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY representation
  ABSTRACT SUPERTYPE OF (ONEOF(
    <A HREF="#definitional_representation">definitional_representation</A>));
  items               : SET [1:?] OF <A HREF="#representation_item">representation_item</A>;
  context_of_items    : <A HREF="#representation_context">representation_context</A>;
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>items<DD>
<DT>context_of_items<DD>
</DL>
