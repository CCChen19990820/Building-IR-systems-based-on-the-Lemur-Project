<HR>
 
<A NAME=S4YOUNGS_MODULUS></A>
<B><I>ENTITY</I></B><B> Youngs_Modulus</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S4YOUNGS_MODULUS_ISO"><B>Youngs_Modulus_Iso</B></A>,
				                <A HREF="#S4YOUNGS_MODULUS_ORTHO"><B>Youngs_Modulus_Ortho</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S4YOUNGS_MODULUS_ISO></A>
<B><I>ENTITY</I></B><B> Youngs_Modulus_Iso</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S4YOUNGS_MODULUS"><B>Youngs_Modulus</B></A> );
	e : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_youngs_modulus_iso :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'e', '/materi(*)/young' ) ];
<B><I>WHERE</I></B>
	WR1 : (E &gt; 0);
<B><I>END_ENTITY</I></B>;

<HR>
 
