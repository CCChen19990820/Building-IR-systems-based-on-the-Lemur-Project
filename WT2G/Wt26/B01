<HR>
 
 
-- Hinges
 
<A NAME=S3HINGE_TYPES></A>
<B><I>TYPE</I></B><B> Hinge_Types</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Phix1</B>,
                                    <B>Phix2</B>,
                                    <B>Phiy1</B>,
                                    <B>Phiy2</B>,
                                    <B>Phiz1</B>,
                                    <B>Phiz2</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S3HINGES_2D></A>
<B><I>ENTITY</I></B><B> Hinges_2d</B>;
	hinges : <B><I>SET</I></B> [1:2] <B><I>OF</I></B> <A HREF="#S3HINGE_TYPES"><B>Hinge_Types</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_hinges_2d :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'hinges', '/geomet(*)/hinge' ) ];
<B><I>WHERE</I></B>
    WR1 : <B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* HINGES | <B><I>NOT</I></B>( ( TEMP = <A HREF="#S3HINGE_TYPES"><B>Phiz1</B></A> ) <B><I>OR</I></B>
                                               ( TEMP = <A HREF="#S3HINGE_TYPES"><B>Phiz2</B></A> ) ))) = 0;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3HINGES_3D></A>
<B><I>ENTITY</I></B><B> Hinges_3d</B>;
	hinges : <B><I>SET</I></B> [1:6] <B><I>OF</I></B> <A HREF="#S3HINGE_TYPES"><B>Hinge_Types</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_hinges_3d :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'hinges', '/geomet(*)/hinge' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
 
 
-- Eccentricities
 
