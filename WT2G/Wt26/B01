	nuxy : <B><I>REAL</I></B>;
	nuyz : <B><I>REAL</I></B>;
<B><I>WHERE</I></B>
	WR1 : ((NUXY &gt;= 0) <B><I>AND</I></B> (NUXY &lt; 0.5));
	WR2 : ((NUYZ &gt;= 0) <B><I>AND</I></B> (NUYZ &lt; 0.5));
<B><I>END_ENTITY</I></B>;

