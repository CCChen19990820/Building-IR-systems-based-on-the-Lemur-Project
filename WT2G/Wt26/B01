  SUBTYPE OF (<A HREF="#element_representation">element_representation</A>);
  model_ref           : <A HREF="#fea_model_3d">fea_model_3d</A>;
  systems_and_freedoms: LIST [2:2] OF <A HREF="#system_and_freedom">system_and_freedom</A>;
  description         : <A HREF="#text">text</A>;
UNIQUE
  UR1:                model_ref, element_id;
WHERE
  WR1:                SIZEOF (QUERY(item <* SELF\definitional_representation.items |
              'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
              'DIRECTIONALLY_EXPLICIT_ELEMENT_COEFFICIENT'
              IN TYPEOF (item))) >=1;
  FU1:                required_1d_nodes (
        SELF\element_representation.required_node_list);
  FU2:                additional_1d_nodes (
        SELF\higher_order_element_representation.additional_node_list, 
        'linear');
END_ENTITY;
