<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S3CROSS_SECTION_CHARACTERISTICS_PLATE"><B>Cross_Section_Characteristics_Plate</B></A>,
					       	    <A HREF="#S3CROSS_SECTION_CHARACTERISTICS_FLAT_SHELL"><B>Cross_Section_Characteristics_Flat_Shell</B></A> ) );
	moment_of_inertia_for_bending_forces : <A HREF="#S3MOMENT_OF_INERTIA_FOR_BENDING_FORCES"><B>Moment_Of_Inertia_For_Bending_Forces</B></A>;
	areas_for_shear_forces               : <A HREF="#S3AREAS_FOR_SHEAR_FORCES"><B>Areas_For_Shear_Forces</B></A>;
<B><I>END_ENTITY</I></B>;

