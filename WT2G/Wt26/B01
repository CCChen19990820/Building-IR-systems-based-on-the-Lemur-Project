        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'weight_nagative_y', '/loads(*)/elemen(*)/weighy' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'weight_nagative_z', '/loads(*)/elemen(*)/weighz' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S10LOAD_ELEMENT_POTENTIAL_HEIGHT></A>
<B><I>ENTITY</I></B><B> Load_Element_Potential_Height</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_ELEMENT"><B>Load_Element</B></A> );
      potential_height  : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
      weight_negative_x : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
      weight_negative_y : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
      weight_negative_z : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_load_element_potential_height :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'on_elements',       '/loads(*)/elemen(*)/elemen' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'potential_height',  '/loads(*)/elemen(*)/potent' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'weight_negative_x', '/loads(*)/elemen(*)/weighx' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'weight_negative_y', '/loads(*)/elemen(*)/weighy' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'weight_negative_z', '/loads(*)/elemen(*)/weighz' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S10LOAD_REINFORCEMENT></A>
<B><I>ENTITY</I></B><B> Load_Reinforcement</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S10LOAD_REINFORCEMENT_PRE_STRESS"><B>Load_Reinforcement_Pre_Stress</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD"><B>Load</B></A> );
      on_reinforcements : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11REINFORCEMENT"><B>Reinforcement</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_REINFORCEMENT_PRE_STRESS></A>
<B><I>ENTITY</I></B><B> Load_Reinforcement_Pre_Stress</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S10LOAD_REINFORCEMENT_PRE_STRESS_SET01"><B>Load_Reinforcement_Pre_Stress_Set01</B></A>,
                                <A HREF="#S10LOAD_REINFORCEMENT_PRE_STRESS_SET02"><B>Load_Reinforcement_Pre_Stress_Set02</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_REINFORCEMENT"><B>Load_Reinforcement</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_REINFORCEMENT_PRE_STRESS_SET01></A>
<B><I>ENTITY</I></B><B> Load_Reinforcement_Pre_Stress_Set01</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_REINFORCEMENT_PRE_STRESS"><B>Load_Reinforcement_Pre_Stress</B></A> );
      sxx : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_load_reinforcement_pre_stress_set01 :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'on_reinforcements', '/loads(*)/reinfo(*)/reinfo' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sxx',               '/loads(*)/reinfo(*)/prestr' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_REINFORCEMENT_PRE_STRESS_SET02></A>
<B><I>ENTITY</I></B><B> Load_Reinforcement_Pre_Stress_Set02</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_REINFORCEMENT_PRE_STRESS"><B>Load_Reinforcement_Pre_Stress</B></A> );
      sxx : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
      syy : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_load_reinforcement_pre_stress_set02 :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'on_reinforcements', '/loads(*)/reinfo(*)/reinfo' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sxx',               '/loads(*)/reinfo(*)/prestr(1,2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'syy',               '/loads(*)/reinfo(*)/prestr(2,2)' ) ];
<B><I>END_ENTITY</I></B>;
 
 
<B><I>END_SCHEMA</I></B>; -- diana_loads

