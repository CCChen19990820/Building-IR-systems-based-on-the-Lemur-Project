        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'axis_direction', '/geomet(*)/cylin(2,2)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3SPHERICAL></A>
<B><I>ENTITY</I></B><B> Spherical</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S3CURVED_SHELL_SHAPE"><B>Curved_Shell_Shape</B></A>, <A HREF="#S3SHELL_OF_REVOLUTION_SHAPE"><B>Shell_Of_Revolution_Shape</B></A>);
	cx : <B><I>REAL</I></B>;
	cy : <B><I>REAL</I></B>;
	cz : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
