*)
<A NAME=S0COORDINATE_SYSTEM></A>
<B><I>TYPE</I></B><B> Coordinate_System</B> = <B><I>SELECT</I></B> (
  <A HREF="#S0AXIS2_PLACEMENT_3D"><B>Axis2_Placement_3d</B></A>,
  <A HREF="#S0GRID_BASED_COORDINATE_SYSTEM"><B>Grid_Based_Coordinate_System</B></A>);
<B><I>END_TYPE</I></B>;

<HR>
(*
*)
<A NAME=S0POSITIVE_INTEGER></A>
<B><I>TYPE</I></B><B> Positive_Integer</B> = <B><I>INTEGER</I></B>;
<B><I>WHERE</I></B>
  WR1: <B><I>SELF</I></B> &gt;= 0;
<B><I>END_TYPE</I></B>;

<HR>
(*
*)
<A NAME=S0EDGE_IDENTIFIER></A>
<B><I>TYPE</I></B><B> Edge_Identifier</B> = <B><I>INTEGER</I></B>;
<B><I>END_TYPE</I></B>;

<HR>
(*
*)
<A NAME=S0FACE_IDENTIFIER></A>
<B><I>TYPE</I></B><B> Face_Identifier</B> = <B><I>INTEGER</I></B>;
<B><I>END_TYPE</I></B>;

<HR>
(*
*)
<A NAME=S0LOCAL_COORDINATE_SYSTEM_SELECT></A>
<B><I>TYPE</I></B><B> Local_Coordinate_System_Select</B> = <B><I>SELECT</I></B> (
  <A HREF="#S0ARBITRARY_VOLUME_3D_ELEMENT_COORDINATE_SYSTEM"><B>Arbitrary_Volume_3d_Element_Coordinate_System</B></A>,
  <A HREF="#S0PARAMETRIC_VOLUME_3D_ELEMENT_COORDINATE_SYSTEM"><B>Parametric_Volume_3d_Element_Coordinate_System</B></A>,
  <A HREF="#S0ARBITRARY_VOLUME_2D_ELEMENT_COORDINATE_SYSTEM"><B>Arbitrary_Volume_2d_Element_Coordinate_System</B></A>,
  <A HREF="#S0PARAMETRIC_VOLUME_2D_ELEMENT_COORDINATE_SYSTEM"><B>Parametric_Volume_2d_Element_Coordinate_System</B></A>,
  <A HREF="#S0ALIGNED_SURFACE_3D_ELEMENT_COORDINATE_SYSTEM"><B>Aligned_Surface_3d_Element_Coordinate_System</B></A>,
  <A HREF="#S0PARAMETRIC_SURFACE_3D_ELEMENT_COORDINATE_SYSTEM"><B>Parametric_Surface_3d_Element_Coordinate_System</B></A>,
  <A HREF="#S0CONSTANT_SURFACE_3D_ELEMENT_COORDINATE_SYSTEM"><B>Constant_Surface_3d_Element_Coordinate_System</B></A>,
  <A HREF="#S0ALIGNED_SURFACE_2D_ELEMENT_COORDINATE_SYSTEM"><B>Aligned_Surface_2d_Element_Coordinate_System</B></A>,
  <A HREF="#S0PARAMETRIC_SURFACE_2D_ELEMENT_COORDINATE_SYSTEM"><B>Parametric_Surface_2d_Element_Coordinate_System</B></A>,
  <A HREF="#S0ALIGNED_CURVE_3D_ELEMENT_COORDINATE_SYSTEM"><B>Aligned_Curve_3d_Element_Coordinate_System</B></A>,
  <A HREF="#S0PARAMETRIC_CURVE_3D_ELEMENT_COORDINATE_SYSTEM"><B>Parametric_Curve_3d_Element_Coordinate_System</B></A>);
<B><I>END_TYPE</I></B>;

