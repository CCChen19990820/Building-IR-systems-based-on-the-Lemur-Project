<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S4SHEAR_MODULUS"><B>Shear_Modulus</B></A> );
<B><I>DERIVE</I></B>
    translation_specification_shear_modulus_1d :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'gxy', '/materi(*)/shrmod' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S4SHEAR_MODULUS_3D></A>
<B><I>ENTITY</I></B><B> Shear_Modulus_3d</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S4SHEAR_MODULUS"><B>Shear_Modulus</B></A> );
	gyz : <B><I>REAL</I></B>;
	gzx : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_shear_modulus_3d :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'gxy', '/materi(*)/shrmod(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'gyz', '/materi(*)/shrmod(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'gzx', '/materi(*)/shrmod(3)' ) ];
<B><I>WHERE</I></B>
	WR1 : (GYZ &gt; 0);
	WR2 : (GZX &gt; 0);
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S4LINEAR_STIFFNESS_MODULUS></A>
<B><I>ENTITY</I></B><B> Linear_Stiffness_Modulus</B>;
	d11 : <B><I>REAL</I></B>;
	d22 : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_linear_stiffness_modulus :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'd11', '/materi(*)/dstif(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'd22', '/materi(*)/dstif(2)' ) ];
<B><I>WHERE</I></B>
	WR1 : (D11 &gt;= 0);
	WR2 : (D22 &gt; 0);
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S4LINEAR_SPRING_STIFFNESS></A>
