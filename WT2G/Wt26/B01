<H2><A NAME="representation_item">representation_item</A></H2>
 topological_representation_item,
                mapped_item,
                styled_item,
                su_parameters,
                representation_item_without_style,
            
<P>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY representation_item
  ABSTRACT SUPERTYPE OF (ONEOF(
    <A HREF="#geometric_representation_item">geometric_representation_item</A>,
    <A HREF="#fea_representation_item">fea_representation_item</A>,
    <A HREF="#fea_material_property_representation_item">fea_material_property_representation_item</A>));
WHERE
  WR1:                SIZEOF(using_representations(SELF)) +
         SIZEOF(using_definitional_representation_items(SELF)) > 0;
END_ENTITY;
(*</PRE>

<H3>Formal propositions:</H3>
<DL>
<DT>WR1<DD>
</DL>
<H2><A NAME="geometric_representation_item">geometric_representation_item</A></H2>
  vector,
                              cartesian_transformation_operator,
                                curve,
                                surface,
                                vertex_point,
                                edge_curve,
                                face_surface,
                                poly_loop,
                                edge_based_wireframe_model,
                                face_based_surface_model,
                                solid_model,
                                csg_model,
                                sphere,
                                block,
                                right_angular_wedge,
                                torus,
                                right_circular_cone,
                                right_circular_cylinder,
                                solid_replica,
