<HR>
 
<A NAME=S3AREAS_FOR_SHEAR_FORCES></A>
<B><I>ENTITY</I></B><B> Areas_For_Shear_Forces</B>;
	axz : <B><I>REAL</I></B>;
	ayz : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_areas_for_shear_forces :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'axz', '/geomet(*)/shrp(1,2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'ayz', '/geomet(*)/shrp(2,2)' ) ];
<B><I>WHERE</I></B>
    WR1 : AXZ &gt; 0.0;
    WR2 : AYZ &gt; 0.0;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3AREAS_FOR_CROSS_SECTION></A>
<B><I>ENTITY</I></B><B> Areas_For_Cross_Section</B>;
	axx : <B><I>REAL</I></B>;
	ayy : <B><I>REAL</I></B>;
	anu : <B><I>REAL</I></B>;
	axy : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_areas_for_cross_section :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'axx', '/geomet(*)/memp(1,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'ayy', '/geomet(*)/memp(2,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'anu', '/geomet(*)/memp(3,4)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'axy', '/geomet(*)/memp(4,4)' ) ];
<B><I>END_ENTITY</I></B>;

