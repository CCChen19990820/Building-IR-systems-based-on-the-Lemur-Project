<A NAME=S10Types></A>
<H3>Types</H3>
<B><A HREF="#S10EXTREME_VALUE">Extreme_Value</A></B>
<B><A HREF="#S10LOAD_CASE_OR_SET_OR_EXTREME">Load_Case_Or_Set_Or_Extreme</A></B>

<HR>
 
 
<A NAME=S10DIANA_LOADS></A>
<B><I>SCHEMA</I></B><B> Diana_Loads</B>;

<HR>
 
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S1S1DIANA_DATA_TYPES">Diana_Data_Types</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S2S2DIANA_COMMON">Diana_Common</A></B>;

<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S3S3DIANA_GEOMETRY_PROPERTIES">Diana_Geometry_Properties</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S4S4DIANA_LINSTA_MATERIAL_PROPERTIES">Diana_Linsta_Material_Properties</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S5S5DIANA_SPECIAL_PROPERTIES">Diana_Special_Properties</A></B>;

<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S7S7DIANA_GEOMETRY_SPECS">Diana_Geometry_Specs</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S8S8DIANA_LINSTA_MATERIAL_SPECS">Diana_Linsta_Material_Specs</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S9S9DIANA_SPECIAL_SPECS">Diana_Special_Specs</A></B>;

<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S6S6DIANA_SPECS">Diana_Specs</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S11S11DIANA_FE_MODEL">Diana_Fe_Model</A></B>;
<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S12S12DIANA_LINSTA_ANALYSIS_OUTPUT">Diana_Linsta_Analysis_Output</A></B>;

<HR>
<A NAME=S10LOAD_CASE_OR_SET_OR_EXTREME></A>
<B><I>TYPE</I></B><B> Load_Case_Or_Set_Or_Extreme</B> = <B><I>SELECT</I></B>( <A HREF="#S10LOAD_SET"><B>Load_Set</B></A>,
                                           <A HREF="#S10LOAD_CASE"><B>Load_Case</B></A>,
                                           <A HREF="#S10EXTREME_VALUE"><B>Extreme_Value</B></A> );
<B><I>END_TYPE</I></B>;

<HR>
<A NAME=S10EXTREME_VALUE></A>
<B><I>TYPE</I></B><B> Extreme_Value</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B>( <B>Minimum</B>,
                                     <B>Maximum</B>,
                                     <B>Fatigue_Failure</B> );
<B><I>END_TYPE</I></B>;

<HR>
<A NAME=S10LOAD_SET></A>
<B><I>ENTITY</I></B><B> Load_Set</B>;
   load_set_id : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
   load_cases  : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S10LOAD_CASE"><B>Load_Case</B></A>;
   factors     : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_load_set :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'load_set_id', '/combin(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'load_cases',  '/combin(*)/cases' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'factors',     '/combin(*)/factor' ) ];
<B><I>UNIQUE</I></B>
   UR1 : LOAD_SET_ID;
<B><I>WHERE</I></B>
    WR1 : ( <B><I>SIZEOF</I></B> (LOAD_CASES) = <B><I>SIZEOF</I></B> (FACTORS) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_CASE></A>
<B><I>ENTITY</I></B><B> Load_Case</B>;
   load_case_id : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
   loads        : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S10LOAD"><B>Load</B></A>;
<B><I>UNIQUE</I></B>
   UR1 : LOAD_CASE_ID;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD></A>
<B><I>ENTITY</I></B><B> Load</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S10LOAD_MODEL"><B>Load_Model</B></A>,
                                <A HREF="#S10LOAD_NODAL"><B>Load_Nodal</B></A>,
                                <A HREF="#S10LOAD_ELEMENT"><B>Load_Element</B></A>,
                                <A HREF="#S10LOAD_REINFORCEMENT"><B>Load_Reinforcement</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_MODEL></A>
<B><I>ENTITY</I></B><B> Load_Model</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S10LOAD_MODEL_DEAD_WEIGHT"><B>Load_Model_Dead_Weight</B></A>,
                                <A HREF="#S10LOAD_MODEL_CENTRIFUGAL"><B>Load_Model_Centrifugal</B></A>,
                                <A HREF="#S10LOAD_MODEL_WIND"><B>Load_Model_Wind</B></A>,
                                <A HREF="#S10LOAD_MODEL_WATER_WAVE"><B>Load_Model_Water_Wave</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD"><B>Load</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_MODEL_DEAD_WEIGHT></A>
<B><I>ENTITY</I></B><B> Load_Model_Dead_Weight</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_MODEL"><B>Load_Model</B></A> );
      gravity_acceleration : <B><I>REAL</I></B>;
      direction            : <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_load_model_dead_weight :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'gravity_acceleration', '/loads(*)/weight(*)/accel' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'direction',            '/loads(*)/weight(*)/dir' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_MODEL_CENTRIFUGAL></A>
<B><I>ENTITY</I></B><B> Load_Model_Centrifugal</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_MODEL"><B>Load_Model</B></A> );
      turning_speed            : <B><I>REAL</I></B>;
      coordinates_first_point  : <B><I>LIST</I></B> [3:3] <B><I>OF</I></B> <B><I>REAL</I></B>;
      coordinates_second_point : <B><I>LIST</I></B> [3:3] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_load_model_centrifugal :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'turning_speed',           '/loads(*)/centri/omega' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'coordinates_first_point', '/loads(*)/centri/points(1,2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'coordinates_second_point', '/loads(*)/centri/points(2,2)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_MODEL_WIND></A>
<B><I>ENTITY</I></B><B> Load_Model_Wind</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_MODEL"><B>Load_Model</B></A> );
      roughness_height    : <B><I>REAL</I></B>;
      displacement_height : <B><I>REAL</I></B>;
      friction_wind_speed : <B><I>REAL</I></B>;
      direction           : <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>;
      air_density         : <B><I>REAL</I></B>;
      global_z_ground     : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    default_global_z_ground : <B><I>REAL</I></B> := 0.0;
    translation_specification_load_model_wind :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'roughness_height',    '/loads(*)/wind(*)/averag(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'displacement_height', '/loads(*)/wind(*)/averag(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'friction_wind_speed', '/loads(*)/wind(*)/averag(3)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'direction',           '/loads(*)/wind(*)/direct' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'air_density',         '/loads(*)/wind(*)/densit' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'global_z_ground',     '/loads(*)/wind(*)/ground' ) ];
<B><I>WHERE</I></B>
      WR1 : (ROUGHNESS_HEIGHT &gt; 0.0);
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_MODEL_WATER_WAVE></A>
<B><I>ENTITY</I></B><B> Load_Model_Water_Wave</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_MODEL"><B>Load_Model</B></A> );
      water_height         : <B><I>REAL</I></B>;
      water_velocity       : <B><I>REAL</I></B>;
      direction            : <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>;
      water_density        : <B><I>REAL</I></B>;
      global_z_ground      : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
      global_z_still_water : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    default_global_z_ground : <B><I>REAL</I></B> := 0.0;
    translation_specification_load_model_water_wave :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'water_heigth',         '/loads(*)/water(*)/averag(1,2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'water_velocity',       '/loads(*)/water(*)/averag(2,2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'direction',            '/loads(*)/water(*)/direct' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'water_density',        '/loads(*)/water(*)/densit' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'global_z_ground',      '/loads(*)/water(*)/ground' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'global_z_still_water', '/loads(*)/water(*)/level' ) ];
<B><I>WHERE</I></B>
      WR1 : (GLOBAL_Z_STILL_WATER &gt; GLOBAL_Z_GROUND);
      WR2 : WATER_HEIGHT &gt; 0.0;
      WR3 : WATER_VELOCITY &gt; 0.0;
      WR4 : WATER_DENSITY &gt; 0.0;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_NODAL></A>
<B><I>ENTITY</I></B><B> Load_Nodal</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S10LOAD_NODAL_FORCE"><B>Load_Nodal_Force</B></A>,
                                <A HREF="#S10LOAD_NODAL_MOMENT"><B>Load_Nodal_Moment</B></A>,
                                <A HREF="#S10LOAD_NODAL_DEFORMATION"><B>Load_Nodal_Deformation</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD"><B>Load</B></A> );
      direction : <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>;
      on_nodes  : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11NODE"><B>Node</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_NODAL_FORCE></A>
<B><I>ENTITY</I></B><B> Load_Nodal_Force</B>
