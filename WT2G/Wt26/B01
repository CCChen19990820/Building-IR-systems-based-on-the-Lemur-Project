	t : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_element_thickness_iso_constant :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 't', '/geomet(*)/thick' ) ];
<B><I>WHERE</I></B>
    WR1 : T &gt; 0.0;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3ELEMENT_THICKNESS_ISO_VARIABLE></A>
<B><I>ENTITY</I></B><B> Element_Thickness_Iso_Variable</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S3ELEMENT_THICKNESS_ISO"><B>Element_Thickness_Iso</B></A>);
	ts: <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_element_thickness_iso_variable :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'ts', '/geomet(*)/thick' ) ];
<B><I>WHERE</I></B>
    WR1 : <B><I>SIZEOF</I></B> ( <B><I>QUERY</I></B> (TEMP &lt;* TS| <B><I>NOT</I></B>({0 &lt;= TEMP &lt;= 1} ) ) ) = 0;
<B><I>END_ENTITY</I></B>;

