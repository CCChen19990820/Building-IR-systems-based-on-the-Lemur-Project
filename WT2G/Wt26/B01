	tzs  : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
	ttys : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
	ttzs : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
