<DL>
<DT>section<DD>
</DL>
<H2><A NAME="fea_parametric_point">fea_parametric_point</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY fea_parametric_point
  SUBTYPE OF (<A HREF="#point">point</A>);
  coordinates         : LIST [1:3] OF <A HREF="#measure_value">measure_value</A>;
WHERE
  WR1:                valid_parametric_coordinate (coordinates);
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>coordinates<DD>
</DL>
<H3>Formal propositions:</H3>
<DL>
<DT>WR1<DD>
</DL>
<H2><A NAME="volume_element_location">volume_element_location</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY volume_element_location
  SUBTYPE OF (<A HREF="#fea_representation_item">fea_representation_item</A>);
  coordinates         : <A HREF="#fea_parametric_point">fea_parametric_point</A>;
WHERE
  WR1:                SIZEOF (QUERY (point <* fea_parametric_point.coordinates |
             SIZEOF (QUERY( representation_item <*
                    using_representations(point) |
                         HIINDEX(point.coordinates) <> 
               representation_item.context_of_items\
               geometric_representation_context.coordinate_space_dimension
               ))>0
          ))=0;
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>coordinates<DD>
</DL>
<H3>Formal propositions:</H3>
<DL>
<DT>WR1<DD>
</DL>
<H2><A NAME="surface_volume_element_location">surface_volume_element_location</A></H2>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY surface_volume_element_location
  SUBTYPE OF (<A HREF="#fea_representation_item">fea_representation_item</A>);
  field_location      : <A HREF="#surface_element_location">surface_element_location</A>;
  section_location    : <A HREF="#surface_section_element_location">surface_section_element_location</A>;
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>field_location<DD>
<DT>section_location<DD>
</DL>
