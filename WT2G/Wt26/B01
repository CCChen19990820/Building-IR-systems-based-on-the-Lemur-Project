<A HREF="#S0ADVANCED_OR_GEM_SHAPE"><B>Advanced_Or_Gem_Shape</B></A> <B><I>FOR</I></B>(<A HREF="#S0SHAPE_REPRESENTATION"><B>Shape_Representation</B></A>);
<B><I>WHERE</I></B>
   WR1: <B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (SR &lt;* <A HREF="#S0SHAPE_REPRESENTATION"><B>Shape_Representation</B></A> |
                <B><I>NOT</I></B>( <B><I>SIZEOF</I></B>(
       ['GENERIC_ENGINEERING_ANALYSIS_MODEL.ADVANCED_BREP_SHAPE_REPRESENTATION',
        'GENERIC_ENGINEERING_ANALYSIS_MODEL.GEM_SHAPE_REPRESENTATION']
           * <B><I>TYPEOF</I></B> (SR)) =1 ))) = 0;
<B><I>END_RULE</I></B>;

