        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'neta',  '/data(*)/ngaus(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nzeta', '/data(*)/ngaus(3)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5NLC_NZETA_GENERAL></A>
