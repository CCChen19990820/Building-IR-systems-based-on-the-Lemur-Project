-- Geometry factors
 
<A NAME=S3SHAPE_FACTOR></A>
<B><I>ENTITY</I></B><B> Shape_Factor</B>;
	kz : <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
	default_kz : <B><I>REAL</I></B> := 1.5;
    translation_specification_shape_factor :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'kz', '/geomet(*)/kfac' ) ];
<B><I>WHERE</I></B>
    WR1 : KZ &gt; 0.0;
<B><I>END_ENTITY</I></B>;

<HR>
 
