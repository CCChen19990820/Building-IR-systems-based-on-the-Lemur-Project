<A NAME=S11ELEMENT></A>
<B><I>ENTITY</I></B><B> Element</B>; -- material, geometry and special data via diana_element_spec
	element_id         : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	of_type            : <A HREF="#S6DIANA_ELEMENT_TYPE"><B>Diana_Element_Type</B></A>;
	nodes              : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT_NODE"><B>Element_Node</B></A>;
    element_axes       : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT_AXIS"><B>Element_Axis</B></A>;
	integration_points : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11INTEGRATION_POINT"><B>Integration_Point</B></A>;
	stress_points      : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:50] <B><I>OF</I></B> <A HREF="#S11STRESS_POINT"><B>Stress_Point</B></A>;
	cross_sections     : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:21] <B><I>OF</I></B> <A HREF="#S11ELEMENT_CROSS_SECTION"><B>Element_Cross_Section</B></A>;
	surfaces           : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT_SURFACE"><B>Element_Surface</B></A>;
	analysis_results   : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S12INTERNAL_ANALYSIS_RESULT"><B>Internal_Analysis_Result</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_element : <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'element_id',            '/elemen(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'of_type',               '/elemen(*)/type' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nodes',                 '/elemen(*)/nodes' ) ];
<B><I>UNIQUE</I></B>
	UR1 : ELEMENT_ID;
<B><I>END_ENTITY</I></B>;

