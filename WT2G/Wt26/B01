<B><I>DERIVE</I></B>
      shape_type : <B><I>STRING</I></B> (2) <B><I>FIXED</I></B> := 'tr';
   shell_flat_regular_triangular_load_in_location_in_direction_type :
   <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S6LOAD_IN_LOCATION_IN_DIRECTION_TYPE"><B>Load_In_Location_In_Direction_Type</B></A> :=
