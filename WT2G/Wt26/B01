(*
*)
<A NAME=S0ADVANCED_BREP_SHAPE_REPRESENTATION></A>
<B><I>ENTITY</I></B><B> Advanced_Brep_Shape_Representation</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S0SHAPE_REPRESENTATION"><B>Shape_Representation</B></A>);
<B><I>WHERE</I></B>
        WR1: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( IT &lt;* <B><I>SELF</I></B>.ITEMS | (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>([
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.MANIFOLD_SOLID_BREP',
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.FACETED_BREP',
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.MAPPED_ITEM',
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.AXIS2_PLACEMENT_3D'] * <B><I>TYPEOF</I></B>(IT)) = 1)) ))
              = 0;
        WR2: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( IT &lt;* <B><I>SELF</I></B>.ITEMS | (<B><I>SIZEOF</I></B>([
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.MANIFOLD_SOLID_BREP',
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.MAPPED_ITEM'] * <B><I>TYPEOF</I></B>(IT)) = 1) )) &gt; 0;
        WR3: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( MSB &lt;* <B><I>QUERY</I></B> ( IT &lt;* <B><I>SELF</I></B>.ITEMS |
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.MANIFOLD_SOLID_BREP' <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) |
              (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( CSH &lt;* <A HREF="#S0MSB_SHELLS"><B>Msb_Shells</B></A>(MSB) |
              (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( FCS &lt;* CSH\<A HREF="#S0CONNECTED_FACE_SET"><B>Connected_Face_Set</B></A>.CFS_FACES |
              (<B><I>NOT</I></B> ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ADVANCED_FACE' <B><I>IN</I></B> <B><I>TYPEOF</I></B>(FCS))) )) = 0)) ))= 0)) )) =
              0;
        WR4: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( MSB &lt;* <B><I>QUERY</I></B> ( IT &lt;* ITEMS |
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.MANIFOLD_SOLID_BREP' <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) |
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ORIENTED_CLOSED_SHELL' <B><I>IN</I></B> <B><I>TYPEOF</I></B>(MSB\
              <A HREF="#S0MANIFOLD_SOLID_BREP"><B>Manifold_Solid_Brep</B></A>.OUTER)) )) = 0;
        WR5: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( BRV &lt;* <B><I>QUERY</I></B> ( IT &lt;* ITEMS |
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.BREP_WITH_VOIDS' <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) | (<B><I>NOT</I></B>
              (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( CSH &lt;* BRV\<A HREF="#S0BREP_WITH_VOIDS"><B>Brep_With_Voids</B></A>.VOIDS |
              (CSH\<A HREF="#S0ORIENTED_CLOSED_SHELL"><B>Oriented_Closed_Shell</B></A>.<A HREF="#S0PREDEFINED_TYPE_OF_QUANTITY"><B>Orientation</B></A>)))  = 0)) )) = 0;
        WR6: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( MI &lt;* <B><I>QUERY</I></B> ( IT &lt;* ITEMS |
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.MAPPED_ITEM' <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) | (<B><I>NOT</I></B>
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ADVANCED_BREP_SHAPE_REPRESENTATION' <B><I>IN</I></B>
              <B><I>TYPEOF</I></B>(MI\<A HREF="#S0MAPPED_ITEM"><B>Mapped_Item</B></A>.MAPPING_SOURCE.MAPPED_REPRESENTATION))) ))= 0;
<B><I>END_ENTITY</I></B>;

<HR>
(*
*)
<A NAME=S0MSB_SHELLS></A>
<B><I>FUNCTION</I></B><B> Msb_Shells</B> (   BREP: <A HREF="#S0MANIFOLD_SOLID_BREP"><B>Manifold_Solid_Brep</B></A>) :
                        <B><I>SET</I></B> [1:?] <B><I>OF</I></B>  <A HREF="#S0CLOSED_SHELL"><B>Closed_Shell</B></A>;
     <B><I>LOCAL</I></B>
       BTYPE      : <B><I>SET</I></B>[1 : ?] <B><I>OF</I></B> <B><I>STRING</I></B> := [];
       WITHVOIDS  : <B><I>BOOLEAN</I></B> := <B><I>FALSE</I></B>;
     <B><I>END_LOCAL</I></B>;
       BTYPE := <B><I>TYPEOF</I></B>(BREP);
     <B><I>REPEAT</I></B> I := 1 <B><I>TO</I></B> <B><I>SIZEOF</I></B>(BTYPE);
       <B><I>IF</I></B> (BTYPE[I] <B><I>LIKE</I></B> '*BREP_WITH_VOIDS') <B><I>THEN</I></B>
         WITHVOIDS := <B><I>TRUE</I></B>;
       <B><I>END_IF</I></B>;
     <B><I>END_REPEAT</I></B>;
     <B><I>IF</I></B> (WITHVOIDS) <B><I>THEN</I></B>
       <B><I>RETURN</I></B> (BREP\<A HREF="#S0BREP_WITH_VOIDS"><B>Brep_With_Voids</B></A>.VOIDS + BREP.OUTER);
     <B><I>ELSE</I></B>
       <B><I>RETURN</I></B>([BREP.OUTER]);
     <B><I>END_IF</I></B>;
<B><I>END_FUNCTION</I></B>;

<HR>
(*
*)
-- END_SCHEMA; -- end AIC_ADVANCED_BREP SCHEMA
