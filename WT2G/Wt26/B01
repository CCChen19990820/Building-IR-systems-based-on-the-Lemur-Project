  WR1: (<B><I>SIZEOF</I></B>(ELEMENTS) &gt; 1) <B><I>OR</I></B>
      ((<B><I>SIZEOF</I></B>(ELEMENTS) = 1) <B><I>AND</I></B> (ELEMENTS[1].EXPONENT &lt;&gt; 1.0));
