    nxi_line    : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    netas_line  : <B><I>OPTIONAL</I></B> <B><I>SET</I></B>[1:?] <B><I>OF</I></B> <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    nzetas_line : <B><I>OPTIONAL</I></B> <B><I>SET</I></B>[1:?] <B><I>OF</I></B> <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
