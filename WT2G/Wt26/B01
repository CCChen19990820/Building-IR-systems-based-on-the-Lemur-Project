                                offset_surface_solid,
                                boolean_result,
                                half_space_solid,
                                planar_extent,
                                camera_image,
                                camera_model,
                                light_source,
                                cross_product
<P>
<I>EXPRESS specification:</I><PRE WIDTH=80>*)
ENTITY geometric_representation_item
  ABSTRACT SUPERTYPE OF (ONEOF(
    <A HREF="#direction">direction</A>,
    <A HREF="#point">point</A>,
    <A HREF="#placement">placement</A>))
  SUBTYPE OF (<A HREF="#representation_item">representation_item</A>);
DERIVE
  dim                 : INTEGER := dimension_of(SELF);
END_ENTITY;
(*</PRE>

<H3>Attribute definitions:</H3>
<DL>
<DT>dim<DD>
</DL>
