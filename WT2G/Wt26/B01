ENTITY volume_3d_element_representation
  SUBTYPE OF (<A HREF="#element_representation">element_representation</A>);
  model_ref           : <A HREF="#fea_model_3d">fea_model_3d</A>;
UNIQUE
  UR1:                model_ref, element_id;
WHERE
  WR1:                SIZEOF (QUERY(item <* SELF\definitional_representation.items |
              'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
              'VOLUME_3D_ELEMENT_DESCRIPTOR'
              IN TYPEOF (item))) = 1;
  WR2:                SIZEOF (QUERY(item <* SELF\definitional_representation.items |
               'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
               'ARBITRARY_VOLUME_3D_ELEMENT_COORDINATE_SYSTEM'
               IN TYPEOF (item))) = 1;
  WR3:                SIZEOF (QUERY(item <* SELF\definitional_representation.items |
               'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
               'PARAMETRIC_VOLUME_3D_ELEMENT_COORDINATE_SYSTEM'
               IN TYPEOF (item))) = 1;
  WR4:                (SIZEOF (QUERY(item <* SELF\definitional_representation.items |
               'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
               'PARAMETRIC_VOLUME_3D_ELEMENT_COORDINATE_SYSTEM'
               IN TYPEOF (item))) = 1)
       XOR
       (SIZEOF (QUERY(item <* SELF\definitional_representation.items |
               'STRUCTURAL_RESPONSE_REPRESENTATION_SCHEMA.' +
               'ARBITRARY_VOLUME_3D_ELEMENT_COORDINATE_SYSTEM'
               IN TYPEOF (item))) = 1);
  FU1:                required_3d_nodes (
        SELF\element_representation.required_node_list,
        items\volume_3d_element_descriptor.shape);
  FU2:                additional_3d_nodes (
        SELF\higher_order_element_representation.additional_node_list,
        items\volume_3d_element_descriptor.shape,
        items\volume_3d_element_descriptor.order);
END_ENTITY;
