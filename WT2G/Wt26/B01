    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'cx', '/geomet(*)/spher(1,3)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'cy', '/geomet(*)/spher(2,3)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'cz', '/geomet(*)/spher(3,3)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S3CONICAL></A>
<B><I>ENTITY</I></B><B> Conical</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (<A HREF="#S3CURVED_SHELL_SHAPE"><B>Curved_Shell_Shape</B></A>);
	top : <A HREF="#S2REFERENCE_POINT"><B>Reference_Point</B></A>;
