        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nxi_interface',  '/data(*)/nlobat(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nxi_interface',  '/data(*)/lumped(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'neta_interface', '/data(*)/ngaus(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'neta_interface', '/data(*)/nnewco(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'neta_interface', '/data(*)/nlobat(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'neta_interface', '/data(*)/lumped(2)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5NUMERICAL_INTEGRATION_TYPE></A>
<B><I>TYPE</I></B><B> Numerical_Integration_Type</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Gauss</B>,
                                                   <B>Hammer</B>,
                                                   <B>Lumped</B>,
                                                   <B>Lobat</B>,
                                                   <B>Newco</B>,
                                                   <B>Simpso</B>,
                                                   <B>Trapez</B> );
<B><I>END_TYPE</I></B>;

<HR>
<A NAME=S5MLC_GENERAL></A>
<B><I>ENTITY</I></B><B> Mlc_General</B>;
    mlc : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>WHERE</I></B>
    WR1 : ( MLC = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Hammer</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5MXI_GENERAL></A>
<B><I>ENTITY</I></B><B> Mxi_General</B>;
    mxi : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>WHERE</I></B>
    WR1 : ( MXI = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5MXI_META_GENERAL></A>
<B><I>ENTITY</I></B><B> Mxi_Meta_General</B>;
	mxi  : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
	meta : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>WHERE</I></B>
    WR1 : ( MXI  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> );
    WR2 : ( META = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5MXI_META_MZETA_GENERAL></A>
<B><I>ENTITY</I></B><B> Mxi_Meta_Mzeta_General</B>;
	mxi   : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
	meta  : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
    mzeta : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>WHERE</I></B>
    WR1 : ( MXI   = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> );
    WR2 : ( META  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> );
    WR3 : ( MZETA = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5MLC_MZETA_GENERAL></A>
<B><I>ENTITY</I></B><B> Mlc_Mzeta_General</B>;
    mlc   : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
    mzeta : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>WHERE</I></B>
    WR1 : ( MLC   = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Hammer</B></A> );
    WR2 : ( MZETA = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A>  );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5MXI_META_MZETA_LINE></A>
<B><I>ENTITY</I></B><B> Mxi_Meta_Mzeta_Line</B>;
    mxi_line    : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
    metas_line  : <B><I>OPTIONAL</I></B> <B><I>SET</I></B>[1:?] <B><I>OF</I></B> <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
    mzetas_line : <B><I>OPTIONAL</I></B> <B><I>SET</I></B>[1:?] <B><I>OF</I></B> <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_mxi_meta_mzeta_line :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_line',    '/data(*)/numint(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'metas_line', '/data(*)/numint(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mzetas_line', '/data(*)/numint(3)' ) ];
<B><I>WHERE</I></B>
    WR1 : ( MXI_LINE  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> ) <B><I>OR</I></B> ( MXI_LINE   = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Simpso</B></A> ) <B><I>OR</I></B>
          ( MXI_LINE  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Newco</B></A> ) <B><I>OR</I></B> ( MXI_LINE   = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Lobat</B></A> );
    WR2 : <B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* METAS_LINE  | <B><I>NOT</I></B>( ( TEMP = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A>  ) <B><I>OR</I></B>
                                                    ( TEMP = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Trapez</B></A> ) <B><I>OR</I></B> 
                                                    ( TEMP = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Simpso</B></A> ) ))) = 0;
    WR3 : <B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* MZETAS_LINE | <B><I>NOT</I></B>( ( TEMP = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A>  ) <B><I>OR</I></B>
                                                    ( TEMP = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Simpso</B></A> ) ))) = 0;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5MXI_INTERFACE></A>
<B><I>ENTITY</I></B><B> Mxi_Interface</B>;
    mxi_interface : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_mxi_interface :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface', '/data(*)/ngaus' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface', '/data(*)/nnewco' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface', '/data(*)/nlobat' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface', '/data(*)/lumped' ) ];
<B><I>WHERE</I></B>
    WR1 : ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A> = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> ) <B><I>OR</I></B> ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A> = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Newco</B></A>  ) <B><I>OR</I></B>
          ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A> = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Lobat</B></A> ) <B><I>OR</I></B> ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A> = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Lumped</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S5MXI_META_INTERFACE></A>
<B><I>ENTITY</I></B><B> Mxi_Meta_Interface</B>;
    mxi_interface  : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
    meta_interface : <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Numerical_Integration_Type</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_mxi_meta_interface :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface',  '/data(*)/ngaus(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface',  '/data(*)/nnewco(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface',  '/data(*)/nlobat(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mxi_interface',  '/data(*)/lumped(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'meta_interface', '/data(*)/ngaus(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'meta_interface', '/data(*)/nnewco(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'meta_interface', '/data(*)/nlobat(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'meta_interface', '/data(*)/lumped(2)' ) ];
<B><I>WHERE</I></B>
    WR1 : ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A>  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> ) <B><I>OR</I></B> ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A>  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Newco</B></A>  ) <B><I>OR</I></B>
          ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A>  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Lobat</B></A> ) <B><I>OR</I></B> ( <A HREF="#S5MXI_INTERFACE"><B>Mxi_Interface</B></A>  = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Lumped</B></A> );
    WR2 : ( META_INTERFACE = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Gauss</B></A> ) <B><I>OR</I></B> ( META_INTERFACE = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Newco</B></A>  ) <B><I>OR</I></B>
          ( META_INTERFACE = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Lobat</B></A> ) <B><I>OR</I></B> ( META_INTERFACE = <A HREF="#S5NUMERICAL_INTEGRATION_TYPE"><B>Lumped</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
-- Assumed strain options

<A NAME=S5BOOLEAN_SHEAR_OPTION></A>
<B><I>TYPE</I></B><B> Boolean_Shear_Option</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>No_Constant_Shear</B>,
                                             <B>Constant_Shear</B>,
                                             <B>Constant_Dilatation</B>,
                                             <B>Incompatible_Bubble_Displacement</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S5ASSUMED_STRAIN_OPTION_PLANE_STRESS></A>
<B><I>ENTITY</I></B><B> Assumed_Strain_Option_Plane_Stress</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S5NO_CONSTANT_SHEAR"><B>No_Constant_Shear</B></A>,
				                <A HREF="#S5CONSTANT_SHEAR"><B>Constant_Shear</B></A>,
				                <A HREF="#S5ENHANCED_ASSUMED_STRAIN_PLANE_STRESS"><B>Enhanced_Assumed_Strain_Plane_Stress</B></A>,
				                <A HREF="#S5INCOMPATIBLE_BUBBLE_DISPLACEMENT"><B>Incompatible_Bubble_Displacement</B></A> ) );
<B><I>DERIVE</I></B>
	default_subtype : <B><I>STRING</I></B> := 'constant_shear';
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S5ASSUMED_STRAIN_OPTION_PLANE_STRAIN></A>
<B><I>ENTITY</I></B><B> Assumed_Strain_Option_Plane_Strain</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S5NO_CONSTANT_SHEAR"><B>No_Constant_Shear</B></A>,
				                <A HREF="#S5CONSTANT_SHEAR"><B>Constant_Shear</B></A>,
				                <A HREF="#S5ENHANCED_ASSUMED_STRAIN_PLANE_STRAIN"><B>Enhanced_Assumed_Strain_Plane_Strain</B></A>,
				                <A HREF="#S5INCOMPATIBLE_BUBBLE_DISPLACEMENT"><B>Incompatible_Bubble_Displacement</B></A>,
				                <A HREF="#S5CONSTANT_DILATATION"><B>Constant_Dilatation</B></A> ) );
<B><I>DERIVE</I></B>
	default_subtype : <B><I>STRING</I></B> := 'constant_shear';
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S5ASSUMED_STRAIN_OPTION_AXIAL_SYMMETRIC></A>
<B><I>ENTITY</I></B><B> Assumed_Strain_Option_Axial_Symmetric</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S5NO_CONSTANT_SHEAR"><B>No_Constant_Shear</B></A>,
				                <A HREF="#S5CONSTANT_SHEAR"><B>Constant_Shear</B></A>,
				                <A HREF="#S5ENHANCED_ASSUMED_STRAIN_AXIAL_SYMMETRIC"><B>Enhanced_Assumed_Strain_Axial_Symmetric</B></A>,
				                <A HREF="#S5INCOMPATIBLE_BUBBLE_DISPLACEMENT"><B>Incompatible_Bubble_Displacement</B></A>,
				                <A HREF="#S5CONSTANT_DILATATION"><B>Constant_Dilatation</B></A> ) );
<B><I>DERIVE</I></B>
	default_subtype : <B><I>STRING</I></B> := 'constant_shear';
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S5ASSUMED_STRAIN_OPTION_SOLID></A>
<B><I>ENTITY</I></B><B> Assumed_Strain_Option_Solid</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S5ENHANCED_ASSUMED_STRAIN_SOLID"><B>Enhanced_Assumed_Strain_Solid</B></A>,
				                <A HREF="#S5INCOMPATIBLE_BUBBLE_DISPLACEMENT"><B>Incompatible_Bubble_Displacement</B></A>,
				                <A HREF="#S5CONSTANT_DILATATION"><B>Constant_Dilatation</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
-- nocshe
<A NAME=S5NO_CONSTANT_SHEAR></A>
<B><I>ENTITY</I></B><B> No_Constant_Shear</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRESS"><B>Assumed_Strain_Option_Plane_Stress</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRAIN"><B>Assumed_Strain_Option_Plane_Strain</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_AXIAL_SYMMETRIC"><B>Assumed_Strain_Option_Axial_Symmetric</B></A> );
    no_constant_shear : <A HREF="#S5BOOLEAN_SHEAR_OPTION"><B>Boolean_Shear_Option</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_no_constant_shear :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'no_constant_shear', '/data(*)/nocshe' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
-- cshear
<A NAME=S5CONSTANT_SHEAR></A>
<B><I>ENTITY</I></B><B> Constant_Shear</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRESS"><B>Assumed_Strain_Option_Plane_Stress</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRAIN"><B>Assumed_Strain_Option_Plane_Strain</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_AXIAL_SYMMETRIC"><B>Assumed_Strain_Option_Axial_Symmetric</B></A> );
    constant_shear : <A HREF="#S5BOOLEAN_SHEAR_OPTION"><B>Boolean_Shear_Option</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_constant_shear :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'constant_shear', '/data(*)/cshear' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
-- eas
<A NAME=S5ENHANCED_ASSUMED_STRAIN_PLANE_STRESS></A>
<B><I>ENTITY</I></B><B> Enhanced_Assumed_Strain_Plane_Stress</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRESS"><B>Assumed_Strain_Option_Plane_Stress</B></A> );
	number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>DERIVE</I></B>
	default_number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A> := 4;
    translation_specification_enhanced_assumed_strain_plane_stress :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'number_of_modes', '/data(*)/eas' ) ];
<B><I>WHERE</I></B>
	WR1 : ( NUMBER_OF_MODES = 4 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 5 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 7 );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S5ENHANCED_ASSUMED_STRAIN_PLANE_STRAIN></A>
<B><I>ENTITY</I></B><B> Enhanced_Assumed_Strain_Plane_Strain</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRAIN"><B>Assumed_Strain_Option_Plane_Strain</B></A> );
	number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>DERIVE</I></B>
	default_number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A> := 2;
    translation_specification_enhanced_assumed_strain_plane_strain :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'number_of_modes', '/data(*)/eas' ) ];
<B><I>WHERE</I></B>
	WR1 : ( NUMBER_OF_MODES = 2 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 4 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 5 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 7 );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S5ENHANCED_ASSUMED_STRAIN_AXIAL_SYMMETRIC></A>
<B><I>ENTITY</I></B><B> Enhanced_Assumed_Strain_Axial_Symmetric</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_AXIAL_SYMMETRIC"><B>Assumed_Strain_Option_Axial_Symmetric</B></A> );
	number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>DERIVE</I></B>
	default_number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A> := 3;
    translation_specification_enhanced_assumed_strain_axial_symmetric :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'number_of_modes', '/data(*)/eas' ) ];
<B><I>WHERE</I></B>
	WR1 : ( NUMBER_OF_MODES = 3 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 5 );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S5ENHANCED_ASSUMED_STRAIN_SOLID></A>
<B><I>ENTITY</I></B><B> Enhanced_Assumed_Strain_Solid</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_SOLID"><B>Assumed_Strain_Option_Solid</B></A> );
	number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>DERIVE</I></B>
	default_number_of_modes : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A> := 9;
    translation_specification_enhanced_assumed_strain_solid :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'number_of_modes', '/data(*)/eas' ) ];
<B><I>WHERE</I></B>
	WR1 : ( NUMBER_OF_MODES =  9 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 15 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 21 ) <B><I>OR</I></B>
		  ( NUMBER_OF_MODES = 30 );
<B><I>END_ENTITY</I></B>;

<HR>
 
-- bubble
<A NAME=S5INCOMPATIBLE_BUBBLE_DISPLACEMENT></A>
<B><I>ENTITY</I></B><B> Incompatible_Bubble_Displacement</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRESS"><B>Assumed_Strain_Option_Plane_Stress</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRAIN"><B>Assumed_Strain_Option_Plane_Strain</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_AXIAL_SYMMETRIC"><B>Assumed_Strain_Option_Axial_Symmetric</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_SOLID"><B>Assumed_Strain_Option_Solid</B></A> );
    bubble : <A HREF="#S5BOOLEAN_SHEAR_OPTION"><B>Boolean_Shear_Option</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_incompatible_bubble_displacement :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'bubble', '/data(*)/bubble' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
-- cdilat
<A NAME=S5CONSTANT_DILATATION></A>
<B><I>ENTITY</I></B><B> Constant_Dilatation</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S5ASSUMED_STRAIN_OPTION_PLANE_STRAIN"><B>Assumed_Strain_Option_Plane_Strain</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_AXIAL_SYMMETRIC"><B>Assumed_Strain_Option_Axial_Symmetric</B></A>,
		     <A HREF="#S5ASSUMED_STRAIN_OPTION_SOLID"><B>Assumed_Strain_Option_Solid</B></A> );
    cdilat : <A HREF="#S5BOOLEAN_SHEAR_OPTION"><B>Boolean_Shear_Option</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_constant_dilatation :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'cdilat', '/data(*)/cdilat' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
 
-- Miscellaneous special data
 
<A NAME=S5REINFORCEMENT_NUMBER_OF_NODES></A>
<B><I>TYPE</I></B><B> Reinforcement_Number_Of_Nodes</B> = <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S5REINFORCEMENT_INTERPOLATION_TYPE></A>
<B><I>TYPE</I></B><B> Reinforcement_Interpolation_Type</B> = <A HREF="#S1DIANA_WORD"><B>Diana_Word</B></A>;
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S5REINFORCEMENT_MOTHER_ELEMENT_TYPE></A>
<B><I>TYPE</I></B><B> Reinforcement_Mother_Element_Type</B> = <A HREF="#S1DIANA_WORD"><B>Diana_Word</B></A>;
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S5THICKNESS_DIRECTION></A>
<B><I>TYPE</I></B><B> Thickness_Direction</B> = <A HREF="#S2ISO_PARAMETRIC_LOCAL_DIRECTION"><B>Iso_Parametric_Local_Direction</B></A>;
<B><I>END_TYPE</I></B>;
 
<B><I>END_SCHEMA</I></B>; -- diana_special_properties

