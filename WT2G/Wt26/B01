<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S11ELEMENT_SURFACE></A>
<B><I>ENTITY</I></B><B> Element_Surface</B>;
    element_surface_id          : <B><I>INTEGER</I></B>;
    element_surface_name        : <A HREF="#S11SURFACE_NAME"><B>Surface_Name</B></A>;
	contains_surface_points     : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> <B><I>OF</I></B> <A HREF="#S11SURFACE_POINT"><B>Surface_Point</B></A>;
    contains_integration_points : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> <B><I>OF</I></B> <A HREF="#S11INTEGRATION_POINT"><B>Integration_Point</B></A>;
    contains_stress_points      : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> <B><I>OF</I></B> <A HREF="#S11STRESS_POINT"><B>Stress_Point</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S11ELEMENT_CROSS_SECTION></A>
<B><I>ENTITY</I></B><B> Element_Cross_Section</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2DIANA_POINT"><B>Diana_Point</B></A> );
	element_cross_section_id  : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	internal_analysis_results : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S12INTERNAL_ANALYSIS_RESULT"><B>Internal_Analysis_Result</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S11SURFACE_POINT></A>
<B><I>ENTITY</I></B><B> Surface_Point</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2DIANA_POINT"><B>Diana_Point</B></A> );
	surface_point_id          : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	internal_analysis_results : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S12INTERNAL_ANALYSIS_RESULT"><B>Internal_Analysis_Result</B></A>;
<B><I>END_ENTITY</I></B>;	

<HR>
 
<A NAME=S11STRESS_POINT></A>
<B><I>ENTITY</I></B><B> Stress_Point</B>;
	stress_point_id           : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	x_coordinate              : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
	y_coordinate              : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
	z_coordinate              : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
	internal_analysis_results : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S12INTERNAL_ANALYSIS_RESULT"><B>Internal_Analysis_Result</B></A>;
<B><I>END_ENTITY</I></B>;	

<HR>
 
<A NAME=S11ELEMENT_SPEC_ASSIGNMENT></A>
<B><I>ENTITY</I></B><B> Element_Spec_Assignment</B>;
	for_elements  : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT"><B>Element</B></A>;
	specification : <A HREF="#S6DIANA_ELEMENT_CLASS_SPEC"><B>Diana_Element_Class_Spec</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 

-- Reinforcements
 
<A NAME=S11REINFORCEMENT></A>
<B><I>ENTITY</I></B><B> Reinforcement</B>;
	reinforcement_id : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	of_type          : <A HREF="#S6DIANA_REINFORCEMENT_TYPE"><B>Diana_Reinforcement_Type</B></A>;
    sections         : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11REINFORCEMENT_SECTION"><B>Reinforcement_Section</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_reinforcement :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'reinforcement_id', '/reinfo(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'of_type',          '/reinfo(*)/type' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections',         '/reinfo(*)/sectio' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/reinforcement_section_id',
            '/reinfo(*)/sectio(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/section_nodes',
            '/reinfo(*)/sectio(*)/comore' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/potential_mother_elements',
            '/reinfo(*)/sectio(*)/potelm' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/parabolic_approximation',
            '/reinfo(*)/sectio(*)/parabo' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/parts',
            '/reinfo(*)/sectio(*)/elemen' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/parts/reinforcement_part_id',
            '/reinfo(*)/sectio(*)/elemen(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/parts/part_nodes',
            '/reinfo(*)/sectio(*)/elemen(*)/comore' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/parts/mother_element',
            '/reinfo(*)/sectio(*)/elemen(*)/elmnr' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/parts/local_y',
            '/reinfo(*)/sectio(*)/elemen(*)/localy' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'sections/parts/local_z',
            '/reinfo(*)/sectio(*)/elemen(*)/localz' ) ];
<B><I>UNIQUE</I></B>
	UR1 : REINFORCEMENT_ID;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_SECTION></A>
<B><I>ENTITY</I></B><B> Reinforcement_Section</B>;
	reinforcement_section_id  : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	section_nodes             : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11SECTION_NODE"><B>Section_Node</B></A>;
    potential_mother_elements : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT"><B>Element</B></A>;
	parabolic_approximation   : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [3:3] <B><I>OF</I></B> <B><I>REAL</I></B>;
    parts                     : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11REINFORCEMENT_PART"><B>Reinforcement_Part</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_PART></A>
<B><I>ENTITY</I></B><B> Reinforcement_Part</B>;
	reinforcement_part_id   : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	mother_element          : <A HREF="#S11ELEMENT"><B>Element</B></A>;
	part_nodes              : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11PART_NODE"><B>Part_Node</B></A>;
	integration_points      : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11INTEGRATION_POINT"><B>Integration_Point</B></A>;
    reinforcement_part_axes : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11REINFORCEMENT_PART_AXIS"><B>Reinforcement_Part_Axis</B></A>;
	local_y                 : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
	local_z                 : <B><I>OPTIONAL</I></B> <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_PART_AXIS></A>
<B><I>ENTITY</I></B><B> Reinforcement_Part_Axis</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2DIANA_VECTOR"><B>Diana_Vector</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11SECTION_NODE></A>
<B><I>ENTITY</I></B><B> Section_Node</B>;
    section_node_id : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    node            : <A HREF="#S11NODE_OR_COORDINATES_IN_SECTION"><B>Node_Or_Coordinates_In_Section</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11PART_NODE></A>
<B><I>ENTITY</I></B><B> Part_Node</B>;
	part_node_id              : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    node                      : <A HREF="#S11NODE_OR_COORDINATES_IN_PART"><B>Node_Or_Coordinates_In_Part</B></A>;
    reinforcement_node_axes   : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11REINFORCEMENT_NODE_AXIS"><B>Reinforcement_Node_Axis</B></A>;
	internal_analysis_results : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S12INTERNAL_ANALYSIS_RESULT"><B>Internal_Analysis_Result</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11NODE_OR_COORDINATES_IN_SECTION></A>
<B><I>ENTITY</I></B><B> Node_Or_Coordinates_In_Section</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S11REINFORCEMENT_NODE_REFERENCE"><B>Reinforcement_Node_Reference</B></A>,
                                <A HREF="#S11REINFORCEMENT_COORDINATES_GLOBAL"><B>Reinforcement_Coordinates_Global</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11NODE_OR_COORDINATES_IN_PART></A>
<B><I>ENTITY</I></B><B> Node_Or_Coordinates_In_Part</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S11REINFORCEMENT_NODE_REFERENCE"><B>Reinforcement_Node_Reference</B></A>,
                                <A HREF="#S11REINFORCEMENT_COORDINATES_LOCAL"><B>Reinforcement_Coordinates_Local</B></A>,
                                <A HREF="#S11REINFORCEMENT_COORDINATES_GLOBAL"><B>Reinforcement_Coordinates_Global</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_NODE_REFERENCE></A>
<B><I>ENTITY</I></B><B> Reinforcement_Node_Reference</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S11NODE_OR_COORDINATES_IN_SECTION"><B>Node_Or_Coordinates_In_Section</B></A>,
             <A HREF="#S11NODE_OR_COORDINATES_IN_PART"><B>Node_Or_Coordinates_In_Part</B></A> );
    referenced_node : <A HREF="#S11NODE"><B>Node</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_COORDINATES_LOCAL></A>
<B><I>ENTITY</I></B><B> Reinforcement_Coordinates_Local</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S11NODE_OR_COORDINATES_IN_PART"><B>Node_Or_Coordinates_In_Part</B></A>,
             <A HREF="#S2DIANA_POINT"><B>Diana_Point</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_COORDINATES_GLOBAL></A>
<B><I>ENTITY</I></B><B> Reinforcement_Coordinates_Global</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S11NODE_OR_COORDINATES_IN_SECTION"><B>Node_Or_Coordinates_In_Section</B></A>,
             <A HREF="#S11NODE_OR_COORDINATES_IN_PART"><B>Node_Or_Coordinates_In_Part</B></A>,
             <A HREF="#S2DIANA_POINT"><B>Diana_Point</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_NODE_AXIS></A>
<B><I>ENTITY</I></B><B> Reinforcement_Node_Axis</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2DIANA_VECTOR"><B>Diana_Vector</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11REINFORCEMENT_SPEC_ASSIGNMENT></A>
<B><I>ENTITY</I></B><B> Reinforcement_Spec_Assignment</B>;
    for_reinforcements : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11REINFORCEMENT"><B>Reinforcement</B></A>;
    specification      : <A HREF="#S6DIANA_REINFORCEMENT_CLASS_SPEC"><B>Diana_Reinforcement_Class_Spec</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
-- Compositions
 
<A NAME=S11DIANA_COMPOSITION_TYPE></A>
<B><I>TYPE</I></B><B> Diana_Composition_Type</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Line</B>,
                                               <B>Plane_Triangular</B>,
                                               <B>Plane_Quadrilateral</B> );
<B><I>END_TYPE</I></B>;

<HR>
<A NAME=S11COMPOSITION_ANALYSIS_RESULT></A>
<B><I>TYPE</I></B><B> Composition_Analysis_Result</B> = <B><I>SELECT</I></B> ( <A HREF="#S12STRESS_TOTAL_FORCE_GLOBAL"><B>Stress_Total_Force_Global</B></A>,
                                            <A HREF="#S12STRESS_TOTAL_MOMENT_GLOBAL"><B>Stress_Total_Moment_Global</B></A>,
                                            <A HREF="#S12STRESS_TOTAL_DISFOR_GLOBAL"><B>Stress_Total_Disfor_Global</B></A>,
                                            <A HREF="#S12STRESS_TOTAL_DISMOM_GLOBAL"><B>Stress_Total_Dismom_Global</B></A> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S11COMPOSITION></A>
<B><I>ENTITY</I></B><B> Composition</B>;
	composition_id        : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    of_type               : <A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Diana_Composition_Type</B></A>;
	contributing_elements : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT"><B>Element</B></A>;
	nodes                 : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11COMPOSITION_NODE"><B>Composition_Node</B></A>;
    composition_axes      : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11COMPOSITION_AXIS"><B>Composition_Axis</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_composition :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'composition_id',        '/compos(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'of_type',               '/compos(*)/type' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'contributing_elements', '/compos(*)/elemen(*)/elmnr' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nodes',                 '/compos(*)/nodes' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'composition_axes',      '/compos(*)/normdi' ) ];
<B><I>UNIQUE</I></B>
	UR1 : COMPOSITION_ID;
<B><I>WHERE</I></B>
    WR1 : <B><I>NOT</I></B> ( (OF_TYPE = <A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Diana_Composition_Type</B></A>.<A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Line</B></A>) <B><I>AND</I></B>
                ( (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt; 2) <B><I>AND</I></B> (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt; 3) ) );
    WR2 : <B><I>NOT</I></B> ( (OF_TYPE = <A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Plane_Triangular</B></A>) <B><I>AND</I></B>
                ( (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt; 3) <B><I>AND</I></B> (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt; 6) <B><I>AND</I></B>
                  (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt; 9) ) );
    WR3 : <B><I>NOT</I></B> ( (OF_TYPE = <A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Plane_Quadrilateral</B></A>) <B><I>AND</I></B>
                ( (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt;  4) <B><I>AND</I></B> (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt;  8) <B><I>AND</I></B>
                  (<B><I>SIZEOF</I></B> (<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>) &lt;&gt; 12) ) );
    WR4 : <B><I>NOT</I></B> (
            (OF_TYPE = <A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Diana_Composition_Type</B></A>.<A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Line</B></A>) <B><I>AND</I></B>
            (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (N &lt;* <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A> |
                     ( (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* N.INTERNAL_ANALYSIS_RESULTS |
                                ( 'stress_total_disfor_global' <B><I>IN</I></B> <B><I>TYPEOF</I></B>
                                  (N.INTERNAL_ANALYSIS_RESULTS) ))) &lt;&gt; 0) <B><I>OR</I></B>
                       (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* N.INTERNAL_ANALYSIS_RESULTS |
                                ( 'stress_total_dismom_global' <B><I>IN</I></B> <B><I>TYPEOF</I></B>
                                  (N.INTERNAL_ANALYSIS_RESULTS) ))) &lt;&gt; 0) )
            )) &lt;&gt; 0)
          );
    WR5 : <B><I>NOT</I></B> (
            (OF_TYPE = <A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Plane_Triangular</B></A>) <B><I>AND</I></B>
            (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (N &lt;* <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A> |
                     ( (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* N.INTERNAL_ANALYSIS_RESULTS |
                                ( 'stress_total_force_global' <B><I>IN</I></B> <B><I>TYPEOF</I></B>
                                  (N.INTERNAL_ANALYSIS_RESULTS) ))) &lt;&gt; 0) <B><I>OR</I></B>
                       (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* N.INTERNAL_ANALYSIS_RESULTS |
                                ( 'stress_total_moment_global' <B><I>IN</I></B> <B><I>TYPEOF</I></B>
                                  (N.INTERNAL_ANALYSIS_RESULTS) ))) &lt;&gt; 0) )
            )) &lt;&gt; 0)
          );
    WR6 : <B><I>NOT</I></B> (
            (OF_TYPE = <A HREF="#S11DIANA_COMPOSITION_TYPE"><B>Plane_Quadrilateral</B></A>) <B><I>AND</I></B>
            (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (N &lt;* <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A> |
                     ( (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* N.INTERNAL_ANALYSIS_RESULTS |
                                ( 'stress_total_force_global' <B><I>IN</I></B> <B><I>TYPEOF</I></B>
                                  (N.INTERNAL_ANALYSIS_RESULTS) ))) &lt;&gt; 0) <B><I>OR</I></B>
                       (<B><I>SIZEOF</I></B> (<B><I>QUERY</I></B> (TEMP &lt;* N.INTERNAL_ANALYSIS_RESULTS |
                                ( 'stress_total_moment_global' <B><I>IN</I></B> <B><I>TYPEOF</I></B>
                                  (N.INTERNAL_ANALYSIS_RESULTS) ))) &lt;&gt; 0) )
            )) &lt;&gt; 0)
          );
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11COMPOSITION_AXIS></A>
<B><I>ENTITY</I></B><B> Composition_Axis</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2DIANA_VECTOR"><B>Diana_Vector</B></A> );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S11COMPOSITION_NODE></A>
<B><I>ENTITY</I></B><B> Composition_Node</B>;
    composition_node_id       : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	node                      : <A HREF="#S11NODE"><B>Node</B></A>;
	internal_analysis_results :
                      <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11COMPOSITION_ANALYSIS_RESULT"><B>Composition_Analysis_Result</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 

-- Groups

<A NAME=S11GROUPED_ENTITY></A>
<B><I>TYPE</I></B><B> Grouped_Entity</B> = <B><I>SELECT</I></B>( <A HREF="#S11NODE"><B>Node</B></A>,
                              <A HREF="#S11ELEMENT"><B>Element</B></A>,
                              <A HREF="#S11REINFORCEMENT"><B>Reinforcement</B></A> );
<B><I>END_TYPE</I></B>;

<HR>
<A NAME=S11GROUP></A>
<B><I>ENTITY</I></B><B> Group</B>;
    group_id         : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    group_name       : <B><I>STRING</I></B> (20);
    group_type       : <B><I>STRING</I></B> (6);
    grouped_entities : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11GROUPED_ENTITY"><B>Grouped_Entity</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_group : <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'group_id',         '/groups(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'group_name',       '/groups(*)/name' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'group_type',       '/groups(*)/type' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'grouped_entities', '/groups(*)/info' ) ];
<B><I>UNIQUE</I></B>
    UR1 : GROUP_ID;
<B><I>END_ENTITY</I></B>;

<HR>
-- Supports
 
<A NAME=S11SUPPORT></A>
<B><I>ENTITY</I></B><B> Support</B>;
	support_id      : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	supported_nodes : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11NODE"><B>Node</B></A>;
	constraints     : <A HREF="#S11DEGREE_OF_FREEDOM"><B>Degree_Of_Freedom</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_support : <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'support_id',            '/suppor(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'constraints/direction', '/suppor(*)/direct' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'supported_nodes',       '/suppor(*)/nodes' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'constraints/of_type',   '/suppor(*)/type' ) ];
<B><I>UNIQUE</I></B>
	UR1 : SUPPORT_ID;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S11DEGREE_OF_FREEDOM></A>
<B><I>ENTITY</I></B><B> Degree_Of_Freedom</B>;
	of_type   : <A HREF="#S2DEGREE_OF_FREEDOM_TYPE"><B>Degree_Of_Freedom_Type</B></A>;
	direction : <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
 
-- Tyings
 
<A NAME=S11TYING></A>
<B><I>ENTITY</I></B><B> Tying</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S11TYING_GENERAL"><B>Tying_General</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S11TYING_GENERAL></A>
<B><I>ENTITY</I></B><B> Tying_General</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S11TYING"><B>Tying</B></A> );
	tying_general_id        : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
	slave_node              : <A HREF="#S11NODE"><B>Node</B></A>;
	slave_degree_of_freedom : <A HREF="#S11DEGREE_OF_FREEDOM"><B>Degree_Of_Freedom</B></A>;
    masters                 : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11TYING_GENERAL_MASTER"><B>Tying_General_Master</B></A>;
<B><I>DERIVE</I></B>
	default_multiplication_factor : <B><I>REAL</I></B> := 1.;
    translation_specification_tying_general :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'tying_general_id',                  '/tyings(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'slave_degree_of_freedom/direction', '/tyings(*)/direct' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'slave_degree_of_freedom/of_type',   '/tyings(*)/type' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'slave_node',                        '/tyings(*)/nodes' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'masters',               '/tyings(*)/master' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'masters/tying_general_master_id',
                                     '/tyings(*)/master(*)/current_index' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'masters/master_node',   '/tyings(*)/master(*)/nodes' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'masters/master_degree_of_freedom/direction',
                                     '/tyings(*)/master(*)/direct' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'masters/master_degree_of_freedom/of_type',
                                     '/tyings(*)/master(*)/type' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'masters/multiplication_factor',
                                     '/tyings(*)/master(*)/factor' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S11TYING_GENERAL_MASTER></A>
<B><I>ENTITY</I></B><B> Tying_General_Master</B>;
    tying_general_master_id  : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    master_node              : <A HREF="#S11NODE"><B>Node</B></A>;
    master_degree_of_freedom : <A HREF="#S11DEGREE_OF_FREEDOM"><B>Degree_Of_Freedom</B></A>;
    multiplication_factor    : <B><I>OPTIONAL</I></B> <B><I>REAL</I></B>;
<B><I>END_ENTITY</I></B>;

<B><I>END_SCHEMA</I></B>; -- diana_fe_model

