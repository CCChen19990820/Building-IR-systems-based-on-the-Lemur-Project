-- Physical Objects
 
<A NAME=S11PHYSICAL_OBJECT_AS_DISCRETIZED></A>
<B><I>ENTITY</I></B><B> Physical_Object_As_Discretized</B>;
	physical_object_as_discretized_id : <A HREF="#S1DIANA_NUMBER"><B>Diana_Number</B></A>;
    time_stamp                        : <B><I>STRING</I></B> (256);
    description                       : <B><I>LIST</I></B> <B><I>OF</I></B> <B><I>STRING</I></B> (256);
    model_dimension                   : <A HREF="#S2MODEL_DIMENSION"><B>Model_Dimension</B></A>;
	units                             : <B><I>OPTIONAL</I></B> <A HREF="#S2UNITS"><B>Units</B></A>;
	nodes                             : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11NODE"><B>Node</B></A>;
	elements                          : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT"><B>Element</B></A>;
	reinforcements                    : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11REINFORCEMENT"><B>Reinforcement</B></A>;
	compositions                      : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11COMPOSITION"><B>Composition</B></A>;
    element_spec_assignments          : <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT_SPEC_ASSIGNMENT"><B>Element_Spec_Assignment</B></A>;
    reinforcement_spec_assignments    : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B>
                                        <A HREF="#S11REINFORCEMENT_SPEC_ASSIGNMENT"><B>Reinforcement_Spec_Assignment</B></A>;
	global_directions                 : <B><I>SET</I></B> [3:?] <B><I>OF</I></B> <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>;
	groups                            : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11GROUP"><B>Group</B></A>;
	supports                          : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11SUPPORT"><B>Support</B></A>;			
	tyings                            : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11TYING"><B>Tying</B></A>;
	load_cases                        : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S10LOAD_CASE"><B>Load_Case</B></A>;
	load_sets                         : <B><I>OPTIONAL</I></B> <B><I>SET</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S10LOAD_SET"><B>Load_Set</B></A>;
<B><I>DERIVE</I></B>
    default_directions : <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A> := [
                         <A HREF="#S2DIANA_VECTOR"><B>Diana_Vector</B></A>( 1., 0., 0. ) || <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>( 1 ),
                         <A HREF="#S2DIANA_VECTOR"><B>Diana_Vector</B></A>( 0., 1., 0. ) || <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>( 2 ),
                         <A HREF="#S2DIANA_VECTOR"><B>Diana_Vector</B></A>( 0., 0., 1. ) || <A HREF="#S2GLOBAL_DIRECTION"><B>Global_Direction</B></A>( 3 ) ];
    default_model_dimension : <A HREF="#S2MODEL_DIMENSION"><B>Model_Dimension</B></A> := <A HREF="#S2MODEL_DIMENSION"><B>Three_D</B></A>;
<B><I>UNIQUE</I></B>
	UR1 : PHYSICAL_OBJECT_AS_DISCRETIZED_ID;
<B><I>END_ENTITY</I></B>;

