<A NAME=S6AXIAL_SYMMETRIC_SPEC></A>
<B><I>ENTITY</I></B><B> Axial_Symmetric_Spec</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S6AXIAL_SYMMETRIC_REGULAR_SPEC"><B>Axial_Symmetric_Regular_Spec</B></A>,
                                <A HREF="#S6AXIAL_SYMMETRIC_RUBBER_SPEC"><B>Axial_Symmetric_Rubber_Spec</B></A>,
                                <A HREF="#S6AXIAL_SYMMETRIC_SHELL_SPEC"><B>Axial_Symmetric_Shell_Spec</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S6STRUCTURE_SPEC"><B>Structure_Spec</B></A> );
      axial_symmetric_geometry        :
        <B><I>OPTIONAL</I></B> <A HREF="#S7AXIAL_SYMMETRIC_GEOMETRY_SPEC"><B>Axial_Symmetric_Geometry_Spec</B></A>;
      axial_symmetric_linsta_material :
        <B><I>OPTIONAL</I></B> <A HREF="#S8AXIAL_SYMMETRIC_LINSTA_MATERIAL_SPEC"><B>Axial_Symmetric_Linsta_Material_Spec</B></A>;
<B><I>DERIVE</I></B>
   axial_symmetric_linsta_analysis_result_in_location_type :
   <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S6ANALYSIS_RESULT_IN_LOCATION_TYPE"><B>Analysis_Result_In_Location_Type</B></A> :=
      [ <A HREF="#S6ANALYSIS_RESULT_IN_LOCATION_TYPE"><B>Analysis_Result_In_Location_Type</B></A> (<A HREF="#S12STRESS_TOTAL_CAUCHY_LOCAL_SET04"><B>Stress_Total_Cauchy_Local_Set04</B></A>,
          [<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Intpnt</B></A>, <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>]),
        <A HREF="#S6ANALYSIS_RESULT_IN_LOCATION_TYPE"><B>Analysis_Result_In_Location_Type</B></A> (<A HREF="#S12STRESS_TOTAL_CAUCHY_GLOBAL"><B>Stress_Total_Cauchy_Global</B></A>,
          [<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Intpnt</B></A>, <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>]),
        <A HREF="#S6ANALYSIS_RESULT_IN_LOCATION_TYPE"><B>Analysis_Result_In_Location_Type</B></A> (<A HREF="#S12STRESS_TOTAL_CAUCHY_PRINCI"><B>Stress_Total_Cauchy_Princi</B></A>,
          [<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Intpnt</B></A>, <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>]),
        <A HREF="#S6ANALYSIS_RESULT_IN_LOCATION_TYPE"><B>Analysis_Result_In_Location_Type</B></A> (<A HREF="#S12STRESS_TOTAL_CAUCHY_VONMIS"><B>Stress_Total_Cauchy_Vonmis</B></A>,
          [<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Intpnt</B></A>, <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>]),
        <A HREF="#S6ANALYSIS_RESULT_IN_LOCATION_TYPE"><B>Analysis_Result_In_Location_Type</B></A> (<A HREF="#S12STRESS_TOTAL_CAUCHY_REAXES"><B>Stress_Total_Cauchy_Reaxes</B></A>,
          [<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Intpnt</B></A>, <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>]),
        <A HREF="#S6ANALYSIS_RESULT_IN_LOCATION_TYPE"><B>Analysis_Result_In_Location_Type</B></A> (<A HREF="#S12STRESS_INITIA_CAUCHY_LOCAL_SET04"><B>Stress_Initia_Cauchy_Local_Set04</B></A>,
          [<A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Intpnt</B></A>, <A HREF="#S6ANALYSIS_RESULT_LOCATION_TYPE"><B>Nodes</B></A>]),
