<A NAME=S10LOAD_ELEMENT></A>
<B><I>ENTITY</I></B><B> Load_Element</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S10LOAD_ELEMENT_FORCE"><B>Load_Element_Force</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_MOMENT"><B>Load_Element_Moment</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_TEMPERATURE"><B>Load_Element_Temperature</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_CONCENTRATION"><B>Load_Element_Concentration</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_PRE_STRESS"><B>Load_Element_Pre_Stress</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_PORE_PRESSURE"><B>Load_Element_Pore_Pressure</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_POTENTIAL_HEIGHT"><B>Load_Element_Potential_Height</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD"><B>Load</B></A> );
      on_elements : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <A HREF="#S11ELEMENT"><B>Element</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_ELEMENT_FORCE></A>
<B><I>ENTITY</I></B><B> Load_Element_Force</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> ( <A HREF="#S10LOAD_ELEMENT_FORCE_LINE"><B>Load_Element_Force_Line</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_FORCE_EDGE"><B>Load_Element_Force_Edge</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_FORCE_FACE"><B>Load_Element_Force_Face</B></A>,
                                <A HREF="#S10LOAD_ELEMENT_FORCE_VOLUME"><B>Load_Element_Force_Volume</B></A> ) )
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_ELEMENT"><B>Load_Element</B></A> );
      direction          : <A HREF="#S2LOCAL_DIRECTION_OR_GLOBAL_DIRECTION"><B>Local_Direction_Or_Global_Direction</B></A>;
      load_location_type : <A HREF="#S6LOAD_LOCATION_TYPE"><B>Load_Location_Type</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S10LOAD_ELEMENT_FORCE_LINE></A>
<B><I>ENTITY</I></B><B> Load_Element_Force_Line</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S10LOAD_ELEMENT_FORCE"><B>Load_Element_Force</B></A> );
      force : <B><I>LIST</I></B> [1:?] <B><I>OF</I></B> <B><I>REAL</I></B>;
<B><I>DERIVE</I></B>
    translation_specification_load_element_force_line :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'on_elements',        '/loads(*)/elemen(*)/elemen' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
