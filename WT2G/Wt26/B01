<HR>
(*
*)
<A NAME=S0GEM_SHAPE_REPRESENTATION></A>
<B><I>ENTITY</I></B><B> Gem_Shape_Representation</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> (
  <A HREF="#S0PROPERTY_REPRESENTATION"><B>Property_Representation</B></A>,
  <A HREF="#S0SHAPE_REPRESENTATION"><B>Shape_Representation</B></A>);
<B><I>WHERE</I></B>
     WR1: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( IT &lt;* <B><I>SELF</I></B>.ITEMS | (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>([
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.MANIFOLD_SOLID_BREP',
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.CONNECTED_FACE_SET',
              'GENERIC_ENGINEERING_ANALYSIS_MODEL.CONNECTED_EDGE_SET'] *
               <B><I>TYPEOF</I></B>(IT)) = 1)) ))
              = 0;
     WR2: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( MSB &lt;* <B><I>QUERY</I></B> ( IT &lt;* <B><I>SELF</I></B>.ITEMS |
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.MANIFOLD_SOLID_BREP'
                <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) |
             ( <B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( CSH &lt;* <A HREF="#S0MSB_SHELLS"><B>Msb_Shells</B></A>(MSB) |
                              (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( FCS &lt;* CSH\
              <A HREF="#S0CONNECTED_FACE_SET"><B>Connected_Face_Set</B></A>.CFS_FACES | (<B><I>NOT</I></B>
                      (('GENERIC_ENGINEERING_ANALYSIS_MODEL.ADVANCED_FACE'
                         <B><I>IN</I></B> <B><I>TYPEOF</I></B>(FCS)) <B><I>OR</I></B>
               ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ORIENTED_FACE'
                 <B><I>IN</I></B> <B><I>TYPEOF</I></B>(FCS)))) )) = 0)) ))
              = 0)) )) = 0;
     WR3: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( MSB &lt;* <B><I>QUERY</I></B> ( IT &lt;* <B><I>SELF</I></B>.ITEMS |
              ('GENERIC_ENGINEERING_ANALYSIS_MODEL.MANIFOLD_SOLID_BREP'
                <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) |
             ( <B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( CSH &lt;* <A HREF="#S0MSB_SHELLS"><B>Msb_Shells</B></A>(MSB) |
                 (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( OFCS &lt;* <B><I>QUERY</I></B>(FCS &lt;*
                      CSH\<A HREF="#S0CONNECTED_FACE_SET"><B>Connected_Face_Set</B></A>.CFS_FACES |
                  ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ORIENTED_FACE'
                    <B><I>IN</I></B> <B><I>TYPEOF</I></B>(FCS))) |
             <B><I>NOT</I></B> ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ADVANCED_FACE' <B><I>IN</I></B>
                 <B><I>TYPEOF</I></B>(OFCS.FACE_ELEMENT) )) ) = 0)) ))
              = 0 )) )) = 0;
     WR4: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( CFS &lt;* <B><I>QUERY</I></B> ( IT &lt;* ITEMS |
            ( 'GENERIC_ENGINEERING_ANALYSIS_MODEL.CONNECTED_FACE_SET'
               <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) |
            (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( FCS &lt;* CFS.CFS_FACES |
            (<B><I>NOT</I></B> (('GENERIC_ENGINEERING_ANALYSIS_MODEL.ADVANCED_FACE'
                    <B><I>IN</I></B> <B><I>TYPEOF</I></B>(FCS)) <B><I>OR</I></B>
               ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ORIENTED_FACE'
                 <B><I>IN</I></B> <B><I>TYPEOF</I></B>(FCS))))) ) = 0)) ))
              = 0;
     WR5: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( CFS &lt;* <B><I>QUERY</I></B> ( IT &lt;* ITEMS |
            ( 'GENERIC_ENGINEERING_ANALYSIS_MODEL.CONNECTED_FACE_SET'
               <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) |
            (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( OFCS &lt;* <B><I>QUERY</I></B>(FCS &lt;* CFS.CFS_FACES |
                  ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ORIENTED_FACE'
                    <B><I>IN</I></B> <B><I>TYPEOF</I></B>(FCS))) |
             <B><I>NOT</I></B> ('GENERIC_ENGINEERING_ANALYSIS_MODEL.ADVANCED_FACE' <B><I>IN</I></B>
                 <B><I>TYPEOF</I></B>(OFCS.FACE_ELEMENT) )) ) = 0)) ))
              = 0;
     WR6: <B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( CES &lt;* <B><I>QUERY</I></B> ( IT &lt;* ITEMS |
            ( 'GENERIC_ENGINEERING_ANALYSIS_MODEL.CONNECTED_EDGE_SET'
               <B><I>IN</I></B> <B><I>TYPEOF</I></B>(IT)) ) |
            (<B><I>NOT</I></B> (<B><I>SIZEOF</I></B>(<B><I>QUERY</I></B> ( EDG &lt;* CES.CES_EDGES |
            (<B><I>NOT</I></B> (('GENERIC_ENGINEERING_ANALYSIS_MODEL.EDGE_CURVE' <B><I>IN</I></B> <B><I>TYPEOF</I></B>(EDG)) <B><I>AND</I></B>
               (<B><I>SIZEOF</I></B> (['GENERIC_ENGINEERING_ANALYSIS_MODEL.LINE',
                         'GENERIC_ENGINEERING_ANALYSIS_MODEL.CONIC',
                         'GENERIC_ENGINEERING_ANALYSIS_MODEL.POLYLINE',
                         'GENERIC_ENGINEERING_ANALYSIS_MODEL.SURFACE_CURVE',
                         'GENERIC_ENGINEERING_ANALYSIS_MODEL.B_SPLINE_CURVE'] *
              <B><I>TYPEOF</I></B>(EDG\<A HREF="#S0EDGE_CURVE"><B>Edge_Curve</B></A>.EDGE_GEOMETRY)) = 1 )
              ))) ) = 0)) ))
              = 0;
 <B><I>END_ENTITY</I></B>;

