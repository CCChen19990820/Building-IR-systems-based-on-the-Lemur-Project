<B><I>REFERENCE</I></B> <B><I>FROM</I></B> <B><A HREF="#S1S1DIANA_DATA_TYPES">Diana_Data_Types</A></B>;

<HR>
-- Mapping of entity attributes to filos

<A NAME=S2TRANSLATION_SPECIFICATION></A>
<B><I>ENTITY</I></B><B> Translation_Specification</B>;
  attribute_name : <B><I>STRING</I></B>;
  filos_path     : <B><I>STRING</I></B>;
<B><I>END_ENTITY</I></B>;

<HR>
-- DIANA Groups

<A NAME=S2GROUP_TYPE></A>
<B><I>TYPE</I></B><B> Group_Type</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Group_Nodes</B>,
                                   <B>Group_Elemen</B>,
                                   <B>Group_Reinfo</B> );
<B><I>END_TYPE</I></B>;

<HR>
-- DIANA units
 
<A NAME=S2LENGTH_UNIT_SI_NAME></A>
<B><I>TYPE</I></B><B> Length_Unit_Si_Name</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> (	<B>Millimetre</B>,
                                            <B>Centimetre</B>,
                                            <B>Decimetre</B>,
                                            <B>Metre</B>,
                                            <B>Kilometre</B>,
                                            <B>Inch</B>,
                                            <B>Foot</B>,
                                            <B>Yard</B>,
                                            <B>Mile</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2MASS_UNIT_SI_NAME></A>
<B><I>TYPE</I></B><B> Mass_Unit_Si_Name</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Gram</B>,
                                          <B>Kilogram</B>,
                                          <B>Tonne</B>,
                                          <B>Ounce</B>,
                                          <B>Pound</B>,
                                          <B>Long_Tonne</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2TIME_UNIT_SI_NAME></A>
<B><I>TYPE</I></B><B> Time_Unit_Si_Name</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Second</B>,
                                          <B>Minute</B>,
                                          <B>Hour</B>,
                                          <B>Day</B>,
                                          <B>Year</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2TEMPERATURE_UNIT_SI_NAME></A>
<B><I>TYPE</I></B><B> Temperature_Unit_Si_Name</B> = <B><I>ENUMERATION</I></B> <B><I>OF</I></B> ( <B>Degree_Celsius</B>,
                                                 <B>Kelvin</B>,
                                                 <B>Fahrenheit</B> );
<B><I>END_TYPE</I></B>;

<HR>
 
<A NAME=S2UNITS></A>
<B><I>ENTITY</I></B><B> Units</B>;
	for_length      : <A HREF="#S2LENGTH_UNIT"><B>Length_Unit</B></A>;
	for_mass        : <A HREF="#S2MASS_UNIT"><B>Mass_Unit</B></A>;
	for_time        : <A HREF="#S2TIME_UNIT"><B>Time_Unit</B></A>;
	for_temperature : <A HREF="#S2TEMPERATURE_UNIT"><B>Temperature_Unit</B></A>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2CONVERSION_TO_SI></A>
<B><I>ENTITY</I></B><B> Conversion_To_Si</B>;
	offset : <B><I>REAL</I></B>;
	factor : <B><I>REAL</I></B>;
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2LENGTH_UNIT></A>
<B><I>ENTITY</I></B><B> Length_Unit</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S2LENGTH_UNIT_BY_SI_NAME"><B>Length_Unit_By_Si_Name</B></A>,
						        <A HREF="#S2LENGTH_UNIT_CONVERSION_TO_SI"><B>Length_Unit_Conversion_To_Si</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2LENGTH_UNIT_BY_SI_NAME></A>
<B><I>ENTITY</I></B><B> Length_Unit_By_Si_Name</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2LENGTH_UNIT"><B>Length_Unit</B></A> );
	length_unit_si_name : <A HREF="#S2LENGTH_UNIT_SI_NAME"><B>Length_Unit_Si_Name</B></A>;
<B><I>DERIVE</I></B>
	default_length_si_unit_name : <A HREF="#S2LENGTH_UNIT_SI_NAME"><B>Length_Unit_Si_Name</B></A> := <A HREF="#S2LENGTH_UNIT_SI_NAME"><B>Metre</B></A>;
    translation_specification_length_unit_by_si_name :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'length_unit_si_name', '/units/lennam' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2LENGTH_UNIT_CONVERSION_TO_SI></A>
<B><I>ENTITY</I></B><B> Length_Unit_Conversion_To_Si</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2LENGTH_UNIT"><B>Length_Unit</B></A> );
	conversion_to_si : <A HREF="#S2CONVERSION_TO_SI"><B>Conversion_To_Si</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_length_unit_conversion_to_si :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'conversion_to_si/offset', '/units/length(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'conversion_to_si/factor', '/units/length(2)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2MASS_UNIT></A>
<B><I>ENTITY</I></B><B> Mass_Unit</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S2MASS_UNIT_BY_SI_NAME"><B>Mass_Unit_By_Si_Name</B></A>,
						        <A HREF="#S2MASS_UNIT_CONVERSION_TO_SI"><B>Mass_Unit_Conversion_To_Si</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2MASS_UNIT_BY_SI_NAME></A>
<B><I>ENTITY</I></B><B> Mass_Unit_By_Si_Name</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2MASS_UNIT"><B>Mass_Unit</B></A> );
	mass_unit_si_name : <A HREF="#S2MASS_UNIT_SI_NAME"><B>Mass_Unit_Si_Name</B></A>;
<B><I>DERIVE</I></B>
	default_mass_unit_si_name : <A HREF="#S2MASS_UNIT_SI_NAME"><B>Mass_Unit_Si_Name</B></A> := <A HREF="#S2MASS_UNIT_SI_NAME"><B>Kilogram</B></A>;
    translation_specification_mass_unit_by_si_name :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'mass_unit_si_name', '/units/masnam' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2MASS_UNIT_CONVERSION_TO_SI></A>
<B><I>ENTITY</I></B><B> Mass_Unit_Conversion_To_Si</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2MASS_UNIT"><B>Mass_Unit</B></A> );
	conversion_to_si : <A HREF="#S2CONVERSION_TO_SI"><B>Conversion_To_Si</B></A>;
<B><I>DERIVE</I></B>
    translation_specification_mass_unit_conversion_to_si :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'conversion_to_si/offset', '/units/mass(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'conversion_to_si/factor', '/units/mass(2)' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2TIME_UNIT></A>
<B><I>ENTITY</I></B><B> Time_Unit</B>
<B><I>ABSTRACT</I></B> <B><I>SUPERTYPE</I></B> <B><I>OF</I></B> ( <B><I>ONEOF</I></B> (	<A HREF="#S2TIME_UNIT_BY_SI_NAME"><B>Time_Unit_By_Si_Name</B></A>,
						        <A HREF="#S2TIME_UNIT_CONVERSION_TO_SI"><B>Time_Unit_Conversion_To_Si</B></A> ) );
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2TIME_UNIT_BY_SI_NAME></A>
<B><I>ENTITY</I></B><B> Time_Unit_By_Si_Name</B>
<B><I>SUBTYPE</I></B> <B><I>OF</I></B> ( <A HREF="#S2TIME_UNIT"><B>Time_Unit</B></A> );
	time_unit_si_name : <A HREF="#S2TIME_UNIT_SI_NAME"><B>Time_Unit_Si_Name</B></A>;
<B><I>DERIVE</I></B>
	default_time_unit_si_name : <A HREF="#S2TIME_UNIT_SI_NAME"><B>Time_Unit_Si_Name</B></A> := <A HREF="#S2TIME_UNIT_SI_NAME"><B>Second</B></A>;
    translation_specification_time_unit_by_si_name :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'time_unit_si_name', '/units/timnam' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
 
<A NAME=S2TIME_UNIT_CONVERSION_TO_SI></A>
