    translation_specification_load_nodal_force :
    <B><I>SET</I></B> <B><I>OF</I></B> <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A> :=
      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'on_nodes',  '/loads(*)/nodal(*)/nodes' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'direction', '/loads(*)/nodal(*)/dir' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'force',     '/loads(*)/nodal(*)/values' ) ];
<B><I>END_ENTITY</I></B>;

<HR>
<A NAME=S10LOAD_NODAL_MOMENT></A>
