      [ <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nxi_line',    '/data(*)/ninteg(1)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'netas_line',  '/data(*)/ninteg(2)' ),
        <A HREF="#S2TRANSLATION_SPECIFICATION"><B>Translation_Specification</B></A>
          ( 'nzetas_line', '/data(*)/ninteg(3)' ) ];
<B><I>END_ENTITY</I></B>;

