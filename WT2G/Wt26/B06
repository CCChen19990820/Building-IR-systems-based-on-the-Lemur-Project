7,136" HREF="/Radio/index.html?ntb.radio"><AREA SHAPE=RECT COORDS="0,136,107,157" HREF="/Perspectives/perspectives.html?ntb.pers"><AREA SHAPE=RECT COORDS="0,157,107,176" HREF="/Newsmakers/index.html?ntb.newsmkr"><AREA SHAPE=RECT COORDS="0,176,107,197" HREF="/Rumors/Entry/0,26,,00.html?ntb.rumor"><!-- Resources --><AREA SHAPE=RECT COORDS="0,233,107,253" HREF="/Personalized/FrontDoor/1,2,,00.html?nocache=1"><AREA SHAPE=RECT COORDS="0,253,107,273" HREF="/Dispatch/Entry/0,65,,00.html"><AREA SHAPE=RECT COORDS="0,273,107,293" HREF="http://registration.cnet.com/MemberServices/1,9,,00.html?webAbbrev=ne&from=http://www.news.com"><AREA SHAPE=RECT COORDS="0,293,107,313" HREF="/Staff/index.html?ntb.staff"><AREA SHAPE=RECT COORDS="0,313,107,333" HREF="/Help/Item/0,24,1,00.html?ntb.help"><AREA SHAPE=RECT COORDS="0,333,107,353" HREF="/Searching/Entry/0,17,0,00.html?ntb.srchg"><!-- Services --><AREA SHAPE=RECT COORDS="0,389,107,409" HREF="http://www.cnet.com/?ntb.cnet"><AREA SHAPE=RECT COORDS="0,409,107,429" HREF="http://www.search.com/?ntb.search"><AREA SHAPE=RECT COORDS="0,429,107,449" HREF="http://www.download.com/?ntb.dl"><AREA SHAPE=RECT COORDS="0,449,107,469" HREF="http://www.shareware.com/?ntb.sw"><AREA SHAPE=RECT COORDS="0,469,107,489" HREF="http://www.gamecenter.com/?ntb.gc"><AREA SHAPE=RECT COORDS="0,489,107,509" HREF="http://www.mediadome.com/index.html?ntb.md"><!-- Sponsorship --><AREA SHAPE=RECT COORDS="0,544,107,564" HREF="/Sponsors/index.html?ntb.spnsr"></MAP> <img border=0 align=center src="/Banners/Portal/advertisement.gif" alt="advertisement">
