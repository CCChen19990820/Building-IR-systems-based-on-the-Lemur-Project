<HTML>

<HEAD>
<TITLE>NestEgg: Retirement Calculator</TITLE>
<SCRIPT language=JavaScript>
<!--

// func: isEmpty
// desc: return TRUE if value is empty or null, else false
function isEmpty(field) {
	return(field.value == null || field.value == "")
}


// func: isNumber
// desc: return TRUE if the string represents a number, else FALSE
function isNumber(s) {
	var oneDecimal
	s = "" + s
	var len = s.length
	for (var i=0; i<len; ++i) {
		var ch = s.charAt(i)
		if (i == 0 && ch == "-") {
			continue
		}
		if (ch == "." && !oneDecimal) {
			oneDecimal = true
			continue
		}
		if (ch < "0" || ch > "9") {
			return false
		}
	}
	return true
}


// func: formatNumber
// desc: return a string representing the number n to d decimal places
function formatNumber(n, d) {
	var base = Math.floor(n)
	var decimal = Math.round((n - base) * Math.pow(10, d))

	// left of the decimal point
	var unformattedBase = "" + base
	var len = unformattedBase.length
	var formattedBase = ""
	for (var i=0; i<len; ++i) {
		if (i && (len - i) % 3 == 0) {
			formattedBase += ","
		}
		formattedBase += unformattedBase.substring(i, i+1)
	}

	// right of the decimal point
	var formattedDecimal = ""
	if (d) {
		for (var i=0; i < d; ++i) {
			formattedDecimal = "" + decimal % 10 + formattedDecimal
			decimal = Math.floor(decimal / 10)
		}
		formattedDecimal = "." + formattedDecimal
	}

	return formattedBase + formattedDecimal
}


// func: isValidForm
// desc: ensure the required entries of there, and of the correct form
//	 if it's valid, return TRUE, else FALSE
function isValidForm(form) {
	// employee salary
	if (isEmpty(form.empeeSalary)) {
		alert("Please enter a salary.")
		form.empeeSalary.focus()
		return false
	}
	if (!isNumber(form.empeeSalary.value)) {
		alert("'" + form.empeeSalary.value + "' is not a valid salary.")
		form.empeeSalary.select()
		form.empeeSalary.focus()
		return false
	}
	if (parseFloat(form.empeeSalary.value) <= 0) {
		alert("Salary must be greater than 0.")
		form.empeeSalary.select()
		form.empeeSalary.focus()
		return false
	}
	// employee contribution
	if (isEmpty(form.empeeContribPct)) {
		alert("Please enter the percentage of employee contribution.")
		form.empeeContribPct.focus()
		return false
	}
	if (!isNumber(form.empeeContribPct.value)) {
		alert("'" + form.empeeContribPct.value + "' is not a valid employee contribution.")
		form.empeeContribPct.select()
		form.empeeContribPct.focus()
		return false
	}
	if (parseFloat(form.empeeContribPct.value) <= 0 || parseFloat(form.empeeContribPct.value) > 25) {
		alert("Since the employee contribution is a percentage, "
			+ "it must be greater than 0.\n"
			+ "Further, by law, neither an employee nor an "
			+ "employer may contribute more than 25% to a 401(k).")
		form.empeeContribPct.select()
		form.empeeContribPct.focus()
		return false
	}
	// employer matching
	if (isEmpty(form.emperMaxPct)) {
		alert("Please enter the percentage upto which the employer will match.")
		form.emperMaxPct.focus()
		return false
	}
	if (!isNumber(form.emperMaxPct.value)) {
		alert("'" + form.emperMaxPct.value
			+ "' is not a valid employer maxmium matching percentage.")
		form.emperMaxPct.select()
		form.emperMaxPct.focus()
		return false
	}
	if (parseFloat(form.emperMaxPct.value) <= 0 || parseFloat(form.emperMaxPct.value) > 25) {
		alert("Since the employer maxmium matching contribution is a percentage, it "
			+ "must be greater than 0.\n"
			+ "Further, by law, neither an employee nor an employer may "
			+ "contribute more than 25% to a 401(k).")
		form.emperMaxPct.select()
		form.emperMaxPct.focus()
		return false
	}
	// annual estimated interest rate of return
	if (isEmpty(form.annRate)) {
		alert("Please enter the estimated rate of return for the portfolio.")
		form.annRate.focus()
		return false
	}
	if (!isNumber(form.annRate.value)) {
		alert("'" + form.annRate.value + "' is not a valid rate of return.")
		form.annRate.select()
		form.annRate.focus()
		return false
	}
	// number of years
	if (isEmpty(form.numYears)) {
		alert("Please enter the number of years.")
		form.numYears.focus()
		return false
	}
	if (!isNumber(form.numYears.value)) {
		alert("'" + form.numYears.value + "' is not a valid number of years.")
		form.numYears.select()
		form.numYears.focus()
		return false
	}
	if (parseFloat(form.numYears.value) <= 0) {
		alert("The number of years must be greater than 0.")
		form.numYears.select()
		form.numYears.focus()
		return false
	}
	// annual inflation rate
	if (!isEmpty(form.inflRate)) {
		if (!isNumber(form.inflRate.value)) {
			alert("'" + form.inflRate.value + "' is not a valid inflation rate.")
			form.inflRate.select()
			form.inflRate.focus()
			return false
		}
	}
	
	// we've passed all validations
	return true
}


// func: numPeriodsPerYear
// desc: how many periods in a  years
function numPeriodsPerYear(periodicity) {
	if (periodicity == "Bi-Weekly") {
		return 26
	}
	if (periodicity == "Monthly") {
		return 12
	}
	if (periodicity == "Quarterly") {
		return 4
	}
	if (periodicity == "Semi-Monthly") {
		return 24
	}
	if (periodicity == "Weekly") {
		return 52
	}
	if (periodicity != "Annual") {
		alert(periodicity + " is an unknown periodicty. Assuming annual.")
	}
	return 1
}


// func: calcRetirement
// desc: main script process to calc the retirement data
function calcRetirement(form) {
	if (!isValidForm(form)) {
		return
	}
	var contribLimit = 9500		// max legal contribution amt
	var empeeSalary = parseFloat(form.empeeSalary.value)
	var empeeContribPct = parseFloat(form.empeeContribPct.value)
	var empeeContrib = empeeSalary * empeeContribPct / 100
	var emperMatch = parseFloat(form.emperMatch.options[form.emperMatch.selectedIndex].text)
	var emperMaxPct = parseFloat(form.emperMaxPct.value)
	var emperContrib = empeeSalary * emperMatch
		* (empeeContribPct > emperMaxPct ? emperMaxPct : empeeContribPct) / 100

	// make sure contribution is within legal limits
	if (empeeContrib > contribLimit) {
		alert("The limit for tax-free contributions to your 401(k) is currently $"
			+ contribLimit + ".  "
			+ "Any money over his amount is taxable.  "
			+ "For the purposes of this calculator, we will set your total "
			+ "tax-free contribution at $"
			+ contribLimit + ".")
		empeeContrib = contribLimit
	}

	var periodicity = form.periodicity.options[form.periodicity.selectedIndex].text
	var periodsPerYear = numPeriodsPerYear(periodicity)
	var periodContrib = empeeContrib / periodsPerYear
	var taxFree = empeeContrib + emperContrib
	var annRate = parseFloat(form.annRate.value) / 100
	var inflRate = parseFloat(form.inflRate.value) / 100
	var numYears = parseFloat(form.numYears.value)
	var futureVal = 0
	var compoundAmt = (empeeContrib + emperContrib) / periodsPerYear
	var numPeriods = numYears * periodsPerYear
	var intRate = annRate / periodsPerYear

	// future value		
	for (var i=0; i<numPeriods; ++i) {
		futureVal += compoundAmt
		compoundAmt *= 1 + intRate
	}

	// present value
	var presentVal = futureVal / Math.pow(1 + inflRate/periodsPerYear, numPeriods)

	// fill in the blanks
	form.empeeContrib.value = "$" + formatNumber(empeeContrib, 2)
	form.periodContrib.value = "$" + formatNumber(periodContrib, 2)
	form.emperContrib.value = "$" + formatNumber(emperContrib, 2)
	form.taxFree.value = "$" + formatNumber(taxFree, 2)
	form.futureVal.value = "$" + formatNumber(futureVal, 2)
	if (inflRate) {
		form.presentVal.value = "$" + formatNumber(presentVal, 2)
	}
	else {
		form.presentVal.value = "";
	}
}
// -->
</SCRIPT>
