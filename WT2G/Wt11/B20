<SCRIPT language=JavaScript>
<!--

// func: isEmpty
// desc: return TRUE if value is empty or null, else false
function isEmpty(field) {
	return(field.value == null || field.value == "")
}

// func: isNumber
// desc: return TRUE if the string represents a number, else FALSE
function isNumber(s) {
	var oneDecimal
	s = "" + s
	var len = s.length
	for (var i=0; i<len; ++i) {
		var ch = s.charAt(i)
		if (i == 0 && ch == "-") {
			continue
		}
		if (ch == "." && !oneDecimal) {
			oneDecimal = true
			continue
		}
		if (ch < "0" || ch > "9") {
			return false
		}
	}
	return true
}

// func: formatNumber
// desc: return a string representing the number n to d decimal places
function formatNumber(n, d) {
	var mult = Math.pow(10, d)
	return ("" + (Math.round(n * mult) / mult))
}

// func: isValidForm
// desc: ensure the required entries of there, and of the correct form
//	 if it's valid, return TRUE, else FALSE
function isValidForm(form) {
	// amount to borrow
	if (isEmpty(form.borrowAmt)) {
		alert("Please enter an amount to borrow.")
		form.borrowAmt.focus()
		return false
	}
	if (!isNumber(form.borrowAmt.value)) {
		alert("'" + form.borrowAmt.value + "' is not a valid amount.")
		form.borrowAmt.select()
		form.borrowAmt.focus()
		return false
	}
	if (parseFloat(form.borrowAmt.value) <= 0) {
		alert("Amount must be greater than 0.")
		form.borrowAmt.select()
		form.borrowAmt.focus()
		return false
	}

	// loan interest
	if (isEmpty(form.interest)) {
		alert("Please enter an interest percentage.")
		form.interest.focus()
		return false
	}
	if (!isNumber(form.interest.value)) {
		alert("'" + form.interest.value + "' is not a valid interest.")
		form.interest.select()
		form.interest.focus()
		return false
	}

	// loan years
	if (isEmpty(form.loanYears)) {
		alert("Please enter the length of the loan in years.")
		form.loanYears.focus()
		return false
	}
	if (!isNumber(form.loanYears.value)) {
		alert("'" + form.loanYears.value + "' is not a valid number of years.")
		form.loanYears.select()
		form.loanYears.focus()
		return false
	}
	if (parseFloat(form.loanYears.value) <= 0) {
		alert("Number of years must be greater than 0.")
		form.loanYears.select()
		form.loanYears.focus()
		return false
	}

	// we've passed all edits
	return true
}

// func: calcPayments
// desc: calculate the monhtly and total payments
function calcPayments(form) {
	if (!isValidForm(form)) {
		return
	}
	var monthRate = parseFloat(form.interest.value) / 12 / 100
	var numMonths = parseFloat(form.loanYears.value) * 12
	var totInt = Math.pow(1 + monthRate , numMonths)
	var top = monthRate * totInt
	var bottom = totInt - 1
	var monthlyPayment = parseFloat(form.borrowAmt.value) * top / bottom
	var totalPayment = monthlyPayment* numMonths
	form.monthlyPayment.value = "$" + formatNumber(monthlyPayment, 2)
	form.totalPayment.value = "$" + formatNumber(totalPayment, 2)
}
// -->
</SCRIPT>

