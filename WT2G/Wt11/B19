</DOCHDR>
<html>
<head>

<title>kidsDoctor&#153;: Fahrenheit to Celsius</title>

<script language="LiveScript">
<!-- 

function addChar(input, character)
{
    if(input.value == null || input.value == "0")
        input.value = character
    else
        input.value += character
}

function deleteChar(input)
{
    input.value = input.value.substring(0, input.value.length - 1)
}


function compute(form) 
{
	form.display.value = eval(form.display.value)
}


function ftoc(form) 
{
	form.display.value = (eval(form.display.value) -32)/1.8
}

function ctof(form) 
{
	form.display.value = (eval(form.display.value) *1.8)+32
}

function checkNum(str) 
{
	for (var i = 0; i < str.length; i++) {
		var ch = str.substring(i, i+1)
		if (ch != "."){
		if (ch < "0" || ch > "9") {
			if (ch != "/" && ch != "*" && ch != "+" && ch != "-" 
				&& ch != "(" && ch!= ")") {
				alert("Invalid input!  Please check your entry.")
				return false
			    }	
			}
		}
	}
	return true
}

<!-- -->
</script>
</head>

<BODY BGCOLOR="#ffffff" TEXT="#5e5ede">
<CENTER>
<a href="http:cgi-bin/kd_ad.pl?www.burstnet.com/ads/ad621b-map.cgi" target="_top"> 
<IMG SRC="http://www.burstnet.com/cgi-bin/ads/ad621b.cgi?852595431.1" alt="Click here for Family.com" ALIGN=CENTER BORDER=1 BORDER=1></a>

</CENTER>

<BR><BR>
<IMG src="/kdmast.gif" HEIGHT="79" WIDTH="297"
BORDER="0">

<blockquote>
<H1><TT>Temperature Converter</TT></H1>
</blockquote>
<IMG src="/hr.gif" BORDER=0>
<BLOCKQUOTE>
<form>
<table border="0" align=center bgcolor="#7F84BD" bcolor="#7F84BD">
<tr align=center>
<td>
<table border="4" align=center bgcolor="#7F84BD" bcolor="#7F84BD">
<tr align=center>
<td colspan = 3>

<table border="2">
<tr>
<td><input name="display" value="0" size=22></td>
</tr>
</table>

</td>
</tr>

<tr align=center><td align=center><input type="button" value="  7  "onClick="addChar(this.form.display, '7')"></td><td align=center><input type="button" value="  8  "onClick="addChar(this.form.display, '8')"></td><td align=center><input type="button" value="  9  "onClick="addChar(this.form.display, '9')"></td></tr>

<tr align=center><td align=center><input type="button" value="  4  "onClick="addChar(this.form.display, '4')"></td><td align=center><input type="button" value="  5  "onClick="addChar(this.form.display, '5')"></td><td align=center><input type="button" value="  6  "onClick="addChar(this.form.display, '6')"></td></tr>

<tr align=center><td align=center><input type="button" value="  1  "onClick="addChar(this.form.display, '1')"></td><td align=center><input type="button" value="  2  "onClick="addChar(this.form.display, '2')"></td><td align=center><input type="button" value="  3  "onClick="addChar(this.form.display, '3')"></td></tr>

<tr align=center><td align="center"><input type="button" value="  0  "onClick="addChar(this.form.display, '0')"></td><td align=center><input type="button" value="   .  "onClick="addChar(this.form.display, '.')"></td><td align=center bgcolor="#8D1517"><input type="button" value="  c  "onClick="this.form.display.value = 0 "></td></tr>

<tr align=center><td colspan="3" align="center"><input type="button" value="Celsius to Fahrenheit"onClick="if (checkNum(this.form.display.value)){ ctof(this.form) }"></td></tr>

<tr align=center><td colspan="3" align="center"><input type="button" value="Fahrenheit to Celsius"onClick="if (checkNum(this.form.display.value)){ ftoc(this.form) }"></td></tr></table></td></tr></table>
</form></BLOCKQUOTE>

