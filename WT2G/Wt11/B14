<P>
<FORM METHOD="POST" ACTION = "/cgi/test_cef">
<B>[CHART] CEF:</B>
<INPUT TYPE="text" SIZE=8 NAME = "sym" VALUE="acg">
<SELECT NAME="type">
<OPTION SELECTED> Daily HiLo Small
<OPTION> Daily HiLo Large
<OPTION> Weekly HiLo Small
<OPTION> Weekly HiLo Large
