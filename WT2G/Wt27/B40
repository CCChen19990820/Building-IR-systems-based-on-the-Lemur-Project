<input type=hidden name="origname" value="Cindy Johns">
<input type=hidden name="origemail" value="kaleidoscapes@kjsl.com">
<input type=hidden name="origsubject" value="Re:  I miss my message board pals.">
<input type=hidden name="origdate" value="November 19, 1996 at 18:31:40">
