        <td valign="top">Utilidad Neta</td>
        <td align="right" valign="top" width="12%">264</td>
        <td align="right" valign="top" width="12%">193</td>
        <td align="right" valign="top" width="12%">667</td>
        <td align="right" valign="top" width="12%">402</td>
    </tr>
    <tr>
        <td valign="top">Utilidad Neta por
        acci&oacute;n-pesos/acci&oacute;n</td>
        <td align="right" valign="top" width="12%">0.75 </td>
        <td align="right" valign="top" width="12%">0.55</td>
