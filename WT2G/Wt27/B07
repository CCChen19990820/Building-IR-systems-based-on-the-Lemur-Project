	' Remember the last location clicked.
	Dim mX, mY

	Sub Image_MouseMove(s, b, x, y)
		mX = x
		mY = y
	
		If InRect(x, y,  245, 68, 293, 185) Then 
			Call DescribeLink("The Chapel")

		ElseIf InRect(x, y, 7, 100, 58, 150) then 
			Call DescribeLink("Northwest Bastion")

		ElseIf InRect(x, y, 526, 545, 578, 595) then 
			Call DescribeLink("Southeast Bastion")

		ElseIf InRect(x, y, 94, 155, 256, 195) then 
			Call DescribeLink("Canedo Adobe")
	
		ElseIf InRect(x, y,  293, 90, 395, 170) then 
			Call DescribeLink("Comandancia")
	
		ElseIf InRect(x, y,  81, 307, 107, 355) then 
			Call DescribeLink("El Cuartel (The Guardhouse)")
	
		ElseIf InRect(x, y,  106, 106, 248, 80) then 
			Call DescribeLink("First defensive wall")
	
		ElseIf InRect(x, y,  414, 0, 503, 240) then 
			Call DescribeLink("Northeast corner")
	
		ElseIf InRect(x, y,  0, 52, 65, 65) then 
			Call DescribeLink("Second  defensive wall")

		ElseIf InRect(x, y,  175, 21, 315, 45) then 
			Call DescribeLink("Second  defensive wall")

		ElseIf InRect(x, y,  502, 0, 650, 45) then 
			Call DescribeLink("True north")

		ElseIf InRect(x, y, 220, 530, 310, 560) then 
			Call DescribeLink("Guard house")

		ElseIf InRect(x, y, 135, 540, 220, 570) then 
			Call DescribeLink("Barracks")

		ElseIf InRect(x, y, 170, 570, 270, 600) then 
			Call DescribeLink("Corral")

		ElseIf InRect(x, y, 390, 540, 490, 570) then 
			Call DescribeLink("Corral")

		ElseIf InRect(x, y, 135, 310, 245, 520) then 
			Call DescribeLink("Dirt")

		Else 
			DescribeLink ""
		End If
	End Sub


	Sub Image_OnClick()
		If InRect(mX, mY,  5, 30, 120, 85) Then 
			location.href = "msprod.htm"
	
		ElseIf InRect(mX, mY,  5, 95, 120, 135) then 
			location.href = "support/"
	
		ElseIf InRect(mX, mY,  5, 150, 120, 190) then 
			location.href = "intprod.htm"
	
		ElseIf InRect(mX, mY,  470, 30, 570, 47) then 
			location.href = "default.html"
	
		ElseIf InRect(mX, mY,  470, 70, 570, 87) then 
			location.href = "allinone.asp"
	
		ElseIf InRect(mX, mY,  470, 105, 570, 122) then 
			location.href = "ref.asp"
	
		ElseIf InRect(mX, mY,  470, 140, 570, 157) then 
			location.href = "other.htm"
		
		ElseIf InRect(mX, mY,  470, 175, 570, 192) then 
			location.href = "msn.htm"
		End If
	End Sub

	Function InRect(x, y, Rect_x1, Rect_y1, Rect_x2, Rect_y2)
		InRect =  x > Rect_x1 And x < Rect_x2 And y > Rect_y1 And y < Rect_y2
	End Function
	
	Sub DescribeLink(Text)
		TxtLinkDescription1.Value = Text
		TxtLinkDescription2.Value = Text
	End Sub
