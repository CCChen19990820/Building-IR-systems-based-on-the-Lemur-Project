        <td align="center" valign="top" colspan="3"><b>PRODUCTOS</b></td>
    </tr>
    <tr>
        <td align="center" valign="top"><b>96</b></td>
        <td align="center" valign="top"><b>97</b></td>
        <td align="center" valign="top"><b>98</b></td>
    </tr>
    <tr>
        <td valign="top">Viscosidad a 100 &#176;C</td>
        <td align="center" valign="top">cSt</td>
        <td align="center" valign="top">D-445</td>
        <td align="center" valign="top">1085</td>
        <td align="center" valign="top">215</td>
        <td align="center" valign="top">440</td>
    </tr>
    <tr>
        <td valign="top">Punto de Inflamaci&oacute;n</td>
        <td align="center" valign="top">&#176;C</td>
        <td align="center" valign="top">D-92</td>
        <td align="center" valign="top">330</td>
        <td align="center" valign="top">280</td>
        <td align="center" valign="top">310</td>
