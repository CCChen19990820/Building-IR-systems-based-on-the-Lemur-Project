  var speed=10;
  document.form2.banner.value=msg.substring(position,position+50);
  if(position++==msg.length) {
      if (revol-- < 2) return;
      position=0;
  }
  id=setTimeout("banner()",1000/speed);
}
// end -->
</SCRIPT>

</HEAD>
<BODY BGCOLOR="#BFFFFF" BACKGROUND="blupaper.gif" onload="banner()">
