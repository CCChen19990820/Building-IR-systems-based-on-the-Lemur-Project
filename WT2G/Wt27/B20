  Phone number: <INPUT SIZE=15 NAME="phone">
          City: <SELECT NAME="city">
                <OPTION SELECTED>Pasadena (free)
                <OPTION>Altadena (free)
                <OPTION>So. Pasadena ($1.00)
                <OPTION>Arcadia ($1.00)
                <OPTION>Monrovia ($2.50)
