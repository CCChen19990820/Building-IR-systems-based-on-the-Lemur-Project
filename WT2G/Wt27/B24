function meopen(bwindow,book){
bwindow = window.open(book,bwindow,'width=600,height=600,scrollbars=yes')
}
function nyaah(){
if (confirm("\nAre You Sure You Want To Know???")){
alert("\nNyaaah Nyaaah nyaah Nyaaah Nyaaaaaaaah!!!!!!")}
else{
alert("\nYour Secret Is Safe With Me!")}
}

</Script>
