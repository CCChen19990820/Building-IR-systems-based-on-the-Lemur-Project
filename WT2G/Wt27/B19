	<TD COLSPAN=1 WIDTH=4><IMG SRC="../assets/images/dot_clear.gif" WIDTH=4 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=5><IMG SRC="../assets/images/dot_clear.gif" WIDTH=5 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=5><IMG SRC="../assets/images/dot_clear.gif" WIDTH=5 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=25><IMG SRC="../assets/images/dot_clear.gif" WIDTH=25 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=272><IMG SRC="../assets/images/dot_clear.gif" WIDTH=272 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=259><IMG SRC="../assets/images/dot_clear.gif" WIDTH=259 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=24><IMG SRC="../assets/images/dot_clear.gif" WIDTH=24 HEIGHT=1 BORDER=0></TD>
