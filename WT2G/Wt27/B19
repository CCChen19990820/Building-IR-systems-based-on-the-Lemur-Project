	<TD COLSPAN=1 WIDTH=1><IMG SRC="../assets/images/dot_clear.gif" WIDTH=1 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=4><IMG SRC="../assets/images/dot_clear.gif" WIDTH=4 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=10><IMG SRC="../assets/images/dot_clear.gif" WIDTH=10 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=20><IMG SRC="../assets/images/dot_clear.gif" WIDTH=20 HEIGHT=1 BORDER=0></TD>
