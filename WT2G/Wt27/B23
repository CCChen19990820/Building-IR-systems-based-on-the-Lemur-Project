where = document.referrer
wherenow = document.title

function cl_clock(){
	wclock.close()
}

function openclock(){
wclock = window.open('clock.htm','clock','width=300,height=100')
setTimeout("cl_clock()",5000)
}
function who(){
whowindow = window.open('blank.htm','newwin','width=700,height=200')
whowindow.focus
whowindow.document.open()

whowindow.document.write("<B>You Came From: </B>",where,"<BR><B>You Are Now At: </B>",wherenow)
whowindow.document.close()

}

</Script>


