<body bgcolor="#ffffff">
<a name = "top"></a>
<!-- begin topnavigation -->
<a href = "/roche/home.htm"><img src="/roche/images/shome.gif" alt="Home" border="0"></a><a href = "/roche/about/esevesa.htm"><img src="/roche/images/sup.gif" alt="Up" border="0"></a><a href = "/roche/about/esevesg3.htm"><img src="/roche/images/sprev.gif" alt="Prev" border="0"></a><a href = "/roche/about/esevesg5.htm"><img src="/roche/images/snext.gif" alt="Next" border="0"></a>
<!-- end topnavigation -->
<!-- begin sectionmenu -->
<a href ="/roche/news/news.htm"><img src ="/roche/images/snews.gif" alt="News" border="0"></a><a href ="/roche/about/about.htm"><img src ="/roche/images/sabout.gif" alt="About Roche" border="0"></a><a href ="/roche/finance/finance.htm"><img src ="/roche/images/sfin.gif" alt="Finance" border="0"></a><a href ="/roche/business/biz.htm"><img src ="/roche/images/sbiz.gif" alt="Business" border="0"></a><a href ="/roche/services/services.htm"><img src ="/roche/images/sserv.gif" alt="Service" border="0"></a>
<!-- end sectionmenu -->
<!-- begin inline -->
<a href="/roche/misc/search.htm"><img src="/roche/images/sfind.gif" alt="Search" border="0"></a><a href="/roche/misc/help.htm"><img src="/roche/images/shelp.gif" alt="Help" border="0"></a>
