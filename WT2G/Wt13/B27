<TR VALIGN=TOP ALIGN=RIGHT><TD ALIGN=CENTER>	9	</TD><TD>		</TD><TD>		</TD><TD>		</TD><TD>	14.49	</TD><TD>	28.68	</TD><TD>	42.59	</TD><TD>	56.22	</TD><TD>	69.57	</TD><TD>	82.66	</TD></TR VALIGN=TOP>

