<TITLE>Our Prices</TITLE>
<SCRIPT LANGUAGE="JavaScript">
<!-- Beginning of WebMania/JavaScript Applet -------------------
/* Copyright (C)1996 Q&D Software Development
   All Rights Reserved.

*/
function scroll_status (seed)
{
        var msg = "For more information on our services and prices please complete included form.!!!";
        var out = " ";
        var c   = 1;

        if (150 < seed) {
                seed--;
                var cmd="scroll_status(" + seed + ")";
                timerTwo=window.setTimeout(cmd,25);
        }
        else if (seed <= 150 && 0 < seed) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="scroll_status(" + seed + ")";
                window.status=out;
                timerTwo=window.setTimeout(cmd,25);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="scroll_status(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,25);
                }
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("scroll_status(150)",25);
                }
        }
}
// -- End of JavaScript code -------------- -->

</SCRIPT>
</HEAD>
<BODY BACKGROUND="whtpaper.gif" BGCOLOR="FFFF80" TEXT="800000" LINK="008080" VLINK="FF8040" ALINK="0000A0" onLoad="timerONE=window.setTimeout('scroll_status(100)',50);">
