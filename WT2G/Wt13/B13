function ChooseCity(value)
{
   if(value==0) document.myform.city_name.value="Unknown";
   if(value==1) document.myform.city_name.value="Chicago";
   if(value==2) document.myform.city_name.value="D.C.";
   if(value==3) document.myform.city_name.value="Baltimore";
   if(value==4) document.myform.city_name.value="New York City";
   if(value==5) document.myform.city_name.value="Boston";
   if(value==6) document.myform.city_name.value="Montreal (Canada)";
   if(value==7) document.myform.city_name.value="Houston";
   if(value==8) document.myform.city_name.value="Denver";
   if(value==9) document.myform.city_name.value="San Diego";
   if(value==10) document.myform.city_name.value="Los Angeles";
   if(value==11) document.myform.city_name.value="San Francisco";
   if(value==12) document.myform.city_name.value="Vancouver, BC (Canada)";
   if(value==13) document.myform.city_name.value="Portland";
   if(value==14) document.myform.city_name.value="Seattle";
   if(value==15) document.myform.city_name.value="Dallas/Ft. Worth";
   if(value==16) document.myform.city_name.value="Las Vegas";
   if(value==17) document.myform.city_name.value="Salt Lake City";
   if(value==18) document.myform.city_name.value="Pittsburgh";
   if(value==19) document.myform.city_name.value="Cleveland";
   if(value==20) document.myform.city_name.value="Buffalo";
   if(value==21) document.myform.city_name.value="Toronto (Canada)";
   if(value==22) document.myform.city_name.value="Ottawa (Canada)";
   if(value==23) document.myform.city_name.value="Quebec (Canada)";
   if(value==24) document.myform.city_name.value="New Orleans";
   if(value==25) document.myform.city_name.value="Baton Rouge";
   if(value==26) document.myform.city_name.value="Austin";
   if(value==27) document.myform.city_name.value="El Paso";
   if(value==28) document.myform.city_name.value="Tucson";
