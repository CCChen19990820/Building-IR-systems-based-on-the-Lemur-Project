function 
scroll_status (seed)



{


var msg = "Welcome to Salvador H. Sanchez Middle School!"; var out = " ";



var c   = 1;






if (200 < seed) {


 
seed--;var cmd="scroll_status(" + seed + ")";

 
timerTwo=window.setTimeout(cmd,50);


}


else if (seed <= 200 && 0 < 
seed) 
{


for (c=0 ; c < seed ; c++) {


out+=" ";


}


out+=msg;


 
seed--;



                var cmd="scroll_status(" + seed + ")";



                
window.status=out;



                timerTwo=window.setTimeout(cmd,50);



}


else if (seed <= 0) {


if (-seed < msg.length) 
{



out+=msg.substring(-seed,msg.length);


seed--;


var 
cmd="scroll_status(" + seed + ")";



                        
window.status=out;


timerTwo=window.setTimeout(cmd,50);


}



else{



window.status="";


timerTwo=window.setTimeout("scroll_status(200)",50);


 
}


}


}



// -- end of javascript applet - -->







</script>



