<head>
<script>
<!-- Scroller

function vikescroll(loop)
{
	var str1 = "Welcome to Viewmont High School's web site....   ";
	var str2 = "We hope you enjoy browsing our pages...   ";
	var str3 = "We welcome your comments...";
	var message = str1+str2+str3;
	var out = " ";
	var cnt = 1;

	if (loop > 0)
	{ for (cnt=0 ; cnt < loop ; cnt++)
	   { out+=" ";}
	  out += message;
	  loop--;
	  var cmd="vikescroll(" + loop + ")";
	  window.status=out;
	  timerTwo=window.setTimeout(cmd,100);}
	else
	{ if (-loop < message.length)
	   {out += message.substring(-loop,message.length);
	    loop--;
	    var cmd="vikescroll(" + loop + ")";
	    window.status=out;
	    timerTwo=window.setTimeout(cmd,100);}
	  else 
	   {window.status=" ";
	    timerTwo=window.setTimeout("vikescroll(100)",75);}
	}
}

// -- End of Scroller code -->
</script>
</head>
<body onload="vikescroll(100)">
