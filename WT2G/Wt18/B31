            <td align="center" width="14%"><font size="-1"
            face="arial, helvetica">growth factor </font></td>
            <td align="center" width="14%">&nbsp;</td>
            <td align="center" width="14%"><font size="-1"
            face="arial, helvetica">Neoangiogenesis</font></td>
            <td align="center" width="15%">&nbsp;</td>
            <td align="center" width="15%">&nbsp;</td>
        </tr>
        <tr>
            <td align="center"><font size="-1" face="arial, helvetica"><b>HIV</b></font></td>
            <td align="center" width="14%">&nbsp;</td>
