<SCRIPT LANGUAGE="JavaScript">
<!-- Beginning of WebForms/JavaScript Applet -------------

function scroll_status (seed)
{
    var msg = "ASOG - der Spezialist fr internationale Datenbanken !";
    var out = " ";
    var c = 1;
    if (150 < seed) {
            seed--;
            var cmd="scroll_status(" + seed + ")";
            timerTwo = window.setTimeout(cmd, 25);
    }
    else if (seed <= 150 && 0 < seed) {
            for (c=0 ; c < seed ; c++) {
                    out+=" ";
            }
            out+=msg;
            seed--;
            var cmd="scroll_status(" + seed + ")";
                window.status=out;
            timerTwo=window.setTimeout(cmd,25);
    }
    else if (seed <= 0) {
            if (-seed < msg.length) {
                    out+=msg.substring(-seed,msg.length);
                    seed--;
            var cmd="scroll_status(" + seed + ")";
                    window.status=out;
                    timerTwo=window.setTimeout(cmd,25);
            }
            else {
                    window.status=" ";
                    timerTwo=window.setTimeout("scroll_status(150)",25);
            }
    }
}
// -- End of JavaScript code -------------- -->
</SCRIPT>

