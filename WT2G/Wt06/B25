<OBJECT
    	ID=cool
    	CLASSID="clsid:99B42120-6EC7-11CF-A6C7-00AA00A47DD2"
        CODEBASE="http://activex.microsoft.com/controls/iexplorer/ielabel.ocx#Version=4,70,0,1161"
        TYPE="application/x-oleobject"
    	WIDTH=700
    	HEIGHT=50
	align=center
	HSPACE=20
    	VSPACE=10
>
	<PARAM NAME="Alignment" VALUE="2" >
	<PARAM NAME="Caption" VALUE="The UnOfficial Magic: The Gathering Home Page">
	<PARAM NAME="ForeColor" VALUE="#H00FF00">
	<PARAM NAME="FontName" VALUE="Arial">
	<PARAM NAME="FontSize" VALUE="24">
</OBJECT>
<OBJECT
    	ID="timer2"
    	CLASSID="clsid:59CCB4A0-727D-11CF-AC36-00AA00A47DD2"
        CODEBASE="http://activex.microsoft.com/controls/iexplorer/ietimer.ocx#version=4,70,0,1161"
        TYPE="application/x-oleobject"
    	ALIGN=middle
>
	<PARAM NAME="Interval" VALUE="1000">
	<PARAM NAME="Enabled" VALUE="True">
</OBJECT>
<SCRIPT LANGUAGE="VBScript">

Sub BtnToggle_OnClick
	Timer1.Enabled = Not Timer1.Enabled             
	Timer2.Enabled = Not Timer2.Enabled
End Sub

Sub Timer1_timer
	Label.Angle = (Label.Angle + 5) mod 360
End Sub

Sub Timer2_Timer
	Randomize
	Cool.ForeColor = rnd() * 16777216
End Sub

</SCRIPT>

<hr width=80%>
<p align=center><font color="#FF0000"><tt>Magic: The Gathering is a game of brain, eye, and hand</tt></font></p>
<p align=center><font color="#FF0000"><tt>---Anymous</tt></font> </p>
<hr width=80%>
<p align=center><a href="http://www.wizards.com/Duelist/Welcome.html"><img src="2_cover.gif" align=bottom border=0 width=108 height=142></a></p>
<p align=center><a href="http://www.wizards.com/Duelist/Welcome.html">Duelist Online</a> is THE source for the latest Magic: The Gathering cards, </p>
<p align=center>tournaments, and artist bios.</p>
<hr width=80%>
<p align=center><a href="lists.htm"><img src="magic2.gif" align=bottom border=0 width=75 height=105></a></p>
<p align=center><a href="lists.htm">Card List</a> is a great resource to find prices, color, casting costs, etc. on ANY card.</p>
<hr width=80%>
<p align=center><a href="artists.htm"><img src="Norritt.gif" align=bottom border=0 width=124 height=100></a></p>
<p align=center><a href="artists.htm">Caravan '96</a> is a great source for artist bios and artwork.</p>
<hr width=80%>
<p align=center><a href="http://www.wizards.com/"><img src="Logo_WotC_Anim.gif" align=bottom border=0 width=180 height=76></a></p>
<p align=center><a href="http://www.wizards.com/">Wizards of the Coast</a> is a great source for artists bios and M:tG rulings, </p>
<p align=center>as well as other tradable card games</p>
<hr width=80%>
<p align=center><a href="MtGStore/MtGS.htm">Magic: The Gathering Store</a></p>
