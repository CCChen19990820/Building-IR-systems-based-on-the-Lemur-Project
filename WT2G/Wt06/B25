<SCRIPT LANGUAGE="VBScript">

Sub BtnToggle_OnClick
	Timer1.Enabled = Not Timer1.Enabled             
	Timer2.Enabled = Not Timer2.Enabled
End Sub

Sub Timer1_timer
	Label.Angle = (Label.Angle + 5) mod 360
End Sub

Sub Timer2_Timer
	Randomize
	Cool.ForeColor = rnd() * 16777216
End Sub

</SCRIPT>

