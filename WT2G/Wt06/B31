<!-- Beginning of JavaScript Applet -------------------

/* Copyright (C)4/28/96 Larry Smith

*/
function scrollit_r2l(seed)
{
var m1 = "Check the latest regarding vitamins and supplements,";
var m2 = "nutrition, herbs, Lifestyle Analysis, and more!";
var m3 = "ELVIRE'S PAGE IN KARINYA for BODY, MIND and SPIRIT!";
var spc = " ";
var msg = m1+spc+m2+spc+m3;
var out = " ";
var c = 1;
var state = 1;
if (seed > 100) {
seed--;
var cmd="scrollit_r2l(" + seed + ")";
timerTwo=window.setTimeout(cmd,50);
}
else if (seed <= 100 && seed > 0) {
for (c=0 ; c < seed ; c++) {
out+=" ";
}
out+=msg;
seed--;
var cmd="scrollit_r2l(" + seed + ")";
window.status=out;
timerTwo=window.setTimeout(cmd,50);
}
else if (seed <= 0) {
if (-seed < msg.length) {
out+=msg.substring(-seed,msg.length);
seed--;
var cmd="scrollit_r2l(" + seed + ")";
window.status=out;
timerTwo=window.setTimeout(cmd,50);
}
else {
window.status=" ";
timerTwo=window.setTimeout("scrollit_r2l(50)",50);
}
}
}
// -- End of JavaScript code -------------- -->

