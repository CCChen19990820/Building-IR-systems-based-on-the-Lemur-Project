<SELECT NAME="errorflag">
<OPTION VALUE=0>None
<OPTION VALUE=1>1 Error
<OPTION VALUE=2>2 Errors
</SELECT>
<br><i><font size="2">- Allows for character misspelling if set to X Error(s)</font> </i>
</ul>
</td>
