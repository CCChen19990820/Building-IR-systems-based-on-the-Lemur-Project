function addcookie(str)
{		
	if(navigator.appName == "Netscape") 
	{
		document.cookie = document.cookie + "@" + str + ";expires=Wednesday, 01-Jan-97 12:00:00 GMT;";
	}
	else
	{
		document.cookie = "@"+str;
	}
}

function addhistorylist()
{
	var j = 0;
	var i = 0;
	document.write("<SELECT name=\"history\" >");
	j = document.cookie.length;
	for(i = document.cookie.length; i > 0 && i > document.cookie.length - 200; i--) 
	{
	  if(document.cookie.charAt(i) == "@")
	  {
		if( j > i+1)
		{
		   document.write("<OPTION> "+document.cookie.substring(i+1,j)+" </OPTION>");
//		   alert("Add option "+ document.cookie.substring(i,j));
		}
	        j = i;
	  }
	}
	if(j > i + 1)
	{
		document.write("<OPTION> "+document.cookie.substring(i,j)+" </OPTION>");
//		alert("Add option "+ document.cookie.substring(i,j));
	}
	document.write("</SELECT>");
}
function addhistoryeditselect()
{
	document.write("<SELECT NAME=\"historedit\">");
	document.write("<OPTION>Edit</OPTION>");
	document.write("<OPTION>History</OPTION>");
	document.write("</SELECT>");
}
	

function convert_string(str)
{
	output_str =""
	for(i=0; i < str.length; i++)
	{	
	   if(str.charAt(i) == ' ')
	   {
		output_str += '+'
	   }
	   else if(str.charAt(i) == '+')
	   {
		output_str += "%2B"
	   }
	   else
	   {
		output_str += str.charAt(i)
	    }
	}
	return output_str
}

function nist_submit()
{ 
	i = 0
	if(document.forms[0].elements[2].selectedIndex == 1)
	{
		if(document.forms[0].elements[1].selectedIndex < 0)
		{
			return;
		}
		string = document.forms[0].elements[1].options[document.forms[0].elements[1].selectedIndex].text;
	}
	else
	{
		string = convert_string(document.forms[0].elements[0].value)
		addcookie(string);
	}
	url_string = "http://search.nist.gov/Harvest/cgi-bin/BrokerQuery.pl.cgi?query="+string+"&broker=search&caseflag=on&wordflag=on&errorflag=0"
	window.location = url_string
}

function nist_phonebook_submit()
{ 
	i = 0
	if(document.forms[0].elements[2].selectedIndex == 1)
	{
		if(document.forms[0].elements[1].selectedIndex < 0)
		{
			return;
		}
		string = document.forms[0].elements[1].options[document.forms[0].elements[1].selectedIndex].text;
	}
	else
	{
		string = convert_string(document.forms[0].elements[0].value)
		addcookie(string);
	}
	pre_string= "http://www.nist.gov/cgi-bin/wwwph?"
	post_string=""
	url_string = pre_string+string+post_string
	window.location = url_string
}

