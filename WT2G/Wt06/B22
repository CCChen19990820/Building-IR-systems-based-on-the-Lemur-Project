	pre_string= "http://metacrawler.cs.washington.edu/cgi-bin/nph-metaquery.p?general="
	post_string="&logic=0&searchType=Normal+Search&region=The+World&orgType=Any&maxwait=1&score=0"
	url_string = pre_string+string+post_string
	window.location = url_string
}

function opentext_submit()
{ 
