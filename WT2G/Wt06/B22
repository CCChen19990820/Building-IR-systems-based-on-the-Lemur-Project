function convert_string(str)
{
	output_str =""
	for(i=0; i < str.length; i++)
	{	
	   if(str.charAt(i) == ' ')
	   {
		output_str += '+'
	   }
	   else if(str.charAt(i) == '+')
	   {
		output_str += "%2B"
	   }
	   else
	   {
		output_str += str.charAt(i)
	    }
	}
	return output_str
}

function nist_submit()
{ 
	i = 0
	string = convert_string(document.forms[0].elements[0].value)
	url_string = "http://search.nist.gov/Harvest/cgi-bin/BrokerQuery.pl.cgi?query="+string+"&broker=search&caseflag=on&wordflag=on&errorflag=0"
	window.location = url_string
}

