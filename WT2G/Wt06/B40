<DOCOLDNO>IA068-000426-B030-34</DOCOLDNO>
<DOCHDR>
http://mime1.marc.gatech.edu:80/imb/projects/LCCD/lccdint.bas 130.207.33.57 19970113080836 text/html 1084
HTTP/1.0 200 OK
Server: WebSTAR
MIME-Version: 1.0
Content-type: text/html
</DOCHDR>
'LCCDINT.BAS

'

'October 1996

'

'This program was used to test the 

'Hamamatsu Linear CCD Parallel Interface

'It writes the address of the pixel

'Waits for the interupt line to go low

'then reads the value for the pixel

'and displays it on the screen 



CONST PORT = &H378

CONST PDATA = PORT + 0

CONST PSTAT = PORT + 1

CONST PCON = PORT + 2

CONST ADDRSTB = PORT + 3

CONST DATASTB = PORT + 4



DIM value%(0 TO 255)



SCREEN 11: WINDOW (0, 255)-(255, 0): CLS



max% = 0

min% = 255



DO

        FOR a% = 0 TO 255

                OUT DATASTB, a%

                'FOR b% = 1 TO 10000: NEXT b%

                DO

                LOOP UNTIL (0 = SGN((64 AND INP(PSTAT))))

                value%(a%) = INP(DATASTB)

                IF value%(a%) < min% THEN min% = value%(a%)

                IF value%(a%) > max% THEN max% = value%(a%)

                LINE (a%, 0)-(a%, 255), 0

                PSET (a%, value%(a%)), 0

                PSET (a%, value%(a%))

        NEXT a%

LOOP



</DOC>
