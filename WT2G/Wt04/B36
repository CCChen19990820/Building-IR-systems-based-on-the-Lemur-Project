<title>Blessed Are They - Submit a Site</title>


<SCRIPT LANGUAGE="JavaScript">

<!-- Beginning of JavaScript Applet -------------------



/* Copyright (C)1996 Web Integration Systems, Inc. DBA Websys, Inc.
   All Rights Reserved.

   This applet can be re-used or modified, if credit is given in 
   the source code.

   We will not be held responsible for any unwanted effects due to the 
   usage of this applet or any derivative.  No warrantees for usability 
   for any specific application are given or implied.

   Chris Skinner, January 30th, 1996.

   Hacked for INFOSEARCH by DJJ Febraury 27, 1996
   Re-hacked for Border TV by Dan Gibson. April 1996.
*/



function scrollit_r2l(seed)
{
        var m1  = "Thank you for taking time to complete the Border TV 'Blessed Are They' submission form.   ";
        var m2  = "   By completing this form you enable us to improve the site, and improve campatability.   ";
        var m3  = "    Please feel free to add a link to Border TV's pages from your own Web Site. Thank you.";
        var m4  = " ";

        var msg=m1+m2+m3+m4;
        var out = " ";
        var c   = 1;

	if (seed > 100)
	{
                seed--;
                var cmd="scrollit_r2l(" + seed + ")";
                timerTwo=window.setTimeout(cmd,100);
	 }

	else if (seed <= 100 && seed > 0)
	{
		for (c=0 ; c < seed ; c++)
		{
			out+=" ";
		}
              out+=msg;
              seed--;
              var cmd="scrollit_r2l(" + seed + ")";
	window.status=out;

              timerTwo=window.setTimeout(cmd,100);
	}
	else if (seed <= 0)
	{
		if (-seed < msg.length)
		{
			out+=msg.substring(-seed,msg.length);
			seed--;
			var cmd="scrollit_r2l(" + seed + ")";
			window.status=out;
			timerTwo=window.setTimeout(cmd,100);
	              }
	              else
		{
			window.status=" ";
			timerTwo=window.setTimeout("scrollit_r2l(100)",75);
		}

	}

}

// -- End of JavaScript code -------------- -->

</SCRIPT>



</head>
