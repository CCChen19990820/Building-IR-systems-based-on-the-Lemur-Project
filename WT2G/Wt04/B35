<TR><TD ALIGN=LEFT>Common Stocks</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>93.0%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Preferred Stocks</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>0.9%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Government Bonds</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>0.0%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Corporate Bonds</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>0.3%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Mortgages</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>0.0%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Cash &amp; Short Term Securities</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>5.8%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Other</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>0.0%</TD><TD></TD></TR>
<TR><TD></TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>100.0%</TD><TD></TD></TR>
<TR><TD></TD><TD></TD><TD></TD><TD></TD><TD></TD><TD></TD></TR>
<TR><TD></TD><TD></TD><TD></TD><TD></TD><TD></TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Foreign Content</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>7.1%</TD><TD></TD></TR>
</TABLE><P><HR>





