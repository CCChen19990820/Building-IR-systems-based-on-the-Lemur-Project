function checkform( thisform ) {
   if (thisform.caller.value == null || thisform.caller.value == "" ) {
      alert ("Please enter your name") ;
      thisform.caller.focus() ;
      thisform.caller.select() ;
      return false ;
   }
   if (thisform.address.value == null || thisform.address.value == "" ) {
      alert ("Please enter your e-mail address") ;
      thisform.address.focus() ;
      thisform.address.select() ;
      return false ;
   }
   if (thisform.letter.value == null || thisform.letter.value == "" ) {
      alert ("Please enter your message/response") ;
      thisform.letter.focus() ;
      thisform.letter.select() ;
      return false ;
   }
   if (thisform.title.value == null || thisform.title.value == "" ) {
      alert ("Please enter a title for your letter") ;
      thisform.title.focus() ;
      thisform.title.select() ;
      return false ;
   }
    return true
}
