<TR><TD ALIGN=LEFT>Denison Mines Limited</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>2.8%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>Verifone, Inc.</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>2.4%</TD><TD></TD></TR>
<TR><TD ALIGN=LEFT>BBN Corporation</TD><TD></TD><TD></TD><TD></TD><TD ALIGN=RIGHT>2.2%</TD><TD></TD></TR>
