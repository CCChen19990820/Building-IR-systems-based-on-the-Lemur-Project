function checkform( thisform ) {
   if (thisform.company.value == null || thisform.company.value == "" ) {
      alert ("Please enter your Company's name") ;
      thisform.company.focus() ;
      thisform.company.select() ;
      return false ;
   }
   if (thisform.streetaddress.value == null || thisform.streetaddress.value == "" ) {
      alert ("Please enter your company's address") ;
      thisform.streetaddress.focus() ;
      thisform.streetaddress.select() ;
      return false ;
   }
   if (thisform.citystatezip.value == null || thisform.citystatezip.value == "" ) {
      alert ("Please enter your company's address") ;
      thisform.citystatezip.focus() ;
      thisform.citystatezip.select() ;
      return false ;
   }
   if (thisform.country.value == null || thisform.country.value == "" ) {
      alert ("Please enter your company's address") ;
      thisform.country.focus() ;
      thisform.country.select() ;
      return false ;
   }
   if (thisform.whatwedo.value == null || thisform.whatwedo.value == "" ) {
      alert ("Please enter a description of your company's products and services") ;
      thisform.whatwedo.focus() ;
      thisform.whatwedo.select() ;
      return false ;
   }
    return true
}
