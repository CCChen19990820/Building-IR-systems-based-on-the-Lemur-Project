var sent = "No" ;
function checkform( thisform ) {
  if (sent == "Yes") {
      alert ("Message was already sent")
      return false ;
}
   if (thisform.personemail.value == null || thisform.personemail.value == "" ) {
      alert ("Please enter your E-mail address") ;
      thisform.personemail.focus() ;
      thisform.personemail.select() ;
      return false ;
   }
   if (thisform.date.value == null || thisform.date.value == "" ) {
      alert ("Please enter the date of the event") ;
      thisform.date.focus() ;
      thisform.date.select() ;
      return false ;
   }
   if (thisform.event.value == null || thisform.event.value == "" ) {
      alert ("Please enter the name of the event") ;
      thisform.event.focus() ;
      thisform.event.select() ;
      return false ;
   }
   if (thisform.description.value == null || thisform.description.value == "" ) {
      alert ("Please enter a description of the event") ;
      thisform.description.focus() ;
      thisform.description.select() ;
      return false ;
    }
sent = "Yes"
    return true
}
// End of script --></SCRIPT> 
