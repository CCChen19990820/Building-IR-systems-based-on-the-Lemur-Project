   <TITLE>Crowcon Detective Specification</TITLE>
   <META NAME="GENERATOR" CONTENT="Mozilla/3.0Gold (Win95; I) [Netscape]">
   <META NAME="Author" CONTENT="Marketing">
   <META NAME="Description" CONTENT="Crowcon 'Detective' gas detector technical specification">
