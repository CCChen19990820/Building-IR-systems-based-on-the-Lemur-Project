<DOCNO>WT04-B34-192</DOCNO>
<DOCOLDNO>IA047-000825-B003-25</DOCOLDNO>
<DOCHDR>
http://www.greatpacific.com:80/gplanner.htm 204.174.16.155 19970119073908 text/html 8466
HTTP/1.0 200 OK
Server: Netscape-Commerce/1.12
Date: Sunday, 19-Jan-97 07:40:44 GMT
Last-modified: Thursday, 02-May-96 01:17:41 GMT
Content-length: 8281
Content-type: text/html
</DOCHDR>
<HTML>

<HEAD>
<TITLE>Life insurance planner</TITLE>
</HEAD>

<BODY BACKGROUND="gpback5.gif" VLINK="#000080">

<SCRIPT LANGUAGE=JavaScript>
<!-- hide this script tag's contents from old browsers

//
//

function checkNumber(obj)
{
	var str = obj.value;

	if (str.length == 0 || str == "" || str == null) {
		return false;
	}

	for (var i = 0; i < str.length; i++) {
		var ch = str.substring(i, i + 1)
		if ((ch < "0" || "9" < ch) && ch != '.' && ch != '$' && ch != ',') {
			return false;
		}
	}

	return true;
}

function tonum(str) 
{
	var     nstr = "";

	for (var i = 0; i < str.length; i++) {
		var ch = str.substring(i, i + 1);
		if ((ch >= "0" && ch <= "9") || ch == '.') {
			nstr += ch;
		}
	}

	return parseFloat(nstr);
}

function valueOrDefault(obj, defval) 
{
	if (!checkNumber(obj)) {
		return defval;
	}

	var val = tonum(obj.value);

	if (val == 0) {
		return defval;
	}
	return val;
}

function format(val, len, decimal, s)
{
	var     scale = 1;

	if (decimal == null)
		decimal = 1;

	for (i = 0; i <= decimal; i++)
		scale *= 10;

	var     str = "" + Math.round(parseFloat(val) * scale);

	if (str.length == 0 || str == "0") {
		str = "000";
	}

	if (s == null) {
		str = str;
	} else {
		str = "$" + str;
	}

	i = len - str.length;
	if (scale != 1)
		i--;
	while (0 < i--) 
		str = " " + str;
	if (scale != 1) {
		var p = len - decimal - 2;
		var a = str.substring(0, p);
		var b = str.substring(p, len);
		return a + "." + b;
	}
	return str;
}

function compute(input)
{
	var form = input.form;

	// calculate #1
	shelter = valueOrDefault(form.shelter, 0);
	food = valueOrDefault(form.food, 0);
	clothing = valueOrDefault(form.clothing, 0);
	car = valueOrDefault(form.car, 0);
	other = valueOrDefault(form.other, 0);
	linc = valueOrDefault(form.linc, 0);

	annual = shelter + food + clothing + car + other;
	income = annual - (linc * -1);
	
	form.annual.value = format(annual, 15, 2, "$");
	form.income.value = format(income, 15, 2, "$");
	form.sectiona.value = format(income, 15, 2, "$");

	// calculate #2
	estate = valueOrDefault(form.estate, 0);
	debts = valueOrDefault(form.debts, 0);
	education = valueOrDefault(form.education, 0);

	expenses = estate + debts + education;

	form.expenses.value = format(expenses, 15, 2, "$");
	form.sectionb.value = format(expenses, 15, 2, "$");

	// calculate #3
	death = valueOrDefault(form.death, 0);
	investments = valueOrDefault(form.investments, 0);
	bank = valueOrDefault(form.bank, 0);
	otherassets = valueOrDefault(form.otherassets, 0);

	assets = death + investments + bank + otherassets;

	form.assets.value = format(assets, 15, 2, "$");
	form.sectionc.value = format(assets, 15, 2, "$");

	// calculate #4
	if (form.years[0].checked) {
		multfactor = 4.6;
	} else if (form.years[1].checked) {
		multfactor = 8.5;
	} else if (form.years[2].checked) {
		multfactor = 11.7;
	} else if (form.years[3].checked) {
		multfactor = 14.3;
	} else {
		multfactor = 16.5;
	}

	incomecapital = (income * multfactor);
	lifeneeds = incomecapital + expenses - assets;
	
	form.multfactor.value = format(multfactor, 15, 2);
	form.incomecapital.value = format(incomecapital, 15, 2, "$");
	form.lifeneeds.value = format(lifeneeds, 15, 2), "$";

	//
 
}

<!-- done hiding from old browsers -->
</SCRIPT>


