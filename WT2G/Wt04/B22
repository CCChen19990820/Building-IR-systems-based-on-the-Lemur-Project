<title>Send a Message to the Webmaster</title>
<script>
<!-- Begin to hide script contents from old browsers.
function moveover(txt) {
   window.status = txt;
   setTimeout("erase()",3000);
}
function erase() {
   window.status="";
}

function isFilled()
   {
   return ( (isEmail()) && (isMessage()) )
   }

// Checks the E-MAIL field.
function isEmail()
   {
   // Return false if e-mail field is blank.
   if (document.forms[0].elements[0].value == "") 
      {
      alert("\nThe E-MAIL field is blank.\n\nPlease enter your e-mail address.")
      document.forms[0].elements[0].focus();
      return false; 
      }

   if (document.forms[0].elements[0].value.indexOf ('\@',0) == -1 || 
       document.forms[0].elements[0].value.indexOf ('.',0) == -1)
      {
      alert("\nThe E-MAIL field requires a \"@\" and a \".\"be used.\n\nPlease re-enter your e-mail address.")
      document.forms[0].elements[0].select();
      document.forms[0].elements[0].focus();
      return false;
      }
   else
      {
      return true;
      }
   }
// Checks the Message field.

function isMessage()
   {
   // If message field is blank, asks for confirmation that no message is desired.
   if (document.forms[0].elements[3].value == "")
      {
      if (confirm("\nYou're about to submit without leaving a message.\n\nClick on CANCEL to write a message.\n\nClick on OK to continue without a message."))
         return true
      else
         {
         document.forms[0].elements[3].focus();
         return false;      
         }
      }
   else
      return true   
   }

// End the hiding here. -->
</script>

</HEAD>
<a name="top">
