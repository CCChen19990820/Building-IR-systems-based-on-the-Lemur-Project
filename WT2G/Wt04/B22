<html>
<head>
<script>
<!-- Begin to hide script contents from old browsers.
// The general checking
function isFilled()
   {
   return ( (isName()) && (isSex()) && (isCountry()) && (isEmail()) );
   }

// Checks the E-MAIL field.
function isEmail()
   {
   // Return false if e-mail field is blank and request for contacting is made.
   if ((document.theform.Email.value == "") && (document.theform.CanContactAgain[1].checked))
      {
      alert("\nThe E-MAIL field is blank.\n\nYou have indicated that we can contact you\nbut we will need your email address to do so.\nPlease enter your e-mail address.")
      document.theform.Email.focus();
      return false; 
      }

   if (document.theform.Email.value.indexOf ('@',0) == -1 || 
       document.theform.Email.value.indexOf ('.',0) == -1)
      {
      alert("\nThe E-MAIL field requires a \"@\" and a \".\"be used.\n\nPlease re-enter your e-mail address.")
      document.theform.Email.select();
      document.theform.Email.focus();
      return false;
      }
   else
      {
      return true;
      }
   }
// Checks the Name field.
function isName()
   {
   if (document.theform.Name.value == "")
      {
      alert("\nPlease enter your Name.\n\n")
      document.theform.Name.focus();
      return false; 
      }
   else
      {
      return true;
      }
   }

// Checks the Country field.
function isCountry()
   {
   if (document.theform.Country.value == "")
      {
      alert("\nPlease enter your Country.\n\n")
      document.theform.Country.focus();
      return false; 
      }
   else
      {
      return true;
      }
   }
// Checks the Sex field.
function isSex()
   {
   if (!((document.theform.Sex[0].checked) || (document.theform.Sex[1].checked)))
      {
      alert("\nPlease indicate your sex.\n\n")
      document.theform.Name.focus();
      return false; 
      }
   else
      {
      return true;
      }
   }

// Checks whether any list has been selected
function isListselected()
   {
   var num = document.forms[0].length;
   var hasvalue = false;
   // start with the second value, the first is the email address
   for (var i = 1; i < num; i++) 
      {
      hasvalue = hasvalue || (document.forms[0].elements[i].checked);
   }
   if (!hasvalue) 
       {
         alert ("You did not select any list to join.");
         return false;
       }
    else 
    { 
         return true;
    }
}

// End the hiding here. -->
</script>

