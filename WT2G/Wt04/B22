 <HEAD>
 
 <TITLE>Welcome to Indozone</TITLE>
 

 <BASE href="http://www.indozone.com/"><META Name="description" content="Indozone is an Indian website with  sections on business, travel, news, Hindi films, detailed weekly  and monthly financial summaries,corporate home pages and much more.">

<META Name="keywords" Content="India,India,India,India business, India business, India business, India business, India news, Indian movies, Indian movies, Indian movies, hindi films, hindi films, hindi films, Indian business, India travel, India travel, India travel, India travel, India travel, India travel, Indian financial analysis, Indian financial analysis, Indian financial analysis, Indian financial analysis, travel,  travel,  travel,  travel">   </HEAD> 


<SCRIPT LANGUAGE="JavaScript">

<!-- Beginning of JavaScript Applet -------------------

/* Copyright (C)1996 Web Integration Systems, Inc. DBA Websys, Inc.   All Rights Reserved.   This applet can be re-used or modified, if credit is given in    the source code.   Chris Skinner, January 30th, 1996.

*/

function scrollit_r2l(seed)

{        var m1  = "NEW BUSINESS PAGES: Trans Travel 97, Chandra Textiles, Merchant Trading Company....Weekly and Monthly Business updates.....";

         var m2  = "BOLLYWOOD: Salman Khan, Juhi Chawla Interviews...";

         var m3  = "Check out our travel section.... ";

         var m4  = "ALL ON INDOZONE";        var msg=m1+m2+m3+m4;

         var out = " ";        var c   = 1;

        if (seed > 100) {

                seed--;

                var cmd="scrollit_r2l(" + seed + ")";

                timerTwo=window.setTimeout(cmd,100);

        }

        else if (seed <= 100 && seed > 0) {

                for (c=0 ; c < seed ; c++) {

                        out+=" ";

                }

                out+=msg;

                seed--;

                var cmd="scrollit_r2l(" + seed + ")";

                    window.status=out;

                timerTwo=window.setTimeout(cmd,100);

        }

        else if (seed <= 0) {

                if (-seed < msg.length) {

                        out+=msg.substring(-seed,msg.length);

                        seed--;

                        var cmd="scrollit_r2l(" + seed + ")";

                        window.status=out;

                        timerTwo=window.setTimeout(cmd,100);

                }

                else {

                        window.status=" ";

                        timerTwo=window.setTimeout("scrollit_r2l(100)",75);

                }

        }}

// -- End of JavaScript code -------------- -->

</SCRIPT>

