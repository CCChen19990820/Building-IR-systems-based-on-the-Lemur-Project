<!-- Begin Java Applet

function ScrollMsg(interval)
{
   var txt  = "Welcome to Grand Casinos, Inc. . . . . . .  Be sure to visit all 8 property pages and don't miss our new Grand Store, our new on-line mall. . . . .    Have a Grand Day !                            Welcome to Grand Casinos, Inc. . . . . . .  Be sure to visit all 8 property pages and don't miss our new Grand Store, our new on-line mall. . . . .    Have a Grand Day !        "

   var i   = 1;
   var out = " ";
   var msg=txt;

   if (interval > 100) {
       interval--;
       var cmd="ScrollMsg(" + interval + ")";
       timerTwo=window.setTimeout(cmd,100);
   }
   else if (interval <= 100 && interval > 0) {
       for (i=0 ; i < interval ; i++) {
           out+=" ";
       }
       out+=msg;
       interval--;
       var cmd="ScrollMsg(" + interval + ")";
       window.status=out;
       timerTwo=window.setTimeout(cmd,100);
   }
   else
   if (interval <= 0) {
       if (-interval < msg.length) {
           out+=msg.substring(-interval,msg.length);
           interval--;
           var cmd="ScrollMsg(" + interval + ")";
           window.status=out;
           timerTwo=window.setTimeout(cmd,100);
       }
       else {
           window.status=" ";
           timerTwo=window.setTimeout("ScrollMsg(100)",75);
       }
   }
}
// -- End Java Applet -->

