	<TD COLSPAN=24 HEIGHT=26></TD>
   </TR>
   <TR VALIGN="top" ALIGN="left">
	<TD COLSPAN=54 HEIGHT=47></TD>
   </TR>
   <TR VALIGN="top" ALIGN="left">
	<TD COLSPAN=39 ROWSPAN=1 WIDTH=420 ALIGN="center" VALIGN="center"><IMG HEIGHT=35 WIDTH=420 SRC="./assets/images/ahead.GIF"  BORDER=0  ALT="manapol: ahead of the class" ></TD>
