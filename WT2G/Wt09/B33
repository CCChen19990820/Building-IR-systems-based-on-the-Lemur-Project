   <TR VALIGN="top" ALIGN="left">
	<TD COLSPAN=17></TD>
	<!-- These 18 columns occupied by an object -->
	<TD COLSPAN=19 HEIGHT=3></TD>
   </TR>
   <TR VALIGN="top" ALIGN="left">
	<TD COLSPAN=7></TD>
	<TD COLSPAN=6 ROWSPAN=1 WIDTH=87 ALIGN="center" VALIGN="center"><IMG HEIGHT=56 WIDTH=87 SRC="./assets/auto_generated_images/low_bandwidth/img_17019363.gif"  BORDER=0  ALT="the" ></TD>
	<TD COLSPAN=4></TD>
	<!-- These 18 columns occupied by an object -->
	<TD COLSPAN=19 HEIGHT=56></TD>
