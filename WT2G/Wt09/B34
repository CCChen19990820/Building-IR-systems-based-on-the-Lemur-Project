	<TD COLSPAN=1 WIDTH=7><IMG SRC="../assets/images/dot_clear.gif" WIDTH=7 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=2><IMG SRC="../assets/images/dot_clear.gif" WIDTH=2 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=4><IMG SRC="../assets/images/dot_clear.gif" WIDTH=4 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=12><IMG SRC="../assets/images/dot_clear.gif" WIDTH=12 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=207><IMG SRC="../assets/images/dot_clear.gif" WIDTH=207 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=48><IMG SRC="../assets/images/dot_clear.gif" WIDTH=48 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=30><IMG SRC="../assets/images/dot_clear.gif" WIDTH=30 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=6><IMG SRC="../assets/images/dot_clear.gif" WIDTH=6 HEIGHT=1 BORDER=0></TD>
	<TD COLSPAN=1 WIDTH=252><IMG SRC="../assets/images/dot_clear.gif" WIDTH=252 HEIGHT=1 BORDER=0></TD>
