<!-- sent="Thu, 19 Dec 1996 07:08:28 LCL" -->
<!-- name="Cevdet Seyhan" -->
<!-- email="Cevdet_Seyhan@CENA.VOA.GOV" -->
<!-- subject="Uzbekistan" -->
