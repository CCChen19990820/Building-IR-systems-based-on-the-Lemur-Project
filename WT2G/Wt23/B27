<SCRIPT LANGUAGE="JavaScript">
                                     
function scrollit_r2l(seed)
{                                           
var m1 = "     Morpeth Dolphins SAC";                
var m2 = "     BSAC 0997"; 
var m3 = "     Meets Thursdays, Morpeth Riverside Leisure Centre,";
var m4 = "     Morpeth, Northumberland";
var m5 = "     8:00pm";
var msg = m1+m2+m3+m4+m5;
var out = " ";                                                           
var c = 1;                                                           
                                                                     
if (seed > 100)                                                      
{                                                                    
seed--;                                                              
var cmd="scrollit_r2l(" + seed + ")";                                
timerTwo=window.setTimeout(cmd,100);                                 
}                                                

else if (seed <= 100 && seed > 0)
{         
for (c=0 ; c < seed ; c++)
{              
out+=" ";
}      
                                     
out+=msg;                           
seed--;

var cmd="scrollit_r2l(" + seed + ")";
          
window.status=out;         
               
timerTwo=window.setTimeout(cmd,100);
}                                                                               
                                     
else if (seed <= 0)                                                  
{      
if (-seed < msg.length)
{                                    
out+=msg.substring(-seed,msg.length);
                          
seed--;      
                                    
var cmd="scrollit_r2l(" + seed + ")";
                                     
window.status=out;                  

window.status=out;                                                   
                                            
timerTwo=window.setTimeout(cmd,100);
                                                                     
}                                    
else                                          
{                                
window.status=" ";                  
                                     
timerTwo=window.setTimeout("scrollit_r2l(100)",75);
}                                   
}                                           
}                                   
//---end of JavaScript code ---------------------------------
                           


</SCRIPT> 
