                <td valign="top" width="34%">PHARMACIST ONLY
                MEDICINE</td>
            </tr>
            <tr>
                <td valign="top" width="15%">S4</td>
                <td valign="top" width="30%">CAUTION S4</td>
                <td valign="top" width="34%">PRESCRIPTION ONLY
                MEDICINE</td>
            </tr>
            <tr>
                <td valign="top" width="15%">S8</td>
                <td valign="top" width="30%">CAUTION S8</td>
                <td valign="top" width="34%">CONTROLLED DRUG</td>
            </tr>
