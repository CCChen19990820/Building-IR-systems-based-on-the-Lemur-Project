<SCRIPT LANGUAGE="JavaScript">
<!------------------------------------------------------------------------
/* Copyright (C)1996 Q&D Software Development
   All Rights Reserved.

*/
function scroll_status (seed)
{
        var msg = "As I found all the answers to Life; someone changed the Questions! ";
        var out = " ";
        var c   = 1;

        if (150 < seed) {
                seed--;
                var cmd="scroll_status(" + seed + ")";
                timerTwo=window.setTimeout(cmd,25);
        }
        else if (seed <= 150 && 0 < seed) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="scroll_status(" + seed + ")";
                window.status=out;
                timerTwo=window.setTimeout(cmd,25);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="scroll_status(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,25);
                }
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("scroll_status(120)",25);
                }
        }
}
// -- End of JavaScript code -------------- -->

</SCRIPT>

</head>
