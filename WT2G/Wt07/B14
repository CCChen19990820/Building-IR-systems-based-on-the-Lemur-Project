<HTML>
<HEAD>
<TITLE> E-Mail Form </TITLE>
</HEAD>
<SCRIPT LANGUAGE="JavaScript">

<!-- Beginning of JavaScript Applet -------------------
/* Copyright 1996 Web Integration Systems, Inc. DBA Websys, Inc. All Rights Reserved. This applet can be re-used or modified, if credit is given in the source code. We will not be held responsible for any unwanted effects due to the usage of this applet or any derivative. No warrantees for usability for any specific application are given or implied. Chris Skinner, January 30, 1996.*/
function scrollit_r2l(seed)
{
  var msg  = "Reuters    UPI    Knight Ridder    Dow Jones    PRNewswire    San Jose Mercury News    USA Today    API   Newhouse    Agence France Press    BPI Entertainment Wire    Asia Info News    Computergram    Nikkei News Service    American Bond/Banker    Scripps Howard News Service    Bloomberg Business News    Toronto Globe & Mail    Intellex    Tacoma News Tribune    Federal Register    Internet News    Chicago Tribune    Fort Lauderdale Sun-Sentinel    Orlando Sentinel    Philadelphia Enquirer    Philadelphia Daily News    Hartford Courant    Troy NY Record    Tacoma News Tribune    Roanoke Times    Evansville Courier    Abilene Reporter-News    Bakersfield Californian    Detroit News    St. Paul Pioneer Press    White House News    Arkansas Democrat-Gazette    Minneapolis Star Tribune    US Newswire    Charlotte Observer    Kansas City Star    National Review    Desert News    Chattanooga Free Press    Business Wire    Indianapolis Star-News    Fort Worth Star Telegram    The Baton Rouge Advocate    Amarillo Globe-News    Benton County Daily Record    Forbes Magazine";
  var out = " ";
  var c = 1;

  if (seed > 100)
  {
    seed--;
    var cmd="scrollit_r2l(" + seed + ")";
    timerTwo=window.setTimeout(cmd,50);
  }
  else if (seed <= 100 && seed > 0)
  {
    for (c=0 ; c < seed ; c++)
    {
      out+=" ";
    }
    out+=msg;
    seed--;
    var cmd="scrollit_r2l(" + seed + ")";
    window.status=out;
    timerTwo=window.setTimeout(cmd,50);
  }
  else if (seed <= 0)
  {
    if (-seed < msg.length)
    {
      out+=msg.substring(-seed,msg.length);
      seed--;
      var cmd="scrollit_r2l(" + seed + ")";
      window.status=out;
      timerTwo=window.setTimeout(cmd,50);
    }
    else
    {
      window.status=" ";
      timerTwo=window.setTimeout("scrollit_r2l(100)",45);
    }
  }
}
// -- End of JavaScript code -------------- -->
</SCRIPT>
<BODY onLoad="timerONE=window.setTimeout('scrollit_r2l(100)',500);" Background="fuzzy.gif">

