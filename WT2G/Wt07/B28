<script language="JavaScript"><!--
 day="";
 month="";
 nday=0;
 nyear=0;

 today=new Date();

 nday=today.getDay()+1;
 nmonth=today.getMonth()+1;
 nyear=1900+today.getYear();

 if (nmonth==1){month="January"};
 if (nmonth==2){month="February"};
 if (nmonth==3){month="March"};
 if (nmonth==4){month="April"};
 if (nmonth==5){month="May"};
 if (nmonth==6){month="June"};
 if (nmonth==7){month="July"};
 if (nmonth==8){month="August"};
 if (nmonth==9){month="September"};
 if (nmonth==10){month="October"};
 if (nmonth==11){month="November"};
 if (nmonth==12){month="December"};
 
 if (nday==1){day="Sunday"};
 if (nday==2){day="Monday"};
 if (nday==3){day="Tuesday"};
 if (nday==4){day="Wednesday"};
 if (nday==5){day="Thursday"};
 if (nday==6){day="Friday"};
 if (nday==7){day="Saturday"};
 document.write ("<font size=2>")
 document.write (day, " ", month ," ", today.getDate(), ", ", nyear);
 document.write ("</font>")
 // --></script>

