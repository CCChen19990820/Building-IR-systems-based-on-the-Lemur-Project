    <AREA shape="rect" coords="163,82 214,99" href="/funds/ABOUT/locations.html#eu"> 
    <AREA shape="rect" coords="344,137 376,148" href="/funds/ABOUT/locations.html#as">
    <AREA shape="rect" coords="295,175 358,189" href="/funds/ABOUT/locations.html#au">
