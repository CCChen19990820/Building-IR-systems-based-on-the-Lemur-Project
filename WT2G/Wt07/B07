<!-- GSWEB_INDEX_KEY_MARKER research -->

<HEAD>

<TITLE>Goldman Sachs: 1995 Annual Review: Research</TITLE>

</HEAD>

<MAP NAME="aboutmap">
  <AREA COORDS="1,1,158,37" HREF="/">
  <AREA COORDS="159,1,283,37" HREF="/utils/search.html">
