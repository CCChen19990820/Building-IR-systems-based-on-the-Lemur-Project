<head>
<title>The JavaScript Resistance Calculator</title>
<meta name="Author" content="Jarid B. Lukin">

<noscript>
You need a JavaScript capable browser to use this calculator.
Netscape Navigator version 3.0 works well.
</noscript>

<script language=JavaScript>
<!-- 
/*--==================================================--
  	Author: Jarid B. Lukin, University of Pennsylvania
  	lukin@eniac.seas.upenn.edu     
  	Copyright 1996, SuLu, Inc. All Rights Reserved
  	Reproduction of this code in anyway is not allowed 
  	without the written permission of the author.
  --=====================================================--*/

function showbox(target){
(confirm('Since you are using Microsoft Internet Explorer, you should use the ActiveX version of this page.') ? location.href=target : "") }

var browser=navigator.appName; 

function loadPage()
   {
   if (browser.substring(0, 9)=="Microsoft")
      showbox('calcvb.html');
   }


function converta(obj, stripe1color) {
obj.stripe1.value=stripe1color;
if (stripe1color=='Black') {obj.col1result.value = "0"}
else{
if (stripe1color=='Brown') {obj.col1result.value = "1"}
else{
if (stripe1color=='Red') {obj.col1result.value = "2"} 
else{
if (stripe1color=='Orange') {obj.col1result.value = "3"}
else{
if (stripe1color=='Yellow') {obj.col1result.value = "4"}
else{
if (stripe1color=='Green') {obj.col1result.value = "5"}
else{
if (stripe1color=='Blue') {obj.col1result.value = "6"}
else{
if (stripe1color=='Violet') {obj.col1result.value = "7"}
else{
if (stripe1color=='Gray') {obj.col1result.value = "8"}
else {obj.col1result.value = "9"}
}}}}}}}}
Calc(obj);
}

function convertb(obj, stripe2color) {
obj.stripe2.value=stripe2color;
if (stripe2color=='Black') {obj.col2result.value = "0"}
else{
if (stripe2color=='Brown') {obj.col2result.value = "1"}
else{
if (stripe2color=='Red') {obj.col2result.value = "2"} 
else{
if (stripe2color=='Orange') {obj.col2result.value = "3"}
else{
if (stripe2color=='Yellow') {obj.col2result.value = "4"}
else{
if (stripe2color=='Green') {obj.col2result.value = "5"}
else{
if (stripe2color=='Blue') {obj.col2result.value = "6"}
else{
if (stripe2color=='Violet') {obj.col2result.value = "7"}
else{
if (stripe2color=='Gray') {obj.col2result.value = "8"}
else {obj.col2result.value = "9"}
}}}}}}}}
Calc(obj);
}

function convertc(obj, stripe3color) {
obj.stripe3.value=stripe3color;
if (stripe3color=='Black') {obj.col3result.value = "0"}
else{
if (stripe3color=='Brown') {obj.col3result.value = "1"}
else{
if (stripe3color=='Red') {obj.col3result.value = "2"} 
else{
if (stripe3color=='Orange') {obj.col3result.value = "3"}
else{
if (stripe3color=='Yellow') {obj.col3result.value = "4"}
else{
if (stripe3color=='Green') {obj.col3result.value = "5"}
else{
if (stripe3color=='Blue') {obj.col3result.value = "6"}
else{
if (stripe3color=='Violet') {obj.col3result.value = "7"}
else{
if (stripe3color=='Gray') {obj.col3result.value = "8"}
else {obj.col3result.value = "9"}
}}}}}}}}
Calc(obj);
}

function convertd(obj, tolcolor) {
obj.tolerance.value=tolcolor;
if (tolcolor=='Silver') {obj.tolresult.value = "10"}
else{
if (tolcolor=='Gold') {obj.tolresult.value = "5"}
else{obj.tolresult.value = "20"}
}
Calc(obj);
}

function Calc(obj) {
   Power=Math.pow(10,obj.col3result.value);
   Answer=eval(obj.col1result.value+obj.col2result.value)*Power;
   Error=Answer * obj.tolresult.value/100;
   if (Answer>=1000000) {
	 Answer=Answer/1000000;
       Error=Error/1000000;
       Units=" MOhms";}
   else {
   if (Answer>=1000) {
	 Answer=Answer/1000;
	 Error=Error/1000;
	 Units=" kOhms";}
   else{Units=" Ohms";}
   }
obj.result.value=Answer + " +/- " + Error + Units;
}

// -->
</script>

<SCRIPT LANGUAGE="JavaScript1.1">
