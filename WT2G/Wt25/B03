            document.write('<a href="/' + rstr + '/index.html" target="_top"><img src="/cookies/' + rstr + 'link.gif" vspace=0 hspace=0 border=0></a>')
        }
    } else {
        document.write('<a href="/npga/index.html" target="_top"><img src="/cookies/link.gif" border=0></a>');
    }
}

// -->
</script>

</HEAD>

<BODY BACKGROUND="/images/bg.gif" BGCOLOR="#000000" TEXT="#FFFFFF" LINK="#77ccff" ALINK="#FFFF00" VLINK="#77ccff">

