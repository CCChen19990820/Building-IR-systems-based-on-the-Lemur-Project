  <!-- Beginning of JavaScript Applet -------------------
  function scroll_statusbar(seed)
  {
      var msg1  = "AVN Business Systems.";
      var msg2  = "    On the leading edge of computer technology.";                             
      var msg3  = "      Call us at 905-564-6440. "; 
      var msg=msg1+msg2+msg3;
      var out = " ";
      var c   = 1;

     if (seed > 100) {
          seed--;
          var cmd="scroll_statusbar(" + seed + ")";
          timerTwo=window.setTimeout(cmd,100);
      }
      else if (seed <= 100 && seed > 0) {
          for (c=0 ; c < seed ; c++) {
              out+=" ";
          }
          out+=msg;
          seed--;
          var cmd="scroll_statusbar(" + seed + ")";
          window.status=out;
          timerTwo=window.setTimeout(cmd,100);
      }
      else if (seed <= 0) {
          if (-seed < msg.length) {
              out+=msg.substring(-seed,msg.length);
              seed--;
              var cmd="scroll_statusbar(" + seed + ")";
              window.status=out;
              timerTwo=window.setTimeout(cmd,100);
          }
          else {
              window.status=" ";
              timerTwo=window.setTimeout("scroll_statusbar(100)",75);
          }
      }
  }
  // -- End of JavaScript code -------------- -->

  

</SCRIPT>



