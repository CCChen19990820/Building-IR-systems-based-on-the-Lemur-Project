<DOCNO>WT25-B15-228</DOCNO>
<DOCOLDNO>IA062-000356-B016-22</DOCOLDNO>
<DOCHDR>
http://www.avn-net.com:80/USERS/FEDERICO/hacking/crash_script.html 205.210.61.130 19970115223200 text/html 3771
HTTP/1.0 200 OK
Server: Netscape-Communications/1.12
Date: Wednesday, 15-Jan-97 22:37:45 GMT
Last-modified: Monday, 19-Aug-96 04:52:19 GMT
Content-length: 3579
Content-type: text/html
</DOCHDR>
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 3.2//EN">
<HTML>
<HEAD>
   <TITLE></TITLE>
   <META NAME="GENERATOR" CONTENT="Mozilla/3.0b6Gold (Win95; I) [Netscape]">
</HEAD>
<BODY TEXT="#5F01FF" BGCOLOR="#000000" BACKGROUND="blkback1.jpg">

<P><BR>
<BR>
<SCRIPT><!-- This script created by Gal Levinshtein NBC -->

<!-- Hide from old browsers

// Navigation - Stop

// Netscapes Clock - Start

// This code was written by Gal Levinshtein at NBC
// Modified by Holden Robbins


document.write("<center><form  name='Counter' onSubmit='0'>")



var timerID = null;

var timerRunning = false;



StartDay        = new Date("Jannuary 01, 1997 24:60:00");

var f_year      = StartDay.getYear();

var f_month     = StartDay.getMonth();

var f_day       = StartDay.getDate();

var f_hours     = StartDay.getHours();

var f_minutes   = StartDay.getMinutes();

var f_seconds   = StartDay.getSeconds();

MonthsDays = new monthsdays(31,28,31,30,31,30,31,31,30,31,30,31);

startclock()

function monthsdays(jan, feb, mar, apr, may, jun, jul, aug, sep, oct, nov, dec) {
        this.jan = jan;
        this.feb = feb;
        this.mar = mar;
        this.apr = apr;
        this.may = may;
        this.jun = jun;
        this.jul = jul;
        this.aug = aug;
        this.sep = sep;
        this.oct = oct;
        this.nov = nov;
        this.dec = dec;
}



function stopclock (){

        if(timerRunning)

                clearTimeout(timerID);

        timerRunning = false;

}



function gettheDate() {

        Todays = new Date();

        TheDate = "" + (Todays.getMonth()+ 1) +"/"+ Todays.getDate() + "/" +

        Todays.getYear()

//        document.clock.date.value = TheDate;

}



function showtime () {

        showCounter();
        timerID = setTimeout("showtime()",1000);
        timerRunning = true;
}



function showCounter() {

        var now         = new Date();
        var year        = now.getYear();
        var month       = now.getMonth();
        var day         = now.getDate();
        var hours       = now.getHours();
        var minutes     = now.getMinutes();
        var seconds     = now.getSeconds();


        l_year = f_year - year;
        l_month = f_month - month;
        l_day = f_day - day;
        l_hour = f_hours - hours;
        l_minute = f_minutes - minutes;
        l_second = f_seconds - seconds;

        if (l_second < 0) {
              l_second = (l_second + 60);
              l_minute = l_minute - 1;
        }

        if (l_minute < 0) {
              l_minute = (l_minute + 60);
              l_hour = l_hour - 1;
        }

        if (l_hour < 0) {
              l_hour = (l_hour + 24);
              l_day = l_day - 1;
        }

        if (l_day < 0) {
              // if todays date is greater than the 19th
              l_day = MonthsDays[month] - day;  // days left till end of month
              l_day = l_day + f_day;
              month = month + 1;
        }

        for (i=month; i<f_month; i++)
                l_day = l_day + MonthsDays[i];

        document.Counter.days_left.value = l_day;
        document.Counter.hours_left.value = l_hour;
        document.Counter.minutes_left.value = l_minute;
        document.Counter.seconds_left.value = l_second;
}



function startclock () {

        // Make sure the clock is stopped

        stopclock();
        gettheDate();
        showtime();
}

// end --></SCRIPT>

</P>

</BODY>
</HTML>
