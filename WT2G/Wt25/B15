<HTML>
<HEAD>
   <TITLE>LIBERCAVE C. T. The Canada-Venezuela Network</TITLE>
   <META NAME="GENERATOR" CONTENT="Mozilla/3.0Gold (Win95; I) [Netscape]">
<SCRIPT>
<!-- Begin code

/* Copyright (C)1996 LIBERCAVE C. T. Net
   All Rights Reserved.

   This applet can be re-used or modified, if credit is given in the source code.

*/



function scrollit_r2l(seed)

{

         var m1  = "Welcome to LIBERCAVE C. T. ...";
 
        var m2  = ".....Thanks for stopping by the LIBERCAVE C. T. Network.........";
 
        var m3  = "Bienvenido a LIBERCAVE C. T..........";
 
        var m4  = "Gracias por detenerse en el LIBERCAVE C. T. Network............";      

        var msg=m1+m2+m3+m4;

        var out = " ";

        var c   = 1;



        if (seed > 100) {

                seed--;

                var cmd="scrollit_r2l(" + seed + ")";

                timerTwo=window.setTimeout(cmd,100);

        }

        else if (seed <= 100 && seed > 0) {

                for (c=0 ; c < seed ; c++) {

                        out+=" ";

                }

                out+=msg;

                seed--;

                var cmd="scrollit_r2l(" + seed + ")";

                    window.status=out;

                timerTwo=window.setTimeout(cmd,100);

        }

        else if (seed <= 0) {

                if (-seed < msg.length) {

                        out+=msg.substring(-seed,msg.length);

                        seed--;

                        var cmd="scrollit_r2l(" + seed + ")";

                        window.status=out;

                        timerTwo=window.setTimeout(cmd,100);

                }

                else {

                        window.status=" ";

                        timerTwo=window.setTimeout("scrollit_r2l(100)",75);

                }

        }

}

// -- End code -->



</SCRIPT>
</HEAD>
