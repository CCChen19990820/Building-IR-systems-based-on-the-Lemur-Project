function openpowerstart() {
	location = "http://personal.netscape.com/custom/page/show_page.html";
}

function opentip() {
	str="/comprod/news/todaystip.html";
	tipWin = window.open(str,'tipWin','width=175,height=150');
	window.open(str,'tipWin','widt=175,height=175');
	tipWin.opener = self;
}
function switch_page() {
	if (document.menuform.elements[1].selectedIndex == 0) location = 'http://home.br.netscape.com/pt';
	else if (document.menuform.elements[1].selectedIndex == 1) location = 'http://home.dk.netscape.com/da';
	else if (document.menuform.elements[1].selectedIndex == 2) location = 'http://home.nl.netscape.com/nl';
	else if (document.menuform.elements[1].selectedIndex == 3) location = 'http://home.fr.netscape.com/fr';
	else if (document.menuform.elements[1].selectedIndex == 4) location = 'http://home.de.netscape.com/de';
	else if (document.menuform.elements[1].selectedIndex == 5) location = 'http://home.it.netscape.com/it';
	else if (document.menuform.elements[1].selectedIndex == 6) location = 'http://home.jp.netscape.com/ja';
	else if (document.menuform.elements[1].selectedIndex == 7) location = 'http://home.kr.netscape.com/ko';
	else if (document.menuform.elements[1].selectedIndex == 8) location = 'http://home.es.netscape.com/es';
	else if (document.menuform.elements[1].selectedIndex == 9) location = 'http://home.se.netscape.com/sv';
}

function pickandclick(what) {
	var selection = document.menuform.menu.selectedIndex;
	var newloc = null;
	if (what == 'try') {
		if (selection == 1 || selection == 2 || selection == 4 || selection == 7 || selection == 9 || selection == 11 || selection == 12 || selection == 13)
			newloc = '/comprod/mirror/client_download.html';
		else if (selection > 13 && selection < 27 ) newloc = '/comprod/mirror/server_download.html';
