<HEAD><TITLE>The World-Wide Web Virtual Library: Forestry: Soils and Substrates</TITLE>
<!-- soil, substrate, geology, growth medium, physics, chemistry, forest, forestry, horticulture, agriculture, plant production -->
<!-- aeration, arable land, air permeability, container seedling, density, diffusivity, hydraulic conductivity, irrigation, organic matter, oxygen diffusion, availability, particle size, peat, pore size, temperature, tree nursery, waterlogging -->
<!-- 156 links 3.02.1997 -->

<SCRIPT LANGUAGE="JavaScript">

      <!-- This is a small scroller script - taken from Yehuda and      -->
      <!-- Tomer Shiran's pages *thanks guys*. Richard Dows             -->
      <!--                                                              -->
      <!-- Permission to copy and use this is granted - just put a link -->
      <!-- back to tequila.randomc.com ;)                               -->
      <!--                         Richard Dows                         -->

      <!-- Hide from old browsers
      function stat(txt) {
        window.status = txt;
        setTimeout("erase()", 5000);
      }

      function erase () {
        window.status = "";
      }

      var i = 0;
      var j = 0;
      var TextNumber = 0;
     var TextInput = new Object();
      var HelpText = "";
      var Text = "";
      var Speed = 200
      var WaitSpace = "                 "
      TextInput[0] = "  Welcome to the 'Soils and Substrates' WWW Site!"
      TextInput[1] = "  Feel Free to Give Feedback!"
      TextInput[2] = "  See you again!"
      TextInput[3] = ""
      TotalTextInput = 3;

      for(var j = 0; j < TotalTextInput; j++) {
        TextInput[j] += WaitSpace
      }
 
      var TimerId
      var TimerSet = false;
  
      function startBanner() {
        TimerSet = !TimerSet
        if(TimerSet)
          banner();
        else
          kill();
      
  
      function banner() {
        Text = rollMessage();
        TimerId = setTimeout("banner()", Speed)
        if(j == 0) {
          document.TextWin.TextArea.value = Text;
        } else {
          window.status = Text;
        }
      }

      function rollMessage() {
        Wait_yn = false;
        i++;
        var CheckSpace = HelpText.substring(i-1, i);
        CheckSpace = "" + CheckSpace;
        if(CheckSpace == " ") { i++; }
        if(i >= HelpText.length+1) {
          i = 0; 
          if (TextNumber < TotalTextInput) {
            TextNumber++;
          } else {
            TextNumber = 0;
          }
          init();
        }
        Text = HelpText.substring(0, i);
        return (Text);
      }

      function init() {
        Text = TextInput[TxtNumber]
        HelpText = Text;
      }

      function kill() {
        clearTimeout(TimerId)
      }

      function changeOutput(txt) {
        if(txt == 'textbox') {
          j = 0;
        } else {
          document.TextWin.TextArea.value = "            Nothing here";
          j = 1;
        }
      }
      // -->
    

</SCRIPT>
    
  </HEAD>


