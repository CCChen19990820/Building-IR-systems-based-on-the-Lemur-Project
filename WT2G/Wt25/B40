    BC_SeparationAssembly,
    BC_SeparationFeature))
  SUBTYPE OF (BC_FunctionObject);
  separates           : OPTIONAL SET [1:?] OF BC_FunctionObject;
  attaches            : OPTIONAL SET [1:?] OF BC_FunctionObject;
  locates             : OPTIONAL SET [1:?] OF BC_FunctionObject;
  is_penetrated_by    : OPTIONAL SET [1:?] OF BC_FunctionObject;
  insulation_factor   : OPTIONAL SET [1:?] OF InsulationFactor;
INVERSE
  is_connected_by     : SeparationConnection FOR connects;
END_ENTITY;

