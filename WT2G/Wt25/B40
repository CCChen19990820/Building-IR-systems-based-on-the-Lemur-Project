  SUBTYPE OF (BC_ElectricityDistributionPart);
END_ENTITY;

ENTITY Transformer
  SUBTYPE OF (BC_ElectricityDistributionPart);
END_ENTITY;

ENTITY InsulationPipe
  SUBTYPE OF (BC_ElectricityDistributionPart);
END_ENTITY;

ENTITY LightSwitch
  SUBTYPE OF (BC_ElectricityDistributionPart);
END_ENTITY;

ENTITY BC_ElectricityDistributionObject
  SUPERTYPE OF (ONEOF(
    BC_ElectricityDistributionPart,
    BC_ElectricityDistributionAssembly))
  SUBTYPE OF (BC_DistributionObject);
  voltage             : OPTIONAL INTEGER;
END_ENTITY;

ENTITY BC_ElectricityDistributionAssembly
  SUBTYPE OF (BC_ElectricityDistributionObject);
  consists_of         : OPTIONAL SET [1:?] OF BC_ElectricityDistributionPart;
  consists_of         : OPTIONAL SET [1:?] OF BC_ElectricityDistributionAssembly;
END_ENTITY;

ENTITY BC_HVAC_Object
  SUPERTYPE OF (ONEOF(
    BC_HVAC_Part,
    BC_HVAC_Assembly))
  SUBTYPE OF (BC_DistributionObject);
END_ENTITY;

ENTITY Radiator
  SUBTYPE OF (BC_HVAC_Part);
END_ENTITY;

ENTITY Boiler
  SUBTYPE OF (BC_HVAC_Part);
END_ENTITY;

ENTITY BC_HVAC_Part
  SUPERTYPE OF (ONEOF(
    Radiator,
    Boiler,
    HeatExchanger))
  SUBTYPE OF (BC_HVAC_Object);
END_ENTITY;

ENTITY BC_HVAC_Assembly
  SUBTYPE OF (BC_HVAC_Object);
  consists_of         : OPTIONAL SET [1:?] OF BC_HVAC_Part;
  consists_of         : OPTIONAL SET [1:?] OF BC_HVAC_Assembly;
END_ENTITY;

ENTITY HeatExchanger
  SUBTYPE OF (BC_HVAC_Part);
END_ENTITY;

ENTITY BC_SignalDistributionObject
  SUPERTYPE OF (ONEOF(
    BC_TV_SignalDistributionObject,
    BC_DetectionSignalDistributionObject))
  SUBTYPE OF (BC_DistributionObject);
END_ENTITY;

ENTITY BC_TV_SignalDistributionObject
  SUBTYPE OF (BC_SignalDistributionObject);
END_ENTITY;

ENTITY BC_DetectionSignalDistributionObject
  SUBTYPE OF (BC_SignalDistributionObject);
END_ENTITY;

ENTITY BC_WaterDistributionObject
  SUPERTYPE OF (ONEOF(
    BC_TapWaterDistributionObject,
    BC_WasteWaterDistributionObject))
  SUBTYPE OF (BC_DistributionObject);
END_ENTITY;

ENTITY BC_TapWaterDistributionObject
  SUBTYPE OF (BC_WaterDistributionObject);
END_ENTITY;

ENTITY BC_WasteWaterDistributionObject
  SUBTYPE OF (BC_WaterDistributionObject);
END_ENTITY;

ENTITY BC_GasDistributionObject
  SUPERTYPE OF (ONEOF(
    BC_GasDistributionPart,
    BC_GasDistributionAssembly))
  SUBTYPE OF (BC_DistributionObject);
END_ENTITY;

ENTITY LPG_Tanlk
  SUBTYPE OF (BC_GasDistributionPart);
END_ENTITY;

ENTITY BC_GasDistributionPart
  SUPERTYPE OF (ONEOF(
    LPG_Tanlk))
  SUBTYPE OF (BC_GasDistributionObject);
END_ENTITY;

ENTITY BC_GasDistributionAssembly
  SUBTYPE OF (BC_GasDistributionObject);
  consists_of         : OPTIONAL SET [1:?] OF BC_GasDistributionAssembly;
  consists_of         : OPTIONAL SET [1:?] OF BC_GasDistributionPart;
END_ENTITY;

END_SCHEMA;

</DOC>
