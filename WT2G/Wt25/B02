<script language="javascript">
<!--
function setOrigin() {
    if(document.cookie) {
        index = document.cookie.indexOf("ORIGIN");
        if (index == -1) {
            document.writeln("<META HTTP-EQUIV=\"Set-Cookie\" CONTENT=\"ORIGIN=bspropane; path=/\">");
