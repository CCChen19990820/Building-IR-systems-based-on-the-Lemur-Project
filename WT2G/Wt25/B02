function writeOrigin() {
    if(document.cookie) {
        b = document.cookie.indexOf("ORIGIN");
        if (b == -1) {
            document.write('<a href="/npga/index.html" target="_top"><img src="/cookies/link.gif" border=0></a>');
        } else {
            originString = unescape(document.cookie.substring(b,document.cookie.length))
            e = originString.indexOf(";");
