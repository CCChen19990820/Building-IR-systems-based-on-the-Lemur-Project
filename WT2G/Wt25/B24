<SCRIPT LANGUAGE="JavaScript">

/* Fill in the "var msg=" variables to change your message
 IMPORTANT, do not remove this comment line.  It is required for correct
 parsing of the scroller */

<!-- Beginning of JavaScript Applet

function scroll_status(seed)
{
        var msg1  = "NEW!!! Take a look at Memo Mail Merge on the programming tools page ";
        var msg2  = "   Now YOU can easily integrate mailmerge capabilities into your Clarion fo Windows application";
        var msg=msg1+msg2;
        var out = " ";
        var c   = 1;
        if (seed > 150) {
                seed--;
                var cmd="scroll_status(" + seed + ")";
                timerTwo=window.setTimeout(cmd,25);
}
        else if (seed <= 150 && seed > 0) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
}
                out+=msg;
                seed--;
                var cmd="scroll_status(" + seed + ")";
                    window.status=out;
                timerTwo=window.setTimeout(cmd,25);
}
       else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="scroll_status(" + seed + ")";
                        window.status=out;
                       timerTwo=window.setTimeout(cmd,25);
}
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("scroll_status(150)",25);
}
}
}
// -- End of JavaScript code -------------- -->
</SCRIPT>
