<HTML>
<HEAD><TITLE>Crescendo Network</TITLE>

<SCRIPT LANGUAGE="JavaScript">


<!-- To hide JavaScript from old browsers
var msg="                                                                              "
msg +="Hi!  This is Crescendo Network Web Site.  We are specialized on the system integration based on PC, "
msg +="Japaniation of the system, Network, Database and data exchange with your Japanese head office, and Web site generation along with general Internet related business...             "
var delay=180
var timerId
var i=0

function scrollMsg(){
  if (i <= 150){
    i++
    document.forms[0].banner.value=msg
    msg=msg.substring(2,msg.length)+msg.substring (0,2)
    timerId=setTimeout("scrollMsg()",delay)
  } else {
    clearTimeout(timerId)
    document.forms[0].banner.value="Ask us any questions..    Click reload button for viewing the message again..."
  }
}
// end of JavaScript-->


</SCRIPT>



