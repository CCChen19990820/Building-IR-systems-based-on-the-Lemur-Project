<select name="minscore"><option value=".1">loose match<option value=".3">fair match <option value=".5">good match <option value=".7">close match <option value=".9">strong match </select>
<select name="maxhits"><option value="10">10 results per page <option value="20">20 results per page <option value="30">30 results per page <option value="40">40 results per page </select>   
