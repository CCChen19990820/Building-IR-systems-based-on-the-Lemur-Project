&quot;&Iuml;&eth;&egrave;&eth;&icirc;&auml;&agrave;&quot; &acirc;
&Egrave;&ETH;&Yacute; &ETH;&Agrave;&Iacute; &icirc;&iuml;&egrave;&ntilde;&agrave;&iacute;
&acirc; <A HREF="../priroda/app1.htm">&Iuml;&eth;&egrave;&euml;&icirc;&aelig;&aring;&iacute;&egrave;&egrave; 6d</A>.

<P>
8. &Aacute;&icirc;&euml;&uuml;&oslash;&icirc;&aring; &acirc;&iacute;&egrave;&igrave;&agrave;&iacute;&egrave;&aring;
&aacute;&ucirc;&euml;&icirc; &oacute; &auml;&aring;&euml;&aring;&iacute;&icirc;
&iuml;&icirc;&auml;&ntilde;&iuml;&oacute;&ograve;&iacute;&egrave;&ecirc;&icirc;&acirc;&icirc;&igrave;&oacute;
&icirc;&aacute;&aring;&ntilde;&iuml;&aring;&#247;&aring;&iacute;&egrave;&thorn;
&iacute;&agrave;&oacute;&#247;&iacute;&icirc;&eacute; &iuml;&eth;&icirc;&atilde;&eth;&agrave;&igrave;&igrave;&ucirc;
&quot;&Iuml;&eth;&egrave;&eth;&icirc;&auml;&agrave;&quot;. &Ntilde;&icirc;&acirc;&aring;&ugrave;&agrave;&iacute;&egrave;&aring;
&icirc;&ograve;&igrave;&aring;&ograve;&egrave;&euml;&icirc; &#247;&ograve;&icirc;
&acirc;&agrave;&euml;&egrave;&auml;&agrave;&ouml;&egrave;&yuml;
&auml;&agrave;&iacute;&iacute;&ucirc;&otilde; &yuml;&acirc;&euml;&yuml;&aring;&ograve;&ntilde;&yuml;
&iacute;&aring;&icirc;&aacute;&otilde;&icirc;&auml;&egrave;&igrave;&ucirc;&igrave;
&yacute;&euml;&aring;&igrave;&aring;&iacute;&ograve;&icirc;&igrave;
&yacute;&ecirc;&ntilde;&iuml;&aring;&eth;&egrave;&igrave;&aring;&iacute;&ograve;&icirc;&acirc;.
&Yacute;&ograve;&icirc; &icirc;&aacute;&uacute;&yuml;&ntilde;&iacute;&yuml;&aring;&ograve;
&iuml;&icirc;&#247;&aring;&igrave;&oacute; &iuml;&icirc;&auml;&ntilde;&iuml;&oacute;&ograve;&iacute;&egrave;&ecirc;&icirc;&acirc;&ucirc;&aring;
&egrave;&ccedil;&igrave;&aring;&eth;&aring;&iacute;&egrave;&yuml;
&auml;&icirc;&euml;&aelig;&iacute;&ucirc; &aacute;&ucirc;&ograve;&uuml;
&auml;&aring;&ograve;&agrave;&euml;&uuml;&iacute;&icirc; &icirc;&iuml;&egrave;&ntilde;&agrave;&iacute;&ucirc;
&acirc; &Ograve;&Ccedil; &iacute;&agrave; &ecirc;&agrave;&aelig;&auml;&ucirc;&eacute;
&yacute;&ecirc;&ntilde;&iuml;&aring;&eth;&egrave;&igrave;&aring;&iacute;&ograve;.
&Iacute;&aring;&icirc;&aacute;&otilde;&icirc;&auml;&egrave;&igrave;&icirc;
&eth;&agrave;&ccedil;&eth;&agrave;&aacute;&icirc;&ograve;&agrave;&ograve;&uuml;
(&acirc; &ccedil;&agrave;&acirc;&egrave;&ntilde;&egrave;&igrave;&icirc;&ntilde;&ograve;&egrave;
&icirc;&ograve; &ograve;&egrave;&iuml;&agrave; &yacute;&ecirc;&ntilde;&iuml;&aring;&eth;&egrave;&igrave;&aring;&iacute;&ograve;&agrave;)
&iuml;&eth;&icirc;&ouml;&aring;&auml;&oacute;&eth;&oacute; &iuml;&aring;&eth;&aring;&auml;&agrave;&#247;&egrave;
&acirc;&ntilde;&aring;&otilde; &iuml;&icirc;&auml;&ntilde;&iuml;&oacute;&ograve;&iacute;&egrave;&ecirc;&icirc;&acirc;&ucirc;&otilde;
&auml;&agrave;&iacute;&iacute;&ucirc;&otilde; &acirc; &Egrave;&ETH;&Yacute;
&ETH;&Agrave;&Iacute; &icirc;&ograve; &oacute;&#247;&agrave;&ntilde;&ograve;&iacute;&egrave;&ecirc;&icirc;&acirc;
&iuml;&eth;&icirc;&aring;&ecirc;&ograve;&agrave; &quot;&Iuml;&eth;&egrave;&eth;&icirc;&auml;&agrave;&quot;
&icirc;&eth;&atilde;&agrave;&iacute;&egrave;&ccedil;&agrave;&ouml;&egrave;&egrave;
&iacute;&agrave;&oacute;&#247;&iacute;&icirc;&atilde;&icirc; &agrave;&eth;&otilde;&egrave;&acirc;&agrave;
&egrave; &iuml;&aring;&eth;&aring;&auml;&agrave;&ograve;&uuml;
&iuml;&eth;&aring;&auml;&euml;&icirc;&aelig;&aring;&iacute;&egrave;&yuml;
&acirc; &Egrave;&ETH;&Yacute; &ETH;&Agrave;&Iacute; &icirc;&auml;&iacute;&icirc;&acirc;&eth;&aring;&igrave;&aring;&iacute;&iacute;&icirc;
&ntilde; &iuml;&aring;&eth;&aring;&auml;&agrave;&#247;&aring;&eacute;
&ecirc;&icirc;&iacute;&ecirc;&eth;&aring;&ograve;&iacute;&ucirc;&otilde;
&Ograve;&Ccedil; &iacute;&agrave; &yacute;&ecirc;&ntilde;&iuml;&aring;&eth;&egrave;&igrave;&aring;&iacute;&ograve;&ucirc;.
<P>
9. &ETH;&agrave;&ccedil;&eth;&agrave;&aacute;&icirc;&ograve;&#247;&egrave;&ecirc;&egrave;
&agrave;&iuml;&iuml;&agrave;&eth;&agrave;&ograve;&oacute;&eth;&ucirc;
&auml;&icirc; 1 &igrave;&agrave;&eth;&ograve;&agrave; 1995&atilde;.
&iuml;&eth;&aring;&auml;&ntilde;&ograve;&agrave;&acirc;&euml;&yuml;&thorn;&ograve;
&acirc; &Egrave;&ETH;&Yacute; &ETH;&Agrave;&Iacute; &egrave; &ETH;&Ecirc;&Ecirc;
&quot;&Yacute;&iacute;&aring;&eth;&atilde;&egrave;&yuml;&quot;&icirc;&iuml;&egrave;&ntilde;&agrave;&iacute;&egrave;&aring;
&icirc;&aacute;&eth;&agrave;&aacute;&icirc;&ograve;&ecirc;&egrave;
&auml;&agrave;&iacute;&iacute;&ucirc;&otilde; &auml;&icirc; &oacute;&eth;&icirc;&acirc;&iacute;&yuml;
1&aacute;. &Yacute;&ograve;&egrave; &icirc;&iuml;&egrave;&ntilde;&agrave;&iacute;&egrave;&yuml;
&auml;&icirc;&euml;&aelig;&iacute;&ucirc; &acirc;&ecirc;&euml;&thorn;&#247;&agrave;&ograve;&uuml;
&icirc;&iuml;&egrave;&ntilde;&agrave;&iacute;&egrave;&aring; &egrave;&ntilde;&iuml;&icirc;&euml;&uuml;&ccedil;&icirc;&acirc;&agrave;&iacute;&iacute;&ucirc;&otilde;
&ocirc;&icirc;&eth;&igrave;&agrave;&ograve;&icirc;&acirc; &auml;&agrave;&iacute;&iacute;&ucirc;&otilde;
&agrave; &ograve;&agrave;&ecirc;&aelig;&aring; &egrave;&ntilde;&iuml;&icirc;&euml;&uuml;&ccedil;&oacute;&aring;&igrave;&ucirc;&aring;
&agrave;&euml;&atilde;&icirc;&eth;&egrave;&ograve;&igrave;&ucirc;
&ecirc;&agrave;&euml;&egrave;&aacute;&eth;&icirc;&acirc;&ecirc;&egrave;
&egrave; &iuml;&eth;&aring;&auml;&euml;&agrave;&atilde;&agrave;&aring;&igrave;&ucirc;&aring;
&agrave;&euml;&atilde;&icirc;&eth;&egrave;&ograve;&igrave;&ucirc;
&ecirc;&icirc;&eth;&eth;&aring;&ecirc;&ouml;&egrave;&egrave;.

<P>
10. &Ccedil;&agrave;&ntilde;&aring;&auml;&agrave;&iacute;&egrave;&aring;
&icirc;&auml;&icirc;&aacute;&eth;&egrave;&euml;&icirc; &iuml;&eth;&aring;&auml;&euml;&icirc;&aelig;&aring;&iacute;&egrave;&yuml;
&iuml;&icirc; &egrave;&ccedil;&igrave;&aring;&iacute;&aring;&iacute;&egrave;&yuml;&igrave;
&ntilde;&icirc;&ntilde;&ograve;&agrave;&acirc;&agrave; &Oacute;&#247;&aring;&iacute;&icirc;&atilde;&icirc;
&Ntilde;&icirc;&acirc;&aring;&ograve;&agrave;, &ETH;&agrave;&aacute;&icirc;&#247;&egrave;&otilde;
&atilde;p&oacute;&iuml;&iuml; &egrave; &Igrave;&aring;&aelig;&auml;&oacute;&iacute;&agrave;p&icirc;&auml;&iacute;&ucirc;&otilde;
&ecirc;&icirc;&icirc;&eth;&auml;&egrave;&iacute;&agrave;&ograve;&icirc;p&icirc;&acirc;.
(<A HREF="../priroda/app1.htm">&Iuml;&eth;&egrave;&euml;&icirc;&aelig;&aring;&iacute;&egrave;&aring; 7</A>).
<P>
&Icirc;&eth;&egrave;&atilde;&egrave;&iacute;&agrave;&euml;&uuml;&iacute;&ucirc;&eacute;
&yacute;&ecirc;&ccedil;&aring;&igrave;&iuml;&euml;&yuml;&eth;
&auml;&agrave;&iacute;&iacute;&icirc;&atilde;&icirc; &iuml;&eth;&icirc;&ograve;&icirc;&ecirc;&icirc;&euml;&agrave;
&iacute;&agrave; &agrave;&iacute;&atilde;&euml;&egrave;&eacute;&ntilde;&ecirc;&icirc;&igrave;
&yuml;&ccedil;&ucirc;&ecirc;&aring; &iuml;&icirc;&auml;&iuml;&egrave;&ntilde;&agrave;&iacute;
&ntilde;&euml;&aring;&auml;&oacute;&thorn;&ugrave;&egrave;&igrave;&egrave;
&iuml;&eth;&aring;&auml;&ntilde;&ograve;&agrave;&acirc;&egrave;&ograve;&aring;&euml;&yuml;&igrave;&egrave;
&ntilde;&ograve;&eth;&agrave;&iacute;: &Iacute;&agrave;&ccedil;&ucirc;&eth;&ntilde;&ecirc;&egrave;&eacute;
&Ograve;. (&Aacute;&icirc;&euml;&atilde;&agrave;&eth;&egrave;&yuml;)
&Oslash;&ograve;&agrave;&eacute;&iacute;&aacute;&icirc;&eth;&iacute;
&Acirc;.(&Atilde;&aring;&eth;&igrave;&agrave;&iacute;&egrave;&yuml;)
&Oslash;&agrave;&iacute;&egrave;&iacute; &Igrave;-&Euml;. (&Ocirc;&eth;&agrave;&iacute;&ouml;&egrave;&yuml;)
&THORN;&eth;&aring;&acirc;&egrave;&#247; &Agrave; (&Iuml;&icirc;&euml;&uuml;&oslash;&agrave;)
&Agrave;&eth;&igrave;&agrave;&iacute;&auml; &Iacute;.&Agrave;.
(&ETH;&icirc;&ntilde;&ntilde;&egrave;&yuml;) &Igrave;&yacute;&eth;&ocirc;&egrave;
&ETH;. (&Ntilde;&Oslash;&Agrave;).
<P>
<I>&Iacute;&agrave;&oacute;&#247;&iacute;&ucirc;&eacute; &eth;&oacute;&ecirc;&icirc;&acirc;&icirc;&auml;&egrave;&ograve;&aring;&euml;&uuml;
</I>
<P>
<I>&Igrave;&aring;&aelig;&auml;&oacute;&iacute;&agrave;&eth;&icirc;&auml;&iacute;&icirc;&atilde;&icirc;
