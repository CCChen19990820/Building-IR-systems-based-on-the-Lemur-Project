<AREA SHAPE="rect" COORDS="35,325,90,360"      target="_self" HREF="begin.htm">
<AREA SHAPE="rect" COORDS="0,0,60,360"      target="_self" HREF="leftbrik.htm">
<AREA SHAPE="rect" COORDS="190,55,245,400"  target="_self" HREF="ritebrik.htm">
