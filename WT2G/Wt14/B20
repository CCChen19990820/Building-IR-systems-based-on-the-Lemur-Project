<script language="Javascript">
<!--

	if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
		dtwpi_btn = new MakeArray(2)
		dtwpi_roll = new MakeArray(2)
		
		dtwpi_btn[1].src = "images/btns/d_01.gif"
		dtwpi_btn[2].src = "images/btns/d_02.gif"
	
		dtwpi_roll[1].src = "images/btns/d_01roll.gif"
		dtwpi_roll[2].src = "images/btns/d_02roll.gif"
	
		}
		
      function MakeArray(n) {
              this.length = n
              for (var i = 1; i<=n; i++) {
                      this[i] = new Image()
                      }
              return this
      }

	function msover(num) {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (dtwpi_roll[num].src != "") {
				document.images[num].src = dtwpi_roll[num].src }
			}
		}

	function msout(num) {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (dtwpi_btn[num].src != "") {
	                  		document.images[num].src = dtwpi_btn[num].src }
			}	            
		}

// -->

</script>
