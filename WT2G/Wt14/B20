      }

	function msover(num) {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (btalk_roll[num].src != "") {
				document.images[num].src = btalk_roll[num].src }
			}
		}

	function msout(num) {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (btalk_btn[num].src != "") {
	                  		document.images[num].src = btalk_btn[num].src }
			}	            
		}
		

// -->

</script>
</HEAD>

