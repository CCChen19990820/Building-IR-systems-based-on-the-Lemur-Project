		norm[10].src = "images/homies/btns/d.gif"
		norm[11].src = "images/homies/btns/d01.gif"
		norm[12].src = "images/homies/btns/d02.gif"
		norm[13].src = "images/homies/btns/e.gif"
		norm[14].src = "images/homies/btns/e01.gif"
		norm[15].src = "images/homies/btns/e02.gif"
		norm[16].src = "images/homies/btns/e03.gif"

		rollstate[2].src = "images/homies/btns/ag_roll.gif"
		rollstate[3].src = "images/homies/btns/dis_roll.gif"
		rollstate[6].src = "images/homies/btns/b01_roll.gif"
		rollstate[7].src = "images/homies/btns/b02_roll.gif"
		rollstate[8].src = "images/homies/btns/b03_roll.gif"
		rollstate[9].src = "images/homies/btns/b04_roll.gif"
		rollstate[11].src = "images/homies/btns/d01_roll.gif"
		rollstate[12].src = "images/homies/btns/d02_roll.gif"
		rollstate[14].src = "images/homies/btns/e01_roll.gif"
		rollstate[15].src = "images/homies/btns/e02_roll.gif"
		rollstate[16].src = "images/homies/btns/e03_roll.gif"
	
		}
		
      function MakeArray(n) {
              this.length = n
              for (var i = 1; i<=n; i++) {
                      this[i] = new Image()
                      }
              return this
      }

	function showjoke() {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (jokenum <= 4 ) {
				if (joke[jokenum].src != "") {
					document.images[1].src = joke[jokenum].src;
					jokenum = (jokenum + 1) }
				}
			else {
				jokenum = 1;
				showjoke();
				}
			}				
		}
		
	function showguests() {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (guests[1].src != "") {
					document.images[1].src = guests[1].src }
				}
		}

	function ovrbtn(num) {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (rollstate[num].src != "") {
				document.images[num + 3].src = rollstate[num].src }
			}
		}
		
	function lvbtn(num) {
		if (navigator.userAgent.substring(0,9) == "Mozilla/3") {
			if (norm[num].src != "") {
	                document.images[num + 3].src = norm[num].src }
			}	            
		}

// -->

</script>

