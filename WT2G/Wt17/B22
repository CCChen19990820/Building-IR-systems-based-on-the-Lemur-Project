<!--Hide from old browsers

// -- function submit query
function submit_query ()
{ document.ControlForm.submit (); }
// -- function onlyrr
function set_onlyrr (val)
{ if (val==0) { this.document.ControlForm.green.checked = false; } }
// -- function green
function set_green ()
{ this.document.ControlForm.onlyrr[0].checked = true; }

