<SCRIPT language=Javascript>
function ga_naar() {		 	if (document.taxiform.taxiselect.options[0].selected)		{open('bedrijf/index.htm','main');}	else if (document.taxiform.taxiselect.options[1].selected)		{open('winkel/index.htm','main');}	else if (document.taxiform.taxiselect.options[2].selected)		{self.location.href = "overheid/index.htm";}	else if (document.taxiform.taxiselect.options[3].selected)		{self.location.href = "vtijd/index.htm";}	else if (document.taxiform.taxiselect.options[4].selected)		{self.location.href = "plein.htm";}	else if (document.taxiform.taxiselect.options[5].selected)		{self.location.href = "bedrijf/aimedsol/index.htm";}	else if (document.taxiform.taxiselect.options[6].selected)		{self.location.href = "bedrijf/nieuws/nieuws.htm";}	else if (document.taxiform.taxiselect.options[7].selected)		{self.location.href = "bedrijf/info/info.htm";}	else if (document.taxiform.taxiselect.options[8].selected)		{self.location.href = "bedrijf/aanbied.htm";}	else if (document.taxiform.taxiselect.options[9].selected)		{self.location.href = "winkel/nieuws/nieuws.htm";}	else if (document.taxiform.taxiselect.options[10].selected)		{self.location.href = "winkel/info/info.htm";}	else if (document.taxiform.taxiselect.options[11].selected)		{self.location.href = "winkel/aanbied.htm";}		return true;	}</script>
<CENTER>

