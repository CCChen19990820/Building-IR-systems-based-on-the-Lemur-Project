	<INPUT TYPE="submit" NAME="seek now" VALUE="Submit">
	<INPUT TYPE=HIDDEN NAME="col" VALUE="WW">
	<INPUT TYPE=HIDDEN NAME="sv" VALUE="N2">
      </FORM>
    </TABLE>
    </CENTER>
