<!-- 
// Banner - Start
var formsize=70;
var bannerID,position=0,msg;
var basemsg="    AIMED Solutions heeft ondermeer gewerkt voor: Heineken, Samson H.D. Tjeenk Willink bv, BSO/Origin, Philips N.V., Hogeschool van Utrecht, European Journalism Centre, Dow Chemicals, Nederlands Bijbelgenootschap, Landis & Gyr, Stichting RegioWeb...     ";
 
function init_banner() {
    var i,k;
    
		msg=basemsg;
    if (basemsg.length >= formsize) {
					k=1;
    }
    else {
          k=(formsize/basemsg.length)+l;
    }
    for (i=0 ; i < k ; i++) {
          msg+=" "+basemsg;
    }
    bannerID=setTimeout("banner()",1000/50);
}
    
 function stopbanner() {
  self.status="";
 }

 function banner() {
    self.status=msg.substring(position,position+formsize);
    if (position++ == basemsg.length) {
          position=0;
    }
    bannerID=setTimeout("banner()",1000/50);
}
// Banner - end -->  
 </SCRIPT>


