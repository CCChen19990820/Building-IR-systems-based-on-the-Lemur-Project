<!--HIDE THE SCRIPT FROM OLD BROWSERS
var sec = 0
var imgsrc
var output = "<BR>"

function getsec(){
	var date = new Date()
	sec = date.getSeconds()
}

function getnum(){
	if (sec > 49){
	sec = sec - 50
}
else{
	if (sec > 39){
	sec = sec - 40
}
else{
	if (sec > 29){
	sec = sec - 30
}
else{
	if (sec > 19){
	sec = sec - 20
}
else{
	if (sec > 9){
	sec = sec - 10
}}}}}}

function display()
{
if (sec == 0){
	imgsrc = '<CENTER><A HREF="bedrijf/aimedsol/index.htm" TARGET="Resource Window"><img src="graphics/ad1.gif"></A></CENTER><HR>'
	output += imgsrc
}
else{
if (sec == 1){
	imgsrc = '<CENTER><A HREF="http://www.utr.bart.nl" TARGET="Resource Window"><img src="graphics/ad5.gif"></A></CENTER><HR>'
	output += imgsrc
}
else{
if (sec == 2){
	imgsrc = '<CENTER><A HREF="http://www.regioweb.nl:8000"><img src="graphics/ad4.gif"></A></CENTER><HR>'
	output += imgsrc
}
else{
if (sec == 3){
	imgsrc = '<CENTER><A HREF="prikbord/prikbord.htm"><img src="graphics/ad3.gif"></A></CENTER><HR>'
	output += imgsrc
