	    <SELECT NAME="reponses" SIZE="0">
	      <OPTION SELECTED>10
	      <OPTION>25
	      <OPTION>50
	      <OPTION>100
	    </SELECT>
	  </TD>
	  <TD ALIGN="right">
	    <TT>Langage : </TT>
	    <SELECT NAME="syntaxe" SIZE="0">
	      <OPTION>strict
	      <OPTION SELECTED>d&eacute;clin&eacute;
	    </SELECT>
	  </TD>
	</TR>
	<TR>
	  <TD ALIGN="left" COLSPAN="2">
	    <TT>Contenant </TT>
	    <SELECT NAME="logique" SIZE="0">
	      <OPTION SELECTED>au moins un
	      <OPTION>tous les
	    </SELECT>
	    <TT> mot(s) cl&eacute;.</TT>
	  </TD>
	  <TD ALIGN="right">
