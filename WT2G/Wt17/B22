<html>
<head>
   <title>Bestellen van WISE producten</title>
<script>
var subtotal=0
function update(form)
{
	form.fldTotal.value    =(form.fldQty1.value * 373.62)+(form.fldQty2.value * 299.00)+(form.fldSub.value * 1)+(form.fldTax.value * 1)
}
function multiply(form)
{
		form.fldExtens1.value = form.fldQty1.value * 373.62
		form.fldExtens2.value = form.fldQty2.value * 299.00
	
		subtotal=	parseFloat(form.fldExtens1.value) + parseFloat(form.fldExtens2.value)
		checkTax(form)
}
function checkTax(form)
{
	if (form.fldCT.checked)
		form.fldTax.value = eval(subtotal*0.175)
    	else form.fldTax.value = 0;
	update(form)

	if(form.fldCT.checked)
		form.fldSub.value=25.00;
	else form.fldSub.value=35.00;
    update(form)
}
//Validation procedures for strings
function CheckEmail(str, min, max)
{
	var foundamper=false
	var foundot=false
	var i=0
	if(str.length < min || str.length > max)
		return false
	while( i < str.length ) {
   		var ch=str.substring(i,i+1)
		if(ch=="@")
			foundamper=true
		if(ch==".")
			foundot=true
	i ++;
	}
	if (foundamper && foundot) 
		return true
	else 
		return false
}

function CheckAlpha(str, min, max)
{
	if(str.length < min || str.length > max)
		return false
	for (var i=0; i < str.length; i++) {
		var ch=str.substring(i,i+1)
		if(ch>="0" && ch<="9") {
			return false
		}
	}
	return true
}
function CheckNum(str, min, max)
{
	if(str.length < min || str.length > max)
		return false
	for (var i=0; i<str.length; i++) {
		var ch=str.substring(i,i+1)
		if(ch<"0" && ch>"9" && ch!="-") {
			return false
		}
	}
	return true
}
//Check all fields in form
function checkForm(form)
{
	if(form.fldTotal.value =="" || form.fldTotal.value =="0") {
		alert("U moet eerst een bestelling plaatsen voordat u het formulier kunt opsturen!")
		return false
	}
	if(!CheckAlpha(form.fldName.value, 1, 65)) {
		alert("U dient een geldige bedrijfsnaam in te vullen")
		return false
	}
	if(!CheckEmail(form.fldEmail.value, 1, 30)) {
		alert("U moet een geldig email adres invullen")
		return false
	}
	if (form.fldAddress.value == "") {
		alert("Dit is een bestelformulier. U moet natuurlijk wel een adres invullen anders kunnen we de goederen niet opsturen!")
		return false
	}
	if (!CheckAlpha(form.fldCity.value, 1, 65)) {
		alert("Ongeldige plaatsnaam")
		return false
	}

	if(!CheckNum(form.fldZip.value,5,10)) {
		alert("Ongeldige Postcode")
		return false
	}
	return true
}

</script>
</head>
