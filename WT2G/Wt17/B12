<AREA SHAPE=RECT COORDS="0,231,101,252" HREF=/public/current/articles/USStocks.htm>
<AREA SHAPE=RECT COORDS="0,253,101,272" HREF=/public/current/articles/SmallUSStocks.htm>
<AREA SHAPE=RECT COORDS="0,273,100,306" HREF=/public/current/articles/AmericasRoundup.htm>
<AREA SHAPE=RECT COORDS="0,307,100,328" HREF=/public/current/articles/AsiaRoundup.htm>
<AREA SHAPE=RECT COORDS="0,329,100,348" HREF=/public/current/articles/EuropeRoundup.htm>
<AREA SHAPE=RECT COORDS="0,349,100,382" HREF=/public/current/articles/HeardOnTheStreet.htm>
<AREA SHAPE=RECT COORDS="0,382,100,403" HREF=/public/current/articles/CreditMarkets.htm>
