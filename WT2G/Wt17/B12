<AREA SHAPE=RECT COORDS="0,506,101,525" HREF=/public/resources/documents/toc.htm>
<AREA SHAPE=RECT COORDS="16,1,84,54" HREF=/public/resources/documents/toc.htm>
</map>




<MAP NAME="toolbar.cmp">
<AREA SHAPE=RECT COORDS="46,0,99,34" HREF=/public/resources/documents/new.htm>
