<MAP NAME="helpmap.cmp">
<AREA SHAPE=RECT COORDS="0,0,98,79" HREF=/>
<AREA SHAPE=RECT COORDS="0,80,98,100" HREF=/tour.htm>
<AREA SHAPE=RECT COORDS="0,100,99,164" HREF=/regchoice.html>
<AREA SHAPE=default HREF=/>
</MAP>





