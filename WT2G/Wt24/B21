	<!-- META TAGS FOR SEARCH ENGINES -->
        <meta name="Duke University Graduate School" content="The Duke University Graduate School is one of the premier graduate institutions of higher learning in the country">
        <meta name="keywords" content="Duke University Graduate School Education Higher Education College Colleges North Carolina Durham Triangle Research Triangle Park Raleigh">
        <meta name="distribution" content="global">


<SCRIPT>
	var timerID = null;
	var imagepath = "/duke/images/";	// image path
        var imageIDX=1;
	var textIDX=1;
	var imageMax=7;
	var imageName='hubimage';
	var textName='hubitext';
	var running=false;

	function changeImage(imgindex, imageN) {
	        document.images[imgindex].src = imagepath + imageN;
		}
	function checkalert() {
		alert("help me!");
		}
	function nextImage(direction) {
		if(direction==1)
			{
			if(imageIDX==imageMax)
				{
				imageIDX=1;
				}
			else{
				imageIDX++;
				}
			}
		if(direction==0)
			{
			if(imageIDX==1)
				{
				imageIDX=7;
				}
			else{
				imageIDX--;
				}
			}
	document.imgHub.src = imagepath+imageName+imageIDX+'.jpg';
	document.imgTitle.src = imagepath+textName+imageIDX+'.gif';
	clearTimeout(timerID);
	timerID=setTimeout("goNextImage()",12000);
	//document.imgAbout.src=imagepath+textName+imageIDX+'.gif';
	}

	function goNextImage(){
		nextImage(1);
		}
</SCRIPT>

</HEAD>

