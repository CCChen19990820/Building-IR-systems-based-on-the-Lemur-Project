	405.	AR	04	0.5%	-0.84
	406.	LA	07	0.5%	-0.84
	407.	SC	05	0.5%	-0.84
	408.	WV	02	0.4%	-0.85
	409.	VA	09	0.4%	-0.85
	410.	OK	03	0.4%	-0.85
	411.	NC	05	0.4%	-0.85
	412.	WV	03	0.4%	-0.85
	413.	SC	03	0.4%	-0.85
	414.	AL	03	0.4%	-0.85
	415.	IL	19	0.4%	-0.87
	416.	TN	08	0.4%	-0.87
	417.	IN	02	0.4%	-0.87
	418.	KY	04	0.4%	-0.87
	419.	IN	09	0.4%	-0.87
	420.	OH	06	0.4%	-0.87
	421.	SC	06	0.3%	-0.88
	422.	MO	08	0.3%	-0.88
	423.	TN	01	0.3%	-0.88
	424.	MS	03	0.3%	-0.88
	425.	KY	01	0.3%	-0.88
	426.	MS	04	0.3%	-0.88
	427.	NC	01	0.3%	-0.88
	428.	OK	02	0.3%	-0.89
	429.	AR	01	0.3%	-0.90
	430.	LA	04	0.3%	-0.90
