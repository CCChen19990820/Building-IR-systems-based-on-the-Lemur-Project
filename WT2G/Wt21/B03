<TITLE>Southern California Gas</TITLE>

<SCRIPT LANGUAGE="JavaScript">


<!-- Beginning of JavaScript Applet -------------------

/* Copyright (C)1996 Web Integration Systems, Inc. DBA Websys, Inc.
   All Rights Reserved.


   Chris Skinner, January 30th, 1996.
*/

function scrollit_r2l(seed)
{


        var m1  = "Welcome to the Southern California Gas Company World's Fair.               ";
        var m2  = "You have entered an environment of clean-energy technology.                         ";
        var m3  = "From Natural Gas Vehicles to Residential Services,   "; 
        var m4  = "you'll find it here.                                                      ";
        var m5  = "Don't forget to tour our Energy Resource Center....It's the first Exhibition Hall on the left.      ";
        var m6  = "                     ";

        
        var msg=m1+m2+m3+m4+m5+m6;
        var out = " ";
        var c   = 1;

        if (seed > 100) {
                seed--;
                var cmd="scrollit_r2l(" + seed + ")";
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 100 && seed > 0) {
                for (c=0 ; c < seed ; c++) {
                        out+=" ";
                }
                out+=msg;
                seed--;
                var cmd="scrollit_r2l(" + seed + ")";
                    window.status=out;
                timerTwo=window.setTimeout(cmd,100);
        }
        else if (seed <= 0) {
                if (-seed < msg.length) {
                        out+=msg.substring(-seed,msg.length);
                        seed--;
                        var cmd="scrollit_r2l(" + seed + ")";
                        window.status=out;
                        timerTwo=window.setTimeout(cmd,100);
                }
                else {
                        window.status=" ";
                        timerTwo=window.setTimeout("scrollit_r2l(100)",75);
                }
        }
}
// -- End of JavaScript code -------------- -->


</SCRIPT>
</head>
<BODY onLoad="timerONE=window.setTimeout('scrollit_r2l(100)',500);"> 

</HEAD>
