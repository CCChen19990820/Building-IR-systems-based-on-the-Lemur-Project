<TR><TD ALIGN=CENTER ROWSPAN=2>Class A & B</TD><TD ALIGN=CENTER>21 & over</TD><TD ALIGN=CENTER>60</TD><TD ALIGN=CENTER>$15</TD></TR>
<TR><TD ALIGN=CENTER>60 & over</TD><TD ALIGN=CENTER>every 5 years</TD><TD ALIGN=CENTER>$10</TD></TR>
<TR><TD ALIGN=CENTER ROWSPAN=2>Class C</TD><TD ALIGN=CENTER>21 & over</TD><TD ALIGN=CENTER>60</TD><TD ALIGN=CENTER>$10</TD></TR>
<TR><TD ALIGN=CENTER>60 & over</TD><TD ALIGN=CENTER>every 5 years</TD><TD ALIGN=CENTER>$10</TD></TR>
